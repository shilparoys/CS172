.cardstrip-container .carousel-container.tablet {
  overflow-x: scroll !important;
  overflow-y: hidden !important;
  -webkit-overflow-scrolling: touch;
}

.cardstrip-container .carousel-container.smartphone ul,
.cardstrip-container .carousel-container.tablet ul {
  padding: 0 10px;
}

.cardstrip-container .carousel-container.smartphone .cardstrip-btn-container,
.cardstrip-container .carousel-container.tablet .cardstrip-btn-container {
  visibility: hidden;
}
</style> 
