   <script type="text/javascript">                
                jQuery(document).ready(function($) {
                    var school_autocomplete_ajax;
                
                    if(_kmq){
                        _kmq.push(['record', 'Viewed homepage']);
                    }

                    $(document).delegate('#school-search-auto-complete ul li', 'mouseenter', function() {
                        $(this).addClass('selected');
                        
                        $(this).bind('mouseleave', function() {                
                            $(this).removeClass('selected');
                        });
                    });

                    $(document).delegate('#school-search-auto-complete ul li', 'click', function() {
                        $('#tbSchoolname').val($(this).text());
                        $('#school-search-auto-complete').hide();
                    });
                    
                    $('#tbSchoolname').keypress(function(e) {
                        if (e.keyCode == 13) {
                            e.preventDefault();
                        } else if (this.value.length > 3) {
                            var result = [];
                            if (typeof school_autocomplete_ajax != "undefined") {
                                school_autocomplete_ajax.abort();
                            }
                            
                            setTimeout(function() {
                                school_autocomplete_ajax = $.ajax({
                                    url: "/ajax/schoolsearch.aspx",
                                    global: false,
                                    type: "post",
                                    dataType: "html",
                                    data: { query: $('#tbSchoolname').val() },
                                    success: function(response) {
                                        $('#school-search-auto-complete').html(response);

                                        if (response == "<ul></ul>" || response == "")
                                            $('#school-search-auto-complete').hide();
                                        else
                                            $('#school-search-auto-complete').show();

                                        $('body').click(function() {
                                            $('#school-search-auto-complete').hide();
                                        });
                                    }
                                });
                            }, 300)
                        }
                    });


    
                    $('#explore_slide').slidesjs({
                        width: 660,
                        height: 310,
                        navigation: true,
                        play: {
                            active: true,
                            auto: true,
                            interval: 8000,
                            swap: false,
                            pauseOnHover: true
                        },
                        effect: {
                          slide: {
                            // Slide effect settings.
                            speed: 2000
                              // [number] Speed in milliseconds of the slide animation.
                          }
                        }                        
                    });

                    $('.slidesjs-pagination-item').click(function() {
                        $('.slidesjs-stop').click();
                    });
      
                
                    //validate inputs

                    $('#btn_explore').click(function(){

                    
                        var school_name = $('#tbSchoolname').val();
                        
                        error_messages = []
                        if (school_name == 'School' || school_name == ''){
                            error_messages.push("Enter your school name.");
                        }

                        var major_id = $('#ddMajor').val();
                        if (major_id == '' || major_id == '0'){
                            error_messages.push("Select your major.");
                        }
                        
                        var grad_year = $('#ddYear').val();
                        if(grad_year == "0"){
                            error_messages.push("Select your grad year");
                        }
                        var grad_month = $('#ddMonth').val();
                        if(grad_month == "0"){
                            error_messages.push("Select your grad month");
                        }
                        
                        
                        if (error_messages.length > 0){
                            $('#validation_messages').hide().html(error_messages.join("<br />")).fadeIn('fast');
                            return;                       
                        }
                        
                        if(_kmq) {
                            _kmq.push(['record', 'Submit home page education info']);
                        }

                        window.location = '/explore/?major_id='+major_id+'&grad_year='+grad_year+'&grad_month='+grad_month+'&school_name='+encodeURIComponent(school_name);
                    });
                    
                    school_name_control = $('#tbSchoolname');
                    school_name_control.val('School').css({'color': '#BBBBBB'});
                
                    school_name_control.focus( function() {
                        if ($(this).val() == 'School') {
                            $(this).css({'color': '#333333'});
                            $(this).val("");
                        }
                    });

                    school_name_control.blur( function() {
                        if ($(this).val() == "") {
                            $(this).css({'color': '#acb2b8'});
                            $(this).val('School');
                        }
                    });
                    
                    $.each($('select'), function(){
                        if($(this).val() == "0"){
                            $(this).css('color','#acb2b8');
                            $.each($(this).children(), function(index, value){
                                $(value).css('color','#333333');
                            });
                        }
                    });
                    
                    $('select').change( function(){
                        if($(this).val() == "0"){
                            $(this).css('color','#acb2b8');
                            $.each($(this).children(), function(index, value){
                                $(value).css('color','#333333');
                            });
                        }
                        else{
                            $(this).css('color','#333333');
                        }
                    });
                });
            </script> 
