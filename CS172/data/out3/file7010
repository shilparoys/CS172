      $('select[name="studytype"]').change(function() { // When make changes
            var options = '<option>Select Programme!</option>';
            $.each(roles[$(this).val()] || [], function(i, v) { // Cycle through each associated role
                  options += '<option>' + v + '</option>';
            });
            $('select[name="programme"]').html(options); // And update the role options
      });
});

</script> 
