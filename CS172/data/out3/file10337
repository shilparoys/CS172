function tButton_over(){	
  var img =document.getElementById("TButton");
	img.src = "../BSL_template_img/Twitter_logo_blue.png";
}

function tButton_out(){	
  var img =document.getElementById("TButton");
	img.src = "../BSL_template_img/Twitter_logo_white.png";
}
function ytButton_over(){	
  var img =document.getElementById("YTButton");
	img.src = "../BSL_template_img/YouTube_red_color_icon.png";
