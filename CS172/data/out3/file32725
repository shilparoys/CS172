      <h1 class="documentFirstHeading">A Unique World of Opportunity</h1> 
     </div> 
     <div class="span-16 allPages last"> 
      <!-- *************************Slide Show************************* --> 
      <div class="slideshowwrap"> 
       <!-- "previous slide" button --> 
