  <script>

function ShowHelp()
{
	
	alert("");
	
}

function CheckEmailField(whichField)
{
	if (whichField == 'f_Email')
		var s = document.DynaForm.f_Email.value;
	else
		return true;
	
	
		if (s == '') return true;
		
	
	var errMsg = '';
	
	// there must be >= 1 character before @, so we
    // start looking at character position 1 
    // (i.e. second character)
    var i = 1;
    var sLength = s.length;

    // look for @
    while ((i < sLength) && (s.charAt(i) != "@"))
    { i++
    }

    if ((i >= sLength) || (s.charAt(i) != "@"))
	{
		errMsg += 'You must have characters before an @\n\n';	
    }
	else i += 2;

    // look for .
    while ((i < sLength) && (s.charAt(i) != "."))
    { i++
    }

    // there must be at least one character after the .
    if ((i >= sLength - 1) || (s.charAt(i) != "."))
	{
		errMsg += 'You must have characters after the dot\n\n';	
    }

    alert(s.indexOf(' '));
	
	if (s.indexOf(' ') != 0)
	{
		errMsg += 'Address can not contain spaces.\n\n';	
    }

   	if (errMsg != '') 
	{
		errMsg = 'Please enter a correctly formatted email address.\n\n' + errMsg;
		alert(errMsg);
		document.DynaForm.f_Email.focus();
		return false;
    }

    return true;
}

function CheckEmailFormat(whichField)
{
	var emptyStr = '';
	var errorStr = 'Email address\n';
	var errMsg = '';
	
	if (whichField == 'f_Email')
		var s = document.DynaForm.f_Email.value;
	else
		return emptyStr;
	
	// there must be >= 1 character before @, so we
    // start looking at character position 1 
    // (i.e. second character)
    var i = 1;
    var sLength = s.length;

    // look for @
    while ((i < sLength) && (s.charAt(i) != "@"))
    { i++
    }

    if ((i >= sLength) || (s.charAt(i) != "@"))
	{
		errMsg += 'You must have characters before an @\n\n';	
    }
	else i += 2;

    // look for .
    while ((i < sLength) && (s.charAt(i) != "."))
    { i++
    }

    // there must be at least one character after the .
    if ((i >= sLength - 1) || (s.charAt(i) != "."))
	{
		errMsg += 'You must have characters after the dot\n\n';	
    }

	if (s.indexOf(' ') != -1)
	{
		errMsg += 'Address can not contain spaces.\n\n';	
    }


   	if (errMsg == '') 
	{
		return emptyStr;
	}
	else	
	{
		errMsg = 'Please enter a correctly formatted email address.\n\n' + errMsg + ' ';
		// alert(errMsg);
		// document.DynaForm.f_Email.focus();
		return errMsg;
    }
}


String.prototype.trim = function() 
{
	return this.replace(/^\s*/,"").replace(/\s*$/,"");
}

function AutoFill(fld)
{
	return;
	
	for (var i = 0; i < lstArray.length; i++) 
	{
		if (lstArray[i][0].toLowerCase().trim() == fld.value.toLowerCase().trim())
		{
			fld.value = lstArray[i][1];
		}	
	}	
}

function valRadio(radiobutton) 
{
	theButton = '';
	if (radiobutton)
	{
		if (radiobutton.length == null) 
		{ // Only one radiobutton 
			if (radiobutton.checked)
				theButton = radiobutton.value;
		}
		else	 
		{
			for (i=0; i< radiobutton.length; i++) 
			{
				if (radiobutton[i].checked) 
					theButton = radiobutton[i].value;
			}
		}
	}
	// alert(radiobutton + " results = " + theButton);
	return theButton;
} 

function valCheck(checkbox) 
{
	
	var theBox = '';
	if (checkbox)
	{
		if (checkbox.length == null) 
		{ // Only one checkbox 
			if (checkbox.checked)
				theBox = checkbox.value;
		}
		else	 
		{
			for (i=0; i< checkbox.length; i++) 
			{
				if (checkbox[i].checked) 
					theBox += checkbox[i].value + ',';
			}
		}		
	}
	// alert("checkbox results = " + theBox);
	return theBox;
} 


	function FillUs()
	{
		if (document.DynaForm.f_Mail_From_Name.value == 'rb')
		{
			document.DynaForm.f_Mail_From_Name.value = 'Ric Bailey';
			document.DynaForm.f_Mail_From_Email.value = 'rbailey@plymouthrocket.com';
		}
		else if (document.DynaForm.f_Mail_From_Name.value == 'js')
		{
			document.DynaForm.f_Mail_From_Name.value = 'Stewart';
			document.DynaForm.f_Mail_From_NameFirst.value = 'Jim';
			document.DynaForm.f_Email.value = 'jstewart@plymouthrocket.com';
		}
	}



function CheckQty(which)
{
	var eMsg = '';
	if (which == '1')
	{
		var theVal = document.DynaForm.f_Qty1.value;
		var preMsg = ' ';
	}
	else
	{
		var theVal = document.DynaForm.f_Qty2.value;
		var preMsg = ' ';
	}
	
	if (isNaN(theVal))
	{
		eMsg = preMsg + ' must be a number';
	}	
	else
	if (parseInt(theVal) != theVal)
	{
		eMsg = preMsg + 'must be an integer';
	}	
	else
	if (theVal < 0)
	{
		eMsg = preMsg + 'must not be less than zero';
	}	
	return eMsg;
}	


function ShowWaitListButton()
{
	alert("Show Wait List Button");
}

function CheckEditValues()
{
	var missingData = '';
	var badEmailStr = '';
	var badQty1Str = '';
	var badQty2Str = '';

	var f = document.DynaForm;
	
	var processMsg = "<i>Submitting form - please wait a moment...TEST</i>";
	var retryMsg = "Please correct your information and try again";
	var waitButtonMsg = "<br><br>Or click on the Wait List button below.";
	document.getElementById("STATUS_DIV").innerHTML = processMsg;
	
	
	document.getElementById("STATUS_DIV").innerHTML = '1013';
	
	
	
	if (document.DynaForm.f_Phone.value.length > 20)
	{
		alert("Phone number can not be more than 20 characters");
		document.getElementById("STATUS_DIV").innerHTML = retryMsg;
		document.getElementById("SEND_BUTTON").disabled = false;
		return false;
 	}
	
	
		if (f.f_Mail_From_Name.value == '')
		{
			missingData += 'Name (Last) *\n';
		}
		if (f.f_Mail_From_NameFirst.value == '')
		{
			missingData += 'Name (First) *\n';
		}
	
	if (f.f_Password.value == 'PASSWORD')
		f.f_Password.value = 'DELCO';
	
	
	if (document.DynaForm.f_Email.value != '')
	{
		var badEmailStr = CheckEmailFormat('f_Email');
	}
	
	if (document.DynaForm.f_Qty1.value != '')
	{
		badQty1Str = CheckQty(1);
	}

	if (document.DynaForm.f_Qty2.value != '')
	{
		badQty2Str = CheckQty(2);
	}
		
		document.getElementById("STATUS_DIV").innerHTML = '1223';

	if ((missingData == '') && (badEmailStr == '') && (badQty1Str == '') && (badQty2Str == ''))
	{
		// alert("1222");
		var q1 = parseInt(f.f_Qty1.value);
		var q2 = parseInt(f.f_Qty2.value);
		
		
			var qTot = q1;
		
		if (qTot < 1)
		{
			alert("You need to register at least one person.");
			document.getElementById("STATUS_DIV").innerHTML = retryMsg;
			document.getElementById("SEND_BUTTON").disabled = false;
			return false;
		}
		else	
		if (qTot > 1000)
		{
			alert("You may only register 1000 people with this form.");
			document.getElementById("STATUS_DIV").innerHTML = retryMsg;
			document.getElementById("SEND_BUTTON").disabled = false;
			return false;
		}
		else	
		
		if (qTot > 4901)
			
				{
					alert("There are currently 4901 registration slot(s) available for this event.\n\nYour request for " + qTot + " of them can not be processed at this time\n\nYou may decrease the number of registrants or cancel your request.");
					document.getElementById("STATUS_DIV").innerHTML = retryMsg + waitButtonMsg;
					document.getElementById("SEND_BUTTON").disabled = false;
					return false;
				}
			
		
		else	
		if (qTot > 9901)
		{
			alert("There are a total of 9901 slot(s) available (including the waiting list).\n\nYou may not request " + qTot + " of them");
			document.getElementById("STATUS_DIV").innerHTML = retryMsg;
			document.getElementById("SEND_BUTTON").disabled = false;
			return false;
		}
		else
		{
			// alert("1305");
			// return true;
		}
			
	}
	else 
	{
		var badStr = '';
		
		if (missingData != '') 
		{
			badStr += 'Required field(s):\n\n' + missingData;
		}
		
		if ((badEmailStr != '') || (badQty1Str != '') || (badQty2Str != ''))
		{
			badStr += '\nIncorrect format(s):\n\n';
			if (badEmailStr != '')
				badStr += badEmailStr;
			if (badQty1Str != '')
				badStr += badQty1Str;
			if (badQty2Str != '')
				badStr += badQty2Str;
		}
						
		alert (badStr);
		document.getElementById("STATUS_DIV").innerHTML = retryMsg;
		document.getElementById("SEND_BUTTON").disabled = false;
		return false;				
	}
	
	// alert("CheckEditValues is returning true");
	document.getElementById("STATUS_DIV").innerHTML = 'Completed checking field values - submitting form';
	return true;
}



</script> 
  <script>

	function ListToFields(thisElement, aList)
	{	
		var tOffset = thisElement + 1;
		var ListArray = new Array();
		var f = document.DynaForm;
		var newValues = aList.options[aList.selectedIndex].value;
		ListArray = newValues.split("%");
		// alert(ListArray.length);
		for (j=0; j<ListArray.length; j++)
		{
			e = j + tOffset;
			eval('f.DF_ELEM_' + e).value = ListArray[j];
		}	
	}

	function FillNextField(thisElement, aList)
	{	
		var tOffset = thisElement + 1;
		var ListArray = new Array();
		var f = document.DynaForm;
		var newValues = aList.options[aList.selectedIndex].value;
		ListArray = newValues.split("%");
		if (ListArray.length > 1)
			eval('f.DF_ELEM_' + tOffset).value = ListArray[1];
		else
			eval('f.DF_ELEM_' + tOffset).value = ListArray[0];
	}

	function FillNextList(thisElement, aList)
	{	
		var tOffset = thisElement + 1;
		var ListArray = new Array();
		var f = document.DynaForm;
		var MasterValue = aList.options[aList.selectedIndex].value;
		// alert("Master Value = " + MasterValue);

		var newListOptions = eval(MasterValue);
		// alert ("newListOptions = " + newListOptions); 
		
		// e2 = 'f.DF_ELEM_' + tOffset;
   		var libArray = newListOptions.split("|");
		
		eval('f.DF_ELEM_' + tOffset).length = 0;
		
	   	for (i=0; i<libArray.length; i++)
	   	{
		  	eval('f.DF_ELEM_' + tOffset).options[i] = new Option(libArray[i],libArray[i],false,false);
	   	}	  
	}

	function KeyListCascade()
	{	
		
		var f = document.DynaForm;
		var list1 = f.Key1List;
		var list2 = f.Key2List;
		
		var MasterValue = list1.options[list1.selectedIndex].value;
		if (MasterValue != '')
		{
			// alert("MasterValue = " + MasterValue);
			
			var newListOptions = eval(MasterValue);
			// alert("newListOptions = " + newListOptions);
			
   			var libArray = newListOptions.split("|");
		
			list2.length = 0;
	   		for (i=0; i<libArray.length; i++)
		  		list2.options[i] = new Option(libArray[i],libArray[i],false,false);
		}
		
		var elem = document.getElementById('KEYFIELD2');

		if (MasterValue.indexOf("_SHOWFIELD") == -1)
	        elem.style.visibility = 'hidden';
		else
		    elem.style.visibility = 'visible';
		
		f.f_Key2.value = '';
		return true;
	}

</script>   
