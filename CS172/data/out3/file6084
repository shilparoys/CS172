      <div class="align-center"> 
       <h1>2014-15 Alfred State Baseball Roster</h1> 
      </div> 
      <div class="roster"> 
       <table cellpadding="0" cellspacing="1" width="100%"> 
        <thead> 
         <tr class="roster-header"> 
          <td class="col1"> <a href="/sports/bsb/2014-15/roster?sort=number" title="Sort using this column">No.</a> </td> 
          <td class="col2"> <a href="/sports/bsb/2014-15/roster?sort=last_name" title="Sort using this column">Name</a> </td> 
          <td class="col3"> Pos. </td> 
          <td class="col4"> B/T </td> 
          <td class="col5"> Cl. </td> 
          <td class="col6"> Ht. </td> 
          <td class="col7"> Hometown/High School </td> 
         </tr> 
        </thead> 
        <tbody> 
         <tr class="roster-row0"> 
          <td data-title="No." class="number"> 1 </td> 
          <td data-title="Name" class="name"> <a href="/sports/bsb/2014-15/bios/mora_pedro_ows6" title="Player's Bio">Pedro Mora</a> </td> 
          <td data-title="Pos."> UTL </td> 
          <td data-title="B/T"> R/R </td> 
          <td data-title="Cl."> So. </td> 
          <td data-title="Ht."> 6-2 </td> 
