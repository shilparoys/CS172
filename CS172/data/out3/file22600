<!--

function getQueryStringParam(key) {
	qs = window.location.search.substring(1);
	qsArray = window.location.search.substring(1).split("&");
	for (i=0;i<qsArray.length;i++) {
		qsItem = qsArray[i].split("=");
		if (qsItem[0] == key) {
			return qsItem[1];
		}
	}
}
