  <script type="text/javascript">
  $(document).ready(function() {
    var channelId = '', options = { showDescription: true, showRelated: true }, parts = null, part = null;
    if (window.location.hash) {
      parts = window.location.hash.substring(1).split('/');
      while (parts.length > 0 && parts[0].length === 0) {
        parts.shift();
      }
      channelId = parts.shift();
      while (parts.length > 0) {
        part = parts.shift();
        if (part.substr(0, 1) === '~') {
          options.state = part;
        } else {
          options.ticketId = part;
        }
      }
    }
    options.onLoadBroadcast = function(broadcast) {
      $('h1.interior-headline').text(broadcast.name);
    };
    boxcast('.video-container').loadChannel(channelId, options);
  });
</script> 
