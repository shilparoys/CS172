  <script type="text/javascript" src="http://www.drew.edu/theological/wp-includes/js/jquery/ui/core.min.js?ver=1.11.2"></script> 
  <script type="text/javascript" src="http://www.drew.edu/wp-content/themes/drew-v8-theo-r/assets/script.js?ver=1417735677"></script> 
  <script type="text/javascript" src="http://www.drew.edu/theological/wp-includes/js/jquery/ui/widget.min.js?ver=1.11.2"></script> 
  <script type="text/javascript" src="http://www.drew.edu/theological/wp-includes/js/jquery/ui/tabs.min.js?ver=1.11.2"></script> 
  <script>
				$(function(){
					$("#tabs1492163306").tabs({
						create: function( event, ui ) {
							window.setTimeout(checkmenu, 100);
						}
					});

					if (window.location.hash && $(window.location.hash).length){
						$("#tabs1492163306").tabs("select", window.location.hash);
						window.scrollTo(0, $("#tabs1492163306").offset().top );
					}

					//make the tab into a sliders
					if(jQuery().touchslider) $("#tabs1492163306>ul:first").touchslider();
					if(jQuery().hammer) {
						$("#tabs1492163306>div").hammer({
								drag_vertical: false
								,css_hacks:false
								,hold:false
								,drag:false
								,transform:false
								,tap_double:false
								,tap:false
							}).bind("swipe", function(ev){
								ev.stopPropagation();
								var active = $("#tabs1492163306").tabs( "option", "active" );
								var total = $("#tabs1492163306 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs1492163306").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs1492163306").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script> 
