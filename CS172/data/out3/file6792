    $(window).resize(function () {
        if ($(this).data('gatimer')) {
            clearTimeout($(this).data('gatimer'));
            $(this).data('gatimer', null);
        }
        $(this).data(
            'gatimer',
            setTimeout(function () {pageTracker._trackEvent('Browser Dimensions', 'resize', $(window).width() + 'x' + $(window).height(), $(window).width());},500)
        );
    });
});
//--></script> 
