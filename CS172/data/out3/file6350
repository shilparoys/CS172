      <script type="text/javascript">

	function validEmailAddress(emailAddress) {
	    var pattern = new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);
	    return pattern.test(emailAddress);
	}

	function validNumber(e){
		var key;
		var keyChar;
		if (window.event)
		   key = window.event.keyCode;
		else if (e)
		   key = e.which;
		else
		   return true;

		keychar = String.fromCharCode(key);
		// control keys
		if ((key==null) || (key==0) || (key==8) ||
		    (key==9) || (key==13) || (key==27) )
		   return true;

		// numbers
		else if ((("0123456789").indexOf(keychar) > -1))
		   return true;

		return false;
	}
	
	$(document).ready(function() {
	
	    $(document).on("click", "#add", function() {
	    	var intId = $(".fieldwrapper").length + 1;
	        var fieldWrapper = $("<div class=\"fieldwrapper\" />");
	        var fName = $("<input type=\"text\" name=\"email\" />");
	        var removeButton = $("<input type=\"button\" class=\"remove\" value=\"  -  \" />");
	        removeButton.click(function() {
	            $(this).parent().remove();
	        });

	        fieldWrapper.append("<label>Email address</label> ");
	        fieldWrapper.append(fName);
	        fieldWrapper.append("&nbsp;");
	        fieldWrapper.append(removeButton);
	        $(".fieldwrapper:last").after(fieldWrapper);
	    });
	    var request;
		$("#shareSubmissionForm").submit(function(event){
			// abort any pending request
		    if (request) {
		        request.abort();
		    }
		    var $form = $(this);
		    var serializedData = $form.serialize();
		    var $inputs = $form.find("input, select, button, textarea");
		    $inputs.prop("disabled", true);
		    request = $.ajax({
		        url: "/action/shareFormSubmission.json",
		        type: "post",
		        data: serializedData,
		        beforeSend : function(event) {
		        	var submit = true;
		        	$.each($form.find("input[name='email']"), function( index ) {
		        		if (!validEmailAddress($(this).val())) {
		        			submit = false;
		        			$('#shareSubmissionForm :input[value="' + $(this).val() + '"]').prev().css("color", "red");
		        		} else {
		        			$('#shareSubmissionForm :input[value="' + $(this).val() + '"]').prev().css("color", "inherit");
		        		}
					});
					if (!submit) {
				    	$("#shareSubmissionMsg").text("One or more email address are invalid. Please review them and submit this form again.");
						$inputs.prop("disabled", false);
						return false;
					}
		        },
		        success : function (response){
			    	$("#shareSubmissionMsg").text('');
			    	$("#shareSubmissionsSent").html('');
					var msg = response.message;
					if (response.error == 0) {
				    	var emails = '';
				    	for (var i = 0; i < response.emails.length; i++) {
				    		emails += response.emails[i].email + ' <br /> ';
				    	}
				    	$("#shareSubmissionsSent").html(msg);
				    	$("#shareSubmissionsEmails").append(emails);
					} else {
				    	$("#shareSubmissionMsg").text(msg);
					}
			    	$('.fieldwrapper:not(:first)').remove();
			    	$("#additionalNotice").each(function(){
						$(this).val('');
					});
		    	},
		    	error: function (xhr, ajaxOptions, thrownError) {
			    	console.error("The following error occured: "+ xhr.status, thrownError);
			    	$("#shareSubmissionMsg").text("The following server error occured: " + thrownError +
			    		". Please contact the system administrator");
				},
				complete : function() {
					$inputs.prop("disabled", false);
				}
		    });
		    
			// prevent default posting of form
	    	event.preventDefault();
	    	return false;		    
		});
	});
</script> 
