    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/33393081/posts/default/4908733477481224524";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/33393081/4908733477481224524/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 4908733477481224524
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/33393081/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/33393081/comments/default";
          
          holder.currentBlogUrl = "http://legallykidnapped.blogspot.com/";
          holder.currentBlogId = "33393081";
        
        return holder;
      });
    }
  </script> 
