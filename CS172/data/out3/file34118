     <script type="text/javascript">
   var ison=1;
  var skipID = 'l_iep_more_info_main';
  var setBackground = 'url(/images/main/featured_engl_prog_bttn.png) 100% 0% no-repeat';
  function ltoggleXpander(item) {  
        $$('.l_xpander').each(function(it) {    
               if( $(it).identify() != $($(item).next('.l_xpander')).identify() && $(it).identify() != skipID){
                      //new Effect.toggle($($(it).identify()),'blind');
                      it.hide();
                       $($(it).previous('.l_xpander-control')).setStyle({
                          backgroundPosition: '0% 100%'
                         });
                       it.toggleClassName('l_xpander-on');                       
                        
		}
         });
         if($(item).getStyle('backgroundPosition') == '0% 100%'){    
          $(item).setStyle({
                          backgroundPosition: '100% 0%'
                          });
         }else{
          $(item).setStyle({
                          backgroundPosition: '0% 100%'
                          });

         }
        new Effect.toggle($($($(item).next('.l_xpander')).identify()),'blind');
	item.toggleClassName('l_xpander-on');
        
         
    
  }
  function ltoggleXpander_after(item) {      
        new Effect.toggle($($($(item).previous('.l_xpander')).identify()),'blind');
	item.toggleClassName('l_xpander-on');
        if(!item.className){
          item.innerHTML = "&nbsp;&nbsp;more &nbsp;&raquo&nbsp;&nbsp;";
         }else{
           item.innerHTML = "&nbsp;&nbsp;more &nbsp;&laquo&nbsp;&nbsp;";
         }  
              
    }

    
  
  function ltoggleXpander_toggle() {
        new Effect.toggle('l_iep_featured_main_container_pos','Appear');
	$($($('l_iep_featured_main_post').next('.l_xpander-control')).identify()).toggleClassName('l_xpander-on');
        
    }


// Look for elements of class "xpander" and hide them, then add a "Show"
// button before them.
// Look for elements of class "this-xpander" and hide them, then add an
// observer to the element before (it will be the control).
//
document.observe('dom:loaded', function () {
        var count=0;
	$$('.l_xpander').each(function(item) {
                item.hide();
                item.insert({before: '<a class="l_xpander-control" href="#" onclick="javascript:ltoggleXpander(this); return false" title="toggle view"></a>\n'});
                
	});

	$$('.this-l_xpander').each(function(item) {
		item.hide();
		control = item.previous();
		control.observe('click', function(event) {
			new Effect.toggle($($($(this).next()).identify()),'blind');
			this.toggleClassName('this-l_xpander-on');
			event.stop();
		});
		control.update('<span class="accessibility">'+control.innerHTML+'</span><a title="toggle view" href="#"></a>');
	});
       ltoggleXpander_toggle(); 
});





</script> 
