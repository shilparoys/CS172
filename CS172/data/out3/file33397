  <!--  initialize the slideshow when the DOM is ready --> 
  <script type="text/javascript">
			$(document).ready(function() {
				$('.rotatedItems,.rotatedItems2').cycle({
					fx : 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
					timeout : 7500,
					pager : '#rotNav',

					// callback fn that creates a thumbnail to use as pager anchor
					pagerAnchorBuilder : function(idx, slide) {
						return ' <a href="#" class="rotNavlink">&nbsp;</a> ';
					},
					after : function(curr, next, opts) {
						var lastSlide = (opts.currSlide == 0) ? (opts.slideCount) : (opts.lastSlide + 1);
						var currSlide = opts.currSlide + 1;
						$("#rotatingBox li:nth-of-type(" + lastSlide + ")").removeClass("current");
						$("#rotatingBox li:nth-of-type(" + currSlide + ")").addClass("current");
					}
				});
				$('.rotatedItems3').cycle({
					fx : 'fade', // choose your transition type, ex: fade, scrollUp, shuffle, etc...
					timeout : 25000
				});

				$('ul.sf-mmenu').superfish({
					dropShadows : false,
					animation : {
						opacity : 'show',
						height : 'show'
					},
					speed : 'normal'
				});

				/* no click to close accordion tabs with JavaScript */
				/* $("#accordion").tabs("#accordion div.pane", {tabs: 'h4', effect: 'slide', initialIndex: null }); */

				/* click to close accordion tabs with JavaScript */
				$("#accordion > h4").bind("click", function() {
					$(".pane").slideUp("slow");
					if ($(this).hasClass("current")) {
						$(this).removeClass("current");
					} else {
						$("#accordion > h4").removeClass("current");
						$(this).addClass("current");
						$(this).next().slideDown("slow");
					}

				});

				/* activate regular tabs with JavaScript */
				$("ul.tabs").tabs("> .pane");
			});
		</script> 
