					var tier=null, size=null;
					try {
						tier = valuation.estimate.tier;
						size = valuation.estimate.size;
					} catch(e) {/*...*/}
					
					var iframe = document.createElement('iframe');
					iframe.setAttribute("id", "iframe-ad-5");
					iframe.className = "iframe-ad";
					iframe.setAttribute("src", "/ajax/iframe_ad/?tile=5&sitename=usn.travelguides/besthotels&params=kw%3Dbesthotels%3Bkw%3Dtravel%3Bkw%3Dsectionfront&tier="+tier+"&size="+size);
					iframe.setAttribute("scrolling", "no");
					iframe.onload = function() {
						checkAdVisibility('iframe-ad-5','adv-5');
						adValidator('adv-5');
					};
					
					document.getElementById("adv-5").appendChild(iframe);
				};
				
				usn.rubicon.callApi();
			</script> 
    </div> 
    <script type="text/javascript">
	       var adFunctionArray;
	       if (adFunctionArray == undefined) {
	            var adFunctionArray = [];
	       }
	       adFunctionArray.push(function() {$('#iframe-ad-5').attr('src', function(i, val) {return val;});});
	    </script> 
    <script type="text/javascript">
			adValidator("adv-5");
		</script> 
   </div> 
