            {{if photourl !== ""}}
            <div style="float:left;width:95px">
                <a href="{{:obitlink}}"><img title="{{>phototitle}}" src="{{:photourl}}" /></a>
            </div>
            {{/if}}
            <span class="ObitHtml">{{:obithtml}}</span>
            <div style="clear:both"></div>
        </div>
        </div>
        {{if gblink !== ""}}
        <a title="Share Memories and Condolences" class="gbLink" href="{{:gblink}}" target="_self">
            Share Memories and Condolences
            <div class="GBCounterContainer" style="padding-left:8px">
                <img src="{{:graphicurl}}obituarybrowse/openboxarrow.png" class="BoxArrow" /><div class="GBCount">{{:gbcount}}</div>
            </div>
        </a>
        {{/if}}
        {{if #parent.parent.parent.parent.data.type == 2}}
        <a href="{{:obitlink}}" title="{{:readtitle}}" class="RightLink" ga-evt-cat="searchclick" ga-evt-label="searchresults" ga-evt-action="obitentryclick">
        {{else}}
        <a href="{{:obitlink}}" title="{{:readtitle}}" class="RightLink">
        {{/if}}
            <span><img src="{{:graphicurl}}spotlight/greenright.png" /></span>{{:readtitle}}
        </a>
        <div class="clearfix"/>
        <div class="obitInfo">
            {{if publishline !== ""}}
            <div><a href="{{:obitlink}}">{{:publishline}}</a></div> 
            {{/if}}
            {{if printline !== ""}}
            <div><a href="{{:obitlink}}">{{:printline}}</a></div> 
            {{/if}}
            {{if fhname !== ""}}
            <div class="FHInfo">
                <a href="{{:fhurl}}" class="FHName">{{:fhname}}</a>
                {{if flowerlink !== ""}}                    
                <a href="{{:flowerlink}}" class="glyphicons flower">Send Flowers</a>
                {{/if}}
                <div class="clearfix"></div>
            </div> 
            {{/if}}
        </div>
        {{if adtext != undefined && adtext !== "" && adlink !== ""}}
        <div class="linksMenu">
            {{if adlinkimage != undefined && adlinkimage !== ""}}
                <img class="linkImage" src="{{:adlinkimage}}" />
            {{/if}}
            <a href="{{:adlink}}" class="adLink" target="_blank">{{:adtext}}</a>
        </div>
        {{/if}}
        {{/if}}
    </div>
{{/if}}
</div>
{{/for}}
</div>
</script> 
