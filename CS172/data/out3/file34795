  <script type="text/javascript">
				function recordOutboundLink(obj, category, action) {
					// set destination--avoid the UNDEFINED error when recording links from forms
					if(obj.href){
						var destination = obj.href;
					} else if(obj.action){
						var destination = obj.action;
					} else {
						var destination = "http://www.metro.net/";
					}
					try {
						var myTracker=_gat._getTrackerByName(); 
						_gaq.push(['myTracker._trackEvent', ' + category + ', ' + action + ']);
						setTimeout('document.location = "' + destination + '"', 100);
						}catch(err){}
					}
			</script> 
