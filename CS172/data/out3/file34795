     <script type="text/javascript">
	var customer_email = "";
	function validEmail(e) {
		var filter = /^([a-zA-Z0-9])(([a-zA-Z0-9])*([\._\+\-])*([a-zA-Z0-9]))*@(([a-zA-Z0-9\-])+(\.))+([a-zA-Z]{2,4})+$/;
		if (String(e).search(filter) != -1) {
			return 0;
		} else {
			return 1;
		}
	}


	$(function(){
		$('form').submit(function(){ return false; });
		$('#form_email').one("click", function(){ $(this).val(''); });
		//
		$('#form_submit').click(function() {
			//Setup any needed variables.
			var input_email = $('#form_email').val(),
				input_subject = $('#form_subject').val(),
				input_message = $('#form_message').val(),
				response_text = $('#response');
			//Make AJAX request
			$.post('http://thesource.metro.net/sendmail.php', {
				to: input_email,
				subject: input_subject,
				body: input_message,
				async: false
			}, function(data) {
				response_text.html(data);
				$('#getemail').slideUp('fast');
				$('#pagesent').slideDown('slow');	
			});
			//Cancel default action
			return false;
		});
		//
		$('#submit_first').click(function(){
			$('#emailthisbutton').slideUp('fast');
			$('#getemail').slideDown('slow');
		});

	});
</script> 
