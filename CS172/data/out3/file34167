  var toShowText = '<span class="loginPageTitleHeaderH4" onClick="changeScheduleShow();">iGrade Schedule (Click to show)</span>';
  function hideSchedule(){
    document.getElementById("sisSchedule").innerHTML=toShowText;
  };
  function showSchedule(){
    document.getElementById("sisSchedule").innerHTML=sisScheduleText;
  };
  function changeScheduleShow(){
    if (vState == 0) {
      vState = 1;
      showSchedule();
    }else {
      vState = 0;
      hideSchedule();      
    }
  }
  
