      <script>
function update_radar_image(state) {
  var us = {'CA':'NV','DE':'PA','MD':'PA','MA':'NY','NJ':'NY','NC':'VA','RI':'NY','TX':'OK','WV':'VA'};
  if (us[state] == undefined) { st = state; } else { st = us[state]; }
  document.getElementById("weather_radar").innerHTML = '<img src="http://sirocco.accuweather.com/nx_mosaic_640x480_public/sir/inmasir'+st+'_.gif" width="280" class="lazy"/>';
}
function determine_weather_state() {
  var site = document.domain.split(".")[1];
  var st = {'al':'AL','cleveland':'OH','gulflive':'LA','lehighvalleylive':'PA','masslive':'MA','mlive':'MI','nj':'NJ','nola':'LA','oregonlive':'OR','pennlive':'PA','silive':'NY','syracuse':'NY'};
  if (window.location.search.split("=")[0] == "?zipcode") {
    var zip = window.location.search.split("=")[1];
    var url = "http://weather-report." + site + ".com/weather/jsonp/3day/zip=" + zip + "/";
    $.ajax({ url: url, dataType: 'jsonp', jsonp: true, jsonpCallback: 'threeDayForecast', cache: true }).done(function(jsonData) {
      var state = jsonData['current']['location'].split(",")[1].trim();
      update_radar_image(state);
      console.log('state from query string is', state);
    });
  } else {
    try {
      var arcs_data = localStorage.getItem("ARCS."+site+".com");
      var arcs = JSON.parse(arcs_data);
      var state = arcs.location_data.state;
    } catch(err) {
      var state = st[site];
    }
    update_radar_image(state);
    console.log('state from local storage is', state);
  }
}
determine_weather_state();
</script> 
      <script>
/* <![CDATA[ */
$(function() {
	var zc = PageQuery.getValue('zipcode');
	var cs = (PageQuery.q_str == PageQuery.getParameters()) ? PageQuery.q_str+'/' : ''; 
	var qstr = (!zc) ? cs : 'zip='+zc+'/';
	var h_url = 'http://weather-report'+ARCS.env+ARCS.cookie_domain+'/weather/html/module_maps/'+qstr;
	var wwSetWeatherPage = function(o){
		console.log(o);
		if (o.zip != '') {
				o.zip = o.zip.substr(0,5);
				window.top.location = '?zipcode='+o.zip;
		} else {						//no zip code
			if (o.city!='' && o.state!='') {
					$.ajax({
						url: ARCS.weather.app+o.city.toLowerCase().replace(/ /g, "-")+'-'+o.state.toLowerCase(),
						dataType: 'jsonp',
						jsonp: false,
						jsonpCallback: 'weather'})
					.done(function(wo,wm){
						if (wo.zip_code!='') {
							o.zip = wo.zip_code;
							window.top.location = '?zipcode='+o.zip;
						}
					})
					.fail(function(){AdvTR.geoCodeError({message: "Location not found",code: 1},weatherWidgetError);});
			} else {				  // show err
					AdvTR.geoCodeError({message: "Location not found",code: 1},weatherWidgetError);
			}
		}	
	}
	
        var weatherWidgetError =  function (e) {
	 if (arguments.length>0 && arguments[0].message) {
	  $('#weather_search_error').html(arguments[0].message);
	 }
        }
        
	// location setting - form submit
	if (navigator.geolocation) $('#search_weather_widget').attr('placeholder','Use My Current Location');
	$('#frm_weather_search').submit(function(){	// vaildate, get zip
		var $t = $('#search_weather_widget', this)
		var f = $.trim($t.val());
                if (navigator.geolocation && f.length == 0) {		  //try and get zip on geo loc
			AdvTR.getWeatherGeoLoc(wwSetWeatherPage, weatherWidgetError);
		} else if (!navigator.geolocation && f.length == 0) {		  // no geoloc - error
			AdvTR.geoCodeError({message: "Location not found",code: 2},weatherWidgetError);
		} else if (f.length > 0) {		  // try and get on string, get hash to use geocode ws
			var ts = new Date();
			AdvTR.getGeoCodeData(f+ AdvTR.domain + 'entertainment' + ts.valueOf(),'type=geocode&loc='+f+'&aff='+ AdvTR.domain +'&dstmp='+ts.valueOf(),wwSetWeatherPage);
		}
		return false;
	});
});
/* ]]> */
</script> 
