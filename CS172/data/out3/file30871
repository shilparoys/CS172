       <script>
/* <![CDATA[ */
$(function() {
	var zc;
        isSearchLocation = false;
	if (PageQuery.getValue('zipcode') != false) {
		zc = PageQuery.getValue('zipcode');
                isSearchLocation = true;
                
	} else if (ARCS.getLocalStorageVal('location_data') != undefined) {
		zc = ARCS.getLocalStorageVal('location_data').zip;
	} else if ($.cookie("gl-weather") != null) {
		zc = $.cookie("gl-weather");
	} else {
		zc = PageQuery.getValue('zipcode');
	}
	var cs = (PageQuery.q_str == PageQuery.getParameters()) ? PageQuery.q_str+'/' : 'zip='+ARCS.default_region.zip_code+'/'; 
	var qstr = (!zc) ? cs : 'zip='+zc+'/';
	var h_url = 'http://weather-report'+ARCS.env+ARCS.cookie_domain+'/weather/html/module/'+qstr;       
	var rh = $.ajax({
		url: h_url,
		crossDomain: true,
		cache: true,
	});
        
        udpdateWeatherLocation = function (co) {
	  ARCS.setLocalStorageVal('weather_state', 'phase2');
	  ARCS.setLocalStorageVal('location_data', co);
	  $.cookie('gl-weather', co.zip, { expires: 365, path: '/', domain: ARCS.cookie_domain });
	  AdvTR.showWeatherData();
	  AdvTR.updateWeather(co.zip);
	}
        
	rh.done(function(o, m){	 //got hash, now get geocode data
		$('#river_weather').html(o);
                if (isSearchLocation == true) {
                    $('.tot-head h2').append("<br><span class='setModuleLink'>Make this location your default &raquo;</span>")
                    $('.tot-head h2 span.setModuleLink').click(function() {
                         var ts = new Date();
                         AdvTR.getGeoCodeData(zc + AdvTR.domain + 'entertainment' + ts.valueOf(),'type=geocode&loc='+zc+'&aff='+ AdvTR.domain +'&dstmp='+ts.valueOf(),udpdateWeatherLocation)   
                         $('.tot-head h2 span.setModuleLink').html("Location Saved");
                         $('.tot-head h2 span.setModuleLink').css("color","#666666");
                         $('.tot-head h2 span.setModuleLink').css("text-decoration","none");
                })
                }
		$('#weather_module .expand-button').click(function() {
			$(".row2, .row3", '#weather_module').toggle();
			$(this).toggleClass('minus');
		});
		if (document.documentElement.clientWidth <= 767) {
			console.log(document.documentElement.clientWidth)
			try {console.log('ok')
				//var $ww = $('#weather_widget'); //temporary use of existing module....
				var $ww = $('section.entertainment_highlight img[data-original*="sirocco.accuweather.com"]');
				if ($ww.length > 0) {
					var $sh = $('section.entertainment_highlight');
					$sh.before('<a name="rr_weatherwidget"></a>');
					$('#weather_module .row1').append('<div class="link"><a href="#rr_weatherwidget">Live Weather Maps</a></div>');
					$sh.css('display','block');
					$ww.css({'display':'block','width':'100%','max-width':'640px'});
					$ww.attr('src',$ww.attr('data-original'));
				}
			} catch (e) {;}
			
		}
	});
	rh.fail(function(x,s){
		console.log(x)
	});
});
function update_weather_title() {
 var site = document.domain.split(".")[1];
 try {
  var arcs_data = localStorage.getItem("ARCS."+site+".com");
  var arcs = JSON.parse(arcs_data);
  document.title = arcs.location_data.city + ", " + arcs.location_data.state + " Weather Forecast, Radar & News";
 } catch(err) {
  //err
 }
}
update_weather_title();
/* ]]> */
</script> 
