          <script type="text/javascript">RotatorIMGS.push('/images/uploads/rotators/world_wide_map_rotator_2014.jpg');</script> 
         </div>
         <!-- // the_rotator --> 
        </div> 
        <!-- // rotator_box --> 
        <script type="text/javascript">
$(document).ready(function(){

	// This is done to prevent loading rotators if a mobile website is loaded
	// Degault load incase desktop is loaded first
	if($(document).width() > 767)
		{
			$('#worldwiderotator_single_1 img').attr('src', RotatorIMGS[0]);
		}
	// Responsive load, incase someone starts mobile but ends up desktop
	$(window).on('resize', function () 
		{
			if($(document).width() > 767)
				{
					$('#worldwiderotator_single_1 img').attr('src', RotatorIMGS[0]);
				}
		});

});
</script> 
       </div>
       <!-- //module --> 
      </div>
      <!-- //mid-column-wide --> 
     </div>
