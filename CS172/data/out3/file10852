  <script type="text/javascript">

	//check to see if the user has their DNT header set
	var skipAnalytics = navigator.doNotTrack == "yes" || navigator.doNotTrack == "1" || navigator.msDoNotTrack == "1";
	
	//if the user does not want us to track them, dont even load the tracking js files
	//if you explicitly tell firefox that you want to be tracked dnt should be 'no' but it is still 'yes'
	if(skipAnalytics == false)
	{
		var get_params = {}
		var url = location.search;
		
		url.substr(1).split("&").forEach(function(item) {get_params[item.split("=")[0]] = item.split("=")[1]})
		
		var piwik_site_id = "1";
		
		var ga_site_id = "UA-15019813-1";
		var ga_domain = ".plu.edu";
		
		var load_www_analytics = document.URL.match(/^https?:\/\/www\.plu\.edu/);
		var siteimprove_url = "//us1.siteimprove.com/js/siteanalyze_66357095.js";
		
		var spuid = "";
		var sfuid = "";
		
		if(get_params['spUserID'])
		{
			var spuid_encoded = get_params['spUserID'];
			var spuid_encoded_short = spuid_encoded.substring(0,spuid_encoded.length -2);
			try
			{
				spuid = atob(spuid_encoded_short);
			}
			catch(err)
			{
				spuid = 'invalid';
			}
		}
		
		if(get_params['pw_uvc'])
		{
			sfuid = get_params['pw_uvc'];
		}
		
		
		//<!-- Piwik -->
		if(piwik_site_id !== undefined)
		{
			var _paq = _paq || [];
			_paq.push(["setDoNotTrack", true]);
			_paq.push(["enableLinkTracking"]);
			
			if(spuid != "")
			{
				_paq.push(["setCustomVariable", '1', "SpId", spuid, "visit"]);
			}
			
			if(sfuid != "")
			{
				_paq.push(["setCustomVariable", '2', "SfId", "" + sfuid, "visit"]);
			}
			
			
			_paq.push(["trackPageView"]);
			
			(function() {
			var u="https://zatar.plu.edu/piwik/";
			_paq.push(["setTrackerUrl", u+"piwik.php"]);
			_paq.push(["setSiteId", piwik_site_id]);
			var d=document, g=d.createElement("script"), s=d.getElementsByTagName("script")[0]; g.type="text/javascript";
			g.defer=true; g.async=true; g.src=u+"piwik.js";
			s.parentNode.insertBefore(g,s);
			})();
		}
		//<!-- End Piwik Code -->
		
		//<!-- Start Google analytics Tracker -->
		if(load_www_analytics)
		{
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');
			
			ga('create', 'UA-15019813-1', 'auto');
			ga('send', 'pageview');		
		}
		//<!-- End Google analytics Tracker -->
		
		//<!-- Site Improve Tracking -->	
		if(load_www_analytics)
		{	
			(function() {
			var sz = document.createElement("script"); sz.type = "text/javascript"; sz.async = true;
			sz.src = siteimprove_url;
			var s = document.getElementsByTagName("script")[0];
			s.parentNode.insertBefore(sz, s);
			})();
		}
		//<!-- End Site Improve Tracking Code -->
	}
	
</script> 
