    avt.sb.$(document).ready(function() {
        avt.sb.$("#dnn_avtSearch_txtSearch").bind("focus", function() {
            if (avt.sb.$(this).val() == avt.sb.$("#dnn_avtSearch_hdnEmptyText").val()) {
                avt.sb.$(this)
                    .removeClass("avtsb_searchbox_empty")
                    .val("");
            }
