        avt.sb.$("#dnn_avtSearch_txtSearch").bind("blur", function() {
            if (avt.sb.$(this).val().length == 0) {
                avt.sb.$(this)
                    .addClass("avtsb_searchbox_empty")
                    .val(avt.sb.$("#dnn_avtSearch_hdnEmptyText").val());
            }
        });

        if (avt.sb.$.trim(avt.sb.$(this).val()).length > 0 && avt.sb.$(this).val() != avt.sb.$("#dnn_avtSearch_hdnEmptyText").val()) {
            avt.sb.$(this).removeClass("avtsb_searchbox_empty");
        }
    });


