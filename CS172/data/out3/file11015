        <section class="block block-boxes block-boxes-follow block-site-wide-follow-box block-boxes-site-wide-follow-box odd" id="block-boxes-site-wide-follow-box"> 
         <div class="block-inner clearfix"> 
          <h2 class="block-title">Follow FEMA:</h2> 
          <div class="content clearfix"> 
           <div id="boxes-box-site_wide_follow_box" class="boxes-box">
            <div class="boxes-box-content">
             <div class="item-list">
              <ul>
               <li class="first"><a href="https://twitter.com/FEMA" class="follow-link follow-link-twitter follow-link-site" title="Follow FEMA.gov on Twitter">Twitter</a> </li>
               <li><a href="http://www.facebook.com/FEMA" class="follow-link follow-link-facebook follow-link-site" title="Follow FEMA.gov on Facebook">Facebook</a> </li>
               <li><a href="http://www.youtube.com/user/FEMA" class="follow-link follow-link-youtube follow-link-site" title="Follow FEMA.gov on YouTube">YouTube</a> </li>
               <li><a href="/email" class="follow-link follow-link-get_updates follow-link-site" title="Follow FEMA.gov on Email Updates">Email Updates</a> </li>
               <li class="last"><a href="/data" class="follow-link follow-link-this-site follow-link-site" title="Follow FEMA.gov on Data Feeds">Data Feeds</a> </li>
              </ul>
             </div>
            </div>
           </div> 
          </div> 
         </div> 
        </section>
