  <script type="text/javascript">/* <![CDATA[ */
		$(document).ready(function() {
			// slider
			var slider = $('#crai-slider');
			slider.append('<ul class="sliderNav"><li id="sliderNavNext">Següent</li><li id="sliderNavPrev">Anterior</li></ul>')
			var sliderNav = slider.find('.sliderNav');
			sliderNav.css('opacity', '.2');
			slider.hover(
				function() {
					sliderNav.css('opacity', '.8');
				},
				function() {
					sliderNav.css('opacity', '.2');
				}
			);
			slider.find('ul.slides').cycle({
				prev: '#sliderNavPrev',
    		next: '#sliderNavNext',
				pause: 1
			});
			
			$('input[name="radios"]').click(function(){
				var newOptions = {
					"Paraula clau": "X",
					"Autor": "a",
					"Matèria": "d",
					"Títol": "t",
					"ISBN": "i",
				};
				if ($(this).attr('id') == 'crai-worldcat') {
					newOptions = {
						"Paraula clau": "X",
						"Autor": "a",
						"Títol": "t",
					};					
				} 
				setSearchSelectOptions(newOptions);
			});

			function setSearchSelectOptions(newOptions)
			{
				var $el = $("#searchtype");
				$el.empty(); // remove old options
				$.each(newOptions, function(key, value) {
				  $el.append($("<option></option>")
				     .attr("value", value).text(key));
				});
			}
			// cercador biblio
			$('#cerca_biblio').submit(function() {
				var t = $(this);
				if ($('#crai-ccuc').is(':checked')) {
					var txtTypeSearch = $('#searchtype').val();
					var txtValue  = $('#searcharg').val();
					var txtValueTrim = '';
					txtValue = txtValue.replace(/^\s+|\s+$/gi,'');
					txtValueTrim = txtValue;
					var base_url = 'http://puc.cbuc.cat/iii/encore/search/C%7CS'+encodeURIComponent(txtValueTrim);
					var facet_fields = '';
					// paraula clau
					if  ( txtTypeSearch == 'X' ) {
						facet_fields = '%7COrightresult%7CU1?lang=cat&suite=def';
					// autor
					} else if ( txtTypeSearch == 'a' ) {
						facet_fields = '%7CFf%3Afacetfields%3Aauthor%3Aauthor%3AAutor%3A%3A%7COrightresult?lang=cat&suite=def';
					// títol
					} else if ( txtTypeSearch == 't' ) {
						facet_fields = '%7CFf%3Afacetfields%3Atitle%3Atitle%3AT%252525C3%252525ADtol%3A%3A%7COrightresult?lang=cat&suite=def';
					// matèria
					} else if ( txtTypeSearch == 'd' ) {
						facet_fields = '|Ff:facetfields:subject:subject:Mat%2525C3%2525A8ria::|Orightresult?lang=cat&suite=def';
					// per defecte es paraula clau
					} else {
						facet_fields = '%7COrightresult%7CU1?lang=cat&suite=def';
					}
					var url = base_url + facet_fields;
					var navigatorWindow = window.open(url, 'window_cataleg', '' );
					navigatorWindow.opener = window;
					
				} else if ($('#crai-worldcat').is(':checked')) {

					var txtTypeSearch = $('#searchtype').val();
					var txtValue  = $('#searcharg').val();
					var txtValueTrim = '';
					txtValue = txtValue.replace(/^\s+|\s+$/gi,'');
					txtValueTrim = txtValue;

					var base_url = 'http://www.urv.worldcat.org/search?';
					var facet_fields = '';
					
					// autor
					if ( txtTypeSearch == 'a' ) {
						params = 'q='+encodeURIComponent('au:' + txtValueTrim)+'&qt=advanced&dblist=638';
					// títol
					} else if ( txtTypeSearch == 't' ) {
						params = 'q='+encodeURIComponent('ti:' + txtValueTrim)+'&qt=advanced&dblist=638';
					// per defecte es paraula clau
					} else {
						params = 'qt=affiliate&ai=Biblioteca_spudb&q='+encodeURIComponent(txtValueTrim) ;
					}
					
					var url = base_url + params;
					var navigatorWindow = window.open(url, 'busqueda_worldcaturv', '' );
					navigatorWindow.opener = window;				
					
				} else {
					
					document.location.href = "http://cataleg.urv.cat/search*cat/" + $('#searchtype').val() + "?" + encodeURIComponent($('#searcharg').val());
				}
				return false;
			});
			// tabs
			$('div.crai-tabbed').each(function(){
				var t = $(this);
				var tabbedDivs = t.find('div.tabbed');
				//tabbedDivs.not(':first').hide();
				tabbedDivs.hide();
				if (t.find('ul.tabs li.current').length > 0) {
					current = t.find('ul.tabs li.current a');
					var toShowID = current.attr('href');
					tabbedDivs.not(toShowID).hide();
					$(toShowID).show();
				} else {
				}

				t.find('ul.tabs a:not(.link)').click(function(){
					var a = $(this);
					a.parent().parent().children('li').removeClass('current');
					a.parent().addClass('current');
					var toShowID = a.attr('href');
					tabbedDivs.not(toShowID).hide();
					$(toShowID).show();
					return false;
				});
			});
			var ccucTooltip = $('#ccuc-tooltip, #worldcat-tooltip');
			ccucTooltip.tipsy({trigger: 'manual'});
			ccucTooltip.hover(
				function() {
					ccucTooltip.tipsy('show');
				},
				function() {
					setTimeout(function(){ccucTooltip.tipsy('hide')},2000);
				}
			);
		});
	/* ]]> */</script>   
