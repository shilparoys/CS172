      <script type="text/javascript"> 
		YAHOO.SolutionSet.util.LoadJSFiles(["/pgw/s20/js/formdialog.js","/pgw/s20/js/selectrecipient.js","/pgw/s20/js/autoCompleteYUI.js"], function(){
		YAHOO.SolutionSet.widget.FormDialog.add(YAHOO.util.Dom.getElementsByClassName("sharePage", "a"), {
			onAfterDataReturned: function() {
				/*var shareDataSource = new YAHOO.widget.DS_XHR("/", ["ResultSet.Result","Title"]);
				shareDataSource.queryMatchContains = true;
				shareDataSource.scriptQueryAppend = "__module=share&type=autocomplete";*/
				var shareDataSource = new YAHOO.widget.DS_JSArray([]);
				//shareDataSource.scriptQueryAppend = "?cturl=" + location.href;
				YAHOO.SolutionSet.shareAutoComplete = new YAHOO.SolutionSet.widget.AutoComplete("shareUsername","shareAutoCompleteContainer", shareDataSource, {
					initUpdateValues: function(values) {
						var newValues = [];
						for (var i=0, value; value = values[i]; i++) {
							var matches = value.match(/^([^<]*[^<\s])\s?<(.*)>\s*$/);
							
							if (matches && matches.length > 2) {
								newValues.push({
									Title: matches[1],
									Value: matches[1],
									TextboxValue: matches[2]
								});
							} else if (!value.match(/^\s*$/)) {
								newValues.push({
									Title: value,
									Value: value,
									TextboxValue: value
								});	
							}
				        }
			            return newValues;
			        },
			        onlyAllowValidEmails: true
			    });
		        YAHOO.SolutionSet.shareAutoComplete.delimChar = [",",";"];

		        YAHOO.SolutionSet.util.LoadJSFiles(["/pgw/s20/js/selectrecipient.js"], function() {
					new YAHOO.SolutionSet.widget.SelectRecipient("selectRecipient", {								
					ajaxURL: '/friend/messaging/selectrecipient?',
					inputId: 'shareUsername'
					});
				}); 	
		        YAHOO.util.Event.addListener("outsidenetwork", "click", function(e) {
					document.getElementById("outsidenetworkcontainer").style.display = (document.getElementById("outsidenetwork").checked) ? "" : "none";
				});
				
		        YAHOO.util.Event.addListener("subject", "blur", function(e) {
		            var subjectEle = YAHOO.util.Dom.get('subject');
					if (subjectEle && subjectEle.value == "") {
    					var panel = new YAHOO.widget.Panel("ErrorDialog", {
    						width:"400px",
    						fixedcenter: false,
    						constraintoviewport: false,
    						underlay:"shadow",
    						modal:true,
    						close:true,
    						visible:false,
    						draggable:true,
    						iframe: true,
    						zIndex:202
    					});
    					panel.setHeader("Error!");
    					panel.setBody('<p>Please enter a subject.</p><span class="yui-button yui-push-button default"><span class="first-child"><button type="button">Okay</button></span></span>');
    					panel.render(document.body);
					
    					panel.cfg.setProperty("fixedcenter",true);
    					panel.show();
    					
						var button = panel.body.getElementsByTagName("button")[0];
						button.focus();
						YAHOO.util.Event.addListener(button, "click", function(e) {
							panel.hide();
						});
					
    					panel.subscribe("hide", function() {
					        subjectEle.focus();
    					}, this, true);
					}
				});
	        }

        });
    });


</script> 
