  <!-- Clear Fields --> 
  <script type="text/javascript" language="javascript">
	YAHOO.namespace("SolutionSet.widget");

	YAHOO.SolutionSet.widget.ClearFields = function(selector) {
		var elements = YAHOO.util.Selector.query(selector);

		YAHOO.util.Event.addListener(elements, "focus", YAHOO.SolutionSet.widget.ClearFields._onElementFocus);
		YAHOO.util.Event.addListener(elements, "blur", YAHOO.SolutionSet.widget.ClearFields._onElementBlur);
	}

	YAHOO.SolutionSet.widget.ClearFields._onElementFocus = function (e) {
		var target = YAHOO.util.Event.getTarget(e);
		if (target.value == target.defaultValue) {
			target.value = "";
		}
	}

	YAHOO.SolutionSet.widget.ClearFields._onElementBlur = function (e) {
		var target = YAHOO.util.Event.getTarget(e);
		if(!YAHOO.util.Dom.hasClass(target, "forceClear")) {
			if (target.value == "") {
				target.value = target.defaultValue;
			}
		}
	}
	
	//Setup function to switch between templates
	YAHOO.SolutionSet.util.bodyTemplateSwitcher = function(oldTemplate, newTemplate) {
		var bodyClass = document.body.className;
		bodyClass = bodyClass.replace("template" + oldTemplate, "template" + newTemplate);
		document.body.className = bodyClass;
	}

	</script> 
