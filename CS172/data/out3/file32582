function rtaClickTrackToPage(rtaToPage, rtaToCode) {
  window.location.href="/get/page/" + rtaToPage + "?rtaCode=1&rtaTcode=" + rtaToCode + "&rtaType=P";
}
function rtaClickTrackToUrl(rtaToUrl, rtaToCode) {
  var i_qn = rtaToUrl.indexOf('?');
if (i_qn < 0) {sDelim = '?'} else {sDelim = '&'}
  window.location.href= rtaToUrl + sDelim + "rtaCode=1&rtaTcode=" + rtaToCode + "&rtaType=P";
}
