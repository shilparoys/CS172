       <script language="javascript">
						var Event = YAHOO.util.Event, Dom = YAHOO.util.Dom, browsePanel;
						var browseBtn = document.getElementById("browseBtn");
						var panelAnchor = document.getElementById("leftSash");

						browsePanel = new YAHOO.widget.Panel("browsePanel", {
							width: "895px",
							underlay: "none",
							close: false,
							visible: true,
							draggable: false,
							zIndex: 100,
							iframe: true,
							context: ["leftSash", "tl", "tl"],
							effect:{effect:YAHOO.widget.ContainerEffect.FADE,duration:0.15}
						});
						browsePanel.renderable = false;
						browsePanel.rendered = false;

						Event.onDOMReady(function(){
							browsePanel.renderable = true;
						});

						Event.on(browseBtn, "click", function() {
							if (browsePanel.rendered && browsePanel.cfg.getProperty("visible")) {
								browsePanel.hide();
							}
							else if (browsePanel.renderable && !browsePanel.rendered) {
								browsePanel.render(panelAnchor);
								browsePanel.rendered = true;
								browsePanel.show();
							}
							else {
								browsePanel.show();
							}
						});

						Event.on(document, "click", function(e) {
							var el = Event.getTarget(e);
							var browsePanelEl = browsePanel.element;
							if (browsePanel.rendered && (el != browsePanelEl && !Dom.isAncestor(browsePanelEl, el) && el != browseBtn && !Dom.isAncestor(browseBtn, el))) {
								browsePanel.hide();
							}
						});
					</script> 
