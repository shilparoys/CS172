          <script type="text/javascript">
$(document).ready(function(){
 	
	// a custom method making the default value for companyurl ("http://") invalid, without displaying the "invalid url" message
	jQuery.validator.addMethod("defaultInvalid", function(value, element) {
		return value != element.defaultValue;
	}, "");
	
	jQuery.validator.messages.required = "";
	$("#alumni-form").validate({
		invalidHandler: function(e, validator) {
			var errors = validator.numberOfInvalids();
			if (errors) {
				var message = errors == 1
					? 'You have 1 required field to complete. It has been highlighted below'
					: 'You have ' + errors + ' required fields to complete.  They have been highlighted below';
				$("div.error_info_request span").html(message);
				$("div.error_info_request").show();
			} else {
				$("div.error_info_request").hide();
			}
		},
		messages: {

			FirstName: {
				required: " ",
				equalTo: "Please enter a valid first name"
			}
			LastName: {
				required: " ",
				equalTo: "Please enter a valid last name"
			}
			Address: {
				required: " ",
				equalTo: "Please enter a valid address"
			}
			City: {
				required: " ",
				equalTo: "Please enter a valid city"
			}
		} //end messages
	});
	
});
</script>  
