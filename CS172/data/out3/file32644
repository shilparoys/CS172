             <rect x="11.505" y="3.767" transform="matrix(0.7071 0.7071 -0.7071 0.7071 8.2817 -10.0783)" fill="#FFFFFF" width="9.603" height="2.381" /> 
             <rect x="15.139" y="5.276" transform="matrix(0.7072 0.707 -0.707 0.7072 11.9049 -8.5935)" fill="#FFFFFF" width="2.378" height="9.6" /> 
             <rect x="5.642" y="6.373" fill="#FFFFFF" width="12.319" height="2.377" /> 
            </g> 
           </svg> </button> 
         </form> 
         <script type="text/javascript">

            var UW = UW || {}

            UW.Donate = Backbone.View.extend({

              el : '#make-a-gift-widget',

              events : {
                'click button' : 'setFrequency',
                'mousedown li.inactive' : 'browseFunds'
              },

              initialize : function() {
              },

              browseFunds : function( e ) {
                if ( $(e.target).parent().data().value === 'others' )
                {
                  e.preventDefault();
                  e.stopPropagation();
                  return window.location = 'http://www.washington.edu/giving/make-a-gift/?tab=1&appeal=15XEX'
                }
              },

              setFrequency :  function( e ) {
                var target = $(e.currentTarget)
                this.$el.find('#frequency').val( target.data().frequency )
                this.$el.find('#recurring').val( target.data().recurring )
                this.$el.find('#duration').val( target.data().duration )
              }

            })


            $(document).ready( function() {
              UW.donate = new UW.Donate()
            })

          </script> 
         <div class="other-funds"> 
