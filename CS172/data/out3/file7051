  $(function() {
    if($.cookie('cookie-info') == 'ok') {
      $('#cookie-panel').animate({right: '-450px'}, 0);
    }
     $('#slide_button').click(function() {
        $('#cookie-panel').animate({right: '-450px'}, 1500);
       $.cookie('cookie-info', 'ok', {expires: 365});
       
    });
  });
  
