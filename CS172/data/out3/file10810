				getScript('js/jquery.waitforimages.min.js',function(){
					getScript('js/reviews.js',function(){});
				});
				
				jQuery('p.form_field_input input').live('change',function(){
							jQuery(this).closest('div.form_col').removeClass('field_error');
						});	
			});
							
		});
	})();
	
