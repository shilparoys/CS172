  <script>
    $(document).ready(function() {

        // find unique ID of this message
        var cookieName = $('.alert-message').attr('id');

        if (!$.cookie(cookieName)) {
            // show the alert if the message has not been previously dismissed
            $(".alert-message").show("blind", {direction: "vertical"}, 800);
        }

        $(".alert-dismiss").click(function() {
            // hide message
            $(".alert-message").hide("blind", {direction: "vertical"}, 800);
            // set cookie so message will not display again
            $.cookie(cookieName, 1);
        });

    });


</script> 
