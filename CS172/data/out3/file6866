  <script>
$(function(){
        $("#hnd").hover(function() {
		if($(".gv_filmstripWrap").is(":visible"))
			$("#othersi").stop(true,true).fadeIn("slow");
		else {
			$("#othersi").stop(true,true).slideDown("slow");
			$(".od").show();
		}
        }, 
	function(){
		if($(".gv_filmstripWrap").is(":visible"))
			$("#othersi").stop(true,true).fadeOut("slow");
		else {
			$(".od").hide();
			$("#othersi").stop(true,true).slideUp("slow");
		}
	});
});

</script> 
