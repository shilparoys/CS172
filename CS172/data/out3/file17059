function mouse_over_menu(oObj)
{
	oObj.style.color = 'blue';
	oObj.style.cursor = 'pointer';
	oObj.style.cursor = 'hand';

}

function mouse_out_menu(oObj)
{
	oObj.style.color = 'black';
}

