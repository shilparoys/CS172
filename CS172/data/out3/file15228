  <script type="text/javascript">
      $(document).ready(function() {
             
             
             if(endsWith(window.location.href,'/')){
                 $('link[rel=canonical]').remove();
             }
        });


       function endsWith(str, suffix) {
            return str.indexOf(suffix, str.length - suffix.length) !== -1;
       }


function addParamsSearchEventsEnt(type){
var newUrlEvents = $("#linkEventsEnt").attr("href");
var date = new Date();
var month = date.getMonth() + 1;
var dateFin = date.getDate() +'/'+ month +'/'+ date.getFullYear();
newUrlEvents = newUrlEvents + "?keyword=" + type + "&from=" + dateFin + "&language=en&languageTeach=en";
window.location = newUrlEvents;
} 
var linkEvents = "";         

</script> 
