  <script type="text/javascript" charset="utf-8">

var cookieMessage = 'This website uses cookies to improve your user experience and to show you content related to your preferences. If you continue browsing, we consider that you agree to their use. <a href="/en/system/modalcookiesresp.html?tcm=tcm:41-117230" data-reveal-id="myModal" data-reveal-ajax="true">More Information</a>';
var htmlcookie = '<div id="cookies-container"><div class="cookies-inner row"><span class="title column small-12 medium-2 large-2 large-offset-1"><p>Cookies on the IESE Website</p></span><div id="cookie-message" class="column small-12 medium-7 large-7">' + cookieMessage + '</div><div id="cookie-policy-buttons" class="column small-12 medium-2 large-2"><span class="cookie-button" id="close-cookies">Accept</span></div></div>';


function getDomain() {
	var parts = location.hostname.split('.');
	var subdomain = parts.shift();
	var upperleveldomain = parts.join('.');
	var sndleveldomain = parts.slice(-2).join('.');
	return sndleveldomain;
};             


function readAcceptCookie() {
	var lista = document.cookie.split(";");
	for (i in lista) {
		var busca = lista[i].search("acceptCookies");
		if (busca > -1) {
			micookie=lista[i]
			var igual = micookie.indexOf("=");
			var valor = micookie.substring(igual+1);
			return valor;
		}
	}
	return null;
};

$(document).ready(function() {
	if(readAcceptCookie()!="true"){
		$('body').prepend(htmlcookie);           
	}
	$('#close-cookies').click(function(){                  
		var currentTime = new Date();
		var month = currentTime.getMonth();
		var day = currentTime.getDate();
		var year = currentTime.getFullYear();
		var dateExpire = new Date(year+1,month,day);
		var strExppire = dateExpire.toGMTString();
		var thecookie = "acceptCookies" + " = " + "true" + "; expires=" + strExppire+"; path=/; domain=."+getDomain();
		document.cookie = thecookie;
		//$('#acceptCookieBanner').hide();

		$('#cookies-container').slideUp('3000', function(){
			$(this).remove();
		});
	});			   
});                

</script> 
