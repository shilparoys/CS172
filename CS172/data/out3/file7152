      <script type="text/javascript">



                    var etkinlikList=Array();
                    var i=0;
                    var j=0;
                    var maxDisplay=4;
                    var total=0;
                    var interval;
                    jQuery(window).load(function(){
                        $('#etkinlikHome li').each(function(){
                            etkinlikList[i]=$(this).html();
                            i++;
                            if(i>maxDisplay)
                                $(this).hide();
                        });
	        total=i;
	        if(total>maxDisplay)
                        	interval=setInterval("moveUp()",6000);
                    });
                    function moveUp(){
                        $('#etkinlikHome').html('');
                        j++;
                        for(var k=0;k<maxDisplay;k++){
                            $('#etkinlikHome').append('<li>'+etkinlikList[(k+j)%total]+'</li>');
                        }
                    }
                    $('#etkinlikHome').mouseover(function(){
		if(total>maxDisplay)
	                        clearInterval(interval);
                    });
                    $('#etkinlikHome').mouseout(function(){
		if(total>maxDisplay)
	                        interval=setInterval("moveUp()",6000);
                    });
                    
	
</script>
      <script type="text/javascript">
