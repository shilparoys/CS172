  <script type="text/javascript">
  //Set margins & indents for left nav tree
  $(document).ready(function() {
	$('ul#nav_categories').tree();
	$('#nav_categories > li').not(':has(ul)').css('padding-left','1.5em');
	$('#nav_categories > li').has('ul').css({
		'text-indent':'-1.3em',
		'margin-left':'1.3em'
	});
  });
</script> 
  <script type="text/javascript">
  $(document).ready(function() {
	/* top nav code */
	var navHtml = '';
	var navHeadHtml = '<ul>';

	//render json
	$.getJSON( "http://www.usc.edu/libraries/inc/topnavlinks.php?callback=?", function(data) {
		$.each(data.sections, function(i, section) {
			if(section.id == 'libraries' || section.id == 'partners') {
				if(section.id == 'libraries') {
					navHeadHtml += '<li data-dropdown="#dropdown-' + i + '"><a href="#">' + section.section + '</a><b class="caret"></b></li>';
					navHtml += '<div id="dropdown-' + i + '" class="dropdown-menu has-tip">';
				}
			}
			else {
				navHeadHtml += '<li data-dropdown="#dropdown-' + i + '"><a href="#">' + section.section + '</a><b class="caret"></b></li>';
				navHtml += '<div id="dropdown-' + i + '" class="dropdown-menu has-tip">';
			}
			//libraries & partners dropdown
			if(section.id == 'libraries' || section.id == 'partners') {
				if(section.id == 'libraries') {
					navHtml += '<div class="nav-header">Libraries &amp; Collections</div>';
					navHtml += '<ul class="menu-items">';
					$.each(section.links, function(j, link) {
						if(j==section.links.length/2-1) {
							navHtml += '</ul><ul class="menu-items">';
						}
						navHtml += '<li><a' + ' href="' + link.url + '">' + link.label + '</a>';
						if(link.sublinks) {
							navHtml += '<ul>';
							$.each(link.sublinks, function(k, sublink) {
								navHtml += '<li><a' + ' href="' + sublink.url + '">' + sublink.label + '</a></li>';
							});
							navHtml += '</ul>';
						}
						navHtml += '</li>';
					});
					navHtml += '</ul>';
				}
				if(section.id == 'partners') {
					navHtml += '<div class="nav-header" style="clear:both; padding-top:20px;">Partners and Other Library Units</div><ul>';
					$.each(section.links, function(j, link) {
						navHtml += '<li><a' + ' href="' + link.url + '">' + link.label + '</a>';
						if(link.sublinks) {
							navHtml += '<ul>';
							$.each(link.sublinks, function(k, sublink) {
								navHtml += '<li><a' + ' href="' + sublink.url + '">' + sublink.label + '</a></li>';
							});
							navHtml += '</ul>';
						}
						navHtml += '</li>';
					});
					navHtml += '</ul></div>';
				}
			}
			//other dropdowns
			else {
				$.each(section.links, function(j, link) {
					if(j==0) {
						navHtml += '<ul>';
					}
					navHtml += '<li><a' + ' href="' + link.url + '">' + link.label + '</a>';
					if(link.sublinks) {
						navHtml += '<ul>';
						$.each(link.sublinks, function(k, sublink) {
							navHtml += '<li><a' + ' href="' + sublink.url + '">' + sublink.label + '</a></li>';
						});
						navHtml += '</ul>';
					}
					navHtml += '</li>';
				});
				navHtml += '</ul></div>';
			}
		});
		navHeadHtml += '</ul>';
		$('#usclibNav').prepend(navHeadHtml);
		$('#dropdown-container').html(navHtml);
	  });
  });
</script> 
