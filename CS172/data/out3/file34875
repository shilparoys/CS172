var headerSlider = new hImageSwapper();

headerSlider.wrapper         = 'headerSlider'; 	//name of div with class of imageswapper
headerSlider.animationType   = 'slide'; //slide or fade
headerSlider.continious      = 1; //0 no, 1 yes  (only matters for sliding)
headerSlider.animationSpeed  = 40; //slide: lower-slower, fade: lower-faster
headerSlider.direction       = 1; //1 or -1
headerSlider.unit            = 'whole'; 	// each or whole (only matters for sliding)
headerSlider.xmlurl          = '/library/xml/usc-12-header-slider.xml' + '?'+ Math.round(Math.random()*100000000);
headerSlider.wrapperWidth    = 262;
headerSlider.wrapperHeight   = 86;
headerSlider.perUnit         = 1; //1 to fit images in horizontally OR enter how many (if in a grid). Fading *must* be set to how many fit.
headerSlider.timeDelay       = 0; //set to zero to not autorotate, (Will stop if this is faster than it takes to complete the animation)
headerSlider.animate         = 1; // 1 yes - 0 no
headerSlider.showControls    = 1; // 1 yes - 0 no

DomReady.ready(function(){
    loadImageSwapper(headerSlider);
});
</script> 
