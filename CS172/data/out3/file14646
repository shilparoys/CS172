			$('#miAcordeon').liteAccordion({
						"containerHeight":220,
						"headerWidth":30,
						"containerWidth":800, 
						"easing":"easeInOutQuart",
						"autoPlay":"true",
						"cycleSpeed":3000
			});				   
		})(jQuery, document);

		$("#thumb1 .overThumb").mouseover(function(e) {
			e.preventDefault();
			document.getElementById("titulo1").style.color='#D22020';
			$("#thumb1 .mascara").fadeIn(600);
		});
		$("#thumb1 .overThumb").mouseout(function(e) {
			e.preventDefault();
			document.getElementById("titulo1").style.color='#666';
			$("#thumb1 .mascara")
				.stop(true,false).fadeOut(600);
		});



		$("#thumb2 .overThumb").mouseover(function(e) {
