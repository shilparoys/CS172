            <script type="text/javascript">
      var player    =  null;
      var playlist  =  null;
		var seek = 0;
		var seekFlag = false;
		var currentState;
		var scheSecHours = 0;	
		var scheSecMin = 0;
		var scheSecSec = 0;
		var firstTime = 1;


      function playerReady(obj)
      {
			 player = gid(obj.id);
			
		   var config = player.getConfig();        
			setTimeout("startTime();", 100); 
			player.addModelListener('STATE', 'stateMonitor');
		 
		  //player.sendEvent("ITEM", "1");				  
			//alert("the width of the player is " + config.width);
        //alert("autostarting is set to " + config.autostart);
        //alert("the player is currently " + config.state);

        //startTime();
      };

		
		function stateMonitor(obj)
		{

			if((obj.newstate == 'PLAYING') && (seekFlag))
			{
				seekFlag = false;
				setTimeout("player.sendEvent('SEEK', seek)", 10);
			}

			if(obj.newstate == 'IDLE' && firstTime==0)
			{
				player.sendEvent("PLAY","true");
				
				//player.sendEvent('NEXT');
			}
			currentState = obj.newstate;
		};

		function seekSome(some)
		{
			seek = some;
			firstTime=0;

			if(currentState == 'PLAYING')
			{
				player.sendEvent('SEEK', seek)
				
			}
			else
			{
				seekFlag = true;
				player.sendEvent('PLAY', 'true');
			}
		};

      function startTime()
      {
        playlist = player.getPlaylist();

        if((playlist !== null) && (playlist !== undefined))
        {
          var currentTime = new Date();
			 var secHours =  currentTime.getHours() * 3600;
			 var secMin = currentTime.getMinutes() * 60;
			 var secs = currentTime.getSeconds()*1;
          var hoursminutes = currentTime.getHours() + '.' + currentTime.getMinutes() + '.' + currentTime.getSeconds();
			 var totalSecs = 	secHours + secMin + secs;
          	
			 //alert ('H: (' +  currentTime.getHours() + ') ' +  secHours + ' M: ' + secMin + ' S: ' +  secs);	 
			 
          
 			
			for(var j in playlist)
          {
				
				//alert('starttime: ' + playlist[j].starttime);				
				var scheduledDate = new Array();
			   scheduledDate = playlist[j].starttime.split('.');
				var scheSecHours = scheduledDate[0]*3600;	
				var scheSecMin = scheduledDate[1]*60;
				var scheSecSec = scheduledDate[2]*1;
				
				//alert ('H: (' + scheduledDate[0] + ') ' + scheSecHours + ' M: ' + scheSecMin + ' S: ' +  scheSecSec);

				var InitialSecs = scheSecHours + scheSecMin + scheSecSec;
				var endSecs = InitialSecs + playlist[j].duration;
				//alert('totalSecs: ' + totalSecs + ' InitialSecs: ' + InitialSecs + ' EndSecs: ' + endSecs);
				
            if( totalSecs > InitialSecs && totalSecs < endSecs)
            {
              //alert('1 Voy a reproducir el Item '  + j);
				  var offset = 0;
				  offset =  totalSecs - InitialSecs;
				  //alert("offset (" + j +") = " + offset);
				  //alert(offset);
				  player.sendEvent("ITEM", j);
				  seekSome(offset);
              //gid('start').innerHTML = 'START TIME: ' + playlist[j - 1].starttime + '   CURRENT TIME: ' + hoursminutes + '   ITEM: ' + (j -1);
              break;
            }
				else
				{
					/*alert(' Voy a reproducir el Item '  + playlist[1].title );
					player.sendEvent("ITEM", "1");
					seekSome(20);		
					break;*/
				}
				
          }
        }
        else
        {
          player.sendEvent("ITEM", "0");
			 
        }
      };


      function gid(name)
      {
        return document.getElementById(name);
      };

		

    </script> 
