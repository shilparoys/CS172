  <div id="content" role="main"> 
   <h1 class="title heading level-one" id="page-title">Penn Trustees Approve Design for Pennovation Center at Pennovation Works Site </h1> 
   <script type="text/javascript" src="/pennnews/sites/all/libraries/shadowbox-3.0.3/shadowbox.js"></script> 
   <script type="text/javascript">Shadowbox.init()</script>
   <div class="news-contact"> 
    <div id="tools"> 
     <a href="https://facebook.com/sharer/sharer.php?u=http%3A%2F%2Fwww.upenn.edu%2Fpennnews%2Fnews%2Fpenn-trustees-approve-design-pennovation-center-pennovation-works-site" target="_blank"><img src="/pennnews/sites/default/themes/penn_news/images/social-facebook.png" alt="facebook"></a> 
     <a href="https://twitter.com/share?text=Penn Trustees Approve Design for Pennovation Center at Pennovation Works Site &amp;url=http%3A%2F%2Fwww.upenn.edu%2Fpennnews%2Fnews%2Fpenn-trustees-approve-design-pennovation-center-pennovation-works-site&amp;via=penn" target="_blank"><img src="/pennnews/sites/default/themes/penn_news/images/social-twitter.png" alt="twitter"></a> 
     <a href="https://plus.google.com/share?url=http%3A%2F%2Fwww.upenn.edu%2Fpennnews%2Fnews%2Fpenn-trustees-approve-design-pennovation-center-pennovation-works-site" target="_blank"><img src="/pennnews/sites/default/themes/penn_news/images/social-google.png" alt="google"></a> 
     <a href="#" onclick="return addthis_sendto('print');"><img src="/pennnews/sites/default/themes/penn_news/images/social-print.png" alt="print"></a> 
     <a href="#" onclick="return addthis_sendto('email');"><img src="/pennnews/sites/default/themes/penn_news/images/social-email.png" alt="email"></a> 
     <!-- AddThis Button BEGIN 
			   <script type="text/javascript">addthis_pub  = 'minicola';</script>
