  <script language="JavaScript" type="text/javascript">
            function den() {
                if (document.getElementById('txtArama').value.length == 0) {
                    document.getElementById('txtArama').value = 'aradığınız kelimeyi yazın_';
                }
                if (document.getElementById('txtMail').value.length == 0) {
                    document.getElementById('txtMail').value = 'E-posta adresinizi girin';
                }
            }
</script> 
  <script type="text/javascript">
        $(document).ready(function () {
            $("img.orj").hover(function () {
                $(this).stop().animate({ "opacity": "0" }, 300);
            },
				function () {
				    $(this).stop().animate({ "opacity": "1" }, 300);
				});
            $("#menuacil").hover(function () {
                $('.altMenu').stop(true, true).fadeIn();
            },
				function () {
				    $('.altMenu').stop(true, true).fadeOut();
				});

        });
    </script> 
