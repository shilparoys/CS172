            function uyari() {
                alert('Arama için en az üç harf girmelisiniz');
            }
            function uyari2() {
                alert('Lütfen Doğru Bir Mail Adresi giriniz...!');
            }
            function uyari3() {
                alert('Mail Adresiniz Başarıyla Kaydedildi Teşekkür Ederiz...');
            }
            function uyari4() {
                alert('Lütfen Yasaklı Karakter Kullanmayızı...');
