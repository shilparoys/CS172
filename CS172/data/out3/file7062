  $(function (){
  $('#login').stop().click(function(){
  if($('.login').is(':visible')){
    $('.login').hide();
    $(this).removeClass('activl');
  }else{
    $(this).addClass('activl');
    $('.login').show();
  }



  return false;
  });


  });
