     <script type="text/javascript">
		$(function () {
			$('#noutati ul,#evenimente ul,#legislatie ul').slimscroll({
				height: '210px',
				width: '370px',
				railVisible: true,
				alwaysVisible: true,
				railColor: '#ccc',
				opacity: 1,
				color: '#888',
				showbuttons: true
			});

			$('#noutati .slimScrollDiv,#legislatie .slimScrollDiv').css({height: 0});
			$('#noutati,#legislatie').addClass('slimclosed');
			$('#evenimente .slimScrollDiv,#evenimente .slimScrollDiv>ul').css({height: 380});
			$('.boxnews .separator').click(function (e) {
				var $t = $(this).parent();
				if ($(e.target).hasClass('more')) {
					return true;
				}
				if (!$t.hasClass('slimclosed')) {
					$t = $('#evenimente');
				}
				$('.boxnews').addClass('slimclosed');
				$t.removeClass('slimclosed');
				$('.boxnews .slimScrollDiv').css({height: 0});
				$t.find('.slimScrollDiv').css({height: 380});
				$t.find('.slimScrollDiv').find('ul').animate({height: 380});
				e.preventDefault();
			});

			///pentru scroll la boxurile de pe prima pagina
			$('.box h4').click(function () {
				$('html, body').stop().animate({
					scrollTop: 180
				}, 2000);
			});
		});
	</script> 
