//<![CDATA[
function getKeyCode(event) //returns the code for the keypressed for all browsers
{
if(event.keyCode != 0)
return event.keyCode;
else if(event.charCode != 0)
return event.charCode;
else
return event.which;
}

function keystrokeIsCommonKey(event) { //Key is null, backspace, delete or an arrow key
   if (event.keyCode == 13 || event.keyCode == 8 || event.keyCode == 9)
      return true;
   return false;
}

function keystrokeIsAllowed(event, allowedCharacters, allowCommonKeys) { //Keystroke is one of a list of allowed characters
   if (keystrokeIsCommonKey(event)) return true;
   var intKeycode = getKeyCode(event);
   var charKey = String.fromCharCode(intKeycode);
   var rgxChars = new RegExp(allowedCharacters, 'i');
   return rgxChars.test(charKey);
}

var ctl00_ContentPlaceHolder1_TabControl1_hdnAreas='';
var ctl00_ContentPlaceHolder1_TabControl1_hdnPanels='TabPanel1;TabPanel2;TabPanel3;';

function getRoot(object) //Gets the object root ID
{
   var strReturn;
   if (object.id != null && object.id.toString().lastIndexOf('_') > 0)
      strReturn = object.id.toString().substr(0,object.id.toString().lastIndexOf('_'));
   else if (object.toString().lastIndexOf('_') > 0)
      strReturn = object.toString().substr(0,object.toString().lastIndexOf('_'));
   else if (object.id != null)
      strReturn = object.id;
   else
      strReturn = object;
   return strReturn;
}

//Gets an Object by ID
function getElement(idName){
   if ( document.all )
      return document.all[idName];
   else if ( document.getElementById )
      return document.getElementById(idName);
   else if ( document[idName] )
      return document[idName];
   else if ( document.layers )
      return document.layers[idName];
   else
      return document.idName;
}

function showTabPanel(tab){
   hideTabPanels(tab);
   var root = getRoot(getRoot(tab.id));
   var hdn = getElement(root + '_hdnCurrentPanel');
   tab.className = 'selectedtab';
   getElement(getRoot(tab)).className='selectedPanel';
   hdnAreas = eval(root + '_hdnAreas');
   var areas = hdnAreas.split(',');
   for (j=0; j<areas.length-1; j++){
      var area = getElement(getRoot(tab) + '_' + areas[j]);
      if (area != null) area.style.display='';
   }
   hdn.value = getRoot(tab.id);
}

function showCurrentTabPanel(panelId){
   var tab = getElement(getElement(panelId + '_hdnCurrentPanel').value + '_tab');
   showTabPanel(tab);
}

function hideTabPanels(tab) {
   var root = getRoot(getRoot(tab));
   var hdn = eval(root + '_hdnPanels');
   var panels = hdn.split(';');
   hdn = eval(root + '_hdnAreas');
   var areas = hdn.split(',');
   for (i=0; i<panels.length; i++){
      var hideMe = getElement(root + '_' + panels[i]);
      if (hideMe != null && hideMe.className != 'panel'){
         hideMe.className = 'panel';
         hideMe = getElement(root + '_' + panels[i] + '_tab');
         hideMe.className = 'tab';
         for (j=0; j<areas.length-1; j++){
            var area = getElement(root + '_' + panels[i] + '_' + areas[j]);
            if (area != null) area.style.display='none';
         }
      }
   }
}

function showProps(obj, obj_name) { //Shows the properties of an object (DEBUG)
   var result = '';
   for (var i in obj)
   {
       try {
          result += obj_name + '.' + i + ' = ' + obj[i] + '<br />'
      } catch (exp){}
   }
   newWindow = window.open()
   newWindow.document.write('Here is the object '+obj_name+':<br /><br />');
   newWindow.document.write(result)
}

function clickButton(event, targetId) //Clicks a button when the enter key is pressed while in a textbox or other control
{
if(getKeyCode(event) == 13) // the 'enter' key
{
getElement(targetId).click();
return false;
} else {
return true;
}
}

