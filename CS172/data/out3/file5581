   <script type="text/javascript">

        // JavaScript Document
        $("#navIco").click(function() {
            $("#headerNav").animate({
                // opacity:1.0,
                height: 'toggle'
            }, 500);
        });
        var onResize = function() {
            if ($(window).width() >= 480) {
                document.getElementById("headerNav").style.display = "block"
                document.getElementById("navIco").style.display = "none"
            }
            else {
                document.getElementById("headerNav").style.display = "none"
                document.getElementById("navIco").style.display = "block"
            }
        };
        $(window).resize(onResize);
        onResize();

    </script> 
