      <script type="text/javascript">
                    $(function () {
                        $('#courseWidget a.tab').click(function (event) {
                            event.stopPropagation();
                            var curr = $(this);
                            curr.addClass('current').siblings('a').removeClass('current');
                            var classes = curr.attr('class').replace(/(tab)|(current)/gi, '').trim();
                            $('#courseWidget  div.billboard').each(function () {
                                $(this).css('display', $(this).hasClass(classes) ? '' : 'none');
                            });
                            return false;
                        }).attr('href', '#');
                    });
                </script> 
