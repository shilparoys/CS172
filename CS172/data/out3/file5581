   <script type="text/javascript">

        var intMaxLength = 140;

        $(function() {
            $('#ctl00_btnSearch').click(function() {
                var vLen = $('#ctl00_txtSearch').val().length;
                if (vLen > intMaxLength) {
                    alert('Search character limit has been reached. Results cannot be displayed until you reduce your entry.');
                    return false;
                }
                $('#ctl00_txtSearch').val(encode($('#ctl00_txtSearch').val()));
            });
        });
    </script> 
   <script type="text/javascript" language="javascript">$(function() {$('#ctl00_txtSearch').autocomplete({source: 'http://www.ed2go.com/AutoComplete.aspx',minLength: 2,open: function(){$(this).autocomplete('widget').css('z-index', 100);return false;},select: function(event, ui) {var v = ui.item ? ui.item.value : '';$('#ctl00_txtSearch').val(v);$('#ctl00_hidIsAutocomplete').val('1');$('#ctl00_btnSearch').click();}});});</script> 
