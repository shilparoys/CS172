YUI.add('ucs-inject-mobile', function (Y) {

  "use strict";
  
  Y.namespace('ucs');
  
  var ucm                     = Y.ucs.ConfigMobile,
      utils                   = Y.ucs.UtilsMobile,
      YArray                  = Y.Array,
      YObject                 = Y.Object,
      YLang                   = Y.Lang;

  Y.ucs.InjectMobile = function (Y) {
      this._initCustomItems();
  };
  Y.ucs.InjectMobile.prototype = {
      _initCustomItems: function() {
  Y.Global.once(ucm.NAV_TOGGLE_EVT, function(e) {
          var et = new Y.EventTarget();                                         
          et.publish(ucm.ADD_NAV_ITEMS, {broadcast:2,emitFacade:true});
          et.fire(ucm.ADD_NAV_ITEMS, YG_LEFT_MENU_DATA);});
      }
  }
}, '3.0', {
  requires: ['node', 'ucs-navigation-mobile', 'event-custom']
});

(function () {
  "use strict";
  
  YUI().use('node','event-custom', 'ucs-inject-mobile', function (Y) {
  
      var container = Y.one('#' + Y.ucs.ConfigMobile.NAV_CONTAINER);

      if (container && !window.ucs.inject) {
  
          window.ucs.inject = new Y.ucs.InjectMobile(container);
      }   
  
  }); 
}());

