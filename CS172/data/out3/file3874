    if (typeof YMaps.TileVersionManagerCallback === "undefined") {
        YMaps.TileVersionManagerCallback = function (v) {
            try {
                YMaps.mapTileVersion = v.BaseResult.maps.map[1].hash;
                YMaps.aerialTileVersion = v.AerialResult.maps.map[1].hash;
                YMaps.trafficTileVersion = v.TrafficResult.maps.map[1].hash;
                YMaps.coverageTileVersion = v.PanoResult.maps.map[1].hash;
            } catch (e) {
                YMaps.mapTileVersion = 'newest';
                YMaps.coverageTileVersion = 'newest';
                YMaps.aerialTileVersion = 'newest';
                YMaps.trafficTileVersion = 'newest';
            }
            YUI.add('ymaps-core-tileversion', function () {
            });
        };
    }
}());

