    <h1 class="page-title">The parts of UC</h1> 
    <div class="content"> 
     <!-- Sidebar Container Desktop --> 
     <nav class="sidebar-container" role="navigation"> 
      <div class="sidebar left-sidebar-upper"> 
       <div class="background"></div> 
       <div class="region region-left-sidebar-upper"> 
