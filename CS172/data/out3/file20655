  <script type="text/javascript">
			//====================================================
			function submitLoginForm() {
				xhr = jQuery.ajax({
					url: "login_process.php",
					type: "POST",
					dataType: "json",
					data: jQuery("#s-libapps-login-form").serialize(),
					success: function(response, textStatus, jqXHR) {
						if (response.data.result == 1) {
							window.location.replace(response.data.target);	
						}
						else {
							resetLoginForm();
							setMessage(response.data.html, "alert alert-danger");
						}
					},
					error: function (jqXHR, textStatus, errorThrown) {
						alert("Oops, sorry! Something unexpected happened: " + errorThrown + " \n\nThat might not mean much to you, but it probably does to the Springy Techs...you can let them know at support@springshare.com.");
					}
				});	
			}
			//====================================================
			function submitResetForm() {
				xhr = jQuery.ajax({
					url: "login_process.php",
					type: "POST",
					dataType: "json",
					data: jQuery("#s-libapps-reset-form").serialize(),
					success: function(response, textStatus, jqXHR) {
						if (response.data.result == 1) {
							setMessage("", "");
							jQuery("#s-libapps-public-content").html("<div style=\"font-size:1.2em;\">" + response.data.html + "</div>");
							jQuery("#s-libapps-reset-div").hide();
						}
						else {
							setMessage(response.data.html, "alert alert-danger");
							resetResetPasswordForm();	
						}
					},
					error: function (jqXHR, textStatus, errorThrown) {
						alert("Oops, sorry! Something unexpected happened: " + errorThrown + " \n\nThat might not mean much to you, but it probably does to the Springy Techs...you can let them know at support@springshare.com.");
					}
				});	
			}
			//====================================================
			function resetLoginForm() {
				jQuery("#s-libapps-login-button").button("reset");
				jQuery("#s-libapps-password").val("").focus();
			}
			//====================================================
			function resetResetPasswordForm() {
				jQuery("#s-libapps-password-reset-button").button("reset");	
			}
			//====================================================
			function setMessage(text, class_name) {
				if (class_name == "") {
					class_name = "alert alert-info";	
				}
				jQuery("#s-libapps-public-message").html(text).attr("class", class_name).toggle(text != "");
			}
			//====================================================
		</script> 
  <!-- END: Page JS -->   
 </body>
