 <head> 
  <title>1098-T Information Login</title> 
  <script>
     function verifySID () {
        var sid=document.hopeLogin.sid.value;
        var pin=document.hopeLogin.pin.value;
        if (sid.length!=9){
          alert("A nine character SID is required");
          return false

        }else{
        if (pin.length<4){
          alert("You must enter a 4 to 6 digit PIN.");
          return false;
        } }
        document.hopeLogin.submit();
        return true;
    }
    </script> 
 </head> 
