            //for new matrix textbox sum total feature 7/14/2012
            function sumRow(cellchanged) {
              var sumCol = 0;
              var sumRow = 0;
              var sum = 0;
              var thisRow = $(cellchanged).parent('tr');

              //below gets Answer23241 the id of the cell in the matrix thats value was changed.
              var cellid = $(cellchanged).attr('id');
              //sum all input boxes in the column where input ids contain Answer23241. All inputs in column have same AnswerID (Answer23241) passed into function as cellchanged.
              $("input[id^=" + cellid + "]").each(function() {
              //only add if it is a number.  this is required or will concatenate as a string.
              if(!isNaN(parseFloat(this.value)))
              { sumCol += parseFloat(this.value);}
              });

            //put sum in the total box below this column only, identified by the Answer23241 (totalfieldAnswer23241 is id of total box for column)
            var totalFieldInputId = "totalfield" + cellid;
            $("input[id=" + totalFieldInputId + "]").val(sumCol);

             //now sum row totals, all input fields with the same SIID will be summed together. 
             //get SIID from current cell. name='QID2324||SIID4047||AID5380'
             var splitChar = "||";
             var rowAID= $(cellchanged).attr('name').split(splitChar)[2];
             var rowSIID = $(cellchanged).attr('name').split(splitChar)[1];

             //get row number part out of title before the space. (title='row2 5383')
             //get row number part out of class attribute
             //var rowNumberPart = $(cellchanged).attr('title').split(' ');
             //remove "row" to get number
             //var rowNumber = rowNumberPart[0].substring(3);
             //class='sum1'
             var rowNumberPart = $(cellchanged).attr('class');
             //remove "sum" to get number
             var rowNumber = rowNumberPart.substring(3);

             //select and sum all inputs that have this rowSIID in substring for name
               $("input[name*=" + rowSIID + "]").each(function() {
                if(!isNaN(parseFloat(this.value)))
              { sumRow += parseFloat(this.value);}
              });

              //set the sum into the total row text box.
              $("input[id=totalRow" + rowNumber + "]").val(sumRow);

            }


       

       //for new clickable image question type 10-7-2012
       
        function clickableImageMapsClick(itemid,mapid,alttext)
        {
	        var imgmapSelectedText = "";
	        var imgmapClickText = 0;
            //see if this area was already clicked
            imgmapSelectedText = $("textarea[id=QID" + itemid + "]").val();
   
            if(!isNaN(parseInt($("input[id=QID" + itemid + "OTHERTEXT]").val()),10))
            {
                imgmapClickText = parseInt($("input[id=QID" + itemid + "OTHERTEXT]").val(),10);
            }

            //split string into array by comma separator
            var selectedAreaArray = imgmapSelectedText.split(",");
            var tmpTextStr = "";
            var newBuiltStringFromArray = "";
            var existsYN = "N";
            for (var i = 0; i < selectedAreaArray.length; i++) 
            { 
                   tmpTextStr = selectedAreaArray[i].trim();
                   if(tmpTextStr.length > 0)
                   {
                       if(tmpTextStr == alttext) 
                       {
                            //exists, so remove it from string by not adding it back to string.
                            existsYN = "Y";
                       }
                       else
                       {
                           //does not exist, add to string
                           newBuiltStringFromArray = newBuiltStringFromArray + tmpTextStr + ", ";
                       }
                   }
            } 
            if(existsYN == "Y")
            {
                imgmapClickText = parseInt(imgmapClickText,10) - 1;
            }
            else
            {
                newBuiltStringFromArray = newBuiltStringFromArray +  alttext + ", ";
                imgmapClickText = parseFloat(imgmapClickText) + 1;
            }
            //replace with rebuilt string
            imgmapSelectedText = newBuiltStringFromArray;
           
            
            //update value of text area
            $("textarea[id=QID" + itemid + "]").val(imgmapSelectedText);

            //update value of click status textbox
            $("input[id=QID" + itemid + "OTHERTEXT]").val(imgmapClickText);

        }
       


    
           

