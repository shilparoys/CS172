	function startResponseTimer(timeLeft, timerDuration, responseId)
	{
	    var idTimeElapsed = document.getElementById('idTimeElapsed');
	    var idTimeRemaining = document.getElementById('idTimeRemaining');
	    var idTimeTotal = document.getElementById('idTimeTotal');
	    if (idTimeElapsed == null || idTimeRemaining == null || idTimeTotal == null)
	    {
	        setTimeout("startResponseTimer(" + timeLeft + ", " + timerDuration + ", " + responseId + ")", 100);
	        return;
	    }
	    
	    idTimeElapsed.innerHTML = timerDuration - timeLeft;
	    idTimeRemaining.innerHTML = timeLeft;
	    idTimeTotal.innerHTML = timerDuration;
	    
	    setTimeout("responseTimerTick(" + timeLeft + ", " + timerDuration + ", " + responseId + ")", 60000);
	}
	
	function alertCommitChanges()
	{
	    //alert('Time has run out.  Last changes committed!');
	    alert('Time has run out.  Last changes committed!');
	    submitCheck(4);
	}
   
	function updateResponseTime(responseId, newTime)
	{
	   var ua = navigator.userAgent.toLowerCase();
       if (!window.ActiveXObject)
         request = new XMLHttpRequest();
       else if (ua.indexOf('msie 5') == -1)
         request = new ActiveXObject("Msxml2.XMLHTTP");
       else
         request = new ActiveXObject("Microsoft.XMLHTTP");
         
       var httpRequest = "SurveyUpdateTime.aspx?ResponseId=" + responseId + "&minutesCompleted=" + newTime;
         
       request.open("GET", httpRequest);
       request.send(null);
	}
	

	
	function responseTimerTick(timeLeft, timerDuration, responseId)
	{
	    timeLeft--;
	    
	    var idTimeElapsed = document.getElementById('idTimeElapsed');
	    var idTimeRemaining = document.getElementById('idTimeRemaining');
	    var idTimeTotal = document.getElementById('idTimeTotal');
	    
	    updateResponseTime(responseId, timerDuration - timeLeft);
	    
	    idTimeElapsed.innerHTML = timerDuration - timeLeft;
	    idTimeRemaining.innerHTML = timeLeft;
	    idTimeTotal.innerHTML = timerDuration;
	    
	    if (timeLeft <= 0)
	    {
	        alertCommitChanges();
	        return;
	    } 
	    
	    setTimeout("responseTimerTick(" + timeLeft + ", " + timerDuration + ", " + responseId + ")", 60000);
	}

