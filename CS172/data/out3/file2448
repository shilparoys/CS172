  <script language="JavaScript">
function updateClock2(iTimeOutAlert)
{
setTimeout("updateClock('" + iTimeOutAlert + "');", 100);
dNow = new Date();
dWatch = dNow.getTime() - dStarted.getTime();
dClock = Math.round(dWatch/1000);
if (dClock == iTimeOutAlert)
{
alert('Please enter a numeric value for question number (#questionnumber#)');
}
}


function isDate2(dateStr) {
var datePat = /^(\d{1,2})(\/|-|.)(\d{1,2})(\/|-|.)(\d{4})$/;
var matchArray = dateStr.match(datePat); // is the format ok?
if (matchArray == null) {
    alert('Please select at least (#number#) for question number (#questionnumber#)');
    return false;
}
month = matchArray[1]; // parse date into variables
day = matchArray[3];
year = matchArray[5];
if (month < 1 || month > 12) { // check month range
    alert('Please select at least (#number#) for question number (#questionnumber#)');
    return false;
}
 if (day < 1 || day > 31) {
     alert('Please enter a numeric value for question number (#questionnumber#)');
     return false;
 }
 if ((month==4 || month==6 || month==9 || month==11) && day==31) {
     alert('Please select no more than (#number#) for (#questionnumber#)')
     return false;
 }
 if (month == 2) { // check for february 29th
    var isleap = (year % 4 == 0 && (year % 100 != 0 || year % 400 == 0));
    if (day > 29 || (day==29 && !isleap)) {
        alert('Please select no more than (#number#) for (#questionnumber#)');
        return false;
    }
 }
return true; // date is valid
}


function isDateEuropean2(dateStr) {
var datePat = /^(\d{1,2})(\/|-|.)(\d{1,2})(\/|-|.)(\d{4})$/;
var matchArray = dateStr.match(datePat); // is the format ok?
if (matchArray == null) {
    alert('Please select at least (#number#) for question number (#questionnumber#)');
return false;
}
day = matchArray[1]; // parse date into variables
month = matchArray[3];
year = matchArray[5];
if (month < 1 || month > 12) { // check month range
    alert('Please select at least (#number#) for question number (#questionnumber#)');
return false;
}
if (day < 1 || day > 31) {
     alert('Please enter a numeric value for question number (#questionnumber#)');
return false;
}
 if ((month==4 || month==6 || month==9 || month==11) && day==31) {
     alert('Please select no more than (#number#) for (#questionnumber#)')
return false;
}
if (month == 2) { // check for february 29th
var isleap = (year % 4 == 0 && (year % 100 != 0 || year % 400 == 0));
if (day > 29 || (day==29 && !isleap)) {
        alert('Please select no more than (#number#) for (#questionnumber#)');
 return false;
 }
}
return true; // date is valid
}

function doSubmit(){
    $('#frmSurvey').unbind('submit');
document.forms['frmSurvey'].action = 'TakeSurveyAction.aspx';
    document.forms['frmSurvey'].submit();
}
function submitCheck(button){setButtonClick(button);if(button == '2'){
    doSubmit();
return;
}
var flgValueSelected;
var lngTotalValue = 0;
var strTemp;
var strSelectedValues ='';
if (trim(document.forms['frmSurvey'].QID8553.value) != ''){if (isDate2(trim(document.forms['frmSurvey'].QID8553.value)) == false){document.forms['frmSurvey'].QID8553.focus();return;}
}
doSubmit();}
