    	function doPrivacyStatement(width, height) {
			var theURL = "http://corp.collegenet.com/privacy.html"
			var left = (screen.width) ? (screen.width - width) / 2 : 100
			var top = (screen.height) ? (screen.height - height) / 2 : 100
			var features = "width=" + width + ",height=" + height + ",top= " + top + ",left= " + left
			features += ",scrollbars=yes,resizable=yes,directories=no"
			windowRef = window.open(theURL, 'popup', features)
			windowRef.focus();
	  }    	
	  
	  function doTermsOfUse(width, height) {
			var theURL = "http://corp.collegenet.com/Terms.html"
			var left = (screen.width) ? (screen.width - width) / 2 : 100
			var top = (screen.height) ? (screen.height - height) / 2 : 100
			var features = "width=" + width + ",height=" + height + ",top= " + top + ",left= " + left
			features += ",scrollbars=yes,resizable=yes,directories=no"
			windowRef = window.open(theURL, 'popup', features)
			windowRef.focus();
	  }
    </script> 
