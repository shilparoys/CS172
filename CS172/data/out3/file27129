   <script>
$("#thumbs_only_btn").click(function() {
	$("#view_thumbs_popup").css({
		visibility:"hidden"
	});
	if (vars.thumbs_only == "not set" || !vars.thumbs_only){
		vars.thumbs_only = 1;
		$("#thumbs_only_img").attr("src", "./v1site_images/graphics/hide_thumbs.png");
		//$("#thumbs_only_btn").html("hide&nbsp;thumbnail&nbspview");
	} else {
		vars.thumbs_only = 0;
		$("#thumbs_only_img").attr("src", "./v1site_images/graphics/view_thumbs.png");
		//$("#thumbs_only_btn").html("show&nbsp;thumbnail&nbspview");
	}
	$.address.path("refresh_gallery_view");
});
</script> 
