	//set sound vars
	var sound_vars = {};
	sound_vars.play_on_load = "yes";
	if (sound_vars.play_on_load == "yes"){
		sound_vars.sound_status = "playing";
	} else {
		sound_vars.sound_status = "paused";
	}
	sound_vars.loop = "yes";
	sound_vars.start_vol = "50";
