   <script type="text/javascript">
$("#menu_btn_over").data("opened", false);
$("#menu_btn_over").data("loaded", false);
$("#menu_btn_over").data("loaded", true);
$("#img_menu_btn_over").load(function(){
	
	
	//alert($("#menu_btn_over").data("loaded"));
	$("#menu_btn_over").data("loaded", true);
});

function position_menu_btn_over(){
	
	/*if ($("#menu_btn_over").data("loaded") && !$("#menu_btn_over").data("opened") && active_btn > 0 && active_btn <= btn_count){
		$("#img_menu_btn_over").width($("#btn_div" + active_btn).width() + 10 * 2);
		$("#img_menu_btn_over").height($("#btn_div" + active_btn).height() + 5 * 2);
		$("#menu_btn_over").data("opened", true);
		$("#menu_btn_over").css({
			top: $("#btn_div" + active_btn).position().top + $("#btn_div" + active_btn).height(),
			left: $("#btn_div" + active_btn).position().left,
			display: "none",
			visibility: "visible"
		});
		$("#menu_btn_over").html($("#btn" + active_btn).html());
		$("#menu_btn_over").css({ 
					color: "#" + vars.curr_color
				});
		$("#menu_btn_over").fadeIn();
	}*/
	if (active_btn > 0 && active_btn <= btn_count){
		var left = $("#btn_div" + active_btn).position().left;
		if (active_sec == "home"){
			left -= 16;
		}
		var top = $("#btn_div" + active_btn).position().top + $("#btn_div" + active_btn).height()
		

		$("#menu_btn_over").css({
			top: top,
			left: left,
			display: "none",
			visibility: "visible"
		});
		//alert($("#btn" + active_btn).html());
		$("#menu_btn_over").html($("#btn" + active_btn).html());
		$("#menu_btn_over").css({ 
					//color: "#" + vars.curr_color
				});
		$("#menu_btn_over").fadeTo("fast", 1);
	}
}

$(window).resize(function(){
	position_menu_btn_over();
});
</script> 
