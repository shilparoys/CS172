       <!-- **************************** START PAGE TITLE ************************************ --> 
       <h1>National Veterans Creative Arts Festival</h1> 
       <!-- **************************** END PAGE TITLE ************************************** --> 
      </div> 
      <!--End main content wrapper --> 
      <div id="tier2gallery">
       <!-- begin apple images gallery--> 
       <div id="gallery">
        <!-- begin apple images gallery--> 
        <img src="http://www.va.gov/opa/speceven/caf/images/nvcaf_banner2.jpg" width="740" height="341" alt="Photo of a Veteran playing a trumpet"> 
       </div>
       <!--close gallery--> 
      </div>
