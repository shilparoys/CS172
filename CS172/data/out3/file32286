 --> 
        <table width="140" border="0" cellpadding="0" cellspacing="0" id="cal_small"> 
         <tbody>
          <tr> 
           <td> 
            <form method="get" action="index.php"> 
             <div id="calendar-container"></div> 
             <script id="flat_calendar_setup" type="text/javascript">
  function dateChanged(calendar) {
    // Beware that this function is called even if the end-user only
    // changed the month/year.  In order to determine if a date was
    // clicked you can use the dateClicked property of the calendar:
    if (calendar.dateClicked) {
      // OK, a date was clicked, redirect to /yyyy/mm/dd/index.php
      var from_date = calendar.date,
          // Add a day's worth of Milliseconds to get tomorrow's date
          to_date = new Date(calendar.date.valueOf() + 86400000),
          ymd1 = encodeURIComponent([
                ("0" + (from_date.getMonth() + 1)).substr(-2), 
                ("0" + from_date.getDate()).substr(-2),
                from_date.getFullYear()
            ].join('/')),
          ymd2 = encodeURIComponent([
            ("0" + (to_date.getMonth() + 1)).substr(-2), 
            ("0" + to_date.getDate()).substr(-2),
            to_date.getFullYear()
            ].join('/'));
      // redirect...
      window.location = "https://www.usc.edu/calendar?from=" + ymd1 + '&to=' + ymd2;
    }
  };

  Calendar.setup(
    {
      weekNumbers  : false,
      flat         : "calendar-container", // ID of the parent element
      flatCallback : dateChanged           // our callback function
          }
  );
</script> 
            </form> </td> 
          </tr> 
         </tbody>
        </table> 
        <!--  - - - - - - - - - - - - - - - - - - - - -
