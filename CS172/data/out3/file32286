    Calendar.setup({
        inputField     :    "from",
        ifFormat       :    "%m/%d/%Y",
        showsTime      :    false,
        weekNumbers    :    false,				
        timeFormat     :    "24"
    });
    Calendar.setup({
        inputField     :    "to",
        ifFormat       :    "%m/%d/%Y",
        showsTime      :    false,
        weekNumbers    :    false,					
        timeFormat     :    "24"
    });
</script> 
