	jQuery(window).load(slideshowResize);
	jQuery(window).resize(slideshowResize);
	function slideshowResize (){
		$slideshow=$("[id^='slideshow'].slideshow");
		$slide=$("[id^='slideshow'].slideshow > *");
		if ( ($slide.width()/$slide.height()) < ($slideshow.width()/$slideshow.height()) ) {
			// slide is narrowish and tallish, while viewport is widish and shortish, so slide width needs to be the same width as the viewport, and height needs to be auto
			$slide.height("auto").width($slideshow.width());
			// should the slide height spill out of the viewport, lets vertically center it
			$imgTop=-(($slide.height()-$slideshow.height() )/2);
			$slideshow.css("top",$imgTop).css("left","0").animate({opacity: 1}, 400);
		} else {
			// slide is widish and shortish, while viewport is narrowish and tallish, so slide height needs to be the same height as the viewport, and width needs to be auto
			$slide.width("auto").height($slideshow.height());
			// should the slide width spill out of the viewport, lets horizontally center it
			$imgLeft=-(($slide.width()-$slideshow.width() )/2);
			$slideshow.css("left",$imgLeft).css("top","0").animate({opacity: 1}, 400);
		}
	}

//]]>
</script> 
