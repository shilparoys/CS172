      <script type="text/javascript">
$(document).ready(function(){
	$(".jw-booking-mask-button").click(function() {
	datein=$("input[name=dateIn]").val();
	//alert(datein)
	//dateout=$("input[name=dateout]").val();
	//document.getElementByID('checkInDate').value=datein;
	var element=datein.split("/");
	$(".checkInDate").val(element[1]);
	arrivalmonth=element[0] - 1;
	if(arrivalmonth<10)
	{
		arrivalmonth='0'+arrivalmonth;
	}

	$(".checkInMonthYear").val(arrivalmonth+element[2]);



		var newDate = new Date(datein);
		var moreNights = parseInt(jQuery('#jw-booking-mask-nights5').val());

		newDate.setDate(newDate.getDate() +moreNights);
		//alert(newDate)
		day = newDate.getDate();
		month = newDate.getMonth();
		month = parseInt(month)+1;
		year = newDate.getFullYear();
		//M-dd-yy

		if(day<10)
		{
			day = '0'+day;
		}
		if(month<10)
		{
			month = '0'+month;
		}
		dateout = month+'/'+day+'/'+year;
		//alert("dateout="+dateout);
		//return false;
		$("input[name=dateOut]").attr("value",dateout);
		//return false;
		});
});
</script> 
