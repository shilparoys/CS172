	function setOverlayDimensions(id,width, height){
		widget_id = "jw-feature-box" + id;
		var activeTab = "." + widget_id + " .jw-feature-box-overlay";
		jQuery(activeTab).css("width",width);
		jQuery(activeTab).css("height",height);
	}
