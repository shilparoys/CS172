  var $twwgt = $(id);
  var decorate = function($el) {
    $el.find(".timeline-header h1.summary").css('font-size', '13px');
    $el.find(".timeline-header h1.summary").css('margin-bottom', '-3px');
    $el.find(".p-author .p-name").css('font-size', '12px');
    $el.find(".p-author .p-name").css('line-height', '14px');
    $el.find(".timeline .e-entry-title").css('font-size', '12px');
    $el.find(".timeline .e-entry-title").css('line-height', '14px');
    return $el;
  };
  //ウィジェットが読み込まれるまで再帰
  if ($twwgt.length > 0 && $twwgt[0].contentWindow.document.body.innerHTML !== "") {
    decorate($($twwgt[0].contentWindow.document));
  } else {
    setTimeout(function() {
      decorateWidget(id);
    }, 500);
  }
}
decorateWidget("#twitter-widget-0");
</script> 
     </div> 
     <!-- _______ SeishinTopics Twitterの表示 end_______ --> 
