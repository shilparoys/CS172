       <div id="main-content"> 
        <span class="posted-on">Friday, 27 February 2015</span> 
        <p class="current">Current Temp: 53°F/12°C<br> Humidity: 71% <br>Wind Speed: 4mph</p> 
        <script language="javascript" type="text/javascript">

var popUpWin=0;
