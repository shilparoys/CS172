   <script>
jQuery(document).ready(function(){

		
	jQuery("li.fb a").mousedown(function () {
		try {
			pageTracker._trackEvent("Outbound links", "Click", "Facebook");
		} catch(err) {}
	});
	jQuery("li.tw a").mousedown(function () {
		try {
			pageTracker._trackEvent("Outbound links", "Click", "Twitter");
		} catch(err) {}
	});
	jQuery("a[title=Enviar email]").mousedown(function () {
		try {
			pageTracker._trackEvent("Outbound links", "Click", "Mail");
		} catch(err) {}
	});
	jQuery("div#location a[title=Campus Alameda]").mousedown(function () {
		try {
			pageTracker._trackEvent("Footer Navigation", "Click", "Campus Alameda");
		} catch(err) {}
	});
	jQuery("div#location a[title=Campus Taguspark]").mousedown(function () {
		try {
			pageTracker._trackEvent("Footer Navigation", "Click", "Campus Taguspark");
		} catch(err) {}
	});
	jQuery("div#location a[title=Campus Tecnológico e Nuclear]").mousedown(function () {
		try {
			pageTracker._trackEvent("Footer Navigation", "Click", "Campus Tecnológico e Nuclear");
		} catch(err) {}
	});	



	jQuery("div.spot_page a").mousedown(function () {

		// Save link clicked and iterator
		var spotPage = jQuery(this).attr("href");
		var spotPagei = 0;

		jQuery("div.spot_page").find("a").each(function (i, l) {
			if(l == spotPage) {
				spotPagei = i+1;
			};
		})
		try {
			pageTracker._trackEvent("Banner links", "Click", spotPage, spotPagei);
		} catch(err) {}
	})

	 
	
	jQuery("div.wallpapers").find("li.image a").each(function () {
		jQuery(this).mousedown(function () {
			try {
				var url = jQuery(this).attr("href");
				var filename;
				url = url.substring(0, (url.indexOf("#") == -1) ? url.length : url.indexOf("#"));
				url = url.substring(0, (url.indexOf("?") == -1) ? url.length : url.indexOf("?"));
				filename = url.substring(url.lastIndexOf("/") + 1, url.length);

				filename = filename.replace(/^wallpaper_IST_/, '');
				filename = filename.replace(/.png/, '');
				filename = filename.replace(/_/, ' ');

				pageTracker._trackEvent("Wallpapers", "Download", filename);

			} catch(err) {}	
		})
	})


})
</script>
