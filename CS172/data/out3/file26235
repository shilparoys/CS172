  <script type="text/javascript">
		function numberWithCommas(x) {
			return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
		}
		
		jQuery(document).ready(function(){
                    var number=jQuery('.rs_weekgoal_currentamount').text();
                    jQuery('.rs_weekgoal_currentamount').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_weekgoal_tar_amount').text();
                    jQuery('.rs_weekgoal_tar_amount').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_fb_share_num').text();
                    jQuery('.rs_fb_share_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_fb_cur_num').text();
                    jQuery('.rs_fb_cur_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_fb_target_num').text();
                    jQuery('.rs_fb_target_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_twit_cur_num').text();
                    jQuery('.rs_twit_cur_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_twit_target_num').text();
                    jQuery('.rs_twit_target_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_pin_cur_num').text();
                    jQuery('.rs_pin_cur_num').text(numberWithCommas(number));
                    
                    var number=jQuery('.rs_pin_target_num').text();
                    jQuery('.rs_pin_target_num').text(numberWithCommas(number));                  
		
		});
</script> 
  <script type="text/javascript">
jQuery(document).ready(function(){

	var start_time  = 1424978580;
	var end_time    = 1425315600;

	var diff = end_time - start_time;
	updateTime();
	var timerHandler  = setInterval(function(){updateTime()}, 1000 );
	
	function updateTime() {
	
		var days =	Math.floor( diff / (60*60*24) );
		var hours = Math.floor( diff / (60*60) );
		var mins  = Math.floor( diff / (60) );
		var secs  = Math.floor( diff );
	
		var dd=days;
		var hh = hours-days*24;
		var mm = mins  - hours * 60;
		var ss = secs  - mins  * 60;
		
		if(dd<=9){
			var dd='0'+dd.toString();
		}
		if(hh<=9){	
			var hh='0'+hh.toString();
		}
		if(mm<=9){	
			var mm='0'+mm.toString();
		}
		if(ss<=9){
			var ss='0'+ss.toString();
		}
		
		if(diff<0){
			dd = '00';
			hh = '00';
			mm = '00';
			ss = '00';
		}
		var ddStr=dd.toString();
		var hhStr=hh.toString();
		var minStr=mm.toString();
		var ssStr=ss.toString();
	
		var ddFirst=ddStr[0];
		var ddSecond=ddStr[1];	
		var hhFirst=hhStr[0];
		var hhSecond=hhStr[1];
		var minFirst=minStr[0];
		var minSecond=minStr[1];
		var ssFirst=ssStr[0];
		var ssSecond=ssStr[1];
	
		jQuery('.rs_campaign_left_ddfirst').text(ddFirst);	
		jQuery('.rs_campaign_left_ddsec').text(ddSecond);	
		jQuery('.rs_campaign_left_hhfirst').text(hhFirst);	
		jQuery('.rs_campaign_left_hhsec').text(hhSecond);	
		jQuery('.rs_campaign_left_minfirst').text(minFirst);	
		jQuery('.rs_campaign_left_minsec').text(minSecond);	
		jQuery('.rs_campaign_left_ssfirst').text(ssFirst);	
		jQuery('.rs_campaign_left_sssec').text(ssSecond);
		
		diff = diff - 1;
	
		if(diff<0){
			clearTimeout(timerHandler);
		}
	}
});	
</script> 
