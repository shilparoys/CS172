        jQuery(function(){
            jQuery(window).scroll(function(){
                if(jQuery(this).scrollTop() > 150 && windowWidth > 700){
                     jQuery('.rs_sticky_targetbar').fadeIn();
                     
                }
                else{
                    jQuery('.rs_sticky_targetbar').fadeOut();
                }
            });
            });      
