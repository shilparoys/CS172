  <script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery.ajaxSetup({ cache: true });
		jQuery.getScript('//connect.facebook.net/en_UK/all.js', function(){
			//FB.init({
			//	appId: '587230251364888',
			//	channelUrl: 'http://modropy.org'
			//});
			jQuery('#loginbutton,#feedbutton,#fb_st_feedbutton').removeAttr('disabled');
			//FB.getLoginStatus(updateStatusCallback);
		});
		jQuery('#feedbutton,#fb_st_feedbutton').click(function(){
			FB.ui({
				method: 'feed',
				link: 'http://modropy.org/week/cam040-barcs/',
				picture: 'https://d101adiav9o6ca.cloudfront.net/wp-content/uploads/2015/02/C40-Cause-5.jpg',
				name: 'Modropy + BARCS',
				caption: 'modropy.org',
				description: 'This week, Modropy is partnering with BARCS to help place these animals in permanent homes through adoption. For each item purchased this week, Modropy will donate $8 to BARCS to support their volunteer, foster and off site adoption programs which are all increasing the opportunity for these animals to find their “forever” homes.'
			}, function(response){
				if (response && response.post_id) {
					alert('Post was Shared.');
				} else {
					alert('Post was not Shared.');
				}
			});
		});
	});
</script> 
