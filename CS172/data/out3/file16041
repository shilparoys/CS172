     <a id="main-content"></a> 
     <h1 class="title" id="page-title">Phonebook</h1> 
     <div class="view view-ldap-person-view view-id-ldap_person_view view-display-id-page_1 phonebook-listing view-dom-id-4d6609ff10fc9e4fcd0df069a3bedc4b"> 
      <div class="view-filters"> 
       <form action="/phonebook" method="get" id="views-exposed-form-ldap-person-view-page-1" accept-charset="UTF-8">
        <div>
         <div class="views-exposed-form"> 
          <div class="views-exposed-widgets clearfix"> 
           <div id="edit-cn-wrapper" class="views-exposed-widget views-widget-filter-cn"> 
            <div class="views-widget"> 
             <div class="form-item form-type-textfield form-item-cn"> 
              <input type="text" id="edit-cn" name="cn" value="" size="30" maxlength="128" class="form-text"> 
             </div> 
            </div> 
           </div> 
           <div id="edit-type-wrapper" class="views-exposed-widget views-widget-filter-attribute_5"> 
            <div class="views-widget"> 
