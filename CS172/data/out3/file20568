                <button id="retail-availability-search-cancel-button" class="button secondary mrs">
                    <span>{{cancelButton}}</span>
                </button>
                <button id="retail-availability-search-select-store-button" class="button merchandising">
                    <span class="select-store">{{selectButton}}</span>
                </button>
            </div>
            {{/selectedStore}}
        </div>
    </div>
</div>
</script> 
