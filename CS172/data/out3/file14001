$('#stories_programs').after('<ul id="storiesSliderNav">').cycle({ 
    fx:     'fade', 
    speed:  '1000', 
    timeout: '6000', 
    pager:  '#storiesSliderNav',
	pause:   1,
    pauseOnPagerHover: true, 
	startingSlide: 0, // zero-based 

pagerAnchorBuilder: function(id, slide) { 
		var s=$(slide);
		var imgsource=s.find('img.storiesIMG').attr('src');
        var new_src = imgsource.substring(0,imgsource.length-12);
        new_src += "-82x50";
        new_src += imgsource.substring(imgsource.length-4,imgsource.length);		
        return '<li><a href="#"><span><img src="' + new_src + '" width="82" height="50" /></a></span></li>';	
}
});

$('#stories').after('<ul id="storiesNav">').cycle({ 
    fx:     'fade', 
