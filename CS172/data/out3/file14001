	jQuery("ul#aea-slider li#ba-degree").hover( function () {
		jQuery(this).children("div.slider-content").children(".caption").clearQueue().stop(true, true).delay(500).animate({ height: "180px", width: "670px" }, 1000 ).children(".info").clearQueue().stop(true, true).delay(1200).fadeIn();
	}, function () {
		jQuery(this).children("div.slider-content").children(".caption").clearQueue().stop(true, true).delay(500).animate({ height: "", width : "190px" }, 1000 ).children(".info").clearQueue().stop(true, true).fadeOut();
	});
	//aea fields of study
	jQuery("ul#aea-slider li#masters-degree").hover( function () {
		jQuery(this).children("div.slider-content").children(".caption").clearQueue().stop(true, true).delay(500).animate({ height: "180px", width: "670px" }, 1000 ).children(".info").clearQueue().stop(true, true).delay(1200).fadeIn();
	}, function () {
		jQuery(this).children("div.slider-content").children(".caption").clearQueue().stop(true, true).delay(500).animate({ height: "", width : "150px" }, 1000 ).children(".info").clearQueue().stop(true, true).fadeOut();
	});
	
