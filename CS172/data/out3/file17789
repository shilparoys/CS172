	    callback:function(cal) {
	      var selectedDate = cal.currentDate.toString('yyyyMMdd');
		  var eventLimit = 2;
	      var eventDataUrl = "/wp-content/themes/tbn-2013/returnEvents2.php?date="+selectedDate+"&formatted="+cal.currentDate.toString("MM/dd/yyyy")+"&limit="+eventLimit;
	      jQuery('#finder-event-details').hide();
	      jQuery('#finder-event-details').load(eventDataUrl, function() {
		  jQuery('#finder-event-details').fadeIn(600);
	    });
	}});
