       <h2>Fields of study</h2> 
       <div class="content">
        <script type="text/javascript">


function tabClick(){
	$("#program-tabs li").hide();
	//$("#"+this.rel).show();
	$("a","#program-titles").each(function(i,el){
			$(this).removeClass('active-tab');
			
		});
		
	var id = $(this).attr('rel');
	$('#program-tabs ul').removeClass('activeList');
	$('#'+id).addClass('activeList');
		
	$(this).addClass('active-tab');
	programFilter();
	return false;
}

// The init function. Sets the keystroke capture and sends it to programFilter
function setUpPrograms(){
	$("#program-tabs").before("<input type='text' id='programID' name='program' placeholder='Enter Keyword' />");
	$("#programID").bind('keyup blur', programFilter);
	programFilter();
}

// Finds the program based on keystrokes
function programFilter(){
$("#program-tabs li").hide();
	$('#program-tabs ul.activeList li').each(function (){
		if($('#programID').val() != '')
		{
			var r=new RegExp($('#programID').val(),'gi');
			if(($(this).text()).match(r))
			{ 
				$('#program-tabs ul').addClass('activeResults');
				$(this).show(); 
				
			
			}
		}
	}); 	 
}

</script> 
        <script type="text/javascript">
$(document).ready(function(){
	
	$("#program-tabs").before('<ul id="program-titles"><li class="doctoral-li"><a href="#tabs-Doctoral" rel="tabs-Doctoral" class="active-tab">Doctoral</a></li><li class="masters-li"><a href="#tabs-Masters" rel="tabs-Masters">Masters</a></li></ul>');
	setUpPrograms();
	$("#program-tabs li").hide();
	$("a","#program-titles").bind("click keypress", tabClick); 
	
});
	
</script> 
