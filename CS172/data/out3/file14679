      <div id="map_canvas" style="width: 644px; height: 500px; "></div> 
      <script type="text/javascript"> 		var myLatlng = new google.maps.LatLng(40.961485054108, -5.6672823429108); 		var myOptions = {  			zoom: 17,  			center: myLatlng,  			mapTypeId: google.maps.MapTypeId.ROADMAP,  			disableAutoPan: true  		};  		var map = new google.maps.Map(document.getElementById("map_canvas"), myOptions); 		var infowindow2 = new google.maps.InfoWindow({  			content: "<div style=\' float:left; margin:0; padding:0; spacing:0;  overflow: hidden;\'><div style=\'align:left; \'><img  style=\'border-left: 0px solid #FFFFFF\' src=\'themes/usal/img/usal_horiz.jpg\'><br></div><div style=\'text-align:right; \'><small><strong> Edificio Histórico</strong></small></div></div>", 			position: myLatlng  		});  		var marker2 = new google.maps.Marker({  			position: myLatlng,  			map: map, 			title: "Universidad de Salamanca()" 		});  		google.maps.event.addListener(marker2, "click", function() { 			infowindow2.open(map,marker2); 		});		infowindow2.open(map,marker2);  		myLatlng = new google.maps.LatLng(40.96148505410822, -5.667282342910767); 		var infowindow = new google.maps.InfoWindow({  			content: "<div style=\' float:left; margin:0; padding:0; spacing:0;  overflow: hidden;\'><div style=\'align:left; \'><img  style=\'border-left: 0px solid #FFFFFF\' src=\'themes/usal/img/usal_horiz.jpg\'><br></div><div style=\'text-align:right; \'><small><strong> Edificio Histórico</strong></small></div></div>"  		});  		var marker = new google.maps.Marker({  			position: myLatlng,  			map: map, 			title: "Universidad de Salamanca (Edificio Histórico)" 		});  		google.maps.event.addListener(marker, "click", function() { 			infowindow.open(map,marker); 		});   </script> 
