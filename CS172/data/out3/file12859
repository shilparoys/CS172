  <script type="text/javascript">
		// -    -   -   -    -   -   -   -   -    -    -    -   -   -    -   -   -   -   -    -
		// Validate Form
		// -    -   -   -    -   -   -   -   -    -    -    -   -   -    -   -   -   -   -    -
		function validateForm(){	
			missinginfo = "";
		 	if(document.getElementById('RequestedBy').value == "") {
				missinginfo += "\n     -  Requested By";
			}		
			if(document.getElementById('AttentionTo').value == "") {
				missinginfo += "\n     -  Attention To";
			}
			if(document.getElementById('Fax').value == "") {
				missinginfo += "\n     -  Fax";
			}
			if(document.getElementById('Email').value == "") {
				missinginfo += "\n     -  Email";
			}
			if (missinginfo != "") {
				missinginfo = "Please complete all required information above.";
					document.getElementById('RequestedBy').style.backgroundColor='#7f9db9';
					document.getElementById('AttentionTo').style.backgroundColor='#7f9db9';
					document.getElementById('Fax').style.backgroundColor='#7f9db9';
					document.getElementById('Email').style.backgroundColor='#7f9db9';
					document.getElementById('Captra').style.backgroundColor='#7f9db9';
			  		document.getElementById('dvValidationMessage').innerHTML = '<img src=\'/images/tip.gif\' alt=\'Tip\'/>' + missinginfo;
			return false;
		}
		//else return true;
		else{
			if(document.getElementById('Captra').value != "98092") {
				document.getElementById('dvValidationMessage').innerHTML = 'Please enter the number as shown on the banner.';
				return false;
			}
			else{
	 		    return true;		
			}
		}

	}
</script>  
