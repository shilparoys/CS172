  <script src="http://www.drew.edu/wp-includes/js/jquery/ui/jquery.ui.datepicker.min.js"></script> 
  <script>
var d;
$(function(){

	$.getJSON('/undergraduate/files/tour_schedule.js',function(data){
		d = data
		$("#frm_field_4812_container input").datepicker({
			gotoCurrent: true
			,minDate: new Date()
			,beforeShowDay:function(dd){
				showday = false;
				dow = dd.getDay();
				for (var i in d) {
					sd =  new Date(d[i].start);
					ed =  new Date(d[i].end);
					//$.jGrowl(i);
					if ( (sd.getTime() - dd.getTime() < 0 ) &&  (ed.getTime() - dd.getTime() > 0) ){
						if (d[i].tour[dow] || d[i].infosession[dow]){
							showday = true;
						}
						try {					
							e = d[i].exceptions[dd.getFullYear()][dd.getMonth()+1];
							if (e){
								for (var a in e){
									if (e[a] == dd.getDate()){
										showday = false;
										//$.jGrowl(e[d])
									}
								}							
							}
						} catch (error) {}							
					}
				}
				return [showday, '']; 
			}
			,onSelect: function(c){
				c = new Date(c);

				day = c.getDay();
				month = c.getMonth() +1 ;
				date = c.getDate();				

				//is there an info session that date? if so show the field and fill in the TIME
				//also hide times that are unavailable. 
				$('#frm_field_4637_container .frm_primary_label, #frm_field_4637_container .frm_radio').hide();
				$("#frm_field_4638_container").hide();
				$("#frm_field_4638_container input").attr('checked', false);
				$("#frm_field_4637_container input").attr('checked', false);
				for (var i in d) {
					sd =  new Date(d[i].start);
					ed =  new Date(d[i].end);
					if ( (sd.getTime() - c.getTime() < 0 ) &&  (ed.getTime() - c.getTime() > 0) ){
						//tours
						try {		
							e = d[i].override.tour[c.getFullYear()][month][date];
							for (x=0;x<e.length;x++){
								$('#frm_field_4637_container .frm_primary_label').show()
								$('#frm_field_4637_container label:contains("'+e[x]+'")').parent().show()
							}
						} catch (error) {
							try {					
								e = d[i].tour[day];
								for (x=0;x<e.length;x++){
									$('#frm_field_4637_container .frm_primary_label').show()									
									$('#frm_field_4637_container label:contains("'+e[x]+'")').parent().show()
								}
							} catch (error) {
								$('#frm_field_4637_container .frm_primary_label, #frm_field_2592_container .frm_radio').hide();
							}									
						}							
						
						//info sessions
						try {
							i = d[i].override.infosession[c.getFullYear()][month][date][0];
								$infos = $("#frm_field_4638_container .frm_description");
								$infos.text($infos.text().replace('TIME', i));
								$("#frm_field_4638_container").show();
						} catch (error) {
							try {
								i = d[i].infosession[day][0];
								$infos = $("#frm_field_4638_container .frm_description");
								$infos.text($infos.text().replace('TIME', i));
								$("#frm_field_4638_container").show();
							} catch (error){
								$("#frm_field_4638_container").hide();
								$("#frm_field_4638_container input").attr('checked', false);
							}							
						}
					}
				}
			}
		});
	});
    

	$('#field_xzdiyl').autocomplete({
		minLength: 3
		,appendTo: '#frm_field_4570_container'
		,source: function( request, response ) {
			$.ajax({
				url: "https://apply.drew.edu/apply/ceebLookupapi.php",
				dataType: "jsonp",
				data: {
					term: request.term
				},
				success: function( data ) {
					response(data);
				}
			});
		}
	});
});
</script> 
