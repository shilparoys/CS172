         <script type="text/javascript" src="/jwpc/dojo.xd.js" djconfig="parseOnLoad:true"></script> 
         <script type="text/javascript" src="/jwpc/jwpc.js"></script> 
         <link rel="stylesheet" href="/jwpc/themes/base/jwpc.css"> 
