/*
Random Image Link Script
By Website Abstraction (http://www.wsabstract.com)
and Java-scripts.net (http://www.java-scripts.net)
*/

function random_imglink(){
  var myimages=new Array()
  //specify random images below. You can have as many as you wish
  myimages[1]="/images/home/oac-home1.jpg"
  myimages[2]="/images/home/oac-home2.jpg"
  myimages[3]="/images/home/oac-home3.jpg"
  myimages[4]="/images/home/oac-home4.jpg"
  myimages[5]="/images/home/oac-home6.jpg"
  myimages[6]="/images/home/oac-home7.jpg"
  myimages[7]="/images/home/oac-home8.jpg" 
  myimages[8]="/images/home/historical_image.jpg" 
    
  //specify corresponding links below
 var imagelinks=new Array()
  imagelinks[1]="/ark:/13030/tf0n39n8km/?brand=oac4"
  imagelinks[2]="/ark:/13030/tf7q2nb7zd/?brand=oac4"
  imagelinks[3]="/ark:/13030/tf7p3012kf/?brand=oac4"
  imagelinks[4]="/ark:/13030/tf2x0nb32s/?brand=oac4"
  imagelinks[5]="/ark:/13030/tf0000012j/?order=12&brand=oac4"
  imagelinks[6]="/ark:/13030/kt809nd0gr/?brand=oac4" 
  imagelinks[7]="/ark:/13030/kt7w10224t/?brand=oac4"
  imagelinks[8]="/ark:/13030/kt3z09q396/?brand=oac4" 
    
  var titles=new Array()
  titles[1]="Grandfather and grandson; Dorothea Lange"
  titles[2]="Clipper ship 'North American' advertisement, 1858"
  titles[3]="Snapshots of agricultural laborers"
  titles[4]="Miscellaneous Views of the Golden Gate Bridge during construction"
  titles[5]="Diary entry; Yoshiko Uchida Papers" 
  titles[6]="Lucy Telles with baskets [1940s]; Boysen Studio" 
  titles[7]="Certificate of residence, 1892"
  titles[8]="Roosevelt and Muir at Glacier Point, May 1903"     
   
  
  var alt=new Array()
  alt[1]="Grandfather and grandson; Dorothea Lange."
  alt[2]="Clipper ship 'North American' advertisement, 1858"
  alt[3]="Snapshots of agricultural laborers"
  alt[4]="Miscellaneous Viewsof the Golden Gate Bridge during construction"
  alt[5]="Diary entry; Yoshiko Uchida Papers" 
  alt[6]="Lucy Telles with baskets [1940s]; Boysen Studio" 
  alt[7]="Certificate of residence, 1892"
  alt[8]="Roosevelt and Muir at Glacier Point, May 1903"   
  
  var ry=Math.floor(Math.random()*myimages.length)

  if (ry==0)
     ry=1
     document.write('<a href='+'"'+imagelinks[ry]+'"'+'><img src="'+myimages[ry]+'" width="313" height="197" border="0" title="'+ titles[ry]+'" alt="'+alt[ry]+'" /></a>')
}

  random_imglink()
//-->
