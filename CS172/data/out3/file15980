  <style type="text/css">
#undergraduate_courses, #graduate_courses {
	display:none;
}

</style> 
  <script language="javascript">
$(document).ready(function(){

	$("#ug").click(function(){
		
		if(document.getElementById("graduate_courses").style.display == "block")
			{
				$( "#graduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });
				$( "#undergraduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });		
			}
		else
			{
				$( "#undergraduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });		
			}
			
	});

	$("#gr").click(function(){
		
		if(document.getElementById("undergraduate_courses").style.display == "block")
			{
				$( "#undergraduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });		
				$( "#graduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });
			}
		else
			{
				$( "#graduate_courses" ).animate({
					"height": "toggle"
				}, { duration: "slow" });
			}
			
	});
	
	
	

	
});
</script> 
