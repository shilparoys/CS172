  <script>
      $(document).ready(function(){
        $('#tweets.carousel').carousel({
          interval: 7500,
          pause: "no"
        });

        $(window).on("scroll",function(){
          $('.navbar.navbar-fixed-top').toggleClass('shadow',$(document).scrollTop()!==0);
        })

        $('.play-video').click(function(e) {
          e.preventDefault();
          $('#ourUsers').toggleClass('videoActive');
          $('#ourUsers .videoContainer').html('<iframe src="http://player.vimeo.com/video/67075072?autoplay=1&amp;color=1bbb66" width="100%" height="600" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>');
        });

        // bind 'slid' function
        $('.carousel').bind('slid', function(e) {
          var target = $(e.target);
          // remove active class
          $('.carousel-nav .active', target).removeClass('active');
          // get index of currently active item
          var idx = $(' .item.active', target).index();
          // select currently active item and add active class
          $('.carousel-nav a:eq(' + idx + ')', target).addClass('active');
        });
      });
    </script> 
  <script>
      var impressions;

      setInterval(function() {
        randomInt = getRandomInt(0, impressions.length - 1);
        $('#counter').text(impressions[randomInt]);
      }, 2000);

      var url = 'http://s3.amazonaws.com/content.issuu.com/about/impressions.json?callback=callme';
      $.ajax({
        type: 'GET',
        url: url,
        async: false,
        contentType: "text/javascript",
        dataType: 'jsonp'
        });

      function callme(data) {
        impressions = data.impressions;
        randomInt = getRandomInt(0, impressions.length - 1);
        $('#counter').text(impressions[randomInt]);
      }

      function getRandomInt(min, max) {
        return Math.floor(Math.random() * (max - min + 1)) + min;
      }
    </script> 
  <script type="text/javascript">
      window._qevents = window._qevents || [];
      window._qevents.push({
          qacct: 'p-5cvC4NOeGmtNA'
      });

      window._gaq = window._gaq || [];
      window._gaq.push(['_setAccount', 'UA-128666-7']);
      window._gaq.push(['_trackPageview']);

      // Quantcast tracking
      (function() {
          var elem = document.createElement('script');
          elem.src = (document.location.protocol == "https:" ? "https://secure" : "http://edge") + ".quantserve.com/quant.js";
          elem.async = true;
          elem.type = "text/javascript";
          var scpt = document.getElementsByTagName('script')[0];
          scpt.parentNode.insertBefore(elem, scpt);
      })();

      // Google analytics
      (function() {
          var ga = document.createElement('script');
          ga.type = 'text/javascript';
          ga.async = true;
          ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
          var s = document.getElementsByTagName('script')[0];
          s.parentNode.insertBefore(ga, s);
      })();
    </script>   
