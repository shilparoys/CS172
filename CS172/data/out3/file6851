    $(document).ready(function () {
        $("a#various").fancybox({
            'hideOnContentClick': false, // so you can handle the map
            'overlayColor': '#666',
            'overlayOpacity': 0.8,
            'autoDimensions': true, // the selector #mapcontainer HAS css width and height
            'onComplete': function () {
                ShowModalPopUp1()
                  },
            'onCleanup': function () {
                var myContent = this.href;
                $(myContent).unwrap();
            } // fixes inline bug
        });
 
        $('#fancybox-close').click(function () {
            $.fancybox.close();
            if (document.getElementById("player")) {
 
                replaceSwfWithEmptyDiv("player");
            }
            return false;
        });
 
    })
 
 
 
    function ShowModalPopUp1() {
    
        var pqs = new ParsedQueryString();
        var parameterNames = pqs.params(false);
        var parameters =
        { id: "1"
        , src: "rtmp://flashmedia.rrd.com/vod/RRD.COM/rrd/ProteousJet"
        , src_title: ""
        , src_namespace_ns: ""
        , src_ns_title: ""
        , autoPlay: "true"
        , width: "600"
        , height: "400"
        , controlBarAutoHide: "false"
        , controlBarPosition: "bottom"
        , configuration: ""
        , poster: "../img/cover.jpg"
        };
 
        for (var i = 0; i < parameterNames.length; i++) {
            var parameterName = parameterNames[i];
            parameters[parameterName]
        = pqs.param(parameterName)
        || parameters[parameterName];
        }
 
        // Embed the player SWF:
 
        swfobject.embedSWF
 
        ("js/StrobeMediaPlayback.swf"
        , "player"
        , parameters["width"], parameters["height"]
        , "9.0.0"
        , {}
        , parameters
        , { allowFullScreen: "true" }
        , { name: "StrobeMediaPlayback" }
        );
       
        return false;
    }
 
 
    function replaceSwfWithEmptyDiv(targetID) {
 
        var el = document.getElementById(targetID);
 
        if (el) {
 
            var div = document.createElement("div");
 
            el.parentNode.insertBefore(div, el);
 
            //Remove the SWF
            swfobject.removeSWF(targetID);
 
            //Give the new DIV the old element's ID
            div.setAttribute("id", targetID);
 
        }
    }
 
 
 
</script> <script type="text/javascript" language="javascript"> 
    $(document).ready(function () {
        $("a#various3").fancybox({
            'hideOnContentClick': false, // so you can handle the map
            'overlayColor': '#666',
            'overlayOpacity': 0.8,
            'autoDimensions': true, // the selector #mapcontainer HAS css width and height
            'onComplete': function () {
                ShowModalPopUp3()
                  },
            'onCleanup': function () {
                var myContent = this.href;
                $(myContent).unwrap();
            } // fixes inline bug
        });
 
        $('#fancybox-close3').click(function () {
            $.fancybox.close();
            if (document.getElementById("player3")) {
 
                replaceSwfWithEmptyDiv("player3");
            }
            return false;
        });
 
    })
 
 
 
    function ShowModalPopUp3() {
    
        var pqs = new ParsedQueryString();
        var parameterNames = pqs.params(false);
        var parameters =
        { id: "3"
        , src: "rtmp://flashmedia.rrd.com/vod/RRD.COM/Financial/translations"
        , src_title: ""
        , src_namespace_ns: ""
        , src_ns_title: ""
        , autoPlay: "true"
        , width: "600"
        , height: "400"
        , controlBarAutoHide: "false"
        , controlBarPosition: "bottom"
        , configuration: ""
        , poster: "../img/cover.jpg"
        };
 
        for (var i = 0; i < parameterNames.length; i++) {
            var parameterName = parameterNames[i];
            parameters[parameterName]
        = pqs.param(parameterName)
        || parameters[parameterName];
        }
 
        // Embed the player SWF:
 
        swfobject.embedSWF
 
        ("js/StrobeMediaPlayback.swf"
        , "player3"
        , parameters["width"], parameters["height"]
        , "9.0.0"
        , {}
        , parameters
        , { allowFullScreen: "true" }
        , { name: "StrobeMediaPlayback" }
        );
       
        return false;
    }
 
 
    function replaceSwfWithEmptyDiv(targetID) {
 
        var el = document.getElementById(targetID);
 
        if (el) {
 
            var div = document.createElement("div");
 
            el.parentNode.insertBefore(div, el);
 
            //Remove the SWF
            swfobject.removeSWF(targetID);
 
            //Give the new DIV the old element's ID
            div.setAttribute("id", targetID);
 
        }
    }
 
 
 
