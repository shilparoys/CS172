	function searchScope(site) {
	var field = document.getElementById('sitelimit');
	var scope;
	switch(site) {
		case 1: scope='www.usgs.gov';
				break;
		case 2: scope='';
				break;
		default: scope='';
	}	
	field.value = scope;
}
</script> 
