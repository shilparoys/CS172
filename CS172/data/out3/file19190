   <script>
//script to log clicks to ga automatically, requires positioning of data-ga=""attributes on selected areas
$('[data-ga] a').on('click',function(){
var el = $(this).closest('[data-ga]');
var linkText = $(this).attr('title') ? $(this).attr('title') : $(this).text();
var debug = false;
function f(s){
return s.replace(/[^a-zA-Z0-9_\- ]/gim,'');
}
var path = [];//the google analytics "path", look for only 2 levels to avoid clutter
if(el.size()===1){
path.push(el.data('ga'));
el = el.parent().closest('[data-ga]');
if(el.size()===1)path.push(el.data('ga'));
}
if(path.length > 0){
if(debug)console.log('GAevent',{'event':'GAevent', 'eventCategory':'Click', 'eventAction': path.reverse().join('/'), 'eventLabel': f(linkText)});
dataLayer.push({'event':'GAevent', 'eventCategory':'Click', 'eventAction': path.reverse().join('/'), 'eventLabel': f(linkText)});
}
if(debug)return false;
});
</script> 
