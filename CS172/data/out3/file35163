  <!-- KECK ENT --> 
  <script type="text/javascript">
$j=jQuery.noConflict();
$j(function() {

var loc = window.location.href; // returns the full URL
if ((loc.match(/ent.keckmedicine.whitehorse.com\//g)) || (loc.match(/ent.keckmedicine.org\//g))  ) {
		$j('ul.breadcrumbs > li > a:contains("About")').addClass('noLink').removeAttr("href");
		$j('ul.breadcrumbs > li > a:contains("Team")').addClass('noLink').removeAttr("href");
		$j('ul.breadcrumbs > li > a:contains("Treatments & Services")').addClass('noLink').removeAttr("href");
		$j('ul.breadcrumbs > li > a:contains("Patient Information")').addClass('noLink').removeAttr("href");
		$j('ul.breadcrumbs > li > a:contains("Medical Professionals")').addClass('noLink').removeAttr("href");
