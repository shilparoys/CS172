  <script type="text/javascript">
	/*@cc_on
	  @if (@_jscript_version == 10)
	    document.write(' <link type= "text/css" rel="stylesheet" href="http://cancer.keckmedicine.org/wp-content/themes/Avada-Spine/css/ie10.css" />');
