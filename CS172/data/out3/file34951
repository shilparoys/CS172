  <!-- /#responsive-wrapper --> 
  <div class="backtotop"></div> 
  <script type="text/javascript">
	(function($){
		/**
		Mobile menu JS
		*/
		$('.menu-button').click(function(){
			$(this).toggleClass('active');
			$('body').toggleClass('mobile-out');
		});
		$('.mobile-out').click(function(){
			$('body').removeClass('.mobile-out');
		});
		$('#responsive-drawer ul > li.expanded > a').on('click',function(e){
			e.preventDefault();
			$(this).next('ul').slideToggle(300);
		});
		/**
		Mobile Search Button
		*/
		var shown = false;
		$('.search-button').click(function(){
			var windowWidth = $(window).width();
			var buttonWidth = $('.search-button').innerWidth();
			var animateWidth = -windowWidth + buttonWidth;
			if(!shown){
				$('.search-container,.search-button').css({
					'-webkit-transform':'translateX(' + animateWidth + 'px)',
					'-moz-transform':'translateX(' + animateWidth + 'px)',
					'transform':'translateX(' + animateWidth + 'px)'
				});
				shown = true;
			}else{
				$('.search-container,.search-button').css({
					'-webkit-transform':'translateX(0)',
					'-moz-transform':'translateX(0)',
					'transform':'translateX(0)'
				});
				shown = false;
			}
		});
		resizeSearch = function(){
			var windowWidth = $(window).width();
			var buttonWidth = $('.search-button').innerWidth();
			var animateWidth = -windowWidth + buttonWidth;
			$('.search-container,.search-button').css({
				'-webkit-transform':'translateX(' + animateWidth + 'px)',
				'-moz-transform':'translateX(' + animateWidth + 'px)',
				'transform':'translateX(' + animateWidth + 'px)'
			});
		}
		resetSearch = function(){
			$('.search-container,.search-button').css({
				'-webkit-transform':'translateX(0)',
				'-moz-transform':'translateX(0)',
				'transform':'translateX(0)'
			});
			shown = false;
		}
	})(jQuery);
</script>  
 </body>
</html>