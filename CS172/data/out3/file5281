  <script>
                function userlikeButtonHandler(visible) {
                    if (visible) {
                        $("#userlikeWrapper").removeClass('hide'); 
                                                                        userlikeReady = function() {
                            setTimeout(function()
                            { 
                                if (
                                    userlikeChatState() == 'Offline' || 
                                    userlikeChatState() == 'Idle' && 
                                    userlikeChatState() != 'Chat' &&
                                    $('#userlikePopup').css('display') == 'none'
                                ) {
                                    userlikeStartChat();
                                }
                            },30000);
                        };
                    } else {
                        $("#userlikeWrapper").addClass('hide');
                    }    
                }
            </script>
