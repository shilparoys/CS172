                $('img.tn').each(function() {
                    if((typeof this.naturalWidth !== "undefined" &&
                        this.naturalWidth === 0 ) 
                        || this.readyState === 'uninitialized' ) {
                        $(this).attr('src', '/thumbnail/'+$(this).data('id'));
                    }
                }); 
            });

