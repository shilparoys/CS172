        if (menu.hasClass('hide')) {
            menu.animate( { opacity: "1" }, { queue: false, duration: 400 }).slideDown(400, function(){
                menu.removeClass('hide');
            });
        } else {
            menu.animate( { opacity: "0" }, { queue: false, duration: 400 }).slideUp(400, function(){
                menu.addClass('hide');
            });
        }
    });

