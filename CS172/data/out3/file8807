  <script type="text/javascript">
		function setRecSec(name){
		  rsImg = document.getElementById('recsec');
		  mapImg = document.getElementById('map');
		  pdfLink = document.getElementById("pdf");
		  if(name == "main"){
		    rsImg.src = "/sod/earthquakes/Event_2015_060_02_47_20/recordSection.png";
		    mapImg.src = "/recordSectionMap.png?orientation=main&eq_url=earthquakes/2015/03/01/02/47/20&";
		    pdfLink.href = "/recordSection.pdf?orientation=main&recordsectionid=local&eq_url=earthquakes/2015/03/01/02/47/20&&pdf=true";
		  }else if(name == "north"){
		    rsImg.src = "/sod/earthquakes/Event_2015_060_02_47_20/nrecordSection.png";
		    mapImg.src = "/recordSectionMap.png?orientation=north&eq_url=earthquakes/2015/03/01/02/47/20&";
            pdfLink.href = "/recordSection.pdf?orientation=north&recordsectionid=local&eq_url=earthquakes/2015/03/01/02/47/20&&pdf=true";
		  }else {
		    rsImg.src = "/sod/earthquakes/Event_2015_060_02_47_20/erecordSection.png";
		    mapImg.src = "/recordSectionMap.png?orientation=east&eq_url=earthquakes/2015/03/01/02/47/20&";
            pdfLink.href = "/recordSection.pdf?orientation=east&recordsectionid=local&eq_url=earthquakes/2015/03/01/02/47/20&&pdf=true";
		  }
		  return true;
		}
		function activate(id){
		  button = document.getElementById(id);
		  button.click();
		  return true;
		}
		
		function addStation(){
		selector = document.getElementById('select');
		if(selector.selectedIndex != 0){
		adder = document.getElementById('addstation');
		adder.submit();
		}
		}
		
		</script> 
