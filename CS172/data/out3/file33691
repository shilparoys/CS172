              <div id="block-views-announcements-block" class="block block-views"> 
               <div class="content"> 
                <div class="view view-announcements view-id-announcements view-display-id-block view-dom-id-7e8559ec1f84c16c66d8698f145854b7"> 
                 <div class="view-content"> 
                  <div class="views-row views-row-1 views-row-odd views-row-first"> 
                   <div id="node-343" class="node node-announcement node-teaser clearfix" about="/announcements/2015/tuition-payment" typeof="sioc:Item foaf:Document"> 
                    <h2 property="dc:title" datatype=""><a href="/announcements/2015/tuition-payment">Tuition Payment </a></h2> 
                    <div class="content"> 
                     <div class="field field-name-field-date field-type-datetime field-label-hidden">
                      <div class="field-items">
                       <div class="field-item even">
                        <div class="date-repeat-instance">
                         <span class="date-display-single" property="dc:date" datatype="xsd:dateTime" content="2015-02-16T00:00:00+00:00">Monday, February 16, 2015</span>
                        </div>
                       </div>
                      </div>
                     </div>
                     <div class="field field-name-field-image field-type-image field-label-hidden">
                      <div class="field-items">
                       <div class="field-item even">
