  <script>
$(function(){
	$section = $("article article>section");
	$section.each(function(){
		$t = $(this);
		if($t.text() == '')	$t.parent().find('header>a').addClass('nolink');
	})
	
	$("article article header a:not(.nolink)").click(function(){
		$s = $(this).parent().next();
		$section.slideUp('fast');
		if (!$s.is(":visible")) $s.slideDown('fase');
		return false;
	});
	$("article article header a.nolink").click(function(){return false;});

});
</script> 
