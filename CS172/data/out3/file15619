                          <td width="40%" align="right"> <script language="JavaScript" type="text/javascript">
											var o_cal = new tcal ({
												// form name
												'formname': 'Archiveform',
												// input name
												'controlname': 'FromIn'
											});
											
											// individual template parameters can be modified via the calendar variable
											o_cal.a_tpl.yearscroll = false;
											o_cal.a_tpl.weekstart = 1;
											</script> </td>
                          <td> </td>
                         </tr> 
                         <tr> 
                          <td width="60%" align="left"> <input type="text" size="12" name="ToIn" id="ToIn"> </td> 
                          <td width="40%" align="right"> <script language="JavaScript" type="text/javascript">
