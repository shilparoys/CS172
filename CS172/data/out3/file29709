    <script> jQuery(function($){ 
                        var prevWidth = $('#slider_controls_1420330997 .prev').outerWidth() + 6;
                        var nextWidth = $('#slider_controls_1420330997 .next').outerWidth() + 6 ;  
                        var sliderwrapper = $('<div>');
                        
                        $('#slider_1420330997').wrap(sliderwrapper);
                        var container = $('#slider_1420330997').parent();
                        container.css('padding-left', prevWidth + 'px');
                        container.css('padding-right', nextWidth + 'px');
                        var shadow = $('.intense.shadow', container.parent().parent());

                        shadow.appendTo($('#slider_1420330997').parent());
                    
                var owl_1420330997 = $('#slider_1420330997').owlCarousel({                    
                    stopOnHover: true,                    
                    items: 3,
                    itemsDesktop: [1000,4],
                    itemsDesktopSmall: [900,3],
                    itemsTablet: [600,2],
                    itemsMobile: [600,1],
                    //autoHeight: true,
                    pagination: false,
                    paginationNumbers: false,                    
                    afterAction: function() {
                        setTimeout(function() {
                            intenseAdjustOwlNavigation();
                        }, 500);
                    },
                    //scrollPerPage: true,
                    autoPlay: 7000,
                    slideSpeed: 1000,
                    transitionStyle: false
                });

                $('#slider_controls_1420330997 .next').click(function(){
                    owl_1420330997.trigger('owl.next');
                });            

                $('#slider_controls_1420330997 .prev').click(function(){
                    owl_1420330997.trigger('owl.prev');
                });

                $('#slider_1420330997').parent().find('.intense.shadow').css('margin-top', '-' + ($('#slider_1420330997 .owl-controls').outerHeight(true)) + 'px');

                function intenseAdjustOwlNavigation() {
                    var prevWidth = $('#slider_controls_1420330997 .prev').outerWidth() + 5;
                    var nextWidth = $('#slider_controls_1420330997 .next').outerWidth() + 5 ;

                    $('#slider_controls_1420330997 .next').css('margin-top', $('.owl-wrapper-outer', $('#slider_1420330997').parent()).height() / 2 - $('#slider_controls_1420330997 .next').height() / 2);
                    $('#slider_controls_1420330997 .prev').css('margin-top', $('.owl-wrapper-outer', $('#slider_1420330997').parent()).height() / 2 - $('#slider_controls_1420330997 .prev').height() / 2);                              
                }                
                
                intenseAdjustOwlNavigation();

                
            }); </script>
