function searchsetv() {
  var dd = document.getElementsByName('search').value ="about us"; 
  document.getElementById('gsc-i-id1').value = document.getElementById('google_search2').value;
  document.getElementsByClassName("gsc-search-button gsc-search-button-v2")[0].click();
}
