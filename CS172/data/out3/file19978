jQuery(document).ready(function () {
jQuery("body").append("<p id='hcmtest' style='position:absolute;top:0;left:-99999px;background-color:#878787;'>T</p>");

  var testcolor = jQuery("#hcmtest").css("background-color").toLowerCase();

  jQuery("#hcmtest").remove();

  if (testcolor != "#878787" && testcolor != "rgb(135, 135, 135)") {

  jQuery("body").append('<link rel="stylesheet" type="text/css" href="/cas/css/hc.css" />');

}



});

