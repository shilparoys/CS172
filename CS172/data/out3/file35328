  <script type="text/javascript">
        var $root = '/';
        $(document).ready(function () {

            // Instantiate the Custom Scrollbar 
            $("div.calendarChklst ul").mCustomScrollbar({
                autoDraggerLength: true,
                set_height: 182,
                scrollButtons: {
                    enable: true
                },
                theme: "dark-thick",
            });

            // Instantiate the EasyTab
            $('#tab-container').easytabs({
                animate: true,
                animationSpeed: 100
            });

            // jQuery Wizard call
            //$('.wizard').stepWizard({
            $('#wizard1').stepWizard({
                width: 700
            });
            $('#wizard2').stepWizard({
                width: 700
            });

            // Ajax calls and post load processing for Wizard content - country specific

            function postProcessStepWizardInfo(country) {
                $('a.backAllCountries').click(function () {
                    $('li#navStep1 a').trigger("click");
                    return false;
                });
                $('h5.wizardBreadcrumb span').text(country);
                // Instantiate the ColorBox
                $(".inlinecBox").colorbox({ inline: true, width: "730px" });
            }

            function LoadStepContent(country, $url) {
                $('#step2').load($root + $url, function () {
                    postProcessStepWizardInfo(country)
                });
                $('#step3').load($root + "HowToApplyContent/Exams.html", function () {
                    postProcessStepWizardInfo(country)
                });
                $('#step4').load($root + "HowToApplyContent/Apply.html", function () {
                    postProcessStepWizardInfo(country)
                });
            }

            $('ul.countryList li a').click(function () {
                $('div.wizardContent > div').addClass('clearfix');
                var $url = $(this).attr('href');
                var country = $(this).text();
                LoadStepContent(country, $url)
                return false;
            });

            // Instantiate the ColorBox
            $(".iframe").colorbox({
                iframe: true,
                innerWidth: 700,
                innerHeight: 520,
            })
        });
    </script> 
