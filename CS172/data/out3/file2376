  <script>
var PlayerEvents = {
  extraData: '',
  trackedEvents: {},
  flashEvent: function(e){
    var flashVersion = swfobject.getFlashPlayerVersion(),
        version = [flashVersion.major,flashVersion.minor,flashVersion.release].join('.'),
        extraParams = {"fv": version},
        eventType = null;
    
    switch(e.type) {
      case 'INITIALIZED':
        eventType = 'initialized';
        break;
      case 'STARTED':
        eventType = 'started';
        break;
      case 'COMPLETED':
        eventType = 'ended';
        break;
      case 'CTA_SHOWN':
        eventType = 'shown_cta_btn';
        break;
      case 'CTA_CLICK':
        eventType = 'click_cta_btn';
        break;
    }

    if (eventType) {
      PlayerEvents.trackOnce(eventType, "flash", extraParams);
    }
  },

  html5Event: function(e){
    if (e.type === "play") {
      PlayerEvents.trackOnce("started", "html5");
    } else if (e.type === "ended") {
      PlayerEvents.trackOnce("ended", "html5");
    }
  },

  trackOnce: function(event, playerType, extraParams) {
    if (!PlayerEvents.trackedEvents[event]) {
      PlayerEvents.trackedEvents[event] = true;
      animoto.util.analytics.loadCustomPixel(
        PlayerEvents.trackingUrl(event, playerType, extraParams)
      );
    }
  },

  trackingUrl: function(event, playerType, extraParams){
    var url = '//d2jy3ctxtc2iu2.cloudfront.net/trail.gif';
    url += "?s="  + "player" + // scope
            "&e="  + event + // event
            "&sid=" + "7286bf4b47a28893322268dd4054caa9" + // request/session id
            "&src=" + "play-page" + // source
            "&own=" + "false" + // is this event for the render owner
            "&pt=" + playerType + //player type
            "&bi=" + "b78d2b73e2ca94249bd893e2c140d26f" + // browser id
            "&vi=" + "" + // vanity id
            "&ri=" + "qF5pbWhDwSCB7UBdjOtY6w" + // render id
            "&rl=" + "0" + // render length
            "&rt=" + "UW+Family+Weekend+2014" + // render title
            "&rc=" + "2014" + // render created at
            "&ui=" + ""  + // user id
            "&uc=" + "" + // user created at
            "&us=" + "" + // user sub
            "&oi=" + "brwnvpfc" + // owner id
            "&oc=" + "1396637887" + // owner created at
            "&os=" + "plus" + // owner sub
            "&stn=" + "original" + // style name
            "&r="  + "" + // http referer
            "&ep="  + encodeURIComponent(window.location) + // http referer
            "&t="  + Math.round((new Date()).getTime() / 1000); // time of event

    if (extraParams) {
      $.each(extraParams, function(k,v) {
        url += "&" + encodeURIComponent(k) + "=" + encodeURIComponent(v);
      });
    }
    return url;
  }
};
