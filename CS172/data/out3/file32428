	jQuery(document).ready(function(){
	// REMOVE FEATURE HEADER
	$('#featureHeader').remove();



	// SET SLIDERKIT FOR FEATURES
	var numOfSlides;
	var slideHeight=0;
	numOfSlides=buildNavigation('sliderkit-panel','frontpage');
			
	jQuery(".sliderkit").sliderkit({auto:true,verticalnav:true,circular:true,autospeed:10000,panelfx:"fading",mousewheel:false,shownavitems:numOfSlides,fashchange:true});	
	var testText=""; //Needed to slow Chrom down enough for it to catch the style
	$('.sliderkit_nav_ul li').each(function(){testText+=$(this).attr('style')+"\n";});
	$('.sliderkit-nav-clip, .sliderkit_nav_ul, .sliderkit_nav_ul li').removeAttr('style');
	
	var maxheightStart=230;
	var maxheight=230;
	$('.sliderkit-panel').each(function(){if($(this).height()>maxheight) maxheight=$(this).height();	});
	$('#whats_happening, .sliderkit-panel').height(maxheight);
	var newCalHeight=$('#calendars_data').height()+maxheight-maxheightStart;
	$('#calendars_data').height($('#calendars_data').height()+maxheight-maxheightStart);
	$('.sliderkit').trigger('mouseover').trigger('mouseout'); // Make sure Chrome rotates through all tabs
	

	
	// CALENDARS
	var myCals=new loadGoogleCals({
