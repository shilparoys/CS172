  <!-- all subdomains --> 
  <script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-3780897-21']);
  _gaq.push(['_setDomainName', 'lib.utexas.edu']);   
  _gaq.push(['_trackPageview']);
  (function() {
