  <script type="text/javascript">
$(document).ready(function(){
  function readCookie(cname){
    var name=cname+"=";
    var ca=document.cookie.split(";");
    for (var i=0;i<ca.length;i++){
      var c=ca[i];
      while (c.charAt(0)==" ") c=c.substring(1);
      if (c.indexOf(name)==0) return c.substring(name.length,c.length);
    }
    return "";
  }
  if (readCookie("jp_popup")!=1){
	var adID="oams-popup2";
    var adSpot="POPUP2";
    var adLabel="FRONTPAGE";
    $("#" + adID).html('<div class="dg-fade"></div><div class="dg-ad-window"><div class="dg-ad-bar"><a href="#"><img class="close-dg-win" src="/images/trans.png" height="30" width="102" alt="Close this window" /></a></div><iframe class="dg-ad" src="/static/adCode.html?server=http://JPads.sx.atl.publicus.com/&subsite=NJ001&random=10068213896111223&spot='+adSpot+'&label='+adLabel+'" id="oams-popup2-'+adSpot+'" height="375" width="430" style="overflow:hidden" scrolling="no" frameborder="0"></iframe></div>');
    $(".dg-fade").css({opacity:0}).animate({opacity:0.7});
    $(".close-dg-win,.dg-fade").click(function(){
	  $(".dg-fade,.dg-ad-window").remove();
	  return false;
    });
    $(document).keyup(function(e){if(e.keyCode==27){$(".dg-fade,.dg-ad-window").remove()}});
	var d=new Date();
	d.setTime(d.getTime()+(4*60*60*1000));
	var expires="expires="+d.toUTCString();
	document.cookie="jp_popup=1;Path=/;"+expires;
  }
});
</script> 
