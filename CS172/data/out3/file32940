			document.getElementById("alumniloginbox").innerHTML = '<p class="rightBlock">You are currently logged in.<br /><a href="/harrislogout" target="_top" onClick=\'_gaq.push(["_trackEvent", "Block_AlumniLogin", "Click", "Logout"]);\'>Click here to Logout</a></p>';
		}
		else
		{
			document.getElementById("alumniloginbox").innerHTML = '<FORM ACTION="https://post.harvard.edu/olc/pub/HAA/login/app.sph/olclogin.app" METHOD="post"><p><label for="user-id">User <abbr>ID</abbr></label> <input NAME="username" TYPE="text" class="text" VALUE="" size="14" maxlength=20 tabindex=1></p><p><label for="user-password">Password</label><input NAME="password" TYPE="password" class="text" VALUE=""  size="14" maxlength=20 tabindex=2></p><p class="single-input"><input type="checkbox" id="remember_me" name="permanent" value="Y" /><label for="remember_me">Remember me?</label></p><input type="submit" class="btn input-submit" value="Login" onClick=\'_gaq.push(["_trackEvent", "Block_AlumniLogin", "Click", "SubmitBtn"]);\' /><ul><li><a  href=https://post.harvard.edu/olc/pub/HAA/register/register.cgi onClick=\'_gaq.push(["_trackEvent", "Block_AlumniLogin", "Click", "Register"]);\'>Register</a></li><li><a href=https://post.harvard.edu/olc/pub/HAA/forgot/forgot.cgi onClick=\'_gaq.push(["_trackEvent", "Block_AlumniLogin", "Click", "Forgot Password"]);\'>Forgot&nbsp;Password</a> </li><li><a href="http://alumni.harvard.edu/help" onClick=\'_gaq.push(["_trackEvent", "Block_AlumniLogin", "Click", "Help"]);\'>Help</a></li></ul><INPUT TYPE="hidden" NAME="referer" VALUE="/olc/pub/HAA/memberservices.html"> <INPUT TYPE="hidden" NAME="SaFormName" VALUE="SubmitLogin__Floginform_html"> </FORM>';
