        <script type="text/javascript" src="/js/jquery.cycle.min.js"></script> 
        <script type="text/javascript">
var heroTransitioned = 0; //fix for the initial cycle function call triggering before
function hero_thumb(currSlideElement, nextSlideElement, options, forwardFlag) {
  if (heroTransitioned) {
   $("#hero-thumbs .current").removeClass("current");
   $("#hero-thumbs a").eq(options.nextSlide).addClass("current");
  } else {
   heroTransitioned = 1;
  }
}

$(document).ready(function () {
 $("#hero").cycle({
   speed:1000,
   delay:-2000,
   timeout:4500,
   before: hero_thumb
  });
});
</script> 
