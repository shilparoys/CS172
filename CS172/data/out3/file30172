			// TA check rates popup
			function checkRates_popup(hotel_id){
				if (hotel_id == undefined) { return;}
				var hotel_popup_id = '#hotel_popup_' + hotel_id;
				var hotel_name = $(hotel_popup_id).children('a.hotel-name').html();
				cr_popup(hotel_id, hotel_name, '.cr-map-btn', 42, -153, 'bottom');
			};

			//TA Popup under
			var ta_pop_w;
			function ta_flightwiget(f_dest) {
				ta_popund_call('FLIGHTWIDGET',null,f_dest);	
			}
			
			function ta_popund_call(f_pt,f_q,f_dest) {
				var f_mcid;
				var l_referrer = '';
				if ( f_pt != 'FLIGHTWIDGET') {
					if  ( l_referrer.indexOf('usnews.com') > -1 || l_referrer == '') {
						if (f_pt == 'DESTINATION_PROFILE') {
							f_mcid = 19176;
						}
						else if (f_pt == 'HOTEL_PROFILE') {
							f_mcid = 19180;
						}
						else if (f_pt == 'HOTEL_LIST') {
							f_mcid = 19178;
						}
					}	
					else {
						if (f_pt == 'DESTINATION_PROFILE') {
							f_mcid = 19175;
						}
						else if (f_pt == 'HOTEL_PROFILE') {
							f_mcid = 19179;
						}
						else if (f_pt == 'HOTEL_LIST') {
							f_mcid = 19177;
						}
					}
				}
				else if (f_pt == 'FLIGHTWIDGET') {
					f_mcid = 19174;
				}
				else {
					return;
				}
				
				if (f_q != undefined) {
					ta_pop_w = TA_create_exit_window(f_mcid,{q:f_q});
				}
				else if (f_dest != undefined) {
					ta_pop_w = TA_create_exit_window(f_mcid,{dest:f_dest});
				}	
			}
			
		</script> 
