	       	try {
	       	   wt_track_event({category:'SwitchSite',action:'Mobile'});
				$.cookie("usn_is_mob",null,{path:"/"});
	           	$.cookie("usn_is_mob","Y",{path:"/"});
	           	
	           	$.cookie("desktop",null,{path:"/"});
	           	window.location.reload(true);
	           } catch(ex) {
	           	   console.log("Error:" + ex);
	           } 
	       }
	       
