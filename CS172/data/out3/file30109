  <script>
          function getWidth()
          {
            xWidth = null;
            if(window.screen != null)
              xWidth = window.screen.availWidth;
         
            if(window.innerWidth != null)
              xWidth = window.innerWidth;
         
            if(document.body != null)
              xWidth = document.body.clientWidth;
         
            return xWidth;
          }
        
        if (/(Android|iPhone|iPod|webOS|NetFront|Opera Mini|SEMC-Browser|PlayStation Portable|Nintendo Wii|BlackBerry)/.test(navigator.userAgent) && (!(getWidth() > 600)|| /Mobile/.test(navigator.userAgent))){
			
			if((bls_getCookie("mobile_redirect") == null || bls_getCookie("mobile_redirect") == "true") && !String(window.location).match(/view_full/)){
            							
			var current_location = _rxvar_href.replace(/home.htm$/gi,""); 
			var bls_referer = new Array();
			var bls_referer_match = false;
			
			if(current_location == "/ooh/" || current_location == "/"){
				bls_deleteCookie("bls_referer");
			}
			
			if(bls_getCookie("bls_referer") != null && bls_getCookie("bls_referer") != ""){
				bls_referer = bls_getCookie("bls_referer").split(","); 
				
    			for(var i = 0; i < bls_referer.length; i++){
    				if(String(bls_referer[i]) == current_location){
						bls_referer.splice(i,1);
						i = bls_referer.length;
    					bls_referer_match = true;
    				}
    			}
				
				if(bls_referer_match){
					bls_setCookie("bls_referer",bls_referer,"");
					window.history.go(-2);
				}else{	
					bls_referer.push(current_location);
					bls_setCookie("bls_referer",bls_referer,"");
					if (_rxvar_mobile_href != ""){
					window.location = _rxvar_mobile_href;	
					}
				}				
			}
			else{
				bls_setCookie("bls_referer",current_location,"");
				if (_rxvar_mobile_href != ""){
					window.location = _rxvar_mobile_href;	
				}
			}
		
				


            }
	
        }
        
        </script> 
