    <script type="text/javascript">
var redirectOverlayArr = [
{
"suffix": ".com",
"redirectFlag": "Footer_USA_large",
"redirectText": "We'll take you to our US site, <b>www.stubhub.com</b>, to view this information. If you already have a StubHub account, you can use it to sign in and any transactions you make will be in US dollars (USD)."
},
{
"suffix": ".co.uk",
"redirectFlag": "Footer_UK_large",
"redirectText": "We'll take you to our UK site, <b>www.stubhub.co.uk</b>, to view this information. If you already have a StubHub account, you can use it to sign in and any transactions you make will be in British pounds sterling (GBP)."
}];
		

 function isDomainRedirect(url, href){
	  var urlSub = url.split('.')[1],
		  link = href,
		  urlExtension = '',
		  compareUrlExtn = '',
		  sIndex = link.indexOf('//')+2,
		  eIndex = link.indexOf('/',sIndex) > -1 ? link.indexOf('/',sIndex) : (link.indexOf('?',sIndex) > -1 ? link.indexOf('?') : link.length),
		  hostname = link.substring(sIndex, eIndex);
	  if(hostname.split(".")[1] != urlSub || $('#redirectOverlay').size() == 0 || hostname.split(".")[1] == "stubhubstatic"){
		 return false;
	  }
	  else{
		 sIndex = url.indexOf('.');
		 eIndex = url.indexOf('.',sIndex+1);
		 urlExtension = url.substring(eIndex);
		 sIndex = hostname.indexOf('.');
		 eIndex = hostname.indexOf('.',sIndex+1);
		 compareUrlExtn = hostname.substring(eIndex);
		 return urlExtension != compareUrlExtn ? true : false;
	  }
   };
 
$(document).ready(function(){

	$('a[href^="http"]').live('click',function(e){

		var $target = $(this),
			isRedirect = isDomainRedirect(location.hostname, $target.attr('href'));
		
		if(isRedirect){
			e.preventDefault();
			if($target.hasClass('crossDomainContinue')){
				$('#cancelBtn').click();
				window.location.href = $target.attr('href');
				return false;
			}
			$target.overlay({
				trigger: 'load',
				modal:'true',
				align:'centered',
				overlayId: 'redirectDomainOverlay',
				showCloseIcon: false,
				source: '#redirectOverlay',
				width:410,
				closeOnClickOutside:true,
				closeSelector: '#cancelBtn',
				open: function(elem){
					var hrefValue = $(elem).attr("href");
					$("a.crossDomainContinue").attr("href", hrefValue);
					//replace the redirectFlag and redirectText
					var $redirectOverlay = $('#redirectOverlay'),
						redirectArr = typeof redirectOverlayArr == 'undefined' ? [] : redirectOverlayArr,
						sIndex = hrefValue.indexOf('//')+2,
						eIndex = hrefValue.indexOf('/',sIndex) > -1 ? hrefValue.indexOf('/',sIndex) : (hrefValue.indexOf('?',sIndex) > -1 ? hrefValue.indexOf('?') : hrefValue.length),
						hostname = hrefValue.substring(sIndex, eIndex),
						redirectObj = $.grep(redirectArr, function(obj){
						    return hostname.toLowerCase().match(obj.suffix) == obj.suffix;})[0];
                    $('.domainTextDiv .flagDiv').removeClass().addClass("flagDiv flagsprite " + redirectObj.redirectFlag);
					$('.domainTextDiv .contentTxt', $redirectOverlay).html(redirectObj.redirectText);
				}
			});
		}
     });
	
})
	
</script>
