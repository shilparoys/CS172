    <script type="text/javascript">getTheCookie = function(name){
	             var nameDel = name + "=",
	             cind = document.cookie.split(';');
	             for (var i = 0, l = cind.length; i < l; i++) {
	             	var c = cind[i];
	                while (c.charAt(0) == ' ') 
	                	c = c.substring(1, c.length);
	                    if (c.indexOf(nameDel) == 0) 
	                            return c.substring(nameDel.length, c.length);
	             };
	             return null;
             };


                setTheCookie = function(name, value, isSession, expHours, domain, path){
                var expStr, hours = (typeof expHours != "undefined") ? expHours : 365 * 24, date = new Date(), domainStr = domain? "; Domain="+domain : "", pathStr = path? "; Path="+path : "; Path=/";
                date.setTime(date.getTime() + (hours * 60 * 60 * 1000));
                expStr = isSession? "" : "; Expires=" + date.toGMTString();
                document.cookie = name + "=" + value + domainStr + expStr + pathStr;
                };
		</script>
