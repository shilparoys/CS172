     <script type="text/javascript">
	$(window).resize(function(){
		var m_w=$('body').width();
		var m_x=650;
		resizetext(m_w,m_x);
	});
	function resizetext(m_w,m_x){
		var w=m_w;
		var x=m_x;
		var y=(w-x)/2;
		var z=y+12;
		$('.vmi2box').css({'left':y+'px','right':z+'px'});
	}
	$(document).ready(function(){
		var l_lb=$(window).height(); 
		var m_lb=$('#vmi2box').height();
		var n_lb=(l_lb-m_lb)/2 ;
		var l_wb=$(window).width();
		var m_wb=650;
		var n_wb=((l_wb-m_wb)/2);
		$('.vmi2box').css({'left':n_wb+'px','right':n_wb+'px'});
		$('.vmi2box').css({'top':n_lb+'px'});
		var bnrbtm_Cookie = $.readCookielet("STUB_SESS","aipfr");
		if(bnrbtm_Cookie == null || bnrbtm_Cookie =='' || typeof(bnrbtm_Cookie) == 'undefined'){
			$('.vmi2box').show();
			$('#vmioverlay').fadeIn('fast',function(){
				$('#vmi2box').animate({'top':'160px'},100);
			});
		}else if(bnrbtm_Cookie.match('aipfr')){
			$('.vmi2box').hide();
			$('.vmioverlay').hide();
		}
		$('#vmi2boxclose').click(function(){
			$('#vmi2box').hide();
			$('#vmioverlay').fadeOut('fast');
			$.writeCookielet("STUB_SESS","aipfr","optout");
		});
		$('.vmioverlay').click(function(){
			$('.vmi2box').hide();
			$('.vmioverlay').hide();
			$.writeCookielet("STUB_SESS","aipfr","optout");
		});
		$("area#closeLightbox").click(function(event){
			event.preventDefault();
			$("#vmi2box").hide();
			$('.vmioverlay').hide();
		});
	});</script>
