						userGeoDetected: "10",
						venueId: "",
						userStatus: "guestuser",
						artistPrimaryStyle: "",
						protocol: "http",
						
						searchStr: searchStr2,
						ref1: refer1,
						ref2: refer2
						
					};
					
									StubhubAds.targetZones = {
										top728: { url: '/9494403/StubHub_Top_728x90', dimensions: [728, 90], el: 'div-gpt-ad-1367877572215-3' },
										leftSide: { url: '/9494403/StubHub_Top_160x600', dimensions: [160, 600], el: 'div-gpt-ad-1367877572215-2' },
										bottom: { url: '/9494403/StubHub_Bottom_728x90', dimensions: [728, 90], el: 'div-gpt-ad-1367877572215-0' },
										banner: { url: '/9494403/StubHub_Persistent_Banner_950x50', dimensions: [950, 50], el: 'div-gpt-ad-1367864948095-0' }			 
									};
								
													
					StubhubAds.load = function(){
						googletag.cmd.push(function() {
							for (var key in StubhubAds.targetZones) {
								val = StubhubAds.targetZones[key];
								googletag.defineSlot(val.url, val.dimensions, val.el).addService(googletag.pubads()).setCollapseEmptyDiv(true,true);
							};
                			googletag.pubads().enableAsyncRendering();
				            googletag.pubads().enableSingleRequest();
							googletag.enableServices();
						});
					 };

					StubhubAds.populateZones = function() {
						googletag.cmd.push(function() {
							for (var key in StubhubAds.targetZones) {
								var val = StubhubAds.targetZones[key];
								googletag.display(val.el);
							};
						});
					};						

					StubhubAds.targetDFP = function(){
						for (var key in StubhubAds.targetingConfig) {
							var val = StubhubAds.targetingConfig[key];
							//the pubads function isn't available until gpt.js is eval'd
					   		googletag.pubads().setTargeting(key, val);			
						};

						StubhubAds.load();
						
								StubhubAds.populateZones();
							
					};
					
					$('#adPLaceHolderTop728').append('<div id="'+StubhubAds.targetZones.top728.el+'" class="adPLaceHolderTop728"></div>');
					
						$('#adPLaceHolderLeft').append('<div id="'+StubhubAds.targetZones.leftSide.el+'" class="adPLaceHolderLeft"></div>');
						$('<div id="'+StubhubAds.targetZones.bottom.el+'" class="adPLaceHolderBottom"></div>').insertAfter('#browsePageWrapper');
						
					$('#adPLaceHolderBanner').append('<div id="'+StubhubAds.targetZones.banner.el+'" class="adPLaceHolderBanner"></div>');	
							
									

					var tempPageUrl = window.location.href;
					if(!(tempPageUrl.match('showAds=off'))) {
						adPromise.then(function() {
							
									StubhubAds.targetDFP();
								
						});
					};
