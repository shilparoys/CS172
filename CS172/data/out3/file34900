    <script type="text/javascript">
				/*  Async fetching of googleadsource scripts */
					var googletag = googletag || {};
					googletag.cmd = googletag.cmd || [];
					StubhubAds = window.StubhubAds || {};
					var refer = document.referrer;
					if(refer !=''){
					var refer1 =  refer.match(/:\/\/(.[^/]+)/)[1];
					var refer2 =  refer1.substr(refer1.indexOf(".") + 1);
					}
					else
					{
						refer1 = 'noreferrer';
						refer2 = 'noreferrer';
					}
					
					if(typeof(searchStr1) === 'undefined'){searchStr2 = '';}else{searchStr2 = searchStr1;}
					/*  
						Utilizing head tags on the CSS content Element for loading the google ad service async, as well as declaring styles.  Also utlizing the HEADER Content Element for ad top place holder.  Left ad placeholder is defined in the TML.  Bottom ad placeholder is dynamically placed by jQuery.
					*/
					
					StubhubAds.targetingConfig = {
						pageType: "genreLeaf",
						oldpageType: "Genre page",
						categoryId: "28",
						genreId: "227755",
						genreParentId: "198988",
						genreGrandParentId: "28",
						ancestorGenreIds: "0 28 198988 227755",
						ancestorGenreIdsCust: "0;28;198988;227755",
						genreDesc: "USC Trojans Tickets",
						eventId: "default",
