     <script type="text/javascript">$(function() {
			$('#srchbtn').click(function() {
				var error_text;
				var search_term = "";
				search_term = $.trim($("#searchStr").val());
				if(search_term=='' || search_term.match("Enter: Team, artist, event, venue"))
				{
					error_text = true;	
				}
				else if (search_term.match(/[^a-zA-Z0-9 ]/g)) 
				{
					if (search_term.match(/[a-zA-Z0-9 ]/g))
						error_text = false;                 				
					else
						error_text = true;								 
		        }
		
				/*Display alert box*/
				if(error_text)

				{
					alert("Please enter the name of the team, artist, event, or venue you're looking for and click the 'Search' button.");
					$("input#searchStr")[0].focus();			
					return false;
				}
				else
					{return true;}
			});		
		});</script>
     <script>var loadScript = function(url,success) {
				$(document).ready(function() {
					$.ajaxSetup({ cache: true });
