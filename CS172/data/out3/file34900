    <script type="text/javascript">

			//Load social objects in the footer.  This must account for all pages.
			//Note: Event pages contain it's own supporting JS (social-content-event-html-XX.js) 
			//in which similar functions are defined with differing names - the result
			//a lot of undefined checks in the execution path to determine which supporting function to call.

			var SHSocial = window.SHSocial || {};
			SHSocial.fbPromise = {};
			SHSocial.twitterLoaded = false;
			SHSocial.fBLoaded = false;
			SHSocial.gPlusLoaded = false;

			SHSocial.loadTwitter = function() {
				if (typeof (twttr) != 'undefined' && typeof(twttr.widgets) != 'undefined'){
			        twttr.widgets.load();
       			    if(typeof (StubHub) != 'undefined' && typeof (StubHub.SocialContent) != 'undefined' && typeof (StubHub.SocialContent.TwitterButton) != 'undefined') {
				    	StubHub.SocialContent.TwitterButton();
				    }
			    } else {
			        $.getScript(document.location.protocol + '//platform.twitter.com/widgets.js', function() {
					    if(typeof (StubHub) != 'undefined' && typeof (StubHub.SocialContent) != 'undefined' && typeof (StubHub.SocialContent.TwitterButton) != 'undefined') {
					    	StubHub.SocialContent.TwitterButton();
					    }					    
			        });
			    }
				SHSocial.twitterLoaded = true;
			};

			SHSocial.loadFacebook = function() {
				if (typeof (FB) != 'undefined') {
					if(typeof (SHSocial.loadFBReporting) != 'undefined') {
						SHSocial.loadFBReporting();
					} 
						if(typeof (StubHub) != 'undefined' && typeof (StubHub.SocialContent) != 'undefined' && typeof (StubHub.SocialContent.fbConnectCalls) != 'undefined') {
						StubHub.SocialContent.fbConnectCalls();
					}
					if(typeof(SHSocial.initFBEvents) != 'undefined') {
						SHSocial.initFBEvents();
					}
			    } else {
				    var src = document.location.protocol + "//connect.facebook.net/en_US/all.js#xfbml=1";
					SHSocial.fbPromise = new Promise(function (resolve, reject) {
						$.ajax({
							type: "GET",
							url: src,
							dataType: "script",
							success: function() { 
								resolve(); 
							},
							error: function(jqXHR) { 
							//console.log(jqXHR); 
							}
						});
					});
					SHSocial.fbPromise.then(function() {
						if(typeof (SHSocial.loadFBReporting) != 'undefined') {
							SHSocial.loadFBReporting();
						} 
						if(typeof (StubHub) != 'undefined' && typeof (StubHub.SocialContent) != 'undefined' && typeof (StubHub.SocialContent.fbConnectCalls) != 'undefined') {
								StubHub.SocialContent.fbConnectCalls();
						}						
						if(typeof(SHSocial.initFBEvents) != 'undefined') {
							SHSocial.initFBEvents();
						}
					});
			    }
			    SHSocial.fBLoaded = true;
			};

			SHSocial.loadGooglePlus = function() {
				if (typeof (gapi) != 'undefined') {
			        $(".g-plusone").each(function () {
			            gapi.plusone.render($(this).get(0));
			        });
			    } else {
			        $.getScript('https://apis.google.com/js/plusone.js');
			    }
			    SHSocial.gPlusLoaded = true;
			};

			$(document).ready(function(){
    			$(window).scroll(function(){
    				if(typeof (SHSocial.twitterLoaded) != 'undefined' && SHSocial.twitterLoaded == false) {
        				SHSocial.loadTwitter();
        			}
        			if(typeof (SHSocial.fBLoaded) != 'undefined' && SHSocial.fBLoaded == false) {
        				SHSocial.loadFacebook();
        			}
    				if(typeof (SHSocial.gPlusLoaded) != 'undefined' && SHSocial.gPlusLoaded == false) {
        				SHSocial.loadGooglePlus();
        			}
    			});
			});
		</script>
    <div id="fb-root" style="display:none"></div>
