       <script language="javascript" type="text/javascript">
	var currentButton = 0;
	var defaultTime = 5;
	var pauseFlag = false;
	//Time is how long each image should remain on screen
	var times = [5,5,5,5];
	var varTimeout;
	//clearTimeout(varTimeout);
								
	function changeImage(number){
		if(number != currentButton){
			//alert("Click "+number);
			$("#imageSelecter .button:eq("+(currentButton)+")").toggleClass("on");
			$("#rotatingItems .item:eq("+(currentButton)+")").toggleClass("on");
			$("#imageSelecter .button:eq("+number+")").toggleClass("on");
			$("#rotatingItems .item:eq("+number+")").toggleClass("on");
			currentButton = number;
		}																	
	}

	function nextImage(){
		var next = (currentButton+1)%4;
		changeImage(next);
		currentButton = next;

	}
	
	function next01(){
		if(!pauseFlag){
			nextImage();
			setTimeout("next02()",times[currentButton]*1000);
		}
	}

	function next02(){
		if(!pauseFlag){
			nextImage();
			setTimeout("next01()",times[currentButton]*1000);
		}		
	}	
	
	$("#imageSelecter .button").click(function(){
		clearTimeout(varTimeout);
		changeImage($(this).index());
		pauseFlag = true;
	}); 

	/*
	$("#imageSelecter .button.next").click(function(){
		clearTimeout(varTimeout);
		nextImage();
		pauseFlag = true;
	}); 
	*/	
	
	$(document).ready(function(){
		setTimeout("next01()",times[currentButton]*1000);
	}); 	
</script> 
