					switch($(this).val()){
						case 'Search Courses':
						case 'Search Programs':
						case 'Search':
							$(this).val('');
						break;
					}
				}).blur(function(){
					clearTimeout( alertTimerId );
					$('#overlay').fadeOut('slow').remove();
					showtimer = false;
				});

				//Get the focus of the search box when clicked/tapped (richgarcia 10/30/2013 Refs #62)
				$("input[name='q']").bind('tap click touchstart',function(event) {
					$(this).focus();
		        });

