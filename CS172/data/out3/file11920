			function showCourseDetails(oid){
				LoadingIndicator_show();
				if(typeof(oid) == 'undefined') {
					oid = '';
				}

				var current_page_id = jQuery('.current').attr('id');
				var page = 'coursedetail';
				if (current_page_id == page) {
					 page = 'coursedetail1';
				}

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&type=course&oid=" + oid + "&showdetails",
					success: function(data) {
