			jQuery.ajaxSetup({
				timeout: (60 * 1000),
				tryCount : 0,
				retryLimit : 1,
				error:function(x,e){
					 if (e == 'timeout') {
						this.tryCount++;
						if (this.tryCount <= this.retryLimit) {
							//try again
							jQuery.ajax(this);
							return;
						}
					}
					alert("Unable to load the requested page. Please check your network connection and try again");
					LoadingIndicator_hide();
				}
			});

			
