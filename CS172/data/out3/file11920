				$('#courseschoose').bind('pageAnimationStart', function(e, info){
					LoadingIndicator_show();

					if (!$('#coursestypeslink').size()) {
						jQuery.ajax({
							url: "mobile_functions.php?catalog=" + currentcatalog + "&type=courses&checktypes",
							success: function(data) {
								if (data == 'true') {
									$('#courseschoose div.vertical-scroll div ul:eq(0)').append('<li class="arrow" id="coursestypeslink"><a href="#" onClick="showCourses_type();">Type</a></li>').fadeIn();
								}
							}
						});
					}
				});
				$('#programschoose').bind('pageAnimationStart', function(e, info){
					LoadingIndicator_show();

					if (!$('#programtypelink').size()) {
						jQuery.ajax({
							url: "mobile_functions.php?catalog=" + currentcatalog + "&type=program&checktypes",
							success: function(data) {
								if (data == 'true') {
								  $('<li class="arrow" id="programtypelink"><a href="#" onClick="showPrograms_type();">Type</a></li>').insertAfter('#programschoose div.vertical-scroll div ul li:eq(0)').fadeIn();
								}
							}
						});
					}
					if (!$('#degreetypeprogram').size()) {
						jQuery.ajax({
							url: "mobile_functions.php?catalog=" + currentcatalog + "&type=degree&checktypes",
							success: function(data) {
								if (data == 'true') {
									$('#programschoose div.vertical-scroll div ul:eq(0)').append('<li class="arrow" id="degreetypeprogram"><a href="#" onclick="showPrograms_degree();">Degree Type</a></li>').fadeIn();
								}
							}
						});
					}
				});

				if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i))) {
					$('a').not('.no_loading').live('tap',function(){LoadingIndicator_show(); });
					$("a[target='_blank']").bind('tap', function(event) {
						LoadingIndicator_hide();
					});
					$('#jqt a.bookmark').live('tap', function(e){
						e.preventDefault();
						LoadingIndicator_show();
						var link_text = $(this).html();
						var bookmark = $(this).attr('href');
						var target = bookmark.substring(1);
						if ( $(".current a[id='"+target+"']").size() ){
							scrollPage('-' + $(".current a[id='"+target+"']").position().top);
						} else if ( $(".current a[name='"+target+"']").size() ){
							scrollPage('-' + $(".current a[name='"+target+"']").position().top);
						} else if ($(this).attr("onclick") == undefined){
							if ( $(this).attr("href") != undefined && $(this).attr("href") != '#') {
								changePage($(this).attr("href"));
							} else {
								// email client
								jQuery.ajax({
									url: "mobile_functions.php?title="+escape($('.current h2:first').text())+"&link_text="+escape(link_text)+"&catalogname="+escape($('#cataloghome #catalog_title').html())+"&emailbrokenlink",
									success: function(data) {
										alert('That link to an anchor is not valid');
									}
								});
							}
						}
						$(this).removeClass('active');
						LoadingIndicator_hide();
						return false;
					});
					$('.current .search .button').bind('tap',function(){
						 switch ($('.current').attr('id')){
							case "courses":
							case "coursestypes":
							case "coursesprefix":
								$('.current .search input[type="search"]').blur().val('Search Courses');
								requestResults();
								break;
							case "programs":
							case "programschoose":
							case "programs_alpha":
							case "programs_sub":
								$('.current .search input[type="search"]').blur().val('Search Programs');
								requestResults();
								break;
							case "toc":
								$('.current .search input[type="search"]').blur().val('Search');
								changePage('#toc');
								break;
						}
						$(".current .search input[name='filter']").val('');

					});
				} else {
					$('a').not('.no_loading').live('click', function() {LoadingIndicator_show();});
					 $('.current .search .button').bind('click',function(){
						 switch ($('.current').attr('id')){
							case "courses":
							case "coursestypes":
							case "coursesprefix":
								$('.current .search input[type="search"]').blur().val('Search Courses');
								break;
							case "programs":
							case "programschoose":
							case "programs_alpha":
							case "programs_sub":
								$('.current .search input[type="search"]').blur().val('Search Programs');
								break;
							case "toc":
								$('.current .search input[type="search"]').blur().val('Search');
								break;
						}
						$(".current .search input[name='filter']").val('');

					});
				}
			});
			function moveTable(id, direction){
				 var table_width = $('#'+id).width();
				 var table_visible = $('#'+id).parent().width();
				 var position_left = $('#'+id).position().left;
				 var movepx = Math.ceil( table_visible / 4 ) * 3;

				if (direction == 'right') {
					 if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i))) {
					  if (Math.abs(position_left) < table_width) {
						if ( (Math.abs($('#'+id).position().left) ) < ( table_width - $(window).width() ) ) {
							$('#'+id).animate({ left: '-='+movepx }, 300);
							$('#'+id).parent().toggleClass('tablewrapper_wide', false);
						} else {
						  $('#'+id).parent().siblings('.right_table').hide();
						  $('#'+id).parent().toggleClass('tablewrapper_wide', true);
						}
					  }
					 } else {
						if ( (Math.abs($('#'+id).position().left) ) < ( table_width - $(window).width() ) ) {
							$('#'+id).animate({ left: '-='+movepx }, 300);
							$('#'+id).parent().toggleClass('tablewrapper_wide', false);
						} else {
						  $('#'+id).parent().siblings('.right_table').hide();
						  $('#'+id).parent().toggleClass('tablewrapper_wide', true);
						}
					 }
					 $('#'+id).parent().siblings('.left_table').show();
				} else {
					// Direction = Left
					if((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i))) {
						if (position_left < 0) {
								$('#'+id).animate({ left: '+='+movepx }, 0);
								$('#'+id).parent().toggleClass('tablewrapper_wide', false);
						} else {
						  $('#'+id).animate({ left: '0' }, 100);
						  $('#'+id).parent().siblings('.left_table').hide();
						  $('#'+id).parent().toggleClass('tablewrapper_wide', true);
						}
					} else {
						if (position_left < 0) {
						  $('#'+id).animate({ left: '+='+movepx }, 300);
						  $('#'+id).parent().toggleClass('tablewrapper_wide', false);
						} else {
						  $('#'+id).parent().siblings('.left_table').hide();
						  $('#'+id).parent().toggleClass('tablewrapper_wide', true);
						}
					 }
					 $('#'+id).parent().siblings('.right_table').show();
				}
			}
			function wrapTable( page ){
				var i = 0;

				$('#'+page + ' table').each(function(){
					var id = '';
					if($(this).attr('id')){
						id = $(this).attr('id');
					} else {
						i++;
						id = 'table_' + i;
						$(this).attr('id', id);
					}

					if (!$(this).parent().parent().hasClass("table_wrapper")) {
						if ( $(this).width() > $(window).width() ){
							$(this)
								.wrap('<div class="display_table" ><span class="tablewrapper tablewrapper_wide" /></div>')
								.parent().before('<span class="left_table" onclick="moveTable(\''+ id +'\',\'left\');"></span>')
								.after('<span class="right_table" onclick="moveTable(\''+ id +'\',\'right\');"></span>');

							$(this).parents('.display_table').children('.left_table, .right_table').css({'height': $(this).height() +'px'});
						}
					}
				});
			}
			function searchCatalog(search_by, search_value) {
				LoadingIndicator_show();
				var on_search_page = false;

				if (typeof(search_by) == 'undefined') {
					search_by = $('#searchWhere').val();
				}

				if (typeof(search_value) == 'undefined') {
					search_value = $('#searchFormQuery').val();
					var on_search_page = true;
				}

				switch(search_by){
				 case "course":
					 jQuery.ajax({
						url: "mobile_functions.php?catalog=" + currentcatalog + "&q=" + search_value + "&type=course&searchcatalog",
						success: function(data) {
							$('#courses ul#course_big_list').empty().append(data);
							$('#searchwords').val(search_value);
							if (on_search_page == true) {
								LoadingIndicator_hide();
								jQT.goTo('#courses');
								$('#overlay').fadeOut('slow');
							} else {
								changePage('#courses');
							}
							showtimer = false;
						}
					});
					 break;
				 case "program":
					jQuery.ajax({
						url: "mobile_functions.php?catalog=" + currentcatalog + "&q=" + search_value + "&type=program&searchcatalog",
						success: function(data) {
							$('#programs div.vertical-scroll div:eq(0) ul').empty().append(data);
							changePage('#programs');
						}
					});
					break;
				 case "hierarchy":
				 case "page":
					var page_id = 'custompage1';
					var current_page_id = jQuery('.current').attr('id');

					if (current_page_id.substring(0,6) == 'custom') {
						var page_number = 1;
						if (current_page_id.substring(current_page_id.length - 1) == page_number) {
							page_number = page_number + 1;
						}
						if (page_number > 9) {
							page_number = 1;
						}
						page_id = 'custompage' + page_number;
					}

					jQuery.ajax({
						url: "mobile_functions.php?catalog=" + currentcatalog + "&q=" + search_value + "&type=" + search_by + "&searchcatalog",
						success: function(data) {
							$('#' + page_id + ' div.vertical-scroll div:eq(0)').empty().append('<ul class="edgetoedge">' + data + '</ul>');
							changePage('#' + page_id);
						}
					});
					break;
				  default:
					  alert('The search request is invalid please try again.');
				}
			}
			function LoadingIndicator(){
				if (!$('.toolbar .loading').size()) {
					jQuery('.toolbar').append('<div class="loading"><img src="//acalog-clients.s3.amazonaws.com/production/drew/img/mobile/loading.gif" alt="Loading" /></div>');
				} else {
					jQuery('.toolbar .loading').show();
				}
				jQuery('.toolbar h1').css("visibility", "hidden");
				return false;
			}
			function LoadingIndicator_show(){
				if (!$('.toolbar .loading').size()) {
					jQuery('.toolbar').append('<div class="loading"><img src="//acalog-clients.s3.amazonaws.com/production/drew/img/mobile/loading.gif" alt="Loading" /></div>');
				} else {
					jQuery('.toolbar .loading').show();
				}
				jQuery('.toolbar h1').css("visibility", "hidden");
				return false;
			}
			function LoadingIndicator_hide(){
				jQuery('.toolbar .loading').remove();
				jQuery('.toolbar h1').css("visibility", "visible");
				return false;
			}
			function showSearchResults( )
			{
			  if ( !showtimer )
			  {
				// Start the timer
				showtimer = true;
				alertTimerId = setTimeout( "requestResults()", 900 );
			  }
			  else
			  {
				showtimer = false;
				clearTimeout( alertTimerId );
			  }
			}

			function requestResults( )
			{
				LoadingIndicator_show();

				var type = '';
				switch ($('.current').attr('id')){
					case "courses"://alpha
					case "coursestypes":
					case "coursesprefix":
						type = 'course';
						break;
					case "programs":
					case "programschoose":
					case "programs_alpha":
					case "programs_sub":
						type = 'program';
						break;
					case "toc":
						type = 'page';
						break;
				}
				var searchfor = $('.current .search  input[type="search"]').val().trim();
				switch(searchfor){
					case 'Search Courses':
					case 'Search Programs':
					case 'Search':
						searchfor = '';
					break;
				}
				if ($(".current .search input[name='filter']").length)
				{
					var filter = $(".current .search input[name='filter']").val().trim();
					if (filter != '' && typeof(filter) != 'undefined') {
						searchfor = filter + ' AND ' + searchfor;
					}
				}

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&q=" + searchfor + "&type=" + type + "&page=1&searchcatalog",
					success: function(data) {
						switch (type){
							case "course":
							case "program":
								$('.current div.vertical-scroll div:eq(0) ul').empty().append(data);
								break;
							case "page":
								var page_id = 'custompage3';
								$('#' + page_id + ' div.vertical-scroll div:eq(0)').empty().append('<ul class="edgetoedge">' + data + '</ul>');
								changePage('#'+page_id);
								break;
							default:
								type = 'course';
								var page_id = 'custompage6';
								$('#' + page_id + ' div.vertical-scroll div:eq(0)').empty().append('<ul class="edgetoedge">' + data + '</ul>');
								changePage('#'+page_id);
								break;
						}
					},
					complete: function(){
						$('#overlay').fadeOut().remove();
						LoadingIndicator_hide();
					}
				});

			}
			function showMoreSearchResults(page, type, keyword)
			{

				if(typeof(page) == 'undefined') {
					page = 2;
				}

				if(typeof(type) == 'undefined') {
					type = 'course';
				}

				if (typeof(keyword) == 'undefined') {
					keyword = $('#searchwords').val();
				}

				LoadingIndicator_show();

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&q=" + keyword + "&type=" + type + "&page=" + page + "&searchcatalog",
					success: function(data) {
						var list = jQuery('.current ul li.showmore').parent();
						$(list).children('li.showmore').remove();
						$(list).append(data);
					},
					complete: function(){
						LoadingIndicator_hide();
					}
				});
			}
			function changePage(page){
				LoadingIndicator_show();
				if(typeof(page) == 'undefined') {
					alert('Sorry, that page is unavailable');
				}
				//var lastpage = $('.current').attr('id');
				page = page.replace(/#/g,'');

				var toPage = '#'+page;

				if (show_archived_message){
					if (!$(toPage + ' .archived').size() && toPage != '#home') {
						$(toPage + ' .toolbar').after('<div class="archived">[ARCHIVED CATALOG]</div>');
					}
				} else {
					$('div.archived').remove();
				}
				if ((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (navigator.userAgent.match(/iPad/i))) {
					$('.search').hide();
					$('#' + page + ' div.vertical-scroll > div').each(function(){
						$(this).css('-webkit-transform','translate3d(0, 0, 0)');
					});
				} else {
					$('.search').show();
				}
				if(!$(toPage).hasClass('current')) {
					jQT.goTo('#'+page, 'slide');
					wrapTable(page);
				}
				$('.active').removeClass('active');
				LoadingIndicator_hide();
			}
			function scrollPage(pos){
				if(typeof(pos) == 'undefined') {
					pos = 0;
				}
				if (pos == 'top') {
					pos = 0;
				} else {
					var screenHeight = $('div.current div.vertical-scroll').height();
					//var toolbarheight = $('div.toolbar').outerHeight();
					var maxScroll = $('div.current div.vertical-scroll > div').height();
					pos = Number(pos);
					screenHeight = Number(screenHeight);
					maxScroll = Number(maxScroll);
					if (pos > 0) {
						pos *= -1;
					}
				}
				if ( isNaN(pos) ) {
					pos = 0;
				}
				//z-index is so the tool bar is in front of the text
				$('div.toolbar').each(function(){
					$(this).css('z-index','2');
				});
				$('div.current div.vertical-scroll > div').each(function(){
					$(this).css('webkitTransitionDuration','300ms');
					// Chrome and Safari use webkit
					$(this).css('webkitTransform','translate3d(0, ' + pos + 'px, 0)');
					// set pos an extra 65px for the header
					if (pos !== 0){
						pos = 65 + pos;
					}
					// IE 10 and Firefox use transform
					$(this).css('transform','translate3d(0px, ' + pos + 'px, 0px)');
				});
			}
			function showArchivedCatalogs(hidepub){
				LoadingIndicator_hide();

				var current_page_id = jQuery('.current').attr('id');
				if ( current_page_id != 'home') {
				  changePage('#home');
				}

				if (!$('#cataloglist .showmore').size() && ($('#cataloglist .arrow').not('.archivedCatalog').size() > 0)) {
					$('#cataloglist').append('<li class="showmore" style="padding: 10px !important;"><a style="padding: 10px ! important;" onclick="showArchivedCatalogs(2);" class="no_loading" href="#">All Catalogs</a></li>');
				}

				if (typeof(hidepub) != 'undefined'){
					if (hidepub == 1) {
						$('#cataloglist .archivedCatalog').slideDown('slow');
						$('#cataloglist .showmore').hide();
						$('#cataloglist .arrow').not('.archivedCatalog').hide();
						if ($('#cataloglist .arrow').not('.archivedCatalog').size() > 0) {
							$('#cataloglist .showmore').html('<a style="padding: 10px ! important;" onclick="showArchivedCatalogs(2);" class="no_loading" href="#">All Catalogs</a>').slideDown('slow');
						}
					} else if (hidepub == 2) {
						$('#cataloglist .arrow').slideDown('slow');
						$('#cataloglist .showmore').slideUp('slow');
					}

				} else {
					$('#cataloglist .archivedCatalog').slideDown('slow');
					$('#cataloglist .showmore').slideUp('slow');
				}

				return false;
			}
			function showCatalog(catalog, id, archived){
				LoadingIndicator_show();
				currentcatalog = id;

				var find_title = /title">(.*?)</i.exec(catalog.innerHTML);
				catalog_title = find_title[1];

				if (typeof(archived) != 'undefined' && archived == 1) {
					show_archived_message = true;
					if (!$('#catalog_title .archived').size()) {
						$('#catalog_title .toolbar').not('#home .toolbar').after('<div class="archived">[ARCHIVED CATALOG]</div>');
					}
				} else {
					show_archived_message = false;
					$('div.archived').remove();
				}

				$.get("mobile_functions.php?catalog=" + currentcatalog, function(data){
					$('#catalog_title').html(catalog_title).slideDown("fast");

					changePage('#cataloghome');

				  var toolbar_height = Number($(".current .toolbar").height());

				var menu_height = (Number(toolbar_height) + Number($(".current .toolbar").siblings(".info").height()) + Number($(".current .toolbar").siblings(".archived").height()));
				if ($('.current .banner').height() > 20 ) {
					menu_height += Number($('.current .banner').height());
				}
				 $('#custom_screenheight').html('.vertical-scroll, .vertical-slide {height: ' + (screen.height - menu_height) + 'px;}.landscape .vertical-scroll, .landscape .vertical-slide {height: ' + (screen.width - menu_height) + 'px;}');

					var sidebuttons = 80;
					var table_width = $(window).width()-sidebuttons;
					var table_width_land = $(window).height()+94;
					$('#custom_screenheight').append(' .tablewrapper {width: ' + table_width + 'px;} .landscape .tablewrapper {width: ' + table_width_land + 'px;}  .tablewrapper_wide {width: ' + (table_width + 40) + 'px;} .landscape .tablewrapper_wide {width: ' + (table_width_land + 40) + 'px;}');
				 });

				return false;
			}

			function chooseProgramDisplay(){
				LoadingIndicator_show();

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&type=program&checktypes",
					success: function(data) {
						if (data == 'true') {
							if (!$('#programtypelink').size()) {
								$('<li class="arrow" id="programtypelink"><a href="#" onClick="showPrograms_type();">Type</a></li>').insertAfter('#programschoose div.vertical-scroll div ul li:eq(0)');
							}
						  changePage('#programschoose');
						} else {
							$('#programtypelink').remove();
							jQuery.ajax({
								url: "mobile_functions.php?catalog=" + currentcatalog + "&type=degree&checktypes",
								success: function(data) {
									if (data == 'true') {
										if (!$('#degreetypeprogram').size()) {
											$('#programschoose div.vertical-scroll div ul:eq(0)').append('<li class="arrow" id="degreetypeprogram"><a href="#" onclick="showPrograms_degree();">Degree Type</a></li>');
										}
										changePage('#programschoose');
									} else {
										$('#degreetypeprogram')
										showPrograms_alpha();
									}
								}
							});
						}
					}
				});

				return false;
			}

			function showPrograms(program_id, page){
				LoadingIndicator_show();

				var program_page = 'programs_sub';

				if(typeof(program_id) == 'undefined') {
					program_id = '';
					program_page = 'programs';
				}

				if(typeof(page) == 'undefined') {
					page = 1;
				}

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&program_id=" + program_id + "&page=" + page + "&showprograms",
					success: function(data) {
						var list = $('#' + program_page + ' div.vertical-scroll div ul:eq(0)');
						if( page <= 1 ) {
							list.empty();
						} else {
							$('#' + program_page + ' div.vertical-scroll div ul:eq(0) li:last-child').remove();
						}
						list.append(data);
						if (page == 1) {
							changePage('#' + program_page);
						}
					}
				});
				return false;
			}
