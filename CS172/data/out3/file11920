				});
				$('#searchFormQuery2').bind('keyup', function(event) {
					if (event.keyCode == '13' || event.keyCode == '10') {
						searchCatalog('program', $('#searchFormQuery2').val());
					}
				});

				$('#searchCatalogForm .submit').click(function(event) {
					searchCatalog();
					return false;
				});

				$('#searchCatalogForm1 .submit').click(function(event) {
					searchCatalog('course', $('#searchFormQuery1').val());
					return false;
				});

				$('#searchCatalogForm2 .submit').click(function(event) {
					searchCatalog('program', $('#searchFormQuery2').val());
					return false;
				});

				$('#courses').bind('pageAnimationEnd', function(e, info){
					$('#courses .search').hide();
