			function showProgramDetails(oid, page_number){
				LoadingIndicator_show();

				if(typeof(oid) == 'undefined') {
					oid = '';
				}

				if(typeof(page_number) == 'undefined') {
					page_number = 1;
				}

				var current_page_id = jQuery('.current').attr('id');
				if (current_page_id.substring(current_page_id.length - 1) == page_number) {
					page_number = page_number + 1;
				}
				if (page_number > 9) {
					page_number = 1;
				}

				var page = 'programdetails' + page_number;

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&type=program&oid=" + oid + "&showdetails",
					success: function(data) {
						$('#' + page + ' div.vertical-scroll div:eq(0)').empty().append(data);
						changePage('#' + page);
					}
				});
				return false;
			}

			function showCoreDetails(oid, coreid){
