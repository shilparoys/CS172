						changePage('#' + page);
					}
				});
				return false;
			}

			function showCustomPage(oid, type, page, options){
				LoadingIndicator_show();
				if(typeof(oid) == 'undefined') {
					oid = '';
				}

				if(typeof(type) == 'undefined') {
					type = 'page';
				}

				if(typeof(page) == 'undefined' || typeof(page) == 'object') {
					page = '1';

					var current_page_id = jQuery('.current').attr('id');

					if (current_page_id.substring(0,6) == 'custom') {
						page = parseInt(current_page_id.substring(current_page_id.length - 1)) + 1;
					}
				}

				if (page > 9) {
					page = 1;
				}

				if (typeof(options) == 'undefined') {
					options = '';
				}

				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&oid=" + oid + "&type=" + type + "&page=" + page + options +"&showCustomPage",
					success: function(data) {
						$('#custompage' + page + ' div.vertical-scroll div:eq(0)').empty().append(data);
						changePage('#custompage' + page);
					}
				});
				return false;
			}
			function showTOC(){
				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&showTOC",
					success: function(data) {
						$('#toc div.vertical-scroll div:eq(0) div.content').empty().append(data);
						changePage('#toc');
					}
				});
				return false;
			}
			function showLocation(location) {
				if(typeof(location) == 'undefined') {
					return false;
				}
				jQuery.ajax({
					url: "mobile_functions.php?location=" + location + "&showcontactdetails",
					success: function(data) {
						$('#contact_details #schoolinfo').empty().append(data);
						changePage('#contact_details');
					}
				});
				return false;
			}
			function addParentProgramInfo(parent_program_info, link){
				if (typeof(parent_program_info) != 'undefined' && typeof(link) != 'undefined') {
					$('body').bind('pageAnimationEnd', function(e, info){
						if ( $('.current .info').length > 0 ) {
							$('.current .info').html(parent_program_info);
							$('.current .info').css('padding', '5px');
							$('.current .info').attr('onclick', '');
							$('.current .info').bind('tap click',function(e){
								eval(link);
							});
						} else {
							$('.current .toolbar').after('<div class="info" style="padding:5px;" onclick="' + link + '">' + parent_program_info + '</div>');
						}
						$('.current .info').hide().slideDown("slow");
						$(this).unbind(e);
					});
				}
			}

			function followAnchor(anchorTag) {
				LoadingIndicator_show();

				if(typeof(anchorTag.id) == 'undefined') {
					return false;
				}

				if (typeof(anchorTag.anchor) != 'undefined'){
					if (anchorTag.anchor.substr(0,1) == '#') {
						anchorTag.anchor = anchorTag.anchor.substr(1);
					}
				}

				var page = '';
				var type = '';
				var page_number = 1;
				var url_tail = 'showdetails';
				var current_page_id = jQuery('.current').attr('id');

				switch(anchorTag.type) {
					case "course":
					case "courses":
						page = 'coursedetail';
						if ( current_page_id == page) {
							page = 'coursedetail1';
						}
						type = 'course';
						anchorTag.type = 'course';
						break;
					case "program":
					case "programs":
					case "core":
						type = 'program';
						page_number = parseInt(current_page_id.substring(current_page_id.length - 1)) + 1;
						break;
					case "entity":
						type = 'entity';
						break;
					case "page":
						url_tail = 'showCustomPage';
						type = 'page';
						break;
					default:
						return false;
				}

				if (type == 'entity' || type == 'page' || type == 'program') {
					if (current_page_id.substring(0,6) == 'custom') {
						page_number = parseInt(current_page_id.substring(current_page_id.length - 1)) + 1;
					} else if (current_page_id.substring(current_page_id.length - 1) == page_number) {
						page_number = page_number + 1;
					}
					if (page_number > 9) {
						page_number = 1;
					}
					if (type == 'program') {
						page = 'programdetails' + page_number;
					} else {
						page = 'custompage' + page_number;
					}
				}

				if (current_page_id != page) {
					var core_url = "";
					if (typeof(anchorTag.coreid) != 'undefined'){
						core_url = "&coreid=" + anchorTag.coreid;
					}
					findAnchor("catalog=" + currentcatalog + "&type=" + type + "&oid=" + anchorTag.id + core_url  + "&" + url_tail, page, anchorTag.anchor, type);
				} else {
					scrollToAnchor(anchorTag.anchor);
				}

				return false;
			}
			function findAnchor(parameter, page, anchor, type){
				if(typeof(type) != 'undefined' && type == 'entity') {
					$.getJSON("mobile_functions.php?" + parameter + "&format=json", function(data) {
						$('#' + page + ' div.toolbar h1').empty().append(data.type);
						$('#' + page + ' div.vertical-scroll div:eq(0)').empty().append(data.content);
						changePage('#' + page);
						waitCurrentPage(page, anchor);
					});
				} else {
					jQuery.ajax({
						url: "mobile_functions.php",
						data: parameter,
						success: function(data) {
								if (type == 'page') {
									$('#' + page + ' div.toolbar h1').empty().append('E-Catalog');
								}
							$('#' + page + ' div.vertical-scroll div:eq(0)').empty().append(data);
							changePage('#' + page);
							waitCurrentPage(page, anchor);
						}
					});
				}
				return;
			}
			function waitCurrentPage(page, anchor) {
				var current_page_id = jQuery('.current').attr('id');
				if (current_page_id == page) {
					window.clearTimeout(alertTimerId);
					scrollToAnchor(anchor);
					return true;
				} else {
					alertTimerId = window.setTimeout("waitCurrentPage('" + page + "', '" + anchor + "')", 1000);
				}
			}
			function scrollToAnchor(target){
				if(typeof(target) != 'undefined') {
					if (target.substr(0,1) == '#') {
						target = target.substring(1);
					}
					if ( $(".current [id='"+target+"']").size() ){
						scrollPage('-' + $(".current a[id='"+target+"']").position().top);
					} else if ( $(".current a[name='"+target+"']").size() ){
						scrollPage('-' + $(".current a[name='"+target+"']").position().top);
					}
				}
				return;
			}
			function TryAgain(url){
				jQuery.ajax({
					url: "mobile_functions.php?"+url,
					success: function(data) {
						jQuery('.current .error').replaceWith(data);
					}
				});
				return false;
			}
		</script> 
 </head> 
