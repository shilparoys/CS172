				if(((navigator.userAgent.match(/iPhone/i)) || (navigator.userAgent.match(/iPod/i)) || (navigator.userAgent.match(/iPad/i))) && !window.navigator.standalone && !(navigator.userAgent.match(/Opera.Mini/i))  ) {
