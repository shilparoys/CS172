			function showSchools(id, type){
				LoadingIndicator_show();

				if(typeof(id) == 'undefined') {
					id = 1;
					type = 'institution';
				}
				if(typeof(type) == 'undefined') {
					followAnchor({id: id,type:"entity"});
				} else {
					type = type.toLowerCase();

					jQuery.ajax({
						url: "mobile_functions.php?catalog=" + currentcatalog + "&id=" + id + "&showhierarchy",
						success: function(data) {
							$('#' + type + ' div.vertical-scroll div:eq(0)').empty().append(data);
							changePage('#' + type);
						}
					});
				}
				return false;
			}

			function showCourses_alpha(page, listsep, options){
				LoadingIndicator_show();
				var options_url = "";

				if(typeof(page) == 'undefined') {
					page = 1;
				}

				if(typeof(listsep) == 'undefined') {
					listsep = '';
				}

				if(typeof(options) == 'string') {
					options_url = options;
				}


				jQuery.ajax({
					url: "mobile_functions.php?catalog=" + currentcatalog + "&page=" + page + "&listsep=" + listsep + options_url + "&showlist",
					success: function(data) {
						if (typeof(options) == 'undefined') {
							var list = $('ul#course_big_list');

							if( page <= 1 ) {
								list.empty();
							} else {
								$('ul#course_big_list li:last-child').remove();
							}

							list.append(data);

							if (page == 1) {
								changePage('#courses');
							}

							$("#courses .search input[name='filter']").val('');
						} else {
							if (page > 1) {
								var list = $('div.current ul:last');
							}

							if( page <= 1 ) {
								list.empty();
							} else if (page > 1) {
								$('div.current ul:last li:last-child').remove();
							}

							$('#jqt').append('<div style="display:none;" id="temp_holder">'+data+'</div>');

							var first_child = $('div#temp_holder').children().get(0).tagName;
							if (first_child == 'UL') {
								$('div.current div.vertical-scroll div ul:last').append($('#temp_holder :first-child').html());
								$('div#temp_holder ul').eq(0).remove();
								$('div.current div.vertical-scroll div').append($('#temp_holder').html());
							} else if (first_child == 'LI')  {
								$('div.current div.vertical-scroll div ul:last').append($('#temp_holder').html());
							} else {
								$('div.current div.vertical-scroll div').append($('#temp_holder').children());
							}
							$('#temp_holder').remove();
						}
					}
				});
				return false;
			}


