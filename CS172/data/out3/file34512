  <script type="text/javascript">
<!--//--><![CDATA[//><!--
(function ($) {
  Drupal.behaviors.nyuSternMobile = {
    attach: function (context, settings) {
      var cdfUserAgentList =  new Array();
      fillCdfUserAgentList();

      var redirectMainUrl = "";

      //must be done immediately and not on page load
      checkMobileRedirect();


      //if no redirect was performed use main redirect
      checkMainRedirect();

      function checkMobileRedirect(){
        //check if cookie exists
        var mobileRedirectCookieValue = getMobileRedirectCookie();
        //alert("Mobile cookie: " + mobileRedirectCookieValue);
        //if yes, redirect or not
        //if no, checkMobile
        if(mobileRedirectCookieValue != undefined && mobileRedirectCookieValue.length > 0){
          //set mobile redirect if mobile nodeId property present
          if(mobileRedirectCookieValue == "mobile"){
            redirectMobile();
          }
        }else{
          checkMobile();
        }
      }

      function getMobileRedirectCookie(){
        //cookie name - site type
        var c_name = "nyus_sttyp";
        var i,x,y,ARRcookies=document.cookie.split(";");
        for (i=0;i<ARRcookies.length;i++)
        {
          x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
          y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
          x=x.replace(/^\s+|\s+$/g,"");
          if (x==c_name)
            {
            return unescape(y);
            }
          }

      }
      function checkMobile(){
        if(checkUserAgent() == 1){
          setMobileCookie(1);
        }else if(jQuery.browser.mobile != undefined ){
          if(jQuery.browser.mobile === false){
            //NYUS:not all touch devices needs to get redirected;disable Modernizr
            //checkModernizr();
            checkScreenWidth();
          }else{
            setMobileCookie(1);
          }
        }else{
        //if detection libraries are not present, set to Main site
          setMobileCookie(0);
        }
      }

      function checkModernizr(){
        if (Modernizr.touch){
          setMobileCookie(1);
        } else {
          checkScreenWidth();
        }
      }

      function checkScreenWidth(){
        //NYUS: redirect all under 700px; was 1100
        //if(screen.width < 1100){

        if(screen.width < 700){
          setMobileCookie(1);
        }else{
          setMobileCookie(0);
        }
      }

      function setMobileCookie(isMobile){
        if(isMobile == 1){
          //alert("MOBILE");
          document.cookie = "nyus_sttyp=mobile;path=/;domain=cons-priv.stern.nyu.edu";
          //set mobile redirect if mobile nodeId property present
          redirectMobile();
        }else{
          document.cookie = "nyus_sttyp=main;path=/;domain=cons-priv.stern.nyu.edu";
          //alert("NOT MOBILE");
        }
      }

      function redirectMobileArticle(){
        var currentDomain = location.host;
        var targetDomain = "";

        switch(currentDomain){
          case "www-dev-aq.stern.nyu.edu":
            targetDomain = "http://m-dev-aq.stern.nyu.edu/";
            break;
          case "www-stg-aq.stern.nyu.edu":
            targetDomain = "http://m-stg-aq.stern.nyu.edu/";
            break;
          case "www.stern.nyu.edu":
            targetDomain = "http://m.stern.nyu.edu/";
            break;
          default:
            alert("Domain info unavailable. Please contact system administrator.");
        }

        var nodeUrl = "";



        var currentUrl = window.location.href;
        var contentName = currentUrl.substr(currentUrl.lastIndexOf('/') + 1);
        window.location = targetDomain + nodeUrl + contentName;
        //alert(targetDomain + nodeUrl + contentName);
      }

      function checkMainRedirect(){
        if(redirectMainUrl.length > 0){
          window.location = redirectMainUrl;
        }
      }

      function fillCdfUserAgentList(){

          cdfUserAgentList.push("");

      }

      function checkUserAgent(){
        var isMobile = 0;
        var currentUserAgent = navigator.userAgent;

        for (var i = 0; i < cdfUserAgentList.length; i++) {
          if(currentUserAgent == cdfUserAgentList[i]){
            isMobile = 1;
          }
        }
        //alert(isMobile);
        return isMobile;
      }

    }
  };
})(jQuery);
function redirectMobile(){
        var redirectMobileUrl = "http://m.stern.nyu.edu/";
        window.location = redirectMobileUrl;
      }
//--><!]]>
</script> 
