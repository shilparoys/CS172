<!--//--><![CDATA[//><!--
(function ($) {

// Tabbed content.
$(document).ready(function(){
  $("#tabbed-block a").click(function(event){
    var href =  $(this).attr('href').toLowerCase();
    $("#tabbed-block li[id^=tab-]").each(function(){
      var linkHref =  $(this).find('a').attr('href').toLowerCase();
      if(linkHref == href){
         $(this).trigger('click');
         event.preventDefault();
      }
    });
  });
});

// iOS fixes.
$(document).ready(function(){
  if(navigator.platform == 'iPad' || navigator.platform == 'iPhone' || navigator.platform == 'iPod') {
   // $("#footer-global").css("position", "static");
    $("#footer-global").css("position", "absolute");
    $("#footer-global").css("bottom", "-340px");
    //alert("Platform6:" + navigator.platform);
  };
});

})(jQuery);

