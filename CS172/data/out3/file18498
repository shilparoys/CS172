          <input id="calWidgetSearchDate" type="hidden" name="d" value=""> 
          <input type="hidden" name="s" value="start_time"> 
          <input type="hidden" name="sd" value="asc"> 
          <input type="hidden" name="unrolled" value="1"> 
          <div id="calendar-widget"></div> 
         </form> 
