$(function() {

    $('#slideshow4').cycle({
        fx:      'scrollLeft',
        timeout:  12000,
        prev:    '#prev4',
        next:    '#next4',
        pager:   '#nav-mp4',
        pagerAnchorBuilder: pagerFactory4,
    after:   onAfter4});
function onAfter4(curr4, next4, opts4) {
    var index4= opts4.currSlide;
    $('#prev4')[index4== 0 ? 'hide' : 'show']();
    $('#next4')[index4== opts4.slideCount - 1 ? 'hide' : 'show']();
}

    function pagerFactory4(idx4, slide) {
        var s4= idx4> 17 ? ' style="display:none"' : '';
        return '<li'+s4+'><a href="#">'+(idx4+1)+'</a></li>';
    };



    // remove loading image
    $('#blox-featured-listings-content').removeClass('blox-loading');
    $('#nav-container-feat-business, #blox-featured-pop-search').removeClass('hide');
    
    $('a.blox-featured-pop-search-toggle').toggle(
       function() {
           $('#blox-featured-pop-search-list').slideDown(400);
        },
        function() {
           $('#blox-featured-pop-search-list').slideUp(400);
        }
    );
    $('.ui-state-default').hover(
    function(){ 
        $(this).addClass('ui-state-hover'); 
    },
    function(){ 
        $(this).removeClass('ui-state-hover'); 
    }
    );

});
