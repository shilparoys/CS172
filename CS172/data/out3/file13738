//contact us script starts
function formValidate( fieldName )
{
  var fieldVal = $.trim ( $('#' + fieldName ).val() );
  if ( fieldVal == '' )
  {
    if ( fieldName == 'message' )
      $('#' + fieldName ).attr('class', 'errtxtArea');
    else
      $('#' + fieldName ).attr('class', 'errInput');
    $('#' + fieldName + 'Txt' ).attr('class', 'errfieldTxt');
    return false;
  }
  else
  {
    if ( fieldName == 'message' )
      $('#' + fieldName ).attr('class', 'txtArea');
    else
      $('#' + fieldName ).attr('class', 'txtBx');
    $('#' + fieldName + 'Txt' ).attr('class', 'fieldTxt');
    return true;
  }
}
function chkAttendee()
{
  attendeeCnt = parseInt($('#ugmAttendeeCounter').val());
  temp_var = 0;
  for ( i = 1; i <= attendeeCnt; i++ )
  {
    if ( $.trim ( $('#ugmAttendeeName' + i).val() ) == '' )
    {
      $('#ugmAttendeeName' + i).attr('class', 'errInput');
      $('#ugmAttendeeNameTxt' ).attr('class', 'errfieldTxt');
      temp_var++;
    }
    else
      $('#ugmAttendeeName' + i).attr('class', 'txtBx');
  }
  if ( temp_var == 0 )
  {
    $('#ugmAttendeeNameTxt' ).attr('class', 'fieldTxt');
    return true;
  }
  else
    return false;
}
function phoneValidate( fieldName )
{
  var phoneVal = $.trim ( $( "#" + fieldName ).val() );
  var phoneRegExp = /^((\+)?[1-9]{1,2})?([-\s\.])?((\(\d{1,4}\))|\d{1,4})(([-\s\.])?[0-9]{1,12}){1,2}$/;
  var numbers = phoneVal.split("").length;
  if (10 <= numbers && numbers <= 20 && phoneRegExp.test(phoneVal))
  {
    $('#' + fieldName + 'Txt' ).attr('class', 'fieldTxt');
    $('#' + fieldName ).attr('class', 'txtBx');
    return true;
  }
  else
  {
    $('#' + fieldName + 'Txt' ).attr('class', 'errfieldTxt');
    $('#' + fieldName ).attr('class', 'errInput');
    return false;
  }
}
function emailValidate( fieldName )
{
  // email validation
  var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
  var emailaddressVal = $.trim ( $('#' + fieldName ).val() );
  if(emailaddressVal == '')
  {
    $('#' + fieldName + 'Txt' ).attr('class', 'errfieldTxt');
    $('#' + fieldName ).attr('class', 'errInput');
    return false;
  }
  else if(!emailReg.test(emailaddressVal))
  {
    $('#' + fieldName + 'Txt' ).attr('class', 'errfieldTxt');
    $('#' + fieldName ).attr('class', 'errInput');
    return false;
  }
  else
  {
    $('#' + fieldName + 'Txt' ).attr('class', 'fieldTxt');
    $('#' + fieldName ).attr('class', 'txtBx');
    return true;
  }
}

function checkRadio (frmName, rbGroupName)
{
  var radios = document[frmName].elements[rbGroupName];
  for (var i=0; i <radios.length; i++)
  {
    if (radios[i].checked)
    {
      $('#' + rbGroupName + 'Txt' ).attr('class', 'fieldTxt');
      return true;
    }
  }
  $('#' + rbGroupName + 'Txt' ).attr('class', 'errfieldTxt');
  return false;
} 

function totFormValidate()
{
  formValidate( 'name' );
  phoneValidate( 'phone' );
  emailValidate( 'email' );
  formValidate( 'message' );
  formValidate( 'securitycode' );
  checkRadio( 'jobapplyForm', 'areaInterest' );
  if ( ( !formValidate( 'name' ) ) || ( !phoneValidate( 'phone' ) ) || ( !emailValidate( 'email' ) ) || ( !formValidate( 'message' ) ) || ( !formValidate( 'securitycode' ) ) || ( !checkRadio( 'jobapplyForm', 'areaInterest' ) ) )
    return false;
  else
  {
    $.post('/contacts.php?' + $("#jobapplyForm").serialize(), function (response) {  if ( response == 1 ) { $('#formDiv').slideUp("slow", function () { $('#thanksDiv').fadeIn("slow") }); } else { $('#securitycodeTxt' ).attr('class', 'errfieldTxt'); $('#securitycode').attr('class', 'errInput'); $('#securitycode').focus(); return false; } });
    return false;
  }
}
function validateOnlineForm()
{
  formValidate( 'contact_store_990' );
  formValidate( 'contact_name_990' );
  formValidate( 'aoitwo_division' );
  emailValidate( 'contact_email' );
  phoneValidate('contact_phone_990');
  formValidate( 'ugm_numattn_990' );
  checkRadio( 'onlineRegistration', 'payment_method' );
  if ( $('#ugmAttendeeCounter').val() == 0 )
  {
    if ( ( !formValidate( 'contact_store_990' ) ) || ( !formValidate( 'contact_name_990' ) ) || ( !formValidate( 'aoitwo_division' ) ) || ( !emailValidate( 'contact_email' ) ) || ( !phoneValidate('contact_phone_990') ) || ( !checkRadio( 'onlineRegistration', 'payment_method' ) ) )
      return false;
    else
    {
      $.post('/register.php?' + $("#onlineRegistration").serialize(), function () { $('#formDiv').slideUp("slow", function () { $('#thanksDiv').fadeIn("slow") }); });
      return false;
    }
  }
  else
  {
    chkAttendee();
    if ( ( !formValidate( 'contact_store_990' ) ) || ( !formValidate( 'contact_name_990' ) ) || ( !chkAttendee() ) || ( !formValidate( 'aoitwo_division' ) ) || ( !emailValidate( 'contact_email' ) ) || ( !phoneValidate('contact_phone_990') ) || ( !checkRadio( 'onlineRegistration', 'payment_method' ) ) )
    {
      return false;
    }
    else
    {
      $.post('/register.php?' + $("#onlineRegistration").serialize(), function () { $('#formDiv').slideUp("slow", function () { $('#thanksDiv').fadeIn("slow") }); });
      return false;
    }
  }
}
function change_captcha()
{
  document.getElementById('captcha_img').src="/captcha/image.php?" + Math.random();
}
//contact us script ends

</script> 
