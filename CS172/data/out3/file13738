  <script type="text/javascript">
$(document).ready( function () { startupFn(); });
function startupFn()
{
  $('#slideImg1').css({'opacity':'1', 'z-index':'5'});
  $('#slideTxt1').css({'opacity':'1', 'z-index':'7'});
  for ( i = 2; i <= 5; i++ )
  {
    $('#slideImg' + i).css({'opacity':'0', 'z-index':'1'});
    $('#slideTxt' + i).css({'opacity':'0', 'z-index':'1'});
  }
  $('#prevImgId').val('1');
  var timerSet = setTimeout("rotateImg('2')", 2000);
}
function rotateImg ( imgId )
{
  var prevId = $('#prevImgId').val();
  nxtImgId = parseInt ( imgId );
  if ( prevId != imgId )
  {
    $('#slideImg' + imgId).css('opacity', '1').css('z-index', '1');
    $('#slideImg' + prevId).css('z-index', '5');
    $('#slideImg' + prevId).animate({opacity: 0}, 500, function() { $('#slideImg' + prevId).css('z-index', '1'); $('#slideImg' + imgId).css('z-index', '5'); });
  
    $('#slideTxt' + imgId).css({'opacity':'0.8', 'z-index':'1'});
    $('#slideTxt' + prevId).css('z-index', '7');
    $('#slideTxt' + prevId).animate({ "opacity": 0}, 500, function() { $('#slideTxt' + prevId).css('z-index', '1'); $('#slideTxt' + imgId).css('z-index', '7'); });
  
    $('#slideBtn' + prevId).attr('class', '');
    $('#slideBtn' + imgId).attr('class', 'active');
  }
  $('#prevImgId').val( imgId );
  if ( imgId == '5' )
    nxtImgId = 1;
  else
    nxtImgId++;
  timerSet = setTimeout( "rotateImg( nxtImgId )", 9000);
}
function selImgChg( chgId )
{
  clearTimeout(timerSet);
  rotateImg ( chgId );
}
</script> 
  <script>

ddmegamenu.docinit({
