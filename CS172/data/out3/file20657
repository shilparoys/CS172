            <script type="text/javascript">
    if (typeof XMLHttpRequest == "undefined"){
      XMLHttpRequest = function () {
        try { return new ActiveXObject("Msxml2.XMLHTTP.6.0"); }
          catch (e) {}
        try { return new ActiveXObject("Msxml2.XMLHTTP.3.0"); }
          catch (e) {}
        try { return new ActiveXObject("Msxml2.XMLHTTP"); }
          catch (e) {}
        //Microsoft.XMLHTTP points to Msxml2.XMLHTTP.3.0 and is redundant
        //throw new Error("This browser does not support XMLHttpRequest.");
      };
    }
    
    function validateInstitutionalUseForm(){
        var valid = true;
        if(valid) valid = validateGenericText("iu_company", "School or Institution");
        if(valid) valid = validateSelectInput("iu_industry", "Organization Type", "");
        if(valid) valid = validateGenericText("iu_state", "State/Province");
        if(valid) valid = validateGenericText("iu_first_name", "First Name");
        if(valid) valid = validateGenericText("iu_last_name", "Last Name");
        if(valid) valid = validateGenericText("iu_title", "Title");
        if(valid) valid = validateEmail("iu_email", "Email");
        if( !valid ) return;
        submitInstitutionalUseForm();
    }

    function submitInstitutionalUseForm(){
        try{
            var url = "https://www.salesforce.com/servlet/servlet.WebToLead?encoding=UTF-8";
            var content = getIuFormContent();
            
            if(content != null){
                //alert(url+"\n\n"+content);
                var iuFormRequest = new XMLHttpRequest();
                iuFormRequest.open("POST", url, true); //true for asynchronicity
                iuFormRequest.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
                //iuFormRequest.onreadystatechange not set because CORS (cross-origin resource sharing) prevents access to response.
                iuFormRequest.send(content);
            }
            alert("Thank you. Your request has been submitted.");
        }
        catch(e){
            alert(e);
            alert("An error has occured. Your request could not be sent. Please try again later.");
        }
    }
    function getIuFormContent(){
        //Spam prevention:
        var description = document.getElementById("iu_description").value;
        if(description.indexOf("http://") >= 0)
            return null;
        
        var content = "oid="+encodeURIComponent("00D300000000SG5");
        if(document.getElementById("iu_debug")) content += "&debug="+encodeURIComponent(document.getElementById("iu_debug").value);
        if(document.getElementById("iu_debugEmail")) content += "&debugEmail="+encodeURIComponent(document.getElementById("iu_debugEmail").value);
        content += "&lead_source="+encodeURIComponent("HippoCampus LeadTab");
        content += "&company="+encodeURIComponent(document.getElementById("iu_company").value);
        content += "&industry="+encodeURIComponent(document.getElementById("iu_industry").options[document.getElementById("iu_industry").selectedIndex].value);
        content += "&state="+encodeURIComponent(document.getElementById("iu_state").value);
        content += "&first_name="+encodeURIComponent(document.getElementById("iu_first_name").value);
        content += "&last_name="+encodeURIComponent(document.getElementById("iu_last_name").value);
        content += "&title="+encodeURIComponent(document.getElementById("iu_title").value);
        content += "&email="+encodeURIComponent(document.getElementById("iu_email").value);
        content += "&00N30000002838t="+encodeURIComponent(document.getElementById("00N30000002838t").value);
        content += "&description="+encodeURIComponent(description);
        return content;
    }
</script> 
