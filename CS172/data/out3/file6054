            <script>
$(function() {
	$('#j-carousel1').presto_Carousel(700, 400, {
		auto: true,
		animation: 1, 
		interval: 5000, 
		animationSpeed: 400,
		control: '.control'
	});
		
		
  $('#j-carousel1 .player-wrapper').hover(function() {
  	$(this).children('.close-player').show();
  }, function() {
  	$(this).children('.close-player').hide();
  });
	
$('#j-carousel1 .close-player').click(function(e) {
  e.preventDefault();
  jwplayer('j-carousel1-player').stop();
  $('#j-carousel1 .player-wrapper').hide();
  $('.player-wrapper > div', '#j-carousel1').remove();
  $('.player-wrapper', '#j-carousel1').prepend('<div id="j-carousel1-player" />');
  $('#j-carousel1').getPRESTO_Carousel().play();
});
		
$('#j-carousel1 .play-video').click(function(e) {
	jwplayer('j-carousel1-player').setup({
	        flashplayer: "/info/jwplayer.flash.swf",
	        html5player: "/info/jwplayer.html5.js",
  		width: $('#j-carousel1').outerWidth(),
  		height: $('#j-carousel1').outerHeight(),
		file: $(this).attr('href'),
		type: $(this).data('provider'),
		autostart: true
  	});
  	e.preventDefault();
  	$('#j-carousel1').getPRESTO_Carousel().stop();
	$('#j-carousel1 .player-wrapper').show();
}).hover(function() {
	$(this).css({'background-position': '0 -42px'});
}, function() {
	$(this).css({'background-position': '0 0'});
});
		

});
</script> 
            <link rel="alternate" type="application/rss+xml" title="RSS" href="/video/index?feed=rss_2.0"> 
