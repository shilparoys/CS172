  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script> 
  <script type="text/javascript" src="http://www.drew.edu/wp-content/themes/Drew-v8-r/scripts/hammer.min.js"></script> 
  <script type="text/javascript" src="http://www.drew.edu/wp-content/themes/Drew-v8-r/scripts/scripts.min.js?ver=1418681484"></script> 
  <script type="text/javascript" src="http://www.drew.edu/humanrights/wp-includes/js/jquery/ui/core.min.js?ver=1.11.2"></script> 
  <script type="text/javascript" src="http://www.drew.edu/humanrights/wp-includes/js/jquery/ui/widget.min.js?ver=1.11.2"></script> 
  <script type="text/javascript" src="http://www.drew.edu/humanrights/wp-includes/js/jquery/ui/tabs.min.js?ver=1.11.2"></script> 
  <script>
				$(function(){
					$("#tabs1340140516").tabs({
						create: function( event, ui ) {
							window.setTimeout(checkmenu, 100);
						}
					});

					if (window.location.hash && $(window.location.hash).length){
						$("#tabs1340140516").tabs("select", window.location.hash);
						window.scrollTo(0, $("#tabs1340140516").offset().top );
					}

					//make the tab into a sliders
					if(jQuery().touchslider) $("#tabs1340140516>ul:first").touchslider();
					if(jQuery().hammer) {
						$("#tabs1340140516>div").hammer({
								drag_vertical: false
								,css_hacks:false
								,hold:false
								,drag:false
								,transform:false
								,tap_double:false
								,tap:false
							}).bind("swipe", function(ev){
								ev.stopPropagation();
								var active = $("#tabs1340140516").tabs( "option", "active" );
								var total = $("#tabs1340140516 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs1340140516").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs1340140516").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script>   
 </body>
</html>