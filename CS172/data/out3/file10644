  //<![CDATA[
    olark('api.chat.onCommandFromOperator', function(event) {
        if (event.command.name == 'oops') {
          olark('api.chat.sendNotificationToOperator', {body: 'autotype on!'});
          var autoMessages = [
            "I work for Olark, the company that makes this chat program",
            "perhaps you arrived here by mistake?",
            "if you close this browser tab, you'll probably be back where you need to be!"
            ];
          delay = 0;
          while (autoMessages.length) {
              (function(msg){
                delay = delay + msg.length*130;
                setTimeout(function(){
                  olark('api.chat.sendMessageToVisitor', {body: msg});
                  olark('api.chat.sendNotificationToOperator', {body: msg});
                }, delay);
              })(autoMessages.shift())
          }
        }
    });
    olark.configure('system.allow_mobile_boot', true);
    
    
    olark('api.chat.onBeginConversation', function() {
      analytics.track("Visitor Chatted with Olark", { site: "web" })
    });
    
