  <!-- /automatically set weekend message --> 
  <script type="text/javascript">
  //<![CDATA[
    (function(){
        var monday = 1;
        var friday = 5;
        var saturday = 6;
        var sunday = 0;
        var endOfDay = 17;
        var beginningOfDay = 9;
        var offsetToSanFrancisco = 8;
        var offsetToScotland = 0;
        var now = moment().utc();
    
        if ( now.isDST() )
            offsetToSanFrancisco -= 1;
            offsetToScotland += 1;
        var isWeekendDay = now.weekday() == saturday || now.weekday() == sunday;
        var isFridayAfter5 = now.weekday() == friday && ( now.hours() - offsetToSanFrancisco ) >= endOfDay;
        var isMondayBefore9 = now.weekday() == monday && ( now.hours() + offsetToScotland ) <= beginningOfDay;
        var isDuringBusinessHours = !isWeekendDay && (now.hours() + offsetToScotland) >= beginningOfDay && (now.hours() - offsetToSanFrancisco) <= endOfDay;
    
        olark.configure( 'system.not_available_text', 'After Hours Help');
        olark.configure( 'system.offline_msg_mode', false );
    
    
        if ( isWeekendDay || isFridayAfter5 || isMondayBefore9 ) { //weekend mode
            olark.configure( 'system.offline_message', "Search our <a href='https://www.olark.com/help'>Help Center</a> for immediate answers. You can Tweet or email from the Help Center and we'll get back to you Monday. <br/>Our live chat hours are Monday through Friday 7am-5pm Pacific." );
        }
        else{ //during the week
            if ( isDuringBusinessHours ) {
                olark.configure('system.offline_message', "Need help? Have a question? Search our <a href='http://olark.com/help'>Help Center</a> for immediate answers. You can Tweet or email from the Help Center and we'll get back to you as soon as we can.")
            }
            else {
                olark.configure('system.offline_message', "Need help? Have a question? Search our <a href='http://olark.com/help'>Help Center</a> for immediate answers. You can Tweet or email from the Help Center and we'll get back to you as soon as we can. <br/> Our live chat hours are Monday through Friday 7am-5pm Pacific.")
            }
        }
    })();
  //]]>
</script> 
