	/* search pagination, moved from profileHead.mtml */
	var offset = 0;
	var sLimit = 20;
	var totalResults = 371;
	var thisPage = 1;

	function galleryDirection(nav) {
		var newOffset;
		if ((nav == 'next') && ((offset + sLimit) < totalResults)) {
			newOffset = offset + sLimit;
			document.write('<li class="next last"><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=' + newOffset + '">Next page &raquo;</a></li>');
		}
		if ((nav == 'previous') && ((offset - sLimit) >= 0)) {
			newOffset = offset - sLimit;
			document.write('<li class="first prev"><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=' + newOffset + '">&laquo; Previous page</a></li>');
		}
	}

	function galleryPagination() {
		if (totalResults > sLimit) {
			document.write('<div class="content-pagination"><ul>');
			galleryDirection('previous');
			document.write('<li class="first">1<li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=20">2</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=40">3</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=60">4</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=80">5</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=100">6</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=120">7</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=140">8</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=160">9</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=180">10</a><li><a href="/cgi-bin/mte/mt-search.cgi?template_id=profile_text_entries&search=&author=njoauditor&affiliate=njo&index=entry&sfilter_blog_type=text&sfilter_affiliate=njo&blog_type=text&limit=20&searchall=1&offset=200">11</a>');
			galleryDirection('next');
			document.write('</ul></div>');
		}
	}

	function tagSearchPagination(ssfUrl) {
		if (totalResults > sLimit) {
			document.write('<div class="content-pagination"><ul>');
			if ((offset - sLimit) >= 0) {
				var prevStr = "";
				if((thisPage) > 2) { prevStr = "-" + (thisPage - 1); }
				document.write('<li class="first prev"><a href="' + ssfUrl + '/tag/searchall/' + prevStr + '.html">&laquo; Previous page</a></li>');
			}
			document.write('<li class="first">1</li><li><a href="' + ssfUrl + '/tag/searchall/-2.html">2</a></li><li><a href="' + ssfUrl + '/tag/searchall/-3.html">3</a></li><li><a href="' + ssfUrl + '/tag/searchall/-4.html">4</a></li><li><a href="' + ssfUrl + '/tag/searchall/-5.html">5</a></li><li><a href="' + ssfUrl + '/tag/searchall/-6.html">6</a></li><li><a href="' + ssfUrl + '/tag/searchall/-7.html">7</a></li><li><a href="' + ssfUrl + '/tag/searchall/-8.html">8</a></li><li><a href="' + ssfUrl + '/tag/searchall/-9.html">9</a></li><li><a href="' + ssfUrl + '/tag/searchall/-10.html">10</a></li><li><a href="' + ssfUrl + '/tag/searchall/-11.html">11</a></li>');
			if ((offset + sLimit) < totalResults) {
				document.write('<li class="next last"><a href="' + ssfUrl + '/tag/searchall/-' + (thisPage + 1) + '.html">Next page &raquo;</a></li>');
			}
			document.write('</ul></div>');
		}
	}


