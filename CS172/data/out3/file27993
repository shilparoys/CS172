  <script type="text/javascript" language="javascript">
			markets_data = function() {
				this.cBuffer = [];
			}
			
			// update all charts
			markets_data.prototype.update = function(data,target) {
				
				common.showLoading(target, 0);
				
				if (!this.cBuffer[target]) {
					this.cBuffer[target] = new ContentBuffer();
				}
				
				this.cBuffer[target].load({
					url: "buffer.asp",
					data: data,
					contentType: "text/javascript",
					onload: function(buffer){
						results = buffer.getResult();
						if (results) {
							Element.get(target).innerHTML = results;
						}
						common.hideLoading(target);
					},
					onerror: this.Error,
					method: "get",
					context: this
				});
			}
			
			var markets = new markets_data();
		</script> 
