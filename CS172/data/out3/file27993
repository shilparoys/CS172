  <script type="text/javascript" language="javascript">
	// wrappers for buffer requests
	function updateWSOD(who,target) {
		var str = String(who.id);
		var sym = str;
		var dur = "";
		if (str && str.indexOf("_") > -1) {
			str = str.split('_');
			var sym = str[0];
			var dur = str[1];
		}
		
		if (target == "UsIndexes") {
			var data = {UsIndexes_Index: sym, UsIndexes_Duration: dur, target:target};
		} else if (target == "GovtBonds") {
			var data = {GovtBonds_Index: sym, GovtBonds_Duration: dur, target:target};
		} else if (target == "CommoditiesSection") {
			var data = {Commodities_Index: sym, Commodities_Duration: dur, target:target};
		} else if (target == "ConsumerRates") {
			var data = {ConsumerRates_Index: sym, ConsumerRates_Duration: dur, target:target};
		} else if (target == "WorldMarkets") {
			var data = {worldMarket:str, target:target};
		} else if (target == "KeyCurrencies") {
			var data = {currencyView:str, target:target};
		}
		markets.update(data,target);
	}
	
	</script> 
