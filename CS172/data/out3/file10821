      <script src="/scripts/qtip2.0/jquery.qtip.min.js" type="text/javascript"></script> 
      <script type="text/javascript">
jQuery(document).ready(function() 
{
   // Use the each() method to gain access to each elements attributes
   jQuery('.popup_link').each(function()
   {
      jQuery(this).qtip(
      {
			content: {
				text: jQuery(jQuery(this).attr('rel')),
				title: {
					text: jQuery(this).attr('title'),
					button: "X"
				}
			},
			position: {
				adjust: {
					method: "flip flip"
				},
				at: "center",
				my: "bottom left",
				viewport: jQuery(window)
			},
			show: {
				solo: true,
				event: "mouseover"
			},
			hide: {
				fixed: true,
				event: "mouseout"
			},
			style: {
				tip: true,
				width: 200,
				classes: "ui-tooltip-plain ui-tooltip-rounded ui-tooltip-shadow"
			}
		})
   });
});
</script> 
