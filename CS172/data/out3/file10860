   <script type="text/javascript">
$(document).ready(function(){
	$('.min-brief').click(function () {
		if ($(this).hasClass("min-brief-show")) {
			$(this).parent().next().show();
			$(this).removeClass("min-brief-show");
            $(this).addClass("min-brief-hide");
            $(this).find('.plus').html('-'); 
            $(this).css('border-bottom','1px solid #FFFFFF');
		}
		else {
			$(this).parent().next().hide();
			$(this).removeClass("min-brief-hide");
            $(this).addClass("min-brief-show");
            $(this).find('.plus').html('+'); 
            $(this).css('border-bottom','1px solid #ccc');	
		}
	});
jQuery('a').click(function() {
	var id = $(this).parent().parent().attr('class');
	if (id == 'dis-table-cont') { 
	    jQuery('html, body').not('a.teach-tabs').animate({
	        scrollTop: jQuery('[name="' + jQuery(this).attr('href').substr(1) + '"]').offset().top
	    }, 1000);
	    return false;
	}
});	
});

var fixmeTop = $('.scroll-top').offset().top;
$(window).scroll(function() {
	var currentScroll = $(window).scrollTop();
		if (currentScroll >= fixmeTop) {
	        $('.ranking-vertical-social-icon .top-social-links ul').css({
	            'position': 'fixed'
	        });
	         
	        $('.ranking-vertical-social-icon').css({
	            'margin-left': '70px',
	            'margin-top' : '-158px' 
	        });
	        
		} else {
		    $('.ranking-vertical-social-icon .top-social-links ul').css({
		        'position': 'relative'
		    });
		    
		    $('.ranking-vertical-social-icon').css({
	            'margin-left': '70px'
	        });
		    
		}
});

$(document).ready(function() {
	//For the font rander issue in chrome browser
	$.browser={};(function(){$.browser.msie=false;
	$.browser.version=0;if(navigator.userAgent.match(/MSIE ([0-9]+)\./)){
	$.browser.msie=true;$.browser.version=RegExp.$1;}})();
	$.browser.chrome = /chrom(e|ium)/.test(navigator.userAgent.toLowerCase()); 
	var userAgent = navigator.userAgent.toLowerCase();
	userAgent = userAgent.substring(userAgent.indexOf('chrome/') +7);
	version = userAgent.substring(0,userAgent.indexOf('.'));
	
	if($.browser.chrome && version == '33'){
		$("body").hide();
		setTimeout(function(){
			$("body").show();
		}, 2000);
	}
});

</script> 
