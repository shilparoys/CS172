  function verifySID () {
    var sid=document.reglogin.sid.value;
    var pin=document.reglogin.pin.value;
    if (sid.length!=9){
      alert("A nine digit SID is required.");
      return false
    }else{
      if (pin.length<4){
         alert("You must enter a 4 to 6 digit PIN.");
