       <div class="tab-content "> 
        <div id="tabs-b0710947-1-tabPane-1" class="tab-pane active"> 
         <div class="row "> 
          <div class="col-sm-7 border-right" id="story-content"> 
           <div class="ndn_embed" id="ndn-video-player-1" data-config-distributor-id="90482" data-config-width="100%" data-config-height="9/16w"></div> 
           <div id="story-target"> 
            <script type="text/javascript">
$(".dateline").hide();
if($.trim($(".dateline").html())!='') {
$( ".dateline" ).show();
}
