                  <div class="panel-pane pane-custom pane-1"> 
                   <h2 class="pane-title">Study at Bastyr</h2> 
                   <div class="pane-content"> 
                    <div class="study-location"> 
                     <input type="radio" name="studyloc" id="studywa" checked="yes" onclick="setStudyLoc('wa');">
                     <label for="studywa">Washington</label> 
                     <input type="radio" name="studyloc" id="studyca" onclick="setStudyLoc('ca');">
                     <label for="studyca">California</label> 
                    </div> 
                    <script type="text/javascript">
$(document).ready(function() {
   var studyWA = $('#studywa');
   var studyCA = $('#studyca');
   if (studyWA.attr('checked')) 
      setStudyLoc('wa');
   else 
      setStudyLoc('ca');
});

function setStudyLoc(loc) {
   $('.study-location label').css('color', '#fff');
   $('.study-location label[for="study' + loc + '"]').css('color', '#faeed8');
   if (loc == 'ca') {
      $('.pane-nice-menus-1').hide();
      $('.pane-nice-menus-2').fadeIn('slow');
   } else {
      $('.pane-nice-menus-2').hide();
      $('.pane-nice-menus-1').fadeIn('slow');
   }
}
</script> 
                   </div> 
                  </div> 
