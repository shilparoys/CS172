  <script type="text/javascript">
/*<![CDATA[*/
$(function(){
	$(".apptype").hide();//.css({"position":"absolute"});
	$("a[href^='#']").each(function(){
		$(this).unbind().click(function(e){
			e.preventDefault();
			x = $(".clusters h4 a[href='"+$(this).attr('href')+"']");
			$(".clusters h4 a").removeClass("current");x.addClass("current");
			$($(this).attr("href")).css({"margin-top": "00px"}).slideDown(500).siblings("div").slideUp(300);
			$(".clusters img").animate({"top": (x.position().top-40)+"px"},500);
		});
	});
	$(".current").click();
	
		
	var isSmallLocal = false;	
	
	//Check window size when window is resized
	$(window).resize(function(){
		checkWindowSizeLocal(cutpoint);
	});
	
	function checkWindowSizeLocal(c){
		//Check the max size of several width attributes to account for scrollbar differences among browsers
		//IE<9 returns 'undefined.' Responsive layout chnages are not triggered.
		var w = window.innerWidth	
		if ($.browser.webkit) {w = document.documentElement.clientWidth;}
		
		if (isSmallLocal == false && w<=c) { // window.matchMedia('(max-width: 480px)').matches) { //
			//Page is using 'standard' view; switch to 'mobile' view
			$('.apptype').show()
			$('h2').removeClass('expandedHeading').addClass('collapsedHeading')
			$('.hChild').hide();
			isSmallLocal = true;
		} else if (isSmallLocal == true && w > c) { //window.matchMedia('(min-width: 481px)').matches) {// 
			//Page is using 'mobile' view; switch to 'standard' view
			$('.apptype').hide();
			$('#app').show();
			isSmallLocal = false;
		}
	}
	checkWindowSizeLocal(cutpoint);
});
/*]]>*/
</script> 
