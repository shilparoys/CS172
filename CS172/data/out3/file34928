                <p><script language="JavaScript"><!-- hide scripts from crusty old browsers

function goTo (page) {
/* This function is called from the navigation menu
   to jump to the designated URL. Empty values
   are ignored and "--" indicates a menu seperator    */
   
    if (page != "" ) {
        if (page == "--" ) {
            resetMenu();
        } else {
            document.location.href = page;
        }
    }
    return false;
}

function resetMenu () {
/* Resets the menu to the specified menu item           */   
    document.jumpmenu.page.options[0].selected = true;
}

--><![CDATA[
]]><![CDATA[
]]><!--//--><![CDATA[// ><!--


//--><!]]><![CDATA[
]]></script></p>
