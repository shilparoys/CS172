function PerformSearch(srchBtn){var btn = $(srchBtn);var term = encodeURIComponent($.trim($('#'+btn.attr('id').replace('btnSearch', 'txtSearch')).val()));if (term && term.length > 0){if (term && term.length > 140){alert('Search character limit has been reached. Results cannot be displayed until you reduce your entry.');return false;}else{var isauto = 'false';if($('#'+btn.attr('id').replace('btnSearch', 'hidIsAutocomplete')).val() == '1'){isauto = 'true';}var searchurl =  'http://careertraining.ed2go.com/greenriver/search-results?term=---&ac=xxx'.replace('---', term).replace('xxx', isauto);window.location.href = searchurl;}}return false;}//]]>
</script> 
