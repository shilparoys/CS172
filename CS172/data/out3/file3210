  <script type="text/javascript">
	onPageLoadCookieInfo();
	
					showLimitWarning('Some of the databases being searched do not support limiting to holding libraries.  Items relevant to your search could be skipped, even if your library owns them.  For the most comprehensive results, choose &quot;Libraries Worldwide&quot;.', false, true, false);
				

	function onPageLoadCookieInfo()
	{
		checkOrClearAll(false);
		var cookie = getCookie("LAC-User-Databases");
		if (cookie == null)
		{
			//use defaults...
			cookie = "283";
		}
		var indivCookie = cookie.split(",");
		 var displayDb = "<ul>";
        var shortDesc;
		for (var i = 0; i < indivCookie.length; i++)
		{
			if (!databaseArrayObj[indivCookie[i]].groupId)
			{
                shortDesc = "undefined"; 
                if(databaseArrayObj[indivCookie[i]].description)
                    shortDesc = databaseArrayObj[indivCookie[i]].description.replace(/'/g,"\\'");
				displayDb += "<li><span class=\"searchableData\" onmouseover=\"tooltip('" + shortDesc + "');\" onmouseout=\"clearTimeout(myTimer); exit();\">" + databaseArrayObj[indivCookie[i]].displayName + "</span></li>";
				databaseArrayObj[indivCookie[i]].selected = true;
				checkDuplicateDatabases(true, databaseArrayObj[indivCookie[i]].targetId.toString());
			}
		}
		displayDb += "</ul>";
		jQuery("#searchedDatabases").html(displayDb);
setAdvSearchDatabasesInput(cookie);
    refreshRightColumn();
		shouldGroupBeChecked();
    isSaveAvailable();
	}

	function libraryDefaults()
	{
		setCookie("LAC-User-Databases", "283", "/", "");
		onPageLoadCookieInfo();
	}
</script>   
