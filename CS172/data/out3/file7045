  <script type="text/javascript">
	function infocus(value)
	{
		if(value =="Search this site...")
		{
			document.getElementById('s').value="";
		}
		else if (value =="")
		{
			document.getElementById('s').value="Search this site...";
		}
	}
	
	function notFocus(value)
	{
		if(value =="")
		{
			document.getElementById('s').value="Search this site...";
		}
	}
	
	function searchvalue()
	{
		document.getElementById('s').value="Search this site...";
	}
</script> 
