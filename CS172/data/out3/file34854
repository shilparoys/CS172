        regex = /.*e=(\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,3}).*$/; 

        var emFromUrl = url.match(regex); 
        if (emFromUrl && emFromUrl.length > 1)
        {
         rtaEmail = emFromUrl[1];   
        }

    }        
    switch (ig_pageType) {
     case "Home":
         // nothing special to do, rtaRetialre is set up and that is all that is required
         break;
