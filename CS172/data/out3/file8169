$(document).ready(function() {
    var h1 = $("#div1").height();
    var h2 = $("#div2").height();
    $("#div1,#div2").height(Math.max(h1, h2));
    $("#palumbo-info").hide();
    $("#werner-info").hide();
    $("#glaze-info").hide();
    $("#poland-info").hide();
    $("#johnson-info").hide();
    $("#sharkey-info").hide();
    $("#wiltsey-info").hide();
    $("#weiserth-info").hide();
    $("#schoener-info").hide();
    $("#greer-info").hide();
    $("#cilona-info").hide();
    $("#gant-info").hide();
    $("#lafferty-info").hide();
    $("#dilks-info").hide();
    $("#gordon-info").hide();
    $("#gurenlian-info").hide();
    $("#komito-info").hide();
    $("#defiore-info").hide();

});


$("#palumbo").live('click', function(event) {
    event.preventDefault();
    $("#palumbo-info").show();
    $("#werner-info").hide();
    $("#glaze-info").hide();
    $("#poland-info").hide();
    $("#johnson-info").hide();
    $("#sharkey-info").hide();
    $("#wiltsey-info").hide();
    $("#weiserth-info").hide();
    $("#schoener-info").hide();
    $("#greer-info").hide();
