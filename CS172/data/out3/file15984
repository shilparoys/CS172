$(document).ready(function(){
	
	$("#qcklnk").click(function(){
		
		$( "#subnavfooter" ).animate({
			  "height": "toggle"
			}, { duration: "slow" });
		
	});	
	$("#subnavfooterclose").click(function(){
		
		$( "#subnavfooter" ).animate({
			  "height": "toggle"
			}, { duration: "slow" });
		
	});
});
</script> 
