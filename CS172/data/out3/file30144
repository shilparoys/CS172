            $('.carousel-slidesjs').each(function () {
                var $img = $(this).find('img').first(),
                        height = $img.attr('height'),
                        width = $img.attr('width');

                $(this).slidesjs({
                    height: '270',
                    width: '300',
                    play: {
                        auto: true
                    }
                });
            });
        });
