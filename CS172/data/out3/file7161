  <script type="text/javascript">
		Cufon.now();
		
		$(document).ready(function(){
			/** Ebulten kayıt **/
			$("#ebulten_modal .btn-primary").click(function(e){
				e.preventDefault();
				$("#ebulten_modal .alert").each(function(){
					if(!$(this).hasClass("hide")) $(this).addClass("hide");
				});
				
				var empty = 0;
				$("#ebulten_modal [type='text']").each(function(){
		    		if($(this).val()=="") empty = 1;
				});
				if(empty==1) $("#ebulten_modal #alert_fields").removeClass("hide");
				else{
					if($("#ebulten_modal :checked").length==0) $("#ebulten_modal #alert_checkbox").removeClass("hide");
					else{
						var addr = "http://www.ieu.edu.tr/anasayfa/ajax/ebulten_save.php";
			    		var params = "?section=yonetim.ieu.edu.tr&lang=tr&type=post_control";
			    		
			    		$("#ebulten_modal [type='text']").each(function(){
			    			params += "&" + $(this).attr("name") + "=" + $(this).val();
			    		});
			    		
			    		var bulten = "&bulten=";
			    		$("#ebulten_modal :checked").each(function(){
			    			bulten += $(this).val() + ",";
			    		});
			    		params += bulten.substring(0, (bulten.length-1));
			    		params += "&ip=104.52.231.160";
			    		params += "&captcha=" + $("[name='captcha']").val();
			    		
			    		$.ajax({
			    			url: addr+params,
			    			async: false,
			    			success: function(data){
			    				if(data==1){
			    					$("#ebulten_modal #alert_ok").removeClass("hide");
			    					$("#ebulten_modal form").html("");
			    					$("#ebulten_modal .modal-footer").html("");
			    				}
			    				else $("#ebulten_modal #alert_captcha").removeClass("hide");
			    			}
			    		});
		    		}
	    		}
			});
			
/**			if("tr"=="tr"){
				$("#gallery a").trigger("click");
				$("#tercih-modal .modal-image").bind("click", function(e){
					e.preventDefault();
					window.location = "http://www.ieu.edu.tr/tercih";
				});
			}
**/			
		});
	</script> 
