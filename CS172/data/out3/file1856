	function focusInput(div1, div2, div3, numberOfDiv){
		if(numberOfDiv==2){
			P7_equalCols2(0,div1,'P',div2,'P');
		}
		else if(numberOfDiv==3){
			P7_equalCols2(0,div1,'P',div2,'P',div3,'P');		
		}		
		if (document.getElementById("focusON")!=null){
			document.getElementById('focusON').focus(); 
		}
		window.setTimeout("sel_sec()", 5000); // 2000 for 2 min 
		//if(document.getElementById("errMessage")!=null){  
		//	document.getElementById("errMessage").focus(); 		//}
		}
	function sel_sec() {
			if (document.getElementById("errorBlock")!=null){
					var errStyle = 	document.getElementById("errorBlock").style;
					if(errStyle.display!='none'){
						if (document.getElementById("errMessage")!=null){
							document.getElementById("errMessage").focus();
						}
					}
			}
	}	
	document.createElement('header');
	document.createElement('footer');
	document.createElement('section');
	document.createElement('article');
