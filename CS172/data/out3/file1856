       <script type="text/javascript">
function googleSearchSubmit(){
	var searchString = document.searchForm.search.value;
	var anchorEle = document.getElementById('googleSearchHeader');
	var currentHref = $(anchorEle).attr('href');
	var updatedHref = currentHref + '&search=' + searchString;
	$(anchorEle).attr('href',updatedHref);
	window.location.href = $(anchorEle).attr('href');
}
</script> 
