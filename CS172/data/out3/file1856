  <script type="text/javascript">
  var userType="guest-user";
  var currentEnv = $('input#miniShopCartEnvironmentName');
  var currentStorePrefix = $('input#miniShopCartStorePrefix');
  var cookieName = 'BNCB_'+$(currentEnv).val()+'_'+$(currentStorePrefix).val()+'_userdetails';
  var cookieVal = $.cookie(cookieName);
  var lgSrcCookieName = 'BNCB_'+$(currentEnv).val()+'_'+$(currentStorePrefix).val()+'_loginsource';
  var loginSource = "WCS";
  var lgSrcCookieVal = $.cookie(lgSrcCookieName);
  if (typeof cookieVal != 'undefined' && cookieVal != null){
  userType="registered-user";
  }
  if (typeof lgSrcCookieVal != 'undefined' && lgSrcCookieVal != null){
  loginSource = lgSrcCookieVal.trim();
  }
  
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-1523307-1']);
  _gaq.push(['_setDomainName', 'bncollege.com']);
  _gaq.push(['_set', 'title', 'Homepage']);        
  _gaq.push(['_setCustomVar',1,'store','522',3]);               
  _gaq.push(['_setCustomVar',3,'user-type',userType,2]);    	
  _gaq.push(['_setCustomVar',4,'pageCategory','Home',3]);  
  _gaq.push(['_setVar', 'store_'+'522']);
  _gaq.push(['_setSessionCookieTimeout', 43200000]);
  if(userType == "registered-user"){
   _gaq.push(['_setCustomVar',5,'loginSource',loginSource,2]);
  }
  
  
  _gaq.push(['_trackPageview']);
//function to be called in case of modal pop up show for google analytics

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script> 
  <!-- BEGIN BNCB_GoogleAdSpotDefination.jsp --> 
  <script type="text/javascript">
	googletag.cmd.push(function() {
		
		var currentEnv = $('input#miniShopCartEnvironmentName');
		var currentStorePrefix = $('input#miniShopCartStorePrefix');
		var bncbStoreId = $('input#bncbStoreId').val();
		
		var cookieName = 'BNCB_'+$(currentEnv).val()+'_'+$(currentStorePrefix).val()+'_userdetails';
		var cookieVal = $.cookie(cookieName);
		
		if(typeof cookieVal != 'undefined' && cookieVal != null){
			var userDetail = cookieVal.split(':');
			var usrType = userDetail[2];
			var userGradYear = userDetail[3];
			var userGender = userDetail[4];	
		}		
		//Adslot 1 declaration
		var count = 0;
		var googleAdNetworkID = '/' + '17936108' + '/';
		var googleAdSite = 'bnc' + '/';
		var adName,adWidth,adHeight,divId,location,position;			
		
			count++;
			adName = googleAdNetworkID + googleAdSite + 'homepage';
			adWidth = 300;
			adHeight = 250;
			divId = 'div-id-AD_SPOT_HOMEPAGE_STUDENT_OFFERS_AD_1';
			if(divId == 'div-id-AD_SPOT_HOMEPAGE_STUDENT_OFFERS_FEATURED_AD_1' || divId == 'div-id-AD_SPOT_COLLECTIONS_PROMO_1')		
				gptadslots[count]= googletag.defineSlot(adName, [[adWidth,adHeight],[970,250]],divId).addService(googletag.pubads());
			else
				gptadslots[count]= googletag.defineSlot(adName, [[adWidth,adHeight]],divId).addService(googletag.pubads());
			location = 'top';
			if(typeof location != 'undefined' && location !=null){
				gptadslots[count].setTargeting('loc',[location]);
			}
			
			position = 'atf';
			if(typeof position != 'undefined' && position !=null){
				gptadslots[count].setTargeting('pos',[position]);
			} 
		
			count++;
			adName = googleAdNetworkID + googleAdSite + 'homepage';
			adWidth = 300;
			adHeight = 250;
			divId = 'div-id-AD_SPOT_HOMEPAGE_STUDENT_OFFERS_AD_2';
			if(divId == 'div-id-AD_SPOT_HOMEPAGE_STUDENT_OFFERS_FEATURED_AD_1' || divId == 'div-id-AD_SPOT_COLLECTIONS_PROMO_1')		
				gptadslots[count]= googletag.defineSlot(adName, [[adWidth,adHeight],[970,250]],divId).addService(googletag.pubads());
			else
				gptadslots[count]= googletag.defineSlot(adName, [[adWidth,adHeight]],divId).addService(googletag.pubads());
			location = 'bot';
			if(typeof location != 'undefined' && location !=null){
				gptadslots[count].setTargeting('loc',[location]);
			}
			
			position = 'btf';
			if(typeof position != 'undefined' && position !=null){
				gptadslots[count].setTargeting('pos',[position]);
			} 
				
		googletag.pubads().setTargeting('s',['bnc']);
		googletag.pubads().setTargeting('id',[bncbStoreId]);
		if(usrType == 'undefined' || usrType == null){
			googletag.pubads().setTargeting('login',['0']);
		}
		else
		{
			googletag.pubads().setTargeting('login',['1']);
			googletag.pubads().setTargeting('user_type',[usrType]);
			googletag.pubads().setTargeting('gyr',[userGradYear]);
			googletag.pubads().setTargeting('g',[userGender]);
		}
		googletag.pubads().setTargeting('pagetype',['home']);
		
		
		googletag.pubads().collapseEmptyDivs(true);
		
		googletag.pubads().enableAsyncRendering();		
		googletag.enableServices();
	});
</script> 
