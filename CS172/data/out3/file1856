		expAttrArray[0] = 'STR_522';
		expAttrArray[2] = 'Home';
		expAttrArray[8] = containerPageId;
		expAttrArray[15] = '4 Year';
		expAttrArray[16] = 'Yes';
		expAttrArray[17] = 'NA';
		expAttrArray[18] = 'State';
		expAttrArray[19] = 'NA';
	cmCreatePageElementTag(elementName, elementCat, 'Homepage', 'Home', null, expAttrArray);
	}
	
