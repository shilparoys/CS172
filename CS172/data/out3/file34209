       <h3>Upcoming Events</h3> 
       <script type="text/javascript">
    function ReloadCalendar(Query, DateQry, ContainerRequired) {

        
        //$("#eventsDisplay").destroyPlugin;

        var searchText = "";
        var userAcct = "ucradmissionssurvey@gmail.com";
        var acctApi = "AIzaSyB7R9d_gTKjnUfxhwHVhDeZqCE5o6-Z0C8";
        var currentDate;

        // Check if Query is defined
        if (Query !== null || Query !== "") {
            searchText = Query
        }
        // Check if Date is defined
        if (Date !== null || Date !== "") {
            currentDate = new Date(DateQry)
        }
        else {
            currentDate = new Date();
        }

        // Check if first time instantiating
        if (ContainerRequired) {
            $("#eventsDisplay").CalendarEvents({
                user: userAcct
                , apiKey: "AIzaSyB7R9d_gTKjnUfxhwHVhDeZqCE5o6-Z0C8"
                , displayType: "perMonth"
                , addLinkType: "link"
                , height: 400
                , maxResult: 0
            });
            setSelectedCalendar("View All");
        }
        else {
            $("#eventsDisplay").CalendarEvents({
                user: userAcct
                , displayType: "perMonth"
                , apiKey: "AIzaSyB7R9d_gTKjnUfxhwHVhDeZqCE5o6-Z0C8"
                , addLinkType: "link"
                , height: 400
                , maxResult: 0
                , timeMin: currentDate
                , timeMax: currentDate
                , searchCriteria: searchText
                , searchCondition: "in"
                , maxResults: 0
                , setContainer: false
            });
        }
        
    };
    

    setSelectedCalendar = function (cal) {
        switch (cal) {
            case "View All":
                $("#calSelector-ViewAll").css("font-weight", "bold");
                $("#calSelector-Counselor").css("font-weight", "normal");
                $("#calSelector-Deadlines").css("font-weight", "normal");
                break;
            case "Counselor":
                $("#calSelector-Counselor").css("font-weight", "bold");
                $("#calSelector-ViewAll").css("font-weight", "normal");
                $("#calSelector-Deadlines").css("font-weight", "normal");
                break;
            case "Deadlines":
                $("#calSelector-Deadlines").css("font-weight", "bold");
                $("#calSelector-ViewAll").css("font-weight", "normal");
                $("#calSelector-Counselor").css("font-weight", "normal");
                break;
        }
    }
</script> 
       <script type="text/javascript">
    $(document).ready(function () {
        $("#accordion").tabs("#accordion div.pane",
            {
                tabs: '.event',
                effect: 'slide'
            });
    });
</script> 
       <script type="text/javascript">
    $(document).ready(function () {
        var now = new Date();
        ReloadCalendar("", now, true);

        $("#calSelector-ViewAll").click(function (e) {
            var splitMonthYear = $('.ed-monthName').text().split(" ");
            var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);

            $("#eventsDisplay").html("");
            var searchText = "";
           
            ReloadCalendar(searchText, currentDate, false);
            // Don't follow the link to the anchor
            e.preventDefault ? e.preventDefault() : e.returnValue = false;
            // Set as selected
            setSelectedCalendar("View All")
        });

        $("#calSelector-Deadlines").click(function (e) {
            var splitMonthYear = $('.ed-monthName').text().split(" ");
            var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);
            //alert("monthNameDate value:" + currentDate);
            $("#eventsDisplay").html("");
            var searchText = "Deadline";
            
            ReloadCalendar(searchText, currentDate, false);
            // Don't follow the link to the anchor
            e.preventDefault ? e.preventDefault() : e.returnValue = false;

            // Set as selected
            setSelectedCalendar("Deadlines")
        });

        $("#calSelector-Counselor").click(function (e) {
            var splitMonthYear = $('.ed-monthName').text().split(" ");
            var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);
            //alert("monthNameDate value:" + currentDate);
            $("#eventsDisplay").html("");
            var searchText = "Counselor";
            
            ReloadCalendar(searchText, currentDate, false);
            // Don't follow the link to the anchor
            e.preventDefault ? e.preventDefault() : e.returnValue = false;
            // Set as selected
            setSelectedCalendar("Counselor")
        });

    });
</script> 
