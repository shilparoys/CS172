  <script src="/Scripts/jquery.livesearch.js"></script> 
  <script type="text/javascript">

        //$(document).ready(function () {
        //    $('ul.staticCats.orange-bullets li a').click(function () {
        //        $('div#livesearch input[type="text"]').val("");
        //    });

        //});
        // ---- BEGIN LIVESEARCH ---- //
        (function ($) {
            $(document).ready(function () {
                $('ul.staticCats.orange-bullets li a, a.clearSearch').click(function () {
                    $('div#livesearch input[type="text"]').val("");
                    resetSearch();
                });

                $('input#MainContent_tbLivesearch').search('#faqWrapper dl dt', function (on) {
                    on.all(function (results) {
                        var size = results ? results.size() : 0
                        $('#count').text(size + ' questions');
                    });

                    // What to display after a search has been made and cleared from input -WB
                    on.reset(function () {
                        resetSearch();
                        //$('#none').hide();
                        //$('#faqWrapper dl dd').hide();
                        //$('#search-results').hide();
                        //$('#faqWrapper dl dt').show();
                        //$('h4.section2').show();
                        //$('a.btt').show();
                    });
                    // What to display on negative search results -WB
                    on.empty(function () {
                        $('#faqWrapper dl dt').hide();
                        $('#search-results').hide();
                        $('#none').show();
                        $('h4.section2').hide();
                        $('a.btt').hide();
                    });


                    // What to display on positive search results -WB
                    on.results(function (results) {
                        console.log(results);
                        $('#none').hide();
                        $('#faqWrapper dl dt').hide();
                        $('h4.section2').show();
                        //$('#faqWrapper dl dd').hide();
                        $('#search-results').show();
                        $('a.btt').show();
                        hideNegResultsCatHeading(results);
                        results.show();
                    });

                    // loop through each Category and see if there are any results that match
                    // if no matches, then traverse up and hide the nearest category heading
                    function hideNegResultsCatHeading(results) {
                        $("h4.section2").each(function () {
                            var rr = $(this).next().find('dt');
                            if (!rr.is(results)) {
                                rr.parent('dl').prev().hide();
                                var backTop = rr.parent('dl').find('a.btt');
                                backTop.hide();
                            }
                        });
                    }
                });

                function resetSearch() {
                    $('#none').hide();
                    $('#faqWrapper dl dd').hide();
                    $('#search-results').hide();
                    $('#faqWrapper dl dt').show();
                    $('h4.section2').show();
                    $('a.btt').show();
                }
                // Triggers 'keydown' on incomming search query
                $('input#MainContent_tbLivesearch').trigger('keydown');
            });
        })(jQuery);

        // The code below makes the live search box case insensitive (VERY IMPORTANT!!!!)
        jQuery.expr[':'].contains = function (a, i, m) {
            return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase()) >= 0;
        };
        // The code below prevents the input form from registering an enter or return command from a keyboard
        // This fixes a bug where a user would press the enter/return key on their keyboard and be taken to a blan UCR Search Page
        // Can be used by activating onkeypress="return bar(event)"  within the <input> tag
        function bar(evt) {
            var k = evt.keyCode || evt.which;
            return k != 13;
        }
        // ---- END LIVESEARCH ---- //


        // ---- Show/Hide Category Menu ---- //
        $(document).ready(function () {

            ///* Simple accordion*/
            //$('a.lQtrigger').each(function () {
            //    //$this = $(this);
            //    //$next = $(this).next();
            //    $(this).click(function () {
            //        $next = $(this).next();
            //        //alert($next.text());
            //        $next.slideToggle(function () {
            //        });
            //        return false;
            //    });
            //});

            $('dt').live('click', function () {
                var dd = $(this).next();
                // If the title is clicked and the dd is not currently animated,
                // start an animation with the slideToggle() method.
                if (!dd.is(':animated')) {
                    dd.slideToggle();
                    $(this).toggleClass('opened');
                }
            });

        });
    </script> 
