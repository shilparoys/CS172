      $(function(){
      $('#tipTitle').click(function(event){
      // alert("yes!");
      if ($("div#tipDiv").height()>30){
      $("div#tipDiv").animate({
      height: "30px",
      top:465      
      }, "slow");
      $("#markImg")[0].src="images/open.png";
      
      }else{
      $("div#tipDiv").animate({
      height: "129px",
      top:367      
      }, "slow");
