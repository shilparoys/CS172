      <script language="javascript">
function stl_tab_1(tabName, no){
	for ( i = 1; i <= 5; i++){
		var el = jQuery('#学院新闻_tabContent_' + i);
		var li = $('#学院新闻_tabHeader_' + i);
		if (i == no){
            try{
			    el.show();
            }catch(e){}
            li.removeClass('new_btn2');
            li.addClass('new_btn1');
		}else{
            try{
			    el.hide();
            }catch(e){}
            li.removeClass('new_btn1');
            li.addClass('new_btn2');
		}
	}
}
</script> 
      <div class="new_btn1" id="学院新闻_tabHeader_1" onmouseover="stl_tab_1('学院新闻', 1);return false;">
