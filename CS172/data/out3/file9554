  <script type="text/javascript">
    $(document).ready(function(){
      $.validator.addMethod("injectioncheck", function(value, element, param) {
        if(value.match(new RegExp("^"+param+"$"))) return false;
        return true
      });
      $("#demobreq").validate({
        rules: {
          exname: { injectioncheck: "[<>!\\?\\*\/].*" },
          piname: { injectioncheck: "[<>!\\?\\*\/].*" },
          email: { injectioncheck: "[<>!\\?\\*\/].*" },
        },
        messages: {
          exname: "Required. Characters banned: <>=!?*/",
          piname: "Required. Characters banned: <>=!?*/",
          email: "Required. Characters banned: <>=!?*/",
        },
        highlight: function(label) {
          $(label).closest('.control-group').addClass('error');
        },
        success: function(label) {
          label.addClass('valid').closest('.control-group').addClass('success');
        }
      });
    });
    </script> 
