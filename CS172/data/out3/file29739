  <script>
        $(document).ready(function() {
            var $el;
            var hash = window.location.hash;

            function showHash( $el ) {
                $el.show();
                if( $el + ':visible' ) {
                    HScms.pageScrollTo( $el.offset().top );
                }
            }
            if( hash != '' ) {
                $el = $( hash );
                if( $el && $el.length ) {
                    $el.hide();

                    if( typeof HScms.pageScrollTo == 'function' ) {
                        showHash( $el )
                    }
                }
            }
        });
    </script>
