  <script src="/include/layout.js"></script> 
  <script type="text/javascript">
		function checkSubmit(e) {
		   if(e && e.keyCode == 13) {
			  doSubmit();
		   }
		}
		
		function doSubmit() {
			var username = document.getElementById('UserId');
			var password = document.getElementById('Password1');
			var email = document.getElementById('UserEmail');
			var bInvalid = false;
			
			if(!username.value || username.value == 'Username') {
				markInvalidField(username);
				bInvalid = true;
			}
			if(!password.value || password.value == 'Password') {
				markInvalidField(password);
				bInvalid = true;
			}
			if(!email.value || email.value == 'Email' || email.value.indexOf('@') == -1 || email.value.indexOf('.') == -1) {
				markInvalidField(email);
				bInvalid = true;
			}
			
			if(bInvalid) {
				return false;
			} else {
				document.getElementById('form-settings').submit();
				return true;
			}
		}
		
		function markInvalidField(field) {
			field.style.color = '#777777';
			field.style.backgroundColor = '#FFCCCC';
		}
		
		function fieldEnter(field, label) {
			field.style.color = '#333333';
			field.style.backgroundColor = '#FFFFFF';
			if (field.value == label) {
				field.value = "";
				if(label == "Password") {
					try {
						field.type = 'password';
					} catch(e) {
						var activeEl = document.activeElement;
						document.getElementById('Password1').value = document.getElementById('Password1text').value;
						document.getElementById('Password1text').style.display = 'none';
						document.getElementById('Password1').style.display = '';
						if(document.getElementById('Password1text') == activeEl) {
							document.getElementById('Password1').focus();
						}
					}
				}
			}
		}
		 
		function fieldLeave(field, label) {
			if (field.value == "") {
				field.style.color = '#777777';
				field.value = label;
				if(label == "Password") {
					try {
						field.type = 'text';
					} catch(e) {
						var activeEl = document.activeElement;
						document.getElementById('Password1text').value = document.getElementById('Password1').value;
						document.getElementById('Password1text').style.display = '';
						document.getElementById('Password1').style.display = 'none';
						if(document.getElementById('Password1') == activeEl) {
						document.getElementById('Password1text').focus();
						}
					}
				}
			}
			if(label == "Password") {
				document.getElementById('Password2').value = field.value;
			}
		}

		document.new_user.reset();
		fieldLeave(document.getElementById('UserId'),'Username')
		fieldLeave(document.getElementById('Password1'),'Password')
		fieldLeave(document.getElementById('UserEmail'),'Email')
	</script>  
