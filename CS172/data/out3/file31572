         <input type="password" name="Password1" id="Password1" class="input-text" maxlength="24" tabindex="2" onclick="fieldEnter(this, 'Password')" onfocus="fieldEnter(this, 'Password')" onblur="fieldLeave(this,'Password')"> 
         <input type="text" name="Password1text" id="Password1text" class="input-text" maxlength="24" tabindex="2" onclick="fieldEnter(this, 'Password')" onfocus="fieldEnter(this, 'Password')" onblur="fieldLeave(this,'Password')" style="display:none;"> 
         <input type="hidden" name="Password2" id="Password2" value=""> 
