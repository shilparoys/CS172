        <script type="text/javascript" src="http://comcastnewsmakers.com/wp-content/plugins/eb_contact_form/js/jquery-1.11.1.min.js"></script> 
        <script type="text/javascript">	

function submitEbForm(){
		
	var error = 0;
	
	var u_name = $('[name=name]').val();
	if(u_name.length < 3){
		$('#name_error').show();
		error = 1;
	}else{
		$('#name_error').hide();
	}
	
	var email = $('[name=email]').val();
	var email_expression=/^([A-Za-z0-9!#$%^&*()._]{3,200})@([A-Za-z. _]{3,600})\.([a-z]{2,15}(?:\.[a-z]{1,6})?)$/;
	if(!email || !email_expression.test(email)){
		$('#email_error').show();
		error = 1;
	}else{
		$('#email_error').hide();
	}
	
	var category_value = $('[name=00N50000002eWhD]').val();
	if(!category_value){
		$('#00N50000002eWhD_error').show();
		error = 1;
	}else{
		$('#00N50000002eWhD_error').hide();
	}
	
	var subject_value = $('[name=subject]').val();
	if(!subject_value){
		$('#subject_error').show();
		error = 1;
	}else{
		$('#subject_error').hide();
	}
	
	var description_value = $('[name=description]').val();
	if(!description_value){
		$('#description_error').show();
		error = 1;
	}else{
		$('#description_error').hide();
	}
	
	var vercode = $('[name=vercode]').val();
	if(!vercode){
		$('#vercode_error').show();
		error = 1;
	}else{
		$('#vercode_error').hide();
	}
			
	
	if(!error){
		
		 $.ajax
			({
				url: 'http://comcastnewsmakers.com/wp-content/plugins/eb_contact_form/ajax.php',
				type: 'POST',
				dataType: 'text',
				
				data: {
					name_value: $('[name=name]').val(), 
					email_value: $('[name=email]').val(), 
					phone_value: $('[name=phone]').val(), 
					org_value: $('[name=organization]').val(), 
					category_name: $('[name=00N50000002eWhD] option:selected').text(),
					category_msg: $('[name=00N50000002eWhD]').val(),
					
					page_value: $('[name=00N50000002eWee]').val(), 
					subject_value: $('[name=subject]').val(), 
					description_value: $('[name=description]').val(), 
					reported_value: $('[name=00N50000002evN]:checked').val(), 
					steps_value: $('[name=00N50000002evNh]').val(), 
					//site_url: "", 
					vercode: $('[name=vercode]').val()
					},
				
				beforeSend: function() {
					$("#eb_loader").show();
					$('#vercode_error').hide();
					$('.success_msg').hide();
					
					},
					
				complete: function() {$('#eb_loader').hide()},
				
				success: function(msg)
				{	
					//alert(msg);
					
					if(msg==0){
						
						$('#vercode_error').show();
						
					}else if(msg==1){
						
						$.ajax({
							type: 'POST',
							url: 'https://www.salesforce.com/servlet/servlet.WebToCase?encoding=UTF-8&orgid=00D50000000JGoI',
							crossDomain: true,
							//contentType: 'application/json; charset=UTF-8',
							dataType: "text",
														
							data: {
								"name": $('[name=name]').val(), 
								"email": $('[name=email]').val(), 
								"phone": $('[name=phone]').val(),
								"organization": $('[name=organization]').val(), 
								"00N50000002eWhD": $('[name=00N50000002eWhD] option:selected').text(),
								"00N50000002eWee": $('[name=00N50000002eWee]').val(), 
								"subject": $('[name=subject]').val(), 
								"description": $('[name=description]').val(), 
								"00N50000002evN": $('[name=00N50000002evN]:checked').val(), 
								"00N50000002eWhI": "Visitor", 
								"00N50000002evNh": $('[name=00N50000002evNh]').val(),
								
								"00N50000002eWhh": "",
								"00N50000002eWhX": "",
								"00N50000002evNm": "",
								"00N50000002f4NX": "http://comcastnewsmakers.com",
								"ip": "66.153.62.42"
								},
								
								//data: {"name_value":"job"},
							
							success: function(data) {
								//var value = responseData.someKey;
								//alert(123);
								
							},
							error:function(jqXHR, textStatus, errorThrown){
								//alert(errorThrown);
								//alert(456);
						  	}
						});
												
						$("#eb_contact_form input[type=text], #eb_contact_form textarea").val("");
						
						$('#vercode_error').hide();
						$('.success_msg').show();
						$('.success_msg').html($('[name=00N50000002eWhD]').val());
					}
					
				}
			})
	}
	
}
</script> 
       </div>
