(function ($, Drupal, window, document, undefined) {
  $(document).ready(function() {
        $('a.fancybox-video-thumb').fancybox({
          openEffect: 'elastic',
          openSpeed: 150,
          closeEffect: 'elastic',
          closeSpeed: 150,

          prevEffect: 'none',
          nextEffect: 'none',
          closeBtn: true,
          arrows: false,
          nextClick: false,

          autoSize: false,
          maxWidth: 625,
          maxHeight: 415,
          fitToView: false,
          width: '100%',
          scrolling : 'no',
          preload   : true,
          scrollOutside: false,
          autoCenter: true,
          wrapCSS: 'fancy-box-iframe',
          helpers: {
            overlay: {
              css: {
                'background': 'rgba(238, 238, 238, 0.8)'
              }
            }
          }
        });

    $('.flexslider').flexslider({
      animation: "slide"
    });
    $('.dele li a').hover(function() {
      $(this).find('.button').addClass('hover-state');
    }, function(){
      $(this).find('.button').removeClass('hover-state');
    });
    //main nav
    $('ul.nice-menu > li').hover(function() {
      $(this).find('a').addClass('hover-state');
      $(this).prev('li').addClass('hide-divider');
    }, function(){
      $(this).find('a').removeClass('hover-state');
      $(this).prev('li').removeClass('hide-divider');
    });
    /* Search Autocomplete */
    var pos = $("#searchbox").offset();
    var left = (pos.left - 114) + "px";
  });
})(jQuery, Drupal, this, this.document);


