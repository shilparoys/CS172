  <div id="main-content"> 
   <div class="row"> 
    <div id="main" class="twelve columns"> 
     <a id="main-content"></a> 
     <div class="two-col-sidebar-right"> 
      <div class="row top"> 
       <div class="large-12 columns"> 
       </div> 
      </div> 
      <div class="row middle"> 
       <div class="large-8 columns"> 
        <div class="panel-pane pane-entity-field pane-node-field-video"> 
         <div class="pane-content"> 
          <div class="field field-name-field-video field-type-ooyala-video field-label-hidden field-wrapper">
           <div id="ooyala_video_player" class="st-loading ooyala-video-player-container flex-video widescreen pac-12"></div>
           <script type="text/javascript">
  function getTimeFromHash() {
    var pos = window.location.hash.indexOf("#time:");
    if (pos < 0) {
      return false;
    }
    return window.location.hash.substr(pos + 6);
  }

  function getAnalyticsTags() {
    tags = [];
    tags.push(getPacificHour());
    return tags;
  }

  function getPacificHour() {
    // This is going to return an "ID" but in reality is a time span in a string.
    // Example : 00000059  12am - 00:59am & 23002359   11pm - 11:59pm
    // The one issue we run into is we always want the time in Pacific for the
    // analytics.
    var r = "", h = "00", m = "59", offset = -8, d = new Date();
    var h = d.getTimezoneOffset() / 60
    var utc = d.getTime() + (d.getTimezoneOffset() * 60000);

    d = new Date(utc + (3600000 * offset));
    h = d.getHours();
    if (h < 10) {
      h = "0" + h;
    }
    m = h + m;
    h = h + "00";
    r = h + m;

    return r;
  }

  jQuery(document).ready(function() {
    OO.ready(function () {
      var params = JSON.parse('{"autoplay":true,"showLoginAtStart":true,"wmode":"transparent","playerContainerId":"ooyala_video_player","is_locked":false,"preview_images":{"promo_image_l":null,"promo_image_m":null,"promo_image_s":null,"promo_image_xs":null},"is_epg":false,"virtual_asset_id":null,"embed_code":"BhNzMwZTquIL0gQP4Qc_PbWngM9eGWPw"}');

      params.onCreate = Drupal.ooyala.onCreate;

      if (typeof AdobePass !== "undefined") {
        AdobePass.playerParams = params;

        AdobePass.playerParams.initialTime = getTimeFromHash();
        AdobePass.playerParams.analytics = { tags: getAnalyticsTags() };
      }
      else if (!params.is_locked && !params.is_epg) {
        Drupal.ooyala.player = OO.Player.create(params.playerContainerId, params.embed_code, params);
      }
    });
  });
  </script>
          </div> 
         </div> 
        </div> 
       </div> 
