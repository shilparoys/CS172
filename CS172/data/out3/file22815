	    $(document).ready(function () {
	        var intervalID = setInterval(twtCheck, 200);

	        function twtCheck() {
	            var twtText = $('.twtr-hd').children('h4').children('a').text();

	            if (twtText != "") {
	                $('.twtr-hd').children('h3').text("");
	                $('.twtr-hd').children('h3').css("margin-bottom", 20);
	                $('.twtr-hd').children('h4').children('a').text("@Comm_College");
	                $('.twtr-hd').children('h4').attr('style', 'padding-top: 5px !important');
	                clearInterval(intervalID);
	            }
	        }
	    });														
