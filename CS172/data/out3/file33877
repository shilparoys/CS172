    <script>
$(document).ready(function(e) {


			
});

$(document).on("click",".showBuzzport_2", function(){
		
		var $this = $(this);
		var buzzContent = $($this).parents('div:eq(0)').find(".buzzContent").html();
		
		$.fancybox(buzzContent,{
		/*minWidth	: 300,
		maxHeight	: 800,*/
		fitToView	: false,
		width		: '80%',
		height		: '80%',
		autoSize	: false,
		closeClick	: false,
		openEffect	: 'fade',
		closeEffect	: 'elastic',
		openSpeed: '1500',
	});
		
		
	});

</script> 
