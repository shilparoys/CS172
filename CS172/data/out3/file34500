  <script src="https://www.cornell.edu/includes/carousel/jquery.tools.min.slideshow.js" type="text/javascript"> </script> 
  <script> 
$(function() {  
	//carousel
	$("div.tabs").tabs(".carousel-item", {effect: 'fade',
	    onBeforeClick: function(event, i) {
			// get the pane to be opened
			var pane = this.getPanes().eq(i);
			//get the current pane to replace with thumbnail
			var currentPane = this.getCurrentPane();
			//kill the current video and caption
			$(".carousel-item-content, .carousel-item-caption",currentPane).html("");
		    // load it with a page specified in the tab's href
		    	pane.load(this.getTabs().eq(i).attr("href")); 
		}, fadeOutSpeed: "slow", rotate: true }).slideshow({autoplay: false, interval:10000, clickable:false});
	//diversity link
	$(".diversity").hover(function(){ $(".diversity-link").slideDown(); },function(){ $(".diversity-link").slideUp(); });
	//email form
	$("#ea").focus(function(){ if ($(this).val() == 'enter email address') { $(this).val(''); } });
});	

//kaltura player cornellCast redirect
	function customFunc1(entryId) { 
		cornellCastRedir(entryId);
	}
	
	function customFunc2(entryId) { 
		cornellCastRedir(entryId);
	}
	
	function cornellCastRedir(entryId) { 
		var cornellcastUrl;
		$.get("ajax/getCornellCastId.cfm",{ kalturaid: entryId } ,function(data) { location.href = data });
	}
	
</script>  
