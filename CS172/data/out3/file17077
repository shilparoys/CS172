               <script type="text/javascript">

  function showMoreEventDesc(i) {
      document.getElementById("descbrief"+i).style.display = 'none';
      document.getElementById("desctext"+i).style.display = 'inline';
      document.getElementById("moredesc"+i).style.display = 'none';
      document.getElementById("hidedesc"+i).style.display = 'inline';
      var imagepath = document.getElementById("imagepath"+i).innerHTML;
      //firefox
      imagepath = imagepath.replace("width=\"50\"","width=\"150\"");
      imagepath = imagepath.replace("height=\"50\"","height=\"150\"");
      //ie
      imagepath = imagepath.replace("width=50","width=\"150\"");
      imagepath = imagepath.replace("height=50","height=\"150\"");
      document.getElementById("imagepath"+i).innerHTML = imagepath;

  }
  
  function hideMoreEventDesc(i) {
      document.getElementById("descbrief"+i).style.display = 'inline';
      document.getElementById("desctext"+i).style.display = 'none';
      document.getElementById("moredesc"+i).style.display = 'inline';
      document.getElementById("hidedesc"+i).style.display = 'none';
      var imagepath = document.getElementById("imagepath"+i).innerHTML;
      //firefox
      imagepath = imagepath.replace("width=\"150\"","width=\"50\"");
      imagepath = imagepath.replace("height=\"150\"","height=\"50\"");
      //ie
      imagepath = imagepath.replace("width=150","width=\"50\"");
      imagepath = imagepath.replace("height=150","height=\"50\"");
      document.getElementById("imagepath"+i).innerHTML = imagepath;
  
  }

</script> 
               <div id="campusEventsContainer_campuseventssyndicat"> 
                <div id="eventItemContainer">
                 Loading events...
                </div> 
               </div> 
               <script type="text/javascript">

    $.ajax({
        url: "/etc/designs/villanova/ajax/campusevents.event.json",
        data: {	maxevents: "5",
        		category: "homepage",
                renderformat: "list",
                semestersdt: "",
                semesteredt: "",
                docurl: document.URL},
        type: 'GET',
        success: function(events) {

            var cat = 'homepage';
            var template = "<div class='eventitem media'>            {{#imagepath}}            <a class='pull-left'>                <span id='{{imageid}}'>{{{imagepath}}}</span>            </a>            {{/imagepath}}            <div class='media-body'>                <h4 class='media-heading'><a href='{{{urllink}}}' target='_blank'>{{{title}}}</a></h4>                <p class='datetime'>                    {{startdate}}{{{at}}}{{starttime}}                </p>                            </div>    </div>";
            var eventContainer = "#campusEventsContainer_campuseventssyndicat #eventItemContainer";
            var charct = 175;
            var showmore = "visible";
            if ( events.length == 0) {
                $(eventContainer).html("<p>There are currently no events to display. Please continue to check back for new events</p>");
                return;
            } else {
                $(eventContainer).html("");
            }


            //iterate over each event and add HTML to eventItemContainer
           for (i = 0; i < events.length; i++) {
               var urllink = "http://campusevents.villanova.edu/vuevents/EventList.aspx?"+
                             "fromdate="+events[i].startdate+
                             "&todate="+events[i].enddate+
                             "&display=Month&type=public"+
                             "&eventidn="+events[i].eventid+
                             "&view=EventDetails"+
                             "&information_id="+events[i].infoid +
                             "&syndicate=syndicate";
              
               var  imagepath = events[i].imagepath != null ? "<img class='media-object' src='" + events[i].imagepath + "' alt='campusevents' width='50' height='50' align='right'/>" :"";

               var startdate = events[i].startdate;
               var  starttime = ""; var at = "";
               if (events[i].starttime != "") {
                  at = "&nbsp;@&nbsp;";
                  starttime = events[i].starttime;
               }
              
               var enddate = "";
                
               if (events[i].enddate != "") {
                  enddate = " - "+events[i].enddate;
               }
                
               var  endtime = ""; var ate = "";
               if (events[i].endtime != "") {
                  ate = "&nbsp;@&nbsp;";
                  endtime = events[i].endtime;
               }
                
               if (events[i].startdate == events[i].enddate &&
                       events[i].starttime == events[i].endtime ) {
                  enddate = "";   
                  endtime = "";
               }
                
               if (cat == "today") {
                    startdate = ""; starttime = ""; at = "";
                    enddate = ""; endtime = ""; ate = "";
               }
                
               if (events[i].enddate != "" && events[i].startdate != events[i].enddate) {
                  startdate += " - "+events[i].enddate;
               }

               //Strip out <span> and <p> tags. repalce </p> with two <br/>
               var descValue = events[i].desc.replace(/(<span[\s\S]+?>)|(<\/span>)/g,'').replace(/(<p[\s\S]+?>)/g,"").replace(/(<\/p>)/g,"<br/><br/>").replace(/(<br \/>|<br>)/g,"<br/>");
         
               var descValue1 = descValue;
               var descValue2 = "";
               var moredesc = "";
               if (events[i].desc.length > charct) {
                   var str175 = descValue.substr(charct);
                   var nextblank = str175.indexOf(" ");
                   if (nextblank >= 0) {
                       descValue2 = str175.substr(0,nextblank);
                       descValue1 = descValue.substr(0,charct);
                       moredesc = " "+str175.substr(nextblank+1);
                   }
               } 

               var view = {
                    urllink: urllink,
                    title: events[i].title,
                    imagepath: imagepath,
                    imageid: "imagepath"+i,
                    desc1: descValue1,
                    desc2: descValue2,
                    showmore: showmore,
                    descbriefid: "descbrief"+i,
                    desctextid: "desctext"+i,
                    moredesc: moredesc,
                    moredescid: "moredesc"+i,
                    descidnum: i,
                    hidedescid: "hidedesc"+i,
                    hidedescidnum: i,
                    startdate: startdate,
                    starttime: starttime,
                    enddate: enddate,
                    endtime: endtime,
                    at: at,
                    ate: ate
               };
               $(eventContainer).append(Mustache.render(template,view));

            }
     
        },
        failure: function(response, opts) {
              // console.log('server-side failure with status code ' + response.status);
        }
     });

    
</script> 
              </div> 
