       <script type="text/javascript">
		$(document).ready(function(){
			setBackgroundImage();
		});

		function setBackgroundImage() {
		
			var fullUrl = window.location.href.toLowerCase();
		
			if(fullUrl.indexOf(".edu:") >= 0){
				var fullUrlPieces = fullUrl.split(".edu:");
				fullUrlPieces[1] = fullUrlPieces[1].substring(fullUrlPieces[1].indexOf("/")+1);
			}
			else{
				var fullUrlPieces = fullUrl.split(".edu/");
			}
			var isAdminSite = fullUrlPieces[0].indexOf(".admin");
			var pageOnlyPath = fullUrlPieces[1];
			var pageOnlyPathPieces = pageOnlyPath.split("/");
			var rootDirectory = pageOnlyPathPieces[0];
			var subPage;
			if(pageOnlyPathPieces[1] == "pages"){
				var subPage = pageOnlyPathPieces[2];
			}
			else{
				var subPage = pageOnlyPathPieces[1];
			}
			
			var bodyClass;
			
			if(pageOnlyPath.length > 0){
				switch(rootDirectory) {
					case "pages":
						if(pageOnlyPathPieces[1] == "default.aspx") {
							$("body").addClass("pageBgHome");
							bodyClass = "pageBgHome";
						}
						else if(pageOnlyPathPieces[1].indexOf("headerfooterpage.aspx") > -1){
							$("body").addClass("pageBgHigherEducation");
							bodyClass = "pageBgHigherEducation";
						}
						else{
							$("body").addClass("pageBgNewsroom");
						}
						break;
						
					case "":
						if(pageOnlyPathPieces[1] == null) {
							$("body").addClass("pageBgHome");
							bodyClass = "pageBgHome";
						}
						break;
						
					case "leadership":
						$("body").addClass("pageBgLeadershipPrograms");
						bodyClass = "pageBgLeadershipPrograms";
						if(subPage == "" || subPage == "default.aspx") {
							$("body").addClass("pageBgSecondary");
						}
						break;
						
					case "advocacy-news":
						$("body").addClass("pageBgAdvocacyNews");
						bodyClass = "pageBgAdvocacyNews";
						if(subPage == "" || subPage == "default.aspx") {
							$("body").addClass("pageBgSecondary");
						}
						break;
						
					case "higher-education":
						$("body").addClass("pageBgHigherEducation");
						bodyClass = "pageBgHigherEducation";
						if(subPage == "" || subPage == "default.aspx") {
							$("body").addClass("pageBgSecondary");
						}
						break;
						
					case "events":
						$("body").addClass("pageBgEvents");
						bodyClass = "pageBgEvents";
						break;
						
					case "membership":
						$("body").addClass("pageBgMembership");
						bodyClass = "pageBgMembership";
						break;
						
					case "contact-us":
						$("body").addClass("pageBgContact");
						bodyClass = "pageBgContact";
						break;
						
					case "about-ace":
						$("body").addClass("pageBgAbout");
						bodyClass = "pageBgAbout";
						break;
						
					case "search":
						$("body").addClass("pageBgSearch");
						break;
						
					case "news-room":
						$("body").addClass("pageBgNewsroom");
						break;
						
					default:
						$("body").addClass("pageBgNewsroom");
						break;	
				}
			}
			else{
				$("body").addClass("pageBgHigherEducation");
				bodyClass = "pageBgHigherEducation";
			}
		}

	</script>
      </footer></span> 
     <!-- end Footer --> 
