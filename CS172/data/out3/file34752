  <script type="text/javascript">
        document.getElementById("superleader-wrap").appendChild(document.getElementById("banner-sky")); 

        $('.asset-body a').attr('target', "_blank");

        //$("#page").bgrotate({ delay: 4000, imagedir: "http://grfx.cstv.com/schools/usc/graphics/blog/", images: ["usc-12-blog-bg-sample-01.jpg", "usc-12-blog-bg-sample-02.jpg", "usc-12-blog-bg-sample-03.jpg"});


        //detect Android & all ipod, iphone and ipads
        if (
            typeof window.orientation != 'undefined' ||
            navigator.userAgent.match(/iphone/i) ||
            navigator.userAgent.match(/ipad/i) ||
            navigator.userAgent.match(/ipod/i) ||
            navigator.userAgent.match(/mobile/i) ||
            navigator.userAgent.match(/android/i) ||
            navigator.userAgent.match(/windows phone/i) || 
            navigator.userAgent.match(/blackberry/i) 
        ) {
            //mobile, do nothing yet       
        } else {

            function getOffset() {
                var w=window;
                if(w.pageYOffset != null) return (w.pageYOffset);
                var z = w.document;
                if (document.compatMode=="CSS1Compat") return z.documentElement.scrollTop;
                return z.body.scrollTop;
            }
            function getHeight() {
                var w=window;
                if(window.innerHeight != null) return window.innerHeight;
            }   
             

            var stickyNav = document.getElementById("blog-sticky-header");
            var stickyLimit = 75;
            var stickyFixedPos = "0px";


            function setStickyElement() {
                if (parseInt(window.innerHeight) <= 437) {
                    stickyNav.style.position = 'absolute';
                    stickyNav.style.top = "75px";
                    return;
                }
                var woff = getOffset();
                if (woff < stickyLimit) {
                    stickyNav.style.position = 'absolute';
                    stickyNav.style.top = "75px";
                } else if (woff >= stickyLimit) {
                    stickyNav.style.position = 'fixed';
                    stickyNav.style.top = stickyFixedPos;
                }   
            }

            if (window.addEventListener) {
                window.addEventListener("scroll", function() { setStickyElement() }, false);
                window.addEventListener("resize", function() { setStickyElement() }, false);
                window.addEventListener("load", function() { setStickyElement() }, false);
            } else { 
                window.attachEvent("onscroll", function() { setStickyElement() }); 
                window.attachEvent("onresize", function() { setStickyElement() }); 
                window.attachEvent("onload", function() { setStickyElement() });
            }
        }




    </script> 
