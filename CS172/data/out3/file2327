          <script type="text/javascript">
	$(function() {
		var URLS = SVG.URLS;
		URLS.EventPopup = '/events/details/';
		URLS.GetAllFlyerBoardImages = 'https://uwyo.collegiatelink.net/Flyerboard/AllEventsForFlyerBoard';
		URLS.GetThisWeekFlyerBoardImages = 'https://uwyo.collegiatelink.net/Flyerboard/ThisWeekEventsForFlyerBoard';
		URLS.GetNextWeekFlyerBoardImages = 'https://uwyo.collegiatelink.net/Flyerboard/NextWeekEventsForFlyerBoard';
		
		$(".flyerboard").each(function() {

			SVG.FlyerBoard.init(this, {
				Canvas: {
					Width: 670,
					Height: 350,
					BorderWidth: 20,
					BorderHeight: 35
				},
				Images: {
					FlyerWidth: 134,
					FlyerHeight: 150,
					ImageSpacingWidth: 32,
					ImageSpacingHeight: [0, 3, 5, 8, 10, 15],
					External: false
				}
			});
		});

		$('#addFlyerboard').click(function (e) {
			e.preventDefault();
			$('#ExternalFlyerboardLink').toggle();
		});
		$('#flyerboardCopyHtml').click(function() {
			$(this).select();
		});
	});
</script> 
