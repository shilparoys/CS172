 $(document).ready(function(){
   $("a#quick_login").click(function(event){
	 $("div#quick_login_menu").toggle();
	 $("#alerttab img").toggle();
	 event.preventDefault();
   });   
	
	$("#its_search_text").focus(function(){
		if (($(this).val())=="Search ITS Site") $(this).val('');
	});
	$("#its_search_text").blur(function(){
		if (($(this).val())=="") $(this).val('Search ITS Site');
	});
	
	
 });
