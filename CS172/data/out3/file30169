		$('#top-nav ul li').hover(function() {
			var regid = $(this).attr('id');
			regid = regid.replace(/[^0-9]/g, '');
			var elem = paper.getById(regid);
			elem.animate({fill: '#31769b'}, 200);
			}, function() {
			var regid = $(this).attr('id');
			regid = regid.replace(/[^0-9]/g, '');
			var elem = paper.getById(regid);
			elem.animate({fill: '#d8d2c0'}, 200);
		});


	}

	function set_hover(setx,paper) {
		setx.click(function () { window.location=setx.url;});

		setx.hover(function(){
				$('#r-' + setx.id).addClass('hover');
				setx.attr({cursor:'pointer'});
				paper.getById(setx.id).animate({
					fill: '#31769b'
				}, 200);
			}, function(){
				$('#r-' + setx.id).removeClass('hover');
				paper.getById(setx.id).animate({
					fill: '#d8d2c0'
				}, 200);
		});
	}
	
	load_svgmap();
</script> 
