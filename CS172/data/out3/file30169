  <script type="text/javascript">
		$(document).ready(function(){
			var hash = window.location.hash;
			if (!hash){
				$('#Popular').addClass('selected').show();
				$('.categories li:first-child a').addClass('selected');
			} else {
				$(hash).addClass('selected').show();
				$('.categories li a').each(function (){
					selected_cat = '#' + $(this).attr('name');
					if (hash == selected_cat){
						$(this).addClass('selected');
					}
				});
			}

			$('.categories li a').click(function (){
				$('.categories li a').removeClass('selected');
				$(this).addClass('selected');
				var cat = '#'+ $(this).attr('name');
				$('.rank-list-cat.selected').removeClass('selected').fadeOut('fast', function(){
			        $(cat).addClass('selected').fadeIn('slow');
			    });
			});
			
			return false;
		});
	</script> 
