				oz_api = usn.rubicon.apiName;
				oz_site = usn.rubicon.site;
				oz_zone = usn.rubicon.getZone("adv-2");
				oz_ad_slot_size = usn.rubicon.getAdSize("adv-2");
				oz_callback = function(valuation) {
					
					var tier=null, size=null;
					try {
						tier = valuation.estimate.tier;
						size = valuation.estimate.size;
					} catch(e) {/*...*/}
					
					var iframe = document.createElement('iframe');
					iframe.setAttribute("id", "iframe-ad-2");
					iframe.className = "iframe-ad";
					iframe.setAttribute("src", "/ajax/iframe_ad/?tile=2&sitename=usn.bestvacations&params=kw%3DWorldwide%3Bkw%3Dtravel%3Bkw%3Dbestvacations%3Bkw%3Dsectionfront&tier="+tier+"&size="+size);
					iframe.setAttribute("scrolling", "no");
					iframe.onload = function() {
						checkAdVisibility('iframe-ad-2','adv-2');
						adValidator('adv-2');
					};
					
					document.getElementById("adv-2").appendChild(iframe);
				};
				
				usn.rubicon.callApi();
			</script> 
