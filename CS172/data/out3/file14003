				gaResizeCompleteHl = setTimeout(function(){
					var viewportWidth=$(window).width();
					_gaq.push(['_trackEvent', 'Viewport Dimensions', 'Viewport Dimensions Resized', viewportWidth+'x'+$(window).height(), viewportWidth, true]);
				}, 500);
			});
		});
	</script>  
 </body>
</html>