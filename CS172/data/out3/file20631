  <script type="text/javascript">

    function ExpCollSection(ctrl) {
        var control = ctrl;
        if ($(ctrl.dataset.target).is(':hidden'))
            $(ctrl.dataset.target).show();
        else
            $(ctrl.dataset.target).hide();

        return false;
    }

    $(document).ready(function () {
        $(".ooyalavideo").each(function(index) {
            ShowVideoPlayerAllVideos($(this).attr("id"), $(this).attr("id"));
        });
        
        
        /* IE 8 & 9 fix for OOYAlA vidoes rendering */
        var agt=navigator.userAgent.toLowerCase();

        if ((agt.indexOf("msie 8.0") != -1) || (agt.indexOf("msie 7.0") != -1))
        {
            var clsName=$(".ooyalavideo").parent().attr('class');
            $(".ooyalavideo").css("width", "300px");
            $(".ooyalavideo").parent().css("width", "300px");
            if (clsName.toLowerCase() == "col-sm-4 bottom-offset-medium")
            {
                $(".ooyalavideo").parent().css("float", "left");
                $(".ooyalavideo").parent().css("margin-right", "20px");
            }
           
        }

        //if( ($.browser.msie) && (parseFloat($.browser.version) <= 8)){
        //    $(".ooyalavideo").css.style("width:300px;")
        //}

    });
</script> 
