  <script type="text/javascript">
    angular.bootstrap( $("#angularHeader").get(0), ["masthead"]);
    $(function() {
        $("#selectAnotherAccountModal").on("show.bs.modal", function(event) {
            var iframeSrc = $(event.relatedTarget).data("iframe-src");
            var height=$(event.relatedTarget).data("height");
            if(iframeSrc) {
                height= height?height:"110px";
                $("#selectAnotherAccountModal iframe").css( "height",height );
                $("#selectAnotherAccountModal iframe").attr("src", iframeSrc);
            }
        });
        $("#selectAnotherAccountModal").on("hide.bs.modal", function(event) {

            $("#selectAnotherAccountModal iframe").attr("src", "");

        });
    });
</script> 
