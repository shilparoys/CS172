  <script>
    function ShowVideoPlayerAllVideos(VideoEmbedCode, containderId) {

        var oPlayer = null;

        OO.ready(function () {
            oPlayer = OO.Player.create(containderId, VideoEmbedCode, {
                "width": "100%",
                "wmode": "opaque",
                "platform": "html5-priority",
                onCreate: function (player) {
                    resizeVideo(containderId);
                    player.subscribe(OO.EVENTS.SIZE_CHANGED, "myPage", function (eventName) {
                        resizeVideo(containderId);
                    });
                }
            });
        });
    }

    function resizeVideo(containderId) {
        var div = document.getElementById(containderId);
        var _newHeight;
        var _oWidth = div.offsetWidth; // get current width of player
        _newHeight = _oWidth * 0.5625;
        div.style.height = _newHeight + "px";
    }

    /* IE 8 & 9 fix for OOYAlA vidoes rendering */
    var agt=navigator.userAgent.toLowerCase();
    if ((agt.indexOf("msie 8.0") != -1) || (agt.indexOf("msie 7.0") != -1))
    {
        var clsName=$(".ooyalavideo").parent().attr('class');
        $(".ooyalavideo").css("width", "300px");
        $(".ooyalavideo").parent().css("width", "300px");
        if (clsName.toLowerCase() == "col-sm-4 bottom-offset-medium")
        {
            $(".ooyalavideo").parent().css("float", "left");
            $(".ooyalavideo").parent().css("margin-right", "20px");
        }
       
   }

</script> 
