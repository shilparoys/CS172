  <script src="/javascripts/vendor/jquery-2.1.1.min-7e1c143b.js" type="text/javascript"></script>
  <script src="/javascripts/vendor/jquery.bgswitcher-dbb1077d.js" type="text/javascript"></script>
  <script src="//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit" type="text/javascript"></script>
  <script src="http://w.sharethis.com/button/buttons.js" type="text/javascript"></script>
  <script src="/javascripts/vendor/jquery.opentable-d0e8ce08.js" type="text/javascript"></script>
  <script src="/javascripts/all-74b9df76.js" type="text/javascript"></script> 
  <script>
  $(function() {
    $("#hero").bgswitcher({
      images: ["/images/photos/pld_gr_015-469e0252.jpg",
      "/images/photos/pld_gr_012-85b2fc31.jpg", "/images/photos/pld_gr_006-feff3162.jpg"]
    });
  
    $.getJSON('http://webcms.kimptongroup.com/api/hotels/76/promotions.json?callback=?', function(data) {
        $(data).each(function(index, promotion) {
          var tpl = slideTemplate(promotion.title, promotion.paragraph, promotion.url, promotion.ga_link_title, promotion.link_text, promotion.link_type);
          $('#hero .container').append(tpl);
        });
      }
    ).then(function() {
      rotateSlides(0);
    });
  
  });
  
  function rotateSlides(index) {
    $('.slide-content:eq(' + index + ')')
      .animate({'opacity': "1"}, 1000, function() {
        $(this).css({'zIndex' : 1});
      })
      .animate({'opacity': "1"}, 4500)
      .animate({'opacity': "0"}, 1000, function() {
        (index === $('.slide-content').length-1) ? index=0 : index++;
        $(this).css({'zIndex' : 0});
        rotateSlides(index);
      });
  }
  
  function slideTemplate(title, paragraph, url, ga_link_title, link_text, link_type) {
    if(link_type === 'booknow') {
      var link = '<a href="' + url + '" data-ga="' + ga_link_title + '" title="Book Now" class="btn book-now">Book Now</a>';
    } else {
      var link = '<a href="' + url + '" data-ga="' + ga_link_title + '" title="Promotion">' + link_text + '</a>';
    }
  
    return '<div class="slide-content"><h3>' + title + '</h3><p>' + paragraph + '</p>' + link + '</div>';
  };
  
  /*lightbox
    jQuery(document).ready(function() {
    $.fancybox(
      '<a href="https://gc.synxis.com/rez.aspx?Hotel=61879&Chain=10179&promo=PLDOPN" data-ga="type:crossdomain,link;label:reservations/push/lightbox/grand-opening"><img src="/images/photos/pld-gr-012-lightbox-cc9251e4.jpg" width="800" height="510" alt="" /></a>',
      {
        autoDimensions: false,
        autoScale: false,
        scrolling: 'no',
        width: 800,
        height: 510,
        padding: 1,
        hideOnContentClick: true,
        hideOnOverlayClick: true
      }
    );
  });*/
</script>   
