			var owlReference = $("#owl-example"); // save jquery object reference into owl variable
			
			owlReference.owlCarousel({
				items:3,
				itemsDesktop: [1199,3],
				navigation: false,
				pagination: false,
				scrollPerPage: false,
				slideSpeed: 1000,
				dragBeforeAnimFinish: true,
				lazyEffect: "fade",
			});
		
