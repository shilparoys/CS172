                <div class="content clearfix"> 
                 <div class="field field-name-field-image-reference field-type-node-reference field-label-hidden">
                  <div class="field-items">
                   <div class="field-item even">
                    <article about="/photo/345991/2015/02/23/edit-thon-logo" typeof="sioc:Item foaf:Document" class="node node-image node-topic_stream_headlines node-topic-stream-headlines node-image-topic-stream-headlines node-published node-not-promoted node-not-sticky author-sns165 odd clearfix" id="node-image-345991"> 
                     <div class="content clearfix"> 
                      <div class="field field-name-field-image field-type-image field-label-hidden">
                       <div class="field-items">
                        <div class="field-item even">
