       <script type="text/javascript">
jQuery(window).bind('resize', (function() {
    var timer;
    return function() {
      clearTimeout(timer);
      timer = setTimeout(function() {
        Cufon.refresh();
      }, 50); // possibly more than 50
    };
})());

$(document).ready(function(){

    $('#_content_sssw_en_home_homeMain').layerSlider({
        skinsPath : '/etc/designs/sssw/plugins/layerslider/skins/',
        skin : 'sssw',
        firstLayer:1,
        animateFirstLayer:true,
        thumbnailNavigation : 'bullets',
        hoverPrevNext : false,
        slideDelay: 7000,
        durationIn: 1500,
        durationOut:1500, //matched for simplicity
        responsive : true,
        responsiveUnder : 940,
        sublayerContainer : 900,
        navPrevNext: false,
        navStartStop: false,
        navButtons: true,
        randomSlideshow: false,
        loops: 3
    });

    var w = $(window).width();
    if (w <= 800) {
        //Cufon.set('fontSize', '12px');
        Cufon.set('fontSize', '12%');
        Cufon.replace('.ls-textLink');
        Cufon.refresh('.ls-textLink');
    }

    if (w <= 320) {
        //Cufon.set('fontSize', '10px');
        Cufon.set('fontSize', '10%');
        Cufon.replace('.ls-textLink');
        Cufon.refresh('.ls-textLink');
    }
});
</script> 
