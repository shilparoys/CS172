     <input type="hidden" name="y" value="PROD_FLICKR"> 
     <input type="hidden" name="source" value="product.landing_search"> 
     <input type="hidden" name="locale" value="en_US"> 
     <input id="searchInput" type="text" name="question_box" autocomplete="off" title="Search input" value="" onblur="blurSearchHelp()" onfocus="focusSearchHelp();"> 
     <a href="#" title="Click to search" onclick="if (isValidSearch()) document.landingSearch.submit();" id="srch-btn">Search Help</a> 
    </form> 
