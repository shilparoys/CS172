      <script>
            require(['jpmc/$', 'jpmc/ui/floater', 'jpmc/event/handler/toggle', 'commonui/component/media-manager'], function($, Floater, Toggle, MediaManager) {
                var f;
                var $search = $('#search');
                var $toggle = $search.find('.jpui-search-toggle');
                var $searchContent = $('#searchContainer');
                
                
 
                function destroyFloater() {
                    f.trigger.destroy();
                    f.destroy();
                    $toggle.removeClass('active');
                }
 
                function initFloater() {
                    var $clone = $searchContent.clone();
                    $search.append($clone);
                    f = new Floater( {
                            positionTarget: $toggle,
                            closeOnDocumentClick: true,
                            focus: true
                        } );
 
                    f.render( $clone );
 
                    f.trigger = new Toggle( $toggle, {
                        closeOnToggleClick: true,
                        disableHoverOnTargetClick: true,
                        displayOn: Toggle.Action.CLICK,
                        target: f
                    } );     
                    
                    var origShow = f.show;
                    var origHide = f.hide;
                    
                    f.show = function(){
                        origShow.call(f);
                        $toggle.addClass('active');
                    };
                    f.hide = function(){
                        origHide.call(f);
                        $toggle.removeClass('active');
                    };
                }
 
                MediaManager.register({
                    small : {
                        init : initFloater,
                        terminate : destroyFloater
                    },
                    xsmall : {
                        init : initFloater,
                        terminate : destroyFloater
                    }
                });
            });
 
			function doSearch() {                    
				if (document.getElementsByName('q').length > 1)
				{
					searchterm = document.getElementsByName('q')[1].value;
				}
				else
				{
					searchterm = document.getElementsByName('q')[0].value;
				}
				window.location.href='/home/search?site=cfsTWF&q='+searchterm;				
            }
 
 
			
        </script> 
