  <script type="text/javascript">

	$(document).ready(function(){
		setTextField('#price-min', '0');
		setTextField('#price-max', '100000+');
		
		$('#browse-used-model').attr('disabled','disabled');
		
		$('#browse-pg-used-make').change(function (){
			$('#browse-used-model').removeAttr('disabled');
		});
				
		if (!$('#zip').val()){
			//$('.select-car input.btn-search-used').addClass('disabled-search-used');
			$('.select-car input.btn-search-used').attr('disabled', 'disabled');
		}
			
	    $('#zip').keyup( function(){
	    	enable_btn();
        });		

	    $('#zip').blur( function(){
	    	enable_btn();				
        });	
       	
	});
					
	function enable_btn(){
		var passed = true;
		var zipRE=/^\d{5}$/;
		if($('#zip').val().search(zipRE)==-1){
			passed = false;
		}
		if(passed){
			//$('.select-car input.btn-search-used').removeClass('disabled-search-used');
			$('.select-car input.btn-search-used').removeAttr('disabled');
		} else {
			//$('.select-car input.btn-search-used').addClass('disabled-search-used');
			$('.select-car input.btn-search-used').attr('disabled', 'disabled');
		}
	}			
	
	function setTextField(fld, text){
		$(fld).focusin(function() {
			if($(this).css('color') != 'rgb(0, 0, 0)' && $(this).css('color') != '#000000'){
				$(this).attr('value', "").css('color', '#000000');
			}
		});
		$(fld).focusout(function() {
			if ($(this).attr('value') == ""){
				$(this).attr('value', text);
				$(this).css('color', '#aaaaaa');
			}
		});
		if($(fld).attr('value') == ""){
			$(fld).attr('value',text);
		}
		if($(fld).attr('value') != text){
			$(fld).css('color', '#000000');
		}
		$(fld).keypress( function(evt){
			var charCd = (evt.which != null) ? evt.which : evt.keyCode;
			if (charCd > 31 && (charCd < 48 || charCd > 57))
	    		return false;
	 		return true;
		});
	}
