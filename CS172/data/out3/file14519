  <script language="javascript">
			function ocultaSeccion(sec)
			{
				if (sec != "")
				{
					var menu = document.getElementById(sec.toString());
					if (menu.style.display == "block")
					{
						menu.style.display = "none";
					}
					else
					{
						menu.style.display = "block";
					}
				}
			}
		</script> 
