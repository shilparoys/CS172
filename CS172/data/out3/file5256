  <script type="application/javascript" src="scripts/validation.js"></script> 
  <script type="application/javascript">
		$(function() {
			$('#brickInfo').hide();
			$('#presidentSociety').hide();
			$('#other').hide();
			$("#phone").mask("(999) 999-9999");
			$("#cell").mask("(999) 999-9999");
			$('#getBrick').change(function() {
				var temp = $("#getBrick").val();							
				if(temp == "1") {
					$('#brickInfo').show('fast');
					$("#getBrick").val(0);
				} else {
					$('#brickInfo').hide('fast');
					$("#getBrick").val(1);
				}
		  	});
			$('#amount').change(function() {
				if($('#amount option:selected').val() == "Other") {
					$('#other').show();
				} else {
					$('#other').hide();	
					$('#otherAmount').val('');					
				}
				if($('#amount option:selected').val() == "1000") {
					$('#presidentSociety').show();
				} else {
					$('#presidentSociety').hide();					
				}
			});
			$('#frmDonate').validate({
				errorPlacement: function(error, element) {}
			});
		});
	</script> 
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"> 
