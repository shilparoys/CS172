function  _CF_checkCFForm_1(_CF_this)
    {
    if  (!_CF_hasValue(_CF_this.Criteria, "TEXT" )) 
        {
        if  (!_CF_onError(_CF_this, _CF_this.Criteria, _CF_this.Criteria.value, "Please enter one or more search terms in the search box."))
            {
            return false; 
            }
        }

    return true;
    }

