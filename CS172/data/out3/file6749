  <script type="text/javascript">
$(document).ready(function() {
$('#subnav').menuTree({
// multiOpenedSubMenu: true,
});
//Hide (Collapse) the toggle containers on load
$("#focusbg").hide();
$("#focusbg.open").show();
$("#openclose").click(function () {
$(this).toggleClass("open");
});
//Slide up and down effect on click
$("#openclose").click(function(){
$(this).next("#focusbg").slideToggle("slow,");
});
//bepaal negatieve left absolute positie laatste element in topnavigatie
function changeLeftPos(w) {
var leftvalue = w - 113;
leftvalue = leftvalue - 113;
$("#nav ul li:last-child ul").css('left', leftvalue);
};
$("#nav ul li.last").hover(function () {
changeLeftPos($("#nav ul li.last a span").width());
});
});
// form styling
$(".field,textarea").focus(function () {
$(this).addClass("active"); //change color onFocus
});
$(".field,textarea").blur(function () {
$(this).removeClass("active"); //change color onBlur
});
$("#zoekenalt input").focus(function () {
$(this).addClass("active"); //change color onFocus
$("#zoekenbuttalt").addClass("active");
});
$("#zoekenalt input").blur(function () {
$(this).removeClass("active"); //change color onBlur
$("#zoekenbuttalt").removeClass("active");
});
</script> 
