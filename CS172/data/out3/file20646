            <div id="content"> 
             <script type="text/javascript">
    $(function () {
        // Language dropdown behaviour
        $(".LanguageSelector")
            .each(function () {
                toggleAssociatedElements(this);
            })
            .change(function () {
                var selectedValue = toggleAssociatedElements(this);
                var parent = $(this).parents(".ProductSection");
                var attr = parent.attr("platform-code");
                if (attr) {
                    var matchingOption = parent.find("#" + selectedValue + " .PlatformSelector option[code=" + attr + "]");
                    if (matchingOption.length !== 0) {
                        parent.find("#" + selectedValue + " .PlatformSelector").val(matchingOption.val());
                        parent.find("#" + selectedValue + " .PlatformSection").hide();
                        parent.find("#" + matchingOption.val()).show();
                    }
                }
            });

        // Platform dropdown behaviour
        $(".PlatformSelector")
            .each(function () {
                var matchingOptions = $(this).find("option[value$='" + $.client.os + "']");
                if (matchingOptions.length !== 0) {
                    $(this).val($(matchingOptions[0]).attr("value"));
                }
                toggleAssociatedElements(this);
            })
            .change(function () {
                var selectedValue = toggleAssociatedElements(this);
                var selectedOption = $(this).find("option[value=" + selectedValue + "]");
                var selectedCode = selectedOption.attr("code");
                $(this).parents(".ProductSection").attr("platform-code", selectedCode);
            });

        function toggleAssociatedElements(selectorElement) {
            var sectionClass = $(selectorElement).hasClass("LanguageSelector") ? "LanguageSection" : "PlatformSection";
            var parentClass = $(selectorElement).hasClass("LanguageSelector") ? "ProductSection" : "LanguageSection";
            var selectedValue = $(selectorElement).val();
            var parent = $($(selectorElement).parents("." + parentClass));
            parent.find("." + sectionClass).hide();
            parent.find("#" + selectedValue).show();
            return selectedValue;
        }

        // Apply mismatched OS confirmation to cart button
        $("a[id$='lbAddToCart']").each(function (index) {
            if ($(this).attr("platform") != '' && $(this).attr("platform").indexOf($.client.os) < 0) {
                $(this).click(function () {
                    return confirm(platformConflictConfirmationText);
                });
            }
        });

    });
</script> 
