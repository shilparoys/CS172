    $(function () {
        $("img.BackupMediaHelpIcon")
            .mouseover(function () {
                $(this).attr("src", "/Static/Images/question_mark_14x14_over.png");
            })
            .mouseout(function () {
                $(this).attr("src", "/Static/Images/question_mark_14x14.png");
            })
            .click(function () {
                showDialog({ "title": "Backup media available in most countries", "htmlContent": "Backup media for this product may be available from your shopping cart. Availability depends on the country from which you are ordering.", "width": 500, "height": 300, "dialogClass": "BackupMediaHelpDialog", "elementID": "backupMediaDialog" });
            });
    });
</script> 
