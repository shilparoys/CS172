function clearStatus()
{
window.status = "";
}
function setStatbar(statbar)
{
var strStatbar=unescape(statbar);
window.status=strStatbar;
}
function onLoadComplete()
{
if( menus_included == 1 )
document.onmouseover = document_mouseover;	// defined in menu_ie.js
