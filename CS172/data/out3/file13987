    function Om_InitialSetup() {
        
        dl.contentcategory = "Browse";
dl.channel = "hmhco: shop: education curriculum: adult education";
dl.level2 = "education-curriculum";
dl.level3 = "adult-education";
dl.pageName = "Adult Education";
dl.hier1 = "shop:education-curriculum:adult-education";
dl.membershiptype = "Unregistered";


        //Derived fields
        dl.membershiptypepathing = dl.membershiptype + ':' + dl.pageName;
        if (DEBUG_CLICK)
            alert('Default tracking complete');
    }

    function Om_AssetLibrarySearch() {
       
        
        dl.contentcategory = "Browse";
dl.channel = "hmhco: shop: education curriculum: adult education";
dl.level2 = "education-curriculum";
dl.level3 = "adult-education";
dl.pageName = "Adult Education";
dl.hier1 = "shop:education-curriculum:adult-education";
dl.membershiptype = "Unregistered";


        //Derived fields
        dl.membershiptypepathing = dl.membershiptype + ':' + dl.pageName;
        dl.searches = 'yes';
        dl.internalsearchterm = '';
        dl.internalsearchscope = '';
        dl.channel = 'Asset Library Search Results';
        dl.level2 = '';
        dl.level3 = '';
        dl.hier1 = 'Asset Library Search Results::';
        dl.pageName = 'ALSR::';
        dl.contentcategory = 'Asset Library Search Results';

        if (DEBUG_CLICK)
            alert('Default tracking complete');
    }

    function Om_AssetLibrary() {
        
        
