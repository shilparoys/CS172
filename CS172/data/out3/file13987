    function ControlNav(key, subKey)
    {
        $J("a[data-key=\"" + key + "\"]").parent().addClass("active");
        $J("ul[data-key=\"sub_" + key + "\"]").show();
        $J("li[data-key=\"ter_" + subKey + "\"]").addClass("active");

    }
