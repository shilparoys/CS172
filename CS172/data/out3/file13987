        function Om_ProductViewed(isbn) {
            var oView = GetOmObj();

            oView.pageName = "Quick View";
            oView.events = "prodView";
            oView.products = ';' + isbn + ';;;;eVar1=' + oView.pageName;
            oView.prop16 = isbn;
            oView.hier1 = s.hier1;
            oView.prop5 = "Quick View";
            oView.linkTrackEvents = "prodView";
            oView.linkTrackVars += ",products,eVar1,prop16,hier1,prop5";
            oView.tl(true, 'o', 'Quick View');

            if (DEBUG_CLICK)
                alert('Quickview Click complete');
        }

        function Om_AddProductToCart(title, currentCartCount, quantity, productCode, isProductDetail) {
            var oAdd = GetOmObj();

            if (currentCartCount == 1) {
                oAdd.events = "scAdd,scOpen";
                oAdd.linkTrackEvents = "scAdd,scOpen";
            }
            else {
                oAdd.events = "scAdd";
                oAdd.linkTrackEvents = "scAdd";
            }

            oAdd.pageName = s.pageName;
            oAdd.eVar1 = '';
            oAdd.products = ';' + productCode + ':' + title;
            oAdd.hier1 = s.hier1;

            oAdd.linkTrackVars += ",products,eVar1,hier1,events";
            oAdd.tl(true, 'o', 'Add to Cart');

            if (DEBUG_CLICK)
                alert('Add to Cart complete');
        }

