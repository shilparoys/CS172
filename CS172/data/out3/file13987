    jsonContainers['SpecialProducts'] = [];

    $(document).ready(function () {
        $('.facetDropdown').bind('change', function(e) {
            window.location.href=$J(this).val();
        });
        $('#sortSelect').bind('change', function(e) {
            window.location.href=$J(this).val();
        });

        $('.Grade_dropdown_div').bind('change', function(e) {
            var grade = $J('.Grade_dropdown_div select').val();
            //|| $J('.Grade_dropdown_div li.dk_option_current').text(); // kluge to get selected value
            if(grade!="" )
            {
                window.location.href=grade;
            }
        });

        $('.State_dropdown_div').bind('change', function(e) {
            var state = $J('.State_dropdown_div select').val();
            //|| $J('.State_dropdown_div li.dk_option_current').text(); // kluge to get selected value
            if(state!="" )
            {
                window.location.href=state;
            }
        });

        $('.Year_dropdown_div').bind('change', function(e) {
            var copyRightYear = $J('.Year_dropdown_div select').val()
            //|| $J('.Year_dropdown_div li.dk_option_current').text(); // kluge to get selected value
            if(copyRightYear!="" )
            {
                window.location.href=copyRightYear;
            }
        });
    });
</script> 
