} 
]
}];

            xRf.cookieBelowMinAge = 'submittedTooMuch';

            function SetupSearchMenuOptions() {
                var opdta = '';
                $J.each(xRf.searchMenuData[0].menu, function (indx, val) {
                    opdta += "<option value=\"" + val.value + "\">" + val.label + "</option>";
                });
                opdta += '<option value="Asset Library">Asset Library</option>';

                if (opdta) {
                    $J('#searchSelect select').empty().append(opdta);
                }

                $J('.searchTrigger').click(function (e) {
                    e.preventDefault();
                    var selectedSegSecParms = $J('#searchSelect select').val();
                    selectedSegSecParms = selectedSegSecParms || '?segment=All;mm=all';
                    var searchTerm = $J.trim($J('#searchSection input').val());
                    Om_TrackSearchTerm(searchTerm);
                    if (searchTerm.length > 0) {
                        if (selectedSegSecParms == 'Asset Library') {
                            window.location.href = '/booksellers/asset-library?q=' + searchTerm;
                        }
                        else {
                            window.location.href = '/search' + selectedSegSecParms +
                                '' + ';q=' + searchTerm;
                        }
                    }
                    return false;
                });
                // searchEntry lies inside the div.searchEntry so prevent event propagation
                $J('#searchEntry').click(function (e) {
                    e.preventDefault();
                    // do nothing
                    return false;
                });

                $J('#searchEntry').bind('keydown', function (e) {
                    if (e.which === 13) { // return
                        e.preventDefault();
                        $J('.searchTrigger').trigger('click');
                        return false;
                    }
                })
            }

            var adacSettings = {
                account: "sp1004efe8",
                searchDomain: "http://hmh.guided.ss-omtrdc.net",
                inputElement: "input#searchEntry",
                inputFormElement: false,
                inputTriggerElement: ".searchTrigger",
                inputTriggerEvent: "click",
                delay: 300,
                minLength: 3,
                maxResults: 10,
                browserAutocomplete: false,
                queryCaseSensitive: false,
                startsWith: false,
                searchOnSelect: false
            }

            $J(document).ready(function () {
                $J(adacSettings.inputElement).autocomplete(adacSettings);
                SetupSearchMenuOptions();
            });

            //Omniture
            $J("#checkoutAnchorDown, #checkoutAnchorUp").click(function () {
                if (typeof Om_CartFunction == 'function')
                    Om_CartFunction('checkout', '');
            });


        </script> 
