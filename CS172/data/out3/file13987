        function om_SearchResults() {
            
            dl.contentcategory = "Browse";
dl.channel = "hmhco: shop: education curriculum: adult education";
dl.level2 = "education-curriculum";
dl.level3 = "adult-education";
dl.pageName = "Adult Education";
dl.hier1 = "shop:education-curriculum:adult-education";
dl.membershiptype = "Unregistered";


            //Derived fields
            dl.membershiptypepathing = dl.membershiptype + ':' + dl.pageName;
            dl.searches = 'yes';
            dl.internalsearchterm = '';
            dl.internasearchscope = '';
            dl.channel = 'Search Results';
            dl.level2 = ' results';
            dl.level3 = ' results;';
            dl.hier1 = 'Search Results: Results:';
            dl.pageName = 'Search Results::';
            dl.contentcategory = 'Search Results';

            // debug
            if (DEBUG_CLICK) {
                alert('Search Results');
            }
        }

        //Function to capture analytics for Product Detail Page//
        function Om_ProductDetailViewed(productCode) {
            // Default Variables which needs to be captured. Replaces Om_Intialsetup()//
            
            dl.contentcategory = "Browse";
dl.channel = "hmhco: shop: education curriculum: adult education";
dl.level2 = "education-curriculum";
dl.level3 = "adult-education";
dl.pageName = "Adult Education";
dl.hier1 = "shop:education-curriculum:adult-education";
dl.membershiptype = "Unregistered";


            //Derived fields
            dl.membershiptypepathing = dl.membershiptype + ':' + dl.pageName;
            dl.customprodview = 'yes';
            dl.prodView = 'yes';
            dl.pageName = ':';
            dl.hier1 = 'Shop:Product Detail Page';
            dl.level2 = 'Product Detail Page';
            dl.contentcategory = 'Product Detail';

            // debug
            if (DEBUG_CLICK) {
                alert('Product Detail Page for ' + productCode);
            }
        }

        function om_ComponentBrowse() {

            var gradeSelect = jQuery('.Grade_dropdown_div option:selected').text();
            gradeSelect = gradeSelect.substring(0, gradeSelect.indexOf('(') - 1);
            var stateSelect = jQuery('.State_dropdown_div option:selected').text();
            stateSelect = stateSelect.substring(0, stateSelect.indexOf('(') - 1);
            var sortSelect = jQuery('#sortSelect option:selected').text();

            // Default Variables which needs to be captured. Replaces Om_Intialsetup()//
            
            dl.contentcategory = "Browse";
dl.channel = "hmhco: shop: education curriculum: adult education";
dl.level2 = "education-curriculum";
dl.level3 = "adult-education";
dl.pageName = "Adult Education";
dl.hier1 = "shop:education-curriculum:adult-education";
dl.membershiptype = "Unregistered";


            //Analytics Variables which replaces omniture/ComponentBrowse
            dl.contentcategory = 'Browse';

            //Derived fields
            dl.membershiptypepathing = dl.membershiptype + ':' + dl.pageName;
            dl.programhpvideoplay = gradeSelect;
            dl.paymenttype = stateSelect;
            dl.programhpshopnow = sortSelect;

            if (DEBUG_CLICK)
                alert('Category Browse complete');
        }

