        // tracks go math keyword search widget go click
        function Om_SearchKeywordGoClick(keyword) {
            try {
                var oGoClick = GetOmObj();
                oGoClick.prop34 = keyword;
                oGoClick.eVar18 = keyword;

                oGoClick.linkTrackVars += ",prop34,eVar18";
                oGoClick.tl(true, 'o', 'Go Math Search Keyword');
                // debug
