			function CortarTexto(texto, longitud)
			{
				if((texto.value.length >= longitud) && (event.keyCode != 8))
				{ 			 
					texto.value = texto.value.substr(0, longitud - 1);
					alert('Has superado el tamaño máximo de caracteres permitido');
					texto.focus();
					return false;											
				}
