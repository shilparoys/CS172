  <!-- Script que activa el cuadro de texto txtOtros sólo si la opción seleccionada es "otros"
			 coloca el foco en dicho cuadro de texto
		--> 
  <script languaje="javascript">
			function activarOtros()
			{
				if (document.sugerencias.comboRelacion.value == "otros")
				{
					document.sugerencias.txtOtros.style.visibility="visible";
					document.sugerencias.txtOtros.focus();
				}
				else
				{
					document.sugerencias.txtOtros.style.visibility="hidden";
				}
				
			}

		</script> 
  <!-- Script que activa el cuadro de texto txtAsunto sólo si la opción seleccionada es "Otros"
