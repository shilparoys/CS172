        $(document).ready(function(){
		$("nav#primary-nav ul.menunavbar").each(function () {
			var primaryNav = $(this);
			var parents = primaryNav.children("li");
			primaryNav.hover(function(){
				parents.children("nav#primary-nav ul.dropdown").stop(true, false).animate({height:"250px"});
			}, function(){
				parents.children("nav#primary-nav ul.dropdown").stop(true, false).animate({height:"0"});
			});
		});
	/* Secondary Dropdown */
	$('.dropdown-toggle').dropdown();
      });  
