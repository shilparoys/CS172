  <link href="https://www.grad.wisc.edu/greu/css/greu.css" rel="stylesheet" type="text/css"> 
  <link href="https://www.grad.wisc.edu/css/jquery_css/jquery-ui-1.8.10.custom.css" rel="stylesheet" type="text/css"> 
  <script type="text/javascript" src="https://www.grad.wisc.edu/greu/js/greu_prod.js"></script> 
  <script type="text/javascript" src="https://www.grad.wisc.edu/js/jquery-1.9.1.min.js"></script> 
  <script type="text/javascript">
	// let's make this bad boy case insensitive
	jQuery.expr[':'].Contains = function(a,i,m){
	     return jQuery(a).text().toUpperCase().indexOf(m[3].toUpperCase())>=0;
	};
	
	function checkForEnter(evt)
	{
		if (evt.keyCode == 13) 
		{
			searchKeywords();
			return false;
		}
	}

	function searchKeywords()
	{
		var v_searchWords = $("#keywords").val();
		v_searchWord = v_searchWords.toUpperCase();

		// this is implementing the extended jQuery method
		$("div:Contains('"+ v_searchWords +"')").parent(".hideable").removeClass("hideable").addClass("showable");
		$("div:Contains('"+ v_searchWords +"')").parent(".hideworthy").removeClass("hideworthy").addClass("showworthy");

		// now, let's see how many we transmogrified
		var n = $(".showable").length;

		// no point in hiding everything, so we test
		if(n>0)
		{
			$(".small").hide();
			$("#search").hide();
			$(".hideable").hide("slow", function() {
				$(".hideworthy").hide("slow");
			});
		}
		else
		{
			alert("No program descriptions contain the keyword(s) you've entered.");
		}
	}

	function unhide()
	{
		$(".small").show();
		$("#search").show();
		$(".hideable").show("fast", function() {
			$(".hideworthy").show("fast");
		});

		//and finally reset to original conditions
		$(".showable").removeClass("showable").addClass("hideable");
		$(".showworthy").removeClass("showworthy").addClass("hideworthy");

	}

	</script> 
