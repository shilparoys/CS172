  <script type="text/javascript">

        $(document).ready(
            function () {
                var $_GET = {};
                document.location.search.replace(/\??(?:([^=]+)=([^&]*)&?)/g, function () {
                    function decode(s) {
                        return decodeURIComponent(s.split("+").join(" "));
                    }
                    $_GET[decode(arguments[1])] = decode(arguments[2]);
                });

                if ($_GET["error"] == "LoggedOutLoR") {
                    alert("You were logged out of your session due to a timeout and your request for a letter of recommendation was not sent. Please log in and retry your request.");
                }                
            }
        );

        function showRegistrationDialog() {
                $('#RegistrationDialog').dialog("open"); 
        }
    </script> 
