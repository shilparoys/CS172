      $('#group-photo-wrapper').css({'width':$('#group-photo-wrapper img').width(),
                         'height':$('#group-photo-wrapper img').height()
      })
      var tooltipDirection;
      for(var i=0;i<$('.group-photo-pin').length;i++) {
        if ($(".group-photo-pin").eq(i).hasClass('group-photo-pin-down')) {
          tooltipDirection = 'tooltip-down';
        } else {
          tooltipDirection = 'tooltip-up';
        }
        // append the tooltip
        var content = [
          "<div style='left:",
          $(".group-photo-pin").eq(i).data('xpos'),
          "px;top:",
          $(".group-photo-pin").eq(i).data('ypos'),
          "px' class='",
          tooltipDirection,
          "'><div class='tooltip'>",
          $(".group-photo-pin").eq(i).html(),
          "</div></div>"
        ].join('');
        $("#group-photo-wrapper").append(content);
      }
      $('.tooltip-up, .tooltip-down').mouseenter(function(){
        $(this).children('.tooltip').fadeIn(100);
      }).mouseleave(function(){
        $(this).children('.tooltip').fadeOut(100);
      })
    });
