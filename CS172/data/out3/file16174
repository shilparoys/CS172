  <script type="text/javascript">
function onBefore() { 
	//activeBanner = $(".slideshow > img:visible")
	var img = $(this).find("img");
	var imgTitle = img.attr('alt')
	var imgText = img.attr('title')
	
	var bannerLinkText = $(this).attr("title");
	var bannerLink = $(this).attr("href");
    $('#ribbon-title').hide().delay(100).fadeOut("fast");
	$('#ribbon-text').hide().delay(100).fadeOut("fast");
    $('#ribbon-title').hide().html(imgTitle).delay(300).fadeIn("fast");
	//alert(imgText)
	$('#ribbon-text').hide().html(imgText).delay(300).fadeIn("fast");
	$("#bannerButton").html(bannerLinkText)
	$("#bannerButton").attr('href', bannerLink)
}
function onAfter() { 
	
}
$(document).ready(function() {
 	$('#ribbon-title').hide();
	$('#ribbon-text').hide();
	$(".slideshow").cycle({timeout:7000, prev: '.prev', next: '.next', before: onBefore});
	$(".spotlight").setEqualHeight();
	$('.play-btn').toggle(function() {
			$('.slideshow').cycle('pause');
			$('.play').attr('src', 'images/play-arrow.png');
		}, function() {
			$('.slideshow').cycle('resume');
			$('.play').attr('src', 'images/pause-btn.png');	
});
});

    </script> 
