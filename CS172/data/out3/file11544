								ev.stopPropagation();
								var active = $("#tabs473008494").tabs( "option", "active" );
								var total = $("#tabs473008494 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs473008494").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs473008494").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script>  
 </body>
</html>