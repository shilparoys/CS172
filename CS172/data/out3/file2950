        .pinboard.cols5 .pinned { width:19%; }
        .pinboard.cols6 .pinned { width:16%; }
        .pinboard.single .pinned {
            width: 97%;
            float: none;
        }

        .pinboard .pinned-inner { cursor: pointer; padding: 15px 5px 0px 15px; }
        .pinboard .pinned-inner img { width: 100%; display: block; }
        .pinned-twipsy { border: none; cursor: pointer; }
        .pinned-image, 
        .pinboard .liner { opacity: 0.9; border: 1px solid rgba(255, 255, 255, 0.4); }
        .pinboard .pinned-image:hover,
        .pinboard .liner:hover { opacity: 1; border-color: rgba(255, 255, 255, 0.6); } 
        .pinboard-modal { width: 800px; background: #000; border: 4px solid #000; height: 655px; top: 40px!important; overflow: hidden; }
        .pinboard-modal .close { font-size: 24px; }
        .pinboard-modal .modal-header { border:0; }
        .pinboard-modal .modal-body { padding: 0; }
        .pinboard .liner { padding: 10px; background-color: #333; color: #fff; font-size: 16px; line-height: 20px; }
        .pinboard.small .liner { font-size: 12px; line-height: 16px; }
        .pinboard .display-ad-window { display: block; text-decoration: none; }
        .pinboard .liner div { overflow: hidden; }
        
        #mt .pinboard .pinned { min-width:33%; }
    </style> 
