  <script language="JavaScript">



function onLoad_Body() {

    var name = navigator.appName;
    var vers = navigator.appVersion;
    vers = vers.substring(0,1); // or 0,4  could return 4.5 instead of just 4

    //alert('In onLoad');

    //if (name != "Microsoft Internet Explorer") 
    //{
    //    return;
    //}

    document.cookie = "cookies=on"; 

    if (document.cookie.indexOf("cookies=on") != -1) 
    { 
        return;
    } 
    
    if (document.cookie.indexOf("cookies=on") == -1) 
    { 
        alert('Your browser does not accept cookies, this will adversely effect the functionality of this site. Please change your browsers security setting before proceeding.'); 
        cookieMessage = true; 
        window.location="/treasury/taxation/CookiesTurnedOff.htm"
        
    } 
    else
    {
        cookieMessage = false; 
    }

    
    


}

function checkCookies()
{

    //alert('In checkCookies');
    document.cookie = "cookies=on"; 

    if (document.cookie.indexOf("cookies=on") == -1) 
    { 
        return true;
    } 

    return false;

}

function submitForm(submitFormValue) {
    
    //alert("In submit form " );
    if (submitFormValue == "no")
    {
        //alert("Will not submit form " );
        return;
    }
    
    //alert("Your log in credentials have been carried over and this form will be automatically submitted for you." );
    alert("You are being automatically logged into the application.");
    document.Login.submit();
    
    
}






  </script>
 </head>   
