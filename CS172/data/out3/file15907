  <script type="text/javascript">

// SLOGAN
    var height = $(".slogan:eq(0)").height();
    height = (60-height)/2;
    $(".slogan").css('padding-top',height);

// TEXT FIT - FitText makes font-sizes flexible. Use this plugin on your responsive design for ratio-based resizing of your headlines.
// https://github.com/davatron5000/FitText.js
    $("#parallaxtext").fitText();
    $("#overlay1").fitText(0.8, { minFontSize: '10px', maxFontSize: '25px' });
    $("#overlay2").fitText(1.2, { minFontSize: '10px', maxFontSize: '60px' });
    $("#overlay3").fitText(1, { minFontSize: '10px', maxFontSize: '15px' });


</script> 
  <script type="text/javascript">
    $(window).load(function(){

// STICKY HEADER
    $(".nav:eq(0)").sticky({ topSpacing: 0 });
    $("#mainimage").sticky({ topSpacing: 60 });


// APPLY BUTTON
    var width =$("#panoramic").width();
    var height =$("#panoramic").height();
    width = (0 - ((width / 2) + 75)); // find the middle and negate
    height = (0 + ((height / 2) - 56));
    $("#applybutton").css('margin-left',width);
    $("#applybutton").css('margin-bottom',height);

// TEXT OVERLAY
    height = $("#slideshowcover").height();
    var height = (0 - height);
    var windowwidth = $( window ).width();
    
    if( windowwidth > 768 ){
        height = (height / 2) + (height * .15);
    }
    
    var halfheight = (height * .75);
    halfheight = 0 - halfheight;
    
    $("#overlaywrap").css('margin-top',height);
    $("#overlaywrap").css('padding-bottom',halfheight);


// LAUNCH SLIDESHOW
    $('#slideshowcoverlink').on('click', function(event) {
        event.preventDefault();
        $('#slideshow').data('galleria').openLightbox();
        // $('#slideshow').data('galleria').enterFullscreen();
        // $('div.galleria-thumbnails').find('div.galleria-image').first().trigger("click");
        // $('#slideshow').data('galleria').exitFullscreen();
   });


// BROWSER RESIZE REFRESH
    $( window ).on("orientationchange", function() {
        location.reload();
    });
    
    var windowwidth = $( window ).width();
    
    if( windowwidth > 768 ){
        $( window ).resize(function() {
            location.href = window.location;
        });
    }
    


});
</script>  
