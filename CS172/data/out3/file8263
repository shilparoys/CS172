        <div id="formBody" style="position: absolute; left: -10000px;"> 
         <script type="text/javascript">
			//<![CDATA[
				jQuery.noConflict();

				var addSchoolClicked = false;

				var profJudgement = "";
				var isCOTW = "false";
				var numSelectedSchools = "0";
				var displayAlertMessage = "false";

				// Put all your code in your document ready area
				jQuery(document).ready(function($) {
					// Do jQuery stuff using $
					loadUpdate();
					jQuery("#addSchoolLink").hide();
				    
				});
				
				var loadUpdateOnComplete = function()
				{
					registerUpdateHidden();	
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();				
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox(); 
					toggleAddButton(); 
				}
				
				var bindCheckbox = function()
				{
					jQuery("input:checkbox").each(function(index) {
					  	jQuery(this).click(function() {
						   toggleAddButton();
						});
					});
				}

			    function onRemoveLink() {
			    	if (isCOTW == "true" && addSchoolClicked == false && profJudgement == "false") {
			    		jQuery("#addSchoolLink").show();
			    	}
			    }

				function updateHiddens() {
				    jQuery("#currentStateValue").val(jQuery("#searchCriteriaSchoolState").val());
				    jQuery("#currentNameValue").val(jQuery("#searchCriteriaSchoolName").val());
				    jQuery("#currentCityValue").val(jQuery("#searchCriteriaSchoolCity").val());
				    jQuery("#currentYearValue").val(jQuery("#schoolSearchCycle").val());
				    jQuery("#currentSchoolCodeValue").val(jQuery("#searchCriteriaSchoolCode").val());

				}

				function updateSearchValues(state, city, name, year, schoolCode) {
				    jQuery("#searchCriteriaSchoolState").val(state);
				    jQuery("#searchCriteriaSchoolName").val(name);
				    jQuery("#searchCriteriaSchoolCity").val(city);
				    jQuery("#schoolSearchCycle").val(year);
				    jQuery("#searchCriteriaSchoolCode").val(schoolCode);
				}

				function toggleAddButton(rowSelected) {
					
					var selectedRowsSize;
					if(rowSelected){
						selectedRowsSize = 1;
					}
					else{
						selectedRowsSize = jQuery("input:checkbox:checked").size();
					}

					if(searchResultSize < 1 && selectedSchoolsSize < 1){
						jQuery("#moveRightBtnDiv").hide();
						jQuery("#moveRightBtnDisabledDiv").hide();
					}
					else if(selectedSchoolsSize < 10 && selectedRowsSize > 0){
						jQuery("#moveRightBtnDiv").show();
						jQuery("#moveRightBtnDisabledDiv").hide();
					}
					else {
						jQuery("#moveRightBtnDiv").hide();
						jQuery("#moveRightBtnDisabledDiv").show();
					}
				}
				
				function setFocusToSortBySchoolName() {
					jQuery("a[id='searchSchoolsTable:sortBySchoolName']").focus();
				}
				
				function setFocusToSortByBestMatch() {
					jQuery("a[id='searchSchoolsTable:sortByBestMatch']").focus();
				}
				
				function focusTop(){
					if(jQuery("#msgerrmessage").is(":visible")){
						jQuery("#skipLink").prop("tabindex",-1).focus(); 
					}
				}
				
				var resetDatatablePage = function()
				{
					var lengthVar = jQuery("div[id$='searchSchoolsPanel'] div").length;
					if(lengthVar === 3)
					{
						null;
					}
					else
					{
						searchSchoolsVar.getPaginator().setPage(0);
					}
					
				}
				
				var searchOnComplete = function(args)
				{
					resetDatatablePage(); 
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox(); 
					toggleAddButton();  
					primeFixes.paginatorFix508Button("en_US");
					setFocusToInstructionPanel();
					hideProcessingModal(); 
					
					if(typeof args !== 'undefined' && typeof args.schoolCriteria !== 'undefined')
					{
						var state = args.schoolCriteria.state;
						var city = args.schoolCriteria.city;
						var schoolName = args.schoolCriteria.schoolName;
						var cycle = args.cycle;
						var schoolCode = args.schoolCriteria.schoolCode;
						
						updateSearchValues(state, city, schoolName, cycle, schoolCode); 
					}
					
				}
				
				var seachSchoolCodeOnComplete = function()
				{
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox(); 
					toggleAddButton();  
					primeFixes.paginatorFix508Button("en_US");
					setFocusToInstructionPanel();
					hideProcessingModal();
				}
				
				var paginatorOnComplete = function(args)
				{
					var state = args.schoolCriteria.state;
					var city = args.schoolCriteria.city;
					var schoolName = args.schoolCriteria.schoolName;
					var cycle = args.cycle;
					var schoolCode = args.schoolCriteria.schoolCode;
					
					updateSearchValues(state, city, schoolName, cycle, schoolCode); 
					pageUpdate();
				}
				
				var pageEventOnComplete = function()
				{
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox();  
					toggleAddButton(); 
					
					updateHiddens();
					hideProcessingModal();
					setTimeout(function(){primeFixes.paginatorFix508Button("en_US");},1000);
					setFocusToInstructionPanel();
				}
				
				var addSchoolOnComplete = function()
				{
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox(); 
					toggleAddButton(); 
					focusTop(); 
					primeFixes.paginatorFix508Button("en_US");
					setFocusToInstructionPanel();
				}
				
				var sortByAjaxComplete = function(args)
				{
					var state = args.schoolCriteria.state;
					var city = args.schoolCriteria.city;
					var schoolName = args.schoolCriteria.schoolName;
					var cycle = args.cycle;
					var schoolCode = args.schoolCriteria.schoolCode;
					
					updateSearchValues(state, city, schoolName, cycle, schoolCode); 
					sortUpdate();
				}
				
				var sortByOnComplete = function()
				{
					adgustSearchSchoolTableHight();
					adgustselectedSchoolTableHight();
					primeFixes.fixVerticalScrollBar(); 
					bindCheckbox();  
					toggleAddButton(); 
					updateHiddens();
					hideProcessingModal();
					setTimeout(function(){primeFixes.paginatorFix508Button("en_US");},1000);
					setFocusToInstructionPanel();
				}
				
                var removeOnComplete = function()
                {
                	bindCheckbox(); 
                	toggleAddButton(); 
                	adgustSearchSchoolTableHight();
                	adgustselectedSchoolTableHight();
                	primeFixes.fixVerticalScrollBar(); 
                	jQuery('a[id*=\'selectedSchoolsTable:0\']').focus();
                }
				
				/**
				 * Determines where to set focus on the page.
				 * If the error box is visible, set focus near the top of the page so that the error box is in view.
				 * If the error box is not visible, set focus on the instruction text above the search results panel
				 */
                var setFocusToInstructionPanel = function()
				{
					if(jQuery("#errorbox").is(':visible'))
					{
						jQuery("#messageContainer").prop("tabindex",0).focus();
					}
					else
					{
						var instructionPanel = jQuery("div[id$='searchInstructionPanel']");
					
						if(instructionPanel !== undefined)
						{
							// In order to position the school search results correctly on the page
							// the focus first goes to the bottom bar and then up to the instruction panel 
							jQuery("#bottombar").prop("tabindex",0).focus();
							instructionPanel.prop("tabindex",0).focus();
						}
					}
				};
				                
                function registerUpdateHidden(){
                	jQuery("#searchCriteriaSchoolState").change(function () {
                        updateHiddens();
                    });
                    jQuery("#searchCriteriaSchoolName").change(function () {
                        updateHiddens();
                    });
                    jQuery("#searchCriteriaSchoolCity").change(function () {
                        updateHiddens();
                    });
                    jQuery("#schoolSearchCycle").change(function () {
                        updateHiddens();
                    });
                    jQuery("#searchCriteriaSchoolCode").change(function () {
                        updateHiddens();
                    });
                }

				function newWindow(url) {
					var newwindow;
					newwindow=window.open(url,'schoolSelectionNewWindow','height=768, width=1024, scrollbars=yes, location=no');
					if (window.focus) {
						newwindow.focus()
					}
					newwindow.resizeTo(1024,768);
				}
				
				var adgustselectedSchoolTableHight = function()
				{
					var tdHight = 73;
					var numberOfRows = jQuery("#selectedSchoolsTablePanel .ui-datatable-scrollable-body tr").size();
					var listHight = tdHight*numberOfRows - (numberOfRows -1);
					
					if(numberOfRows > 7)
					{
						listHight = tdHight*7 - (numberOfRows -1);
					}
					jQuery("div[id$='selectedSchoolsTablePanel'] .ui-datatable-scrollable-body").css("height", listHight+"px");
					
				}
				
				var adgustSearchSchoolTableHight = function()
				{
					var tdHight = 73;
					var numberOfRows = jQuery("div[id$='schoolResultPanelBlock'] .ui-datatable-scrollable-body tr").size();
					var listHight = tdHight*numberOfRows - (numberOfRows -1);
					
					if(numberOfRows > 7)
					{
						listHight = tdHight*7 - (numberOfRows -1);
					}
					jQuery("div[id$='schoolResultPanelBlock'] .ui-datatable-scrollable-body").css("height", listHight+"px");
					
				}

			//]]>

			
		</script> 
