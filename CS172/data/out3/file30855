     <script type="text/javascript">
		var url = document.URL;
		var staff = "";
		if( (url.search("/user/") != -1) && (staff != '') ){
			var redirect = url.replace("/user/", "/staff/");
			window.location=redirect;
		}

		if( (url.search("/staff/") != -1) && (staff == '') ){
			var redirect = url.replace("/staff/", "/user/");
			window.location=redirect;
		}
	</script> 
