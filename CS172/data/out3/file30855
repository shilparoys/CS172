			/* <![CDATA[ */
			function relations(following, followed, u) {
				var profile_self = (u && (u.name == 'Kristina Pritchett | South Jersey Times'));
				if ( u && u.profile ) {
					conditional_block((profile_self), 'edit_profile');
					conditional_block((profile_self), 'profile_actions');
					conditional_block((!profile_self), 'follow_actions');
				}
				if (profile_self) {
					// var link = 'https://signup.nj.com/dashboard/edit.html?return_to=' + encodeURIComponent(document.URL);
					var link = 'https://signup.nj.com/dashboard/edit.html?return_to=http%3A%2F%2Fprofile';
					jQuery(".edit_profile_link").attr("href", link);
				}
				if (u && u.name && document.getElementById('not_following') && !following && !profile_self) {
					document.getElementById('not_following').style.display = 'block';
				}
				conditional_block(followed, 'followed_10722382');
				conditional_block(following, 'following_10722382');
			}
			/* ]]> */
