    <script type="text/javascript">
    $(document).ready(function() {
        $(".pagedrop_container").hide();
        $(".active").slideToggle("slow,");
        $(".active").prev("h2.page_drop").addClass("active"); 

        /*$("h2.page_drop").pagedrop(function(){
            $(this).addClass("active"); 
            }, function () {
            $(this).removeClass("active");
        });*/

        $("h2.page_drop").click(function(){		
            $(this).next(".pagedrop_container").slideToggle("slow,");

            if ($(this).hasClass("active")){
                $(this).removeClass("active");
            }else{
                //pagedrop all other open containers first
                $(".active").next(".pagedrop_container").slideToggle("slow,");
                $(".active").removeClass("active");
                $(this).addClass("active"); 
            }
        });
    });
</script> 
