        function drawAnnouncements() {
    		$.ajax({
    			url : "/json/announcements.json",
    			type : "GET",
    			dataType : "json",
    			cache : false,
    			success : function(data) {
	    			$.each(data.announcements, function(index, announcement) {
	    				$("#announcements").append('<div class="alert">' + announcement + '</div>');
	    			});
    			},
    			error : function(jqXHR, textStatus, errorThrown) {
					console.log("Error fetching announcements: " + textStatus + " " + errorThrown);
    			}
    		});

        }
        
        function setFocus() {
            var f = null;
            if (document.getElementById) {
                f = document.getElementById("loginForm");
            } else if (window.loginForm) {
                f = window.loginform;
            }
            if (f) {
                if (f.j_username.value == null || f.j_username.value == "") {
                    f.j_username.focus();
                } else {
                    f.j_password.focus();
                } 
            }
        }
        
        $(function(){
        	drawAnnouncements();
        	setFocus();
        });
        
        //-->
