$( document ).ready(function() {
  /* the following click events allow you to use the link in the login tabs to refocus on the first input of that tabs form when clicked */
  $('#app-login').click(function() {
    $('#ApplicantEmailAddress').focus()
  });

  $('#member-login').click(function() {
    $('#MemberEmailAddress').focus()
  });

  $('#rec-login').click(function() {
    $('#RecommenderEmailAddress').focus()
  });

  /* if an error is returned by the server, alert screenreaders on page load */
  if( $('.validation-summary-errors').is(':visible') ) {
    $('body').prepend('<div class="for-screenreader-only" role="alert">Login was unsuccessful. Please correct the errors and try again. The user name or password provided is incorrect.</div>')
  }
  
                            
});
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=113435562081260";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script> 
  </section>   
 </body>
</html>