  <script language="javascript" type="text/javascript">


        ///////////////////////////////
        function trim(str) { while (str.substring(0, 1) == ' ') { str = str.substring(1, str.length); } while (str.substring(str.length - 1, str.length) == ' ') { str = str.substring(0, str.length - 1); } return str; }

        //////////////////////////////twelve/////////////////////////////////////////////////
        function chkddlCampus() {
            if (document.getElementById('ddlCampus').selectedIndex == 0) {
                alert("Select Campus");
                document.getElementById('ddlCampus').focus();
                return false;
            }
            return true;
        }
        function chkddlAcademicsHeadermenu() {
            if (document.getElementById('ddlAcademicsHeadermenu').selectedIndex == 0) {
                alert("Select header menu");
                document.getElementById('ddlAcademicsHeadermenu').focus();
                return false;
            }
            return true;
        }

        function chkddlAcademicsSubmenu() {
            if (document.getElementById('ddlAcademicsSubmenu').selectedIndex == 0) {
                alert("Select menu");
                document.getElementById('ddlAcademicsSubmenu').focus();
                return false;
            }
            return true;
        }

        function validation() {
            var flag = false;
            flag = chkddlCampus();
            if (flag == true)
                flag = chkddlAcademicsHeadermenu();
            else
                return false;
            if (flag == true)
                flag = chkddlAcademicsSubmenu();
            else
                return false;


            return flag;
        }
    </script> 
