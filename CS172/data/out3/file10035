    <h1>Stations - All Networks</h1> 
    <script type="text/javascript">

//<![CDATA[

function load()
{
	if (GBrowserIsCompatible())
	{
		//Create our map
		var map = new GMap2(document.getElementById("map"));
		map.setCenter(new GLatLng(25, -90), 2);
		map.addControl(new GLargeMapControl);
		map.addControl(new GMapTypeControl);
		
		
		setPoint(map, 41.7, -70.731, '01MA', 'BRAINWAVE', '660099');
		
		setPoint(map, 57.075, -2.797, 'ABAB', 'Aboyne Academy', 'FFFC17');
		
