  <script type="text/javascript">
$(function(){
	$(".feedback_form form").bind("submit",function(){
		if ($("#name").val() != '' && $("#email").val() != '' && $("#comment").val() != '') {
		$(".feedback_form").slideUp(500,function(){
			$.post("includes/send_feedback.cfm", { name: $("#name").val(), email: $("#email").val(), comment: $("#comment").val(), contactemail: "accessibility@cornell.edu"},function(data){
					$(".feedback_form").html(data);
					$(".feedback_form").slideDown(500);
				});
		});
		} else {
			alert("Please enter your name, email, and message");	
		}
		return false;

		});
});
</script> 
