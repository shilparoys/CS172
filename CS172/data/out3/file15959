         <!-- NECESSARY JAVASCRIPT --><script type="text/javascript" language="javascript">
function doTheSlide(daSlide)
{
if($(daSlide).css("display") == 'none')
{
//Close all other divs before you open the one selected
$('.slider').each(function(){
if($(this).css("display") == 'block')
{
$(this).animate({height:'toggle', opacity: 0}, 500);
}
});

//Open the selected slider
$(daSlide).animate({height:'toggle', opacity: 1}, 500);
}
else
{
$(daSlide).animate({height:'toggle', opacity: 0}, 500);
}

}
</script></p> 
