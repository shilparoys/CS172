     <script type="text/javascript">
var client = client || new TC.JSONRPCClient( 'https://www.typepad.com/secure/services/json-rpc' );
YAHOO.util.Event.on( 'generate-captcha', 'click', function() {
    doMethod( 'System.GenerateCaptcha', {}, function( r ) {
        YAHOO.util.Dom.get('captcha-img').src = 'https://www.typepad.com/secure/services/captcha?code=' + r.code_encrypted;
        YAHOO.util.Dom.get('captcha-chal').value = r.challenge;
    } );
} );
</script> 
     <script type="text/javascript">
( function() {
    var Event = YAHOO.util.Event,
        Dom   = YAHOO.util.Dom;

    Event.onDOMReady( function() {
        var f = Dom.get( 'username' );
        if( f ) {
            // IE will die if you try to focus a hidden element, and
            // username could be hidden (openid, etc). So catch exceptions.
            try { f.focus(); }
            catch( e ) { }
        }

        var customProviderConfig = {"google":{"show_template":null,"button_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/openid_buttons/google.gif","prompt":"Sign in using your Google account","unavailable":null,"identifier_template":"https://www.google.com/accounts/o8/id","order":"0","show_standard_button":"1","no_register":null,"list_caption":"Google"},"vox":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/vox.png","show_template":"1","prompt":"Sign in using your Vox URL","unavailable":"1","identifier_template":"http://{ident}.vox.com/","order":"5","show_standard_button":null,"no_register":"1","field_caption":"Vox URL","list_caption":"Vox"},"blogger":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/blogger.png","show_template":"1","prompt":"Sign in using your Blogger URL","unavailable":null,"identifier_template":"http://{ident}.blogspot.com/","order":"3","show_standard_button":null,"no_register":null,"field_caption":"Blogspot URL","list_caption":"Blogger"},"aol":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/aol.png","show_template":"0","prompt":"Sign in using your AOL Username","unavailable":null,"identifier_template":"http://openid.aol.com/{ident}","order":"2","show_standard_button":null,"no_register":null,"field_caption":"AOL Username","list_caption":"AOL/AIM"},"livejournal":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/livejournal.png","show_template":"1","prompt":"Sign in using your LiveJournal Username","unavailable":null,"identifier_template":"http://{ident}.livejournal.com/","order":"4","show_standard_button":null,"no_register":null,"field_caption":"LiveJournal Username","list_caption":"LiveJournal"},"myspace":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/myspace.png","show_template":"1","prompt":"Sign in using your MySpace URL","unavailable":null,"identifier_template":"http://myspace.com/{ident}","order":"0","show_standard_button":null,"no_register":null,"field_caption":"MySpace URL","list_caption":"MySpace"},"yahoo":{"show_template":null,"button_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/openid_buttons/yahoo.png","prompt":"Sign in with your Yahoo account","unavailable":null,"identifier_template":"https://me.yahoo.com/","order":"1","show_standard_button":null,"no_register":null,"list_caption":"Yahoo!"},"wordpressdotcom":{"field_icon_url":"https://www.typepad.com/.s/static.typepad.com/.shared/images/profile/service_icons/wordpress.png","show_template":"1","prompt":"Sign in using your WordPress.com username","unavailable":null,"identifier_template":"http://{ident}.wordpress.com/","order":"6","show_standard_button":null,"no_register":null,"field_caption":"WordPress.com username","list_caption":"WordPress.com"}};

        var hideShowForms = function( type ) {
            var el = Dom.get( type );
            var fieldsets = [
                Dom.get( type + '-facebook' ),
                Dom.get( type + '-gplus' ),
                Dom.get( type + '-twitter' ),
                Dom.get( type + '-custom' ),
                Dom.get( type + '-openid' )
            ];

            var selection = el.value;
            var customProvider = null;
            var matches;

            /* If the user came to the signin form with the dropdown preselected for an OpenAuth service
               and they change the drop down *back* to Typepad we need to show the standard Typepad login
               form. After we unhide the Typepad form we set the dropdown back to whatever active_form is */
            if( selection == "typepad" ) {
                Dom.removeClass('sf-tp', 'hidden'); /* signin-form typepad */
                Dom.removeClass('it-tp', 'hidden'); /* introtext typepad */
                Dom.addClass('it-oa', 'hidden');    /* introtext openauth */
                Dom.removeClass('ou-tp', 'hidden'); /* oruse typepad */

                /* Set selection to active_form */
                selection = "typepad";
                el.value = selection;

                /* remove Typepad as an option since it's pointless now */
                el.remove(0);
            }

            if (matches = selection.match(/^c_(.*?)$/)) {
                selection = 'custom';
                customProvider = matches[1];
            }

            for( var i = 0; i < fieldsets.length; i++ ) {
                if( fieldsets[ i ] == null ) {
                    continue;
                }
                if( type + '-' + selection == fieldsets[ i ].id ) {
                    Dom.removeClass( fieldsets[ i ], 'hidden' );
                } else {
                    Dom.addClass( fieldsets[ i ], 'hidden' );
                }
            }

            if (customProvider) {
                configureCustomForm(customProviderConfig[customProvider], type);
                Dom.get(type + "-custom-active-form").value = "c_" + customProvider;
            }
        };

        Event.on( 'auth', 'change', function( evt ) {
            Event.stopEvent( evt );
            hideShowForms( 'auth' );
        } );

        Event.on( 'reg', 'change', function( evt ) {
            Event.stopEvent( evt );
            hideShowForms( 'reg' );
        } );

        Event.on( [ 'change-to-register', 'change-to-register2' ], 'click', function( ev ) {
            Event.stopEvent( ev );
            Dom.addClass( 'signin-form', 'hidden' );
            Dom.addClass( 'learn-more', 'hidden' );
            Dom.removeClass( 'register-form', 'hidden' );
        } );

        Event.on( [ 'change-to-signin', 'change-to-signin2' ], 'click', function( ev ) {
            Event.stopEvent( ev );
            Dom.removeClass( 'signin-form', 'hidden' );
            Dom.removeClass( 'learn-more', 'hidden' );
            Dom.addClass( 'register-form', 'hidden' );
        } );

        Event.on( 'signin-form-typepad', 'submit', function( evt ) {
            Event.stopEvent( evt );

            /* Strip leading/trailing spaces out of username */
            var usr = Dom.get( 'username');
            usr.value = usr.value.replace(/(^\s*)|(\s*$)/gi,"");

            var pwd = Dom.get( 'password' ).value;
            var token = Dom.get( 'password-token' ).value;

            Dom.get( 'password-response' ).value = hex_sha1( token + hex_sha1( pwd ) );
            Dom.get( 'password' ).value = '';

            this.submit();
        } );

        var currentCustomForm = null;

        var updateCustomForm = function ( type ) {
            if (! currentCustomForm) return;

            var submitElem = Dom.get( type + '-custom-openid');
            var fieldElem = Dom.get( type + '-custom-username');
            var fullIdent = currentCustomForm.identifier_template;
            var enteredIdent = fieldElem.value;
            fullIdent = fullIdent.replace(/\{ident\}/, enteredIdent);
            submitElem.value = fullIdent;
        };

        var configureCustomForm = function (config, type) {
            currentCustomForm = config;

            var identChunks = config.identifier_template.split('{ident}');

            var fieldCaptionElem = Dom.get(type + '-custom-username-caption');
            var fieldElem = Dom.get(type + '-custom-username');
            var fieldPrefixElem = Dom.get(type + '-custom-username-prefix');
            var fieldSuffixElem = Dom.get(type + '-custom-username-suffix');

            if (identChunks.length == 2) {
                // Need a username text field
                fieldCaptionElem.innerHTML = config.prompt;

                if (config.show_template == "1") {
                    identChunks[0] = identChunks[0].replace(/^https?:\/\//, '');
                    if (identChunks[1].match(/^[^\/]*\/$/)) identChunks[1] = identChunks[1].replace(/\/$/, '');
                    fieldPrefixElem.innerHTML = identChunks[0];
                    fieldSuffixElem.innerHTML = identChunks[1];
                    fieldElem.style.width = '40%';

                }
                else {
                    fieldPrefixElem.innerHTML = "";
                    fieldSuffixElem.innerHTML = "";
                    fieldElem.style.width = '';
                }

                fieldElem.style.backgroundImage = "url("+config.field_icon_url+")";

                Dom.removeClass(type + '-custom-username-container', 'hidden');
                Dom.removeClass(type + '-custom-signin-button', 'hidden');
                Dom.addClass(type + '-custom-button-container', 'hidden');
            }
            else {
                // Just a "Go" button
                if (config.button_url) {
                    var buttonElem = Dom.get(type + '-custom-button');
                    buttonElem.src = config.button_url;
                    Dom.removeClass(type + '-custom-button-container', 'hidden');

                    if (config.show_standard_button) {
                        Dom.removeClass(type + '-custom-signin-button', 'hidden');
                    }
                    else {
                        Dom.addClass(type + '-custom-signin-button', 'hidden');
                    }
                }
                else {
                    Dom.removeClass(type + '-custom-signin-button', 'hidden');
                    Dom.addClass(type + '-custom-button-container', 'hidden');
                }
                Dom.addClass(type + '-custom-username-container', 'hidden');
            }

            updateCustomForm( type );
        };

        Event.on( 'auth-custom-username', 'change', function( evt ) {
            updateCustomForm( 'auth' );
        } );

        Event.on( 'reg-custom-username', 'change', function( evt ) {
            updateCustomForm( 'reg' );
        } );


        // If we've come back to the login form in the browser history,
        // we need to make sure we're showing the right form on load.
        hideShowForms( 'auth' );
        hideShowForms( 'reg' );

    } );
} )();
</script> 
