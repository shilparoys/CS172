window.fbAsyncInit = function() {
    FB.init({ appId: '55753818692', status: true, cookie: true, xfbml: true, oauth: true });
};
(function(d){
     var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement('script'); js.id = id; js.async = true;
     js.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
     //ref.parentNode.insertBefore(js, ref);
     //d.getElementsByTagName('head')[0].appendChild(js);
     document.getElementById('fb-root').appendChild(js, ref);
   }(document));
</script> 
     <script type="text/javascript">
function signin_Facebook( type ) {
  var form = document.getElementById( type + '-form-openid');
  if (form) {
    form.signin_facebook.value = 1;
    form.submit();
  }
}

function signin_Twitter( type ) {
  var form = document.getElementById( type + '-form-openid');
  if (form) {
     form.signin_twitter.value = 1;
     form.submit();
  }
}
</script> 
