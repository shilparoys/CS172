	$(document).ready(function(){
	        templatePath = "/cms/lib/NY01914031/Centricity/Template/8/";
		schoolLevel();
	});
	function initialize() {
		dynamicStylesheet();
		customLogo();
		useChannelForSection();
		templateSpecific();
		dynamicContent();
		globalIcons();
		loadFlash();
		
		// PHOTO GALLERY CONFIGURATIONS
		//DOC #sw-content-container10 -- INITIALIZE PHOTO GALLERY
		// LEFT SLIDESHOW
		$("#sw-content-container10.ui-hp div.ui-widget.app.gallery").galleryRotate({
			"width" : 355,
			"height" : 240
		});
		// PHOTO GALLERY CONFIGURATION
		//DOC #sw-content-container11 -- INITIALIZE PHOTO GALLERY
		// RIGHT SLIDESHOW
		$("#sw-content-container11.ui-hp div.ui-widget.app.gallery").galleryRotate({
			"width" : 355,
			"height" : 240
		});
		templateLibrary();
		drawCurve();
		if($("#sp-page").size() > 0) {
			sameHeight($(".column"));
		}
	}
	function schoolLevel() {
		// IF schoolTextFiles IS ON THEN ADD THE DYNAMIC TEXT FILE
		if(schoolTextFiles == "on")
		{
			$.ajax({
			  url: "/cms/lib/NY01914031/Centricity/Template/8/setup/home.txt",
			  dataType: "script",
			  success: function() {
			    initialize();
			  },
			  error: function()
			  {
			  	alert("An error has occured initializing setup files.");
			  }
			});
		}
		else
		{
			initialize();
		}
		
	}
	function dynamicStylesheet(){
		// .append NEW STYLE SHEET TO HEAD OF THE PAGE TO STYLE CHANNEL BAR ITEMS AND MYSTART ITEMS
		var dynStyleSheet = document.createElement('style');
		if(dynStyleSheet) {
			dynStyleSheet.setAttribute('type', 'text/css');
			var head = document.getElementsByTagName('head')[0];
			if(head) {
	      			head.appendChild(dynStyleSheet);
			}
			var dynStyles = "a, a:visited {" +
					"	color:" + linkColor + ";" +
					"	text-decoration:" + linkDecoration + ";" +
					"}" +
					"ul.sw-channel-list li.sw-channel-item > a {" +
					"	color: " + channelTextColor + ";" +
					"}" +
					"ul.sw-channel-list li.sw-channel-item.active > a, ul.sw-channel-list li.sw-channel-item.hover > a, ul.sw-channel-list li.sw-channel-item > a:hover {" +
					"	background-color: " + channelBarColorActive + ";" +
					"	color: " + channelTextColorActive + ";" +
					"}" +
					"ul.sw-channel-dropdown li a {" +
					"	color: " + channelDropdownTextColor + ";" +
					"}" +
					"ul.sw-channel-dropdown li a:hover {" +
					"	background-color: " + channelDropdownColorHover + ";" +
					"	color: " + channelDropdownTextColorHover + ";" +
					"}" +
					".sw-mystart-button, #sw-mystart-account, #sw-mystart-search, .sw-mystart-dropdown .sw-dropdown-list li {" +
					"	background-color: " + mystartButtonColor + ";" +
					"}" +
					".sw-mystart-button:hover, #sw-mystart-account:hover, .sw-mystart-dropdown .sw-dropdown-list li:hover {" +
					"	background-color: " + mystartButtonColorHover + ";" +
					"}" +
					".sw-mystart-button:hover a, #sw-mystart-account:hover {" +
					"	color: " + mystartTextColorHover + ";" +
					"}" +
					".sw-mystart-button a, .sw-mystart-button a:visited, #sw-search-input, #sw-mystart-account, .sw-mystart-dropdown .sw-dropdown-list li a {" +
					"	color: " + mystartTextColor + ";" +
					"}" +
					".sw-mystart-button a:hover, .sw-mystart-dropdown .sw-dropdown-list li:hover a, .sw-mystart-dropdown .sw-dropdown-list li a:hover  {" +
					"	color: " + mystartTextColorHover + ";" +
					"}" +
					"#gb-footer a:visited {" +
					"	color: " + footerTextColor + ";" +
					"}";
			var rules = document.createTextNode(dynStyles);
			if(dynStyleSheet.styleSheet){// IE
				dynStyleSheet.styleSheet.cssText = dynStyles;
			} else {
				dynStyleSheet.appendChild(rules);
			}
		}
	}
	function customLogo() {
		if(customLogos != "none") {
			// #gb-logo a img -- IF customLogos IS ON THEN ADD THE customLogo
			if(customLogos == "on") {
				$("#gb-logo a img").attr("src","/cms/lib/NY01914031/Centricity/Template/8/logos/home.png");
			} else {
				$("#gb-logo a img").attr("src","/cms/lib/NY01914031/Centricity/Template/8/logos/_logo.png");
			}
			$("#gb-logo").show(); 
		}
	}
	function useChannelForSection() {
		//DOC .ui-widget-header h1, .ui-widget-header -- SETS USE ChannelName FOR THE PAGELISTNAVIGATION HEADER IF ONE IS NOT PRESENT
		if(!$("div.sp.column.one .ui-widget-header h1").length > 0) {
			$("div.sp.column.one .ui-widget-header").append("<h1></h1>");
		}
	}
	function templateSpecific() {
		// BODY MODS
		$("li.sw-channel-item").prepend("<span class='sw-channel-background'></span>");
		$("ul.sw-channel-dropdown").each(function(){
			$("li:first a", this).css({"-moz-border-radius" : "10px 10px 0px 0px", "-webkit-border-radius" : "10px 10px 0px 0px", "-khtml-border-radius" : "10px 10px 0px 0px", "border-radius" : "10px 10px 0px 0px"});
			$("li:last a", this).css({"-moz-border-radius" : "0px 0px 10px 10px", "-webkit-border-radius" : "0px 0px 10px 10px", "-khtml-border-radius" : "0px 0px 10px 10px", "border-radius" : "0px 0px 10px 10px"});
		});
		$("div.gb.footer.two").append($("div#sw-footer-right"));
	}
	function dynamicContent() {
		//DOC #sp-breadcrumbs -- TOGGLES BREADCRUMBS ON/OFF
		if(breadcrumbToggle == "on"){
			$("#sp-breadcrumbs").show();
		}

		// MODIFY MYSTART
		//DOC .sw-myaccount-chevron -- REPLACE .sw-myaccount-chevron DROPDOWN ARROWS
		$(".sw-myaccount-chevron").text("▼");
		$(".sw-dropdown-arrow").text("▼");
	        $("#sw-search-button").text("search");
	        
		//DOC .sw-mystart-button.home -- SETS DISTTRICT BUTTON ON/OFF
		if(districtButton == "off"){
			$(".sw-mystart-button.home").hide();
		}
		
		// SETS BACKGROUND COLORS
		//$("body").css("background-color",backgroundColor);
		
		// SETS PRIMARY COLORS
		$(".gb-grunge-repeater-top.header, .gb-grunge-repeater-bottom.header, #hp-slideshow-outer, #gb-channel-list-outer, #sp-grunge-bottom-bg").css("background-color",primaryColor);
		 
		// SETS SECONDARY COLORS
		$("#hp-slideshow, #left-column").css("background-color",secondaryColor);
		
		// SETS ACCENT COLORS
		$("div#gb-footer-outer, .gb-grunge-repeater-top.footer, .gb-grunge-repeater-bottom.footer").css("background-color", accentColor);
		
		// SETS CHANNEL BAR STYLES
		//$("#sw-channel-list-container").css("background-color", channelBarColor);
		$("ul.sw-channel-dropdown").css("background-color", channelDropdownColor);
		//**DROPDOWN TEXT COLOR AND TEXT HOVER STATE IS SET IN THE DYNAMIC STYLESHEET FUNCTION**
		
		// SETS MYSTART STYLES
		$("#sw-mystart-outer").css("background-color", mystartBarColor);
		$(".sw-mystart-dropdown, .sw-dropdown, .sw-dropdown-selected").css({"background-color":mystartButtonColor,"color":mystartTextColor});
		$("#sw-search-button").css({"background-color":mystartButtonColorHover,"color":mystartTextColorHover});
		
		// SETS SCHOOLNAME     
		if(schoolNameText == "default"){
			schoolNameText = "Wellsville Central School District";
		}
		// #gb-name a -- SETS SCHOOLNAME
		$("h1#gb-name a").html(schoolNameText);
		// #gb-name a -- SETS SCHOOLNAME COLOR
		$("h1#gb-name a").css("color",schoolNameColor);
		
		// #gb-tagline -- SETS TAGLINE
		$("h2#gb-tagline").html(taglineText);
		// #gb-tagline -- SETS TAGLINE COLOR
		$("h2#gb-tagline").css("color",taglineColor);
		
		//SETS REGION COLORS
		//REGION 1
		//DOC #sw-content-container1 .ui-widget-header h1 -- HEADER COLOR
		$("div.ui-hp#sw-content-container1 .ui-widget-header h1").css({"color": region1HeaderColor, "border-color": region1HeaderColor});
		//DOC #sw-content-container1 -- BODY COLOR
		$("div.ui-hp#sw-content-container1").css("color",region1BodyColor);
		//DOC #sw-content-container1 -- LINK COLOR
		$("div.ui-hp#sw-content-container1 a, div.ui-hp#sw-content-container1 > a > span").css("color",region1LinkColor);
		
		//REGION 2
		//DOC #sw-content-container2 .ui-widget-header h1 -- HEADER COLOR
		$("div.ui-hp#sw-content-container2 .ui-widget-header h1").css({"color": region2HeaderColor, "border-color": region2HeaderColor});
		//DOC #sw-content-container2 -- BODY COLOR
		$("div.ui-hp#sw-content-container2").css("color",region2BodyColor);
		//DOC #sw-content-container2 -- LINK COLOR
		$("div.ui-hp#sw-content-container2 a, div.ui-hp#sw-content-container2 > a > span").css("color",region2LinkColor);
		
		//REGION 3
		//DOC #sw-content-container3 .ui-widget-header h1 -- HEADER COLOR
		$("div.ui-hp#sw-content-container3 .ui-widget-header h1").css({"color": region3HeaderColor, "border-color": region3HeaderColor});
		//DOC #sw-content-container3 -- BODY COLOR
		$("div.ui-hp#sw-content-container3").css("color",region3BodyColor);
		//DOC #sw-content-container3 -- LINK COLOR
		$("div.ui-hp#sw-content-container3 a, div.ui-hp#sw-content-container3 > a > span").css("color",region3LinkColor);
		
		//REGION 4
		//DOC #sw-content-container4 .ui-widget-header h1 -- HEADER COLOR
		$("div.ui-hp#sw-content-container4 .ui-widget-header h1").css({"color": region3HeaderColor, "border-color": region4HeaderColor});
		//DOC #sw-content-container4 -- BODY COLOR
		$("div.ui-hp#sw-content-container4").css("color",region4BodyColor);
		//DOC #sw-content-container4 -- LINK COLOR
		$("div.ui-hp#sw-content-container4 a, div.ui-hp#sw-content-container4 > a > span").css("color",region4LinkColor);
		
		//PAGE NAVIGATION
		//DOC .sp.column.one .ui-widget-header h1 -- HEADER COLOR
		$("div.sp.column.one .ui-widget-header h1").css("color", region1HeaderColor);
		// .sp.column.one -- BODY COLOR
		$("div.sp.column.one").css("color",region1BodyColor);
		// .sp.column.one -- LINK COLORE
		$("div.sp.column.one a, div.sp.column.one > a > span").css("color",region1LinkColor);
		
		// SETS FOOTER COLORS
		$("div#gb-footer, div.gb.footer.one a, div.gb.footer.two a, #sw-footer-copyright, #sw-footer-links").css("color", footerTextColor);
	}
	function globalIcons(){
		var iconSize = 50;
		for(i=0; i<numberIcons; i++){ 
			var globalLink = window["icon" + (i+1) + "Link"];
			var globalTarget = window["icon" + (i+1) + "Target"];
			var globalImage = window["icon" + (i+1) + "Image"];
			var globalText = window["icon" + (i+1) + "Text"];
			var flashIcon = "#icon-movie-number"+ (i+1);
			var currentFlash = "flash-icon-number" + (i+1);
			var currentFlashHolder = "flash-icon-holder-number" + (i+1);
			var sheetSize = iconSize * 20; 
			// GET POSITION OF THE ICON
			var myIconType = globalImage.split(":");
			// CHECK FOR SITEALIAS STRING
			globalLink = globalLink.replace("[SITEALIAS]","home");
			// CHECK FOR GLOBAL CALENDAR LINK
			globalLink = globalLink.replace("[SITECALENDARLINK]","/Page/2");
			// CHECK FOR GLOBAL SITEMAP LINK
			globalLink = globalLink.replace("[SITEMAPLINK]","/site/Default.aspx?PageType=15&SiteID=4&SectionMax=15&DirectoryType=6");
			var positionVal = Math.round(((80-iconSize)/2));
			$("#gb-global-icons").append("<a class='global-icon' id='icon-number" + (i+1) + "' href='" + globalLink +"' target='" + globalTarget + "'><span class='global-image' style='width:" + iconSize +"px;height:" + iconSize + "px;left:" + positionVal + "px' id='" + currentFlash + "'><span id='" + currentFlashHolder + "' class='global-image-holder'></span></span><span class='text'>" + globalText + "</span></a>");
			$("a.global-icon span.text").css("color",iconColor);
			if(myIconType[0] == "system"){
				var myPositions = myIconType[1].split("_");
		        	var myTop = (myPositions[0].replace("r","") - 1) * (iconSize*-1);
		        	var myLeft = (myPositions[1].replace("c","") - 1) * (iconSize*-1);
		        	// ANDROID CONDITION
			        var agent = navigator.userAgent.toLowerCase();
			        if(agent.indexOf("android") != -1 || agent.indexOf("series60") != -1 || agent.indexOf("symbian") != -1 || agent.indexOf("windows ce") != -1 || agent.indexOf("blackberry") != -1) {
			        	// DO NOTHING
			        } else {
					// LOAD FLASH ICON SHEET
					var so = new SWFObject("/cms/lib/NY01914031/Centricity/Template/8/icons/icons.swf", "icon-movie-number" + (i+1), sheetSize, sheetSize, "9", "#FFFFFF");
					so.addParam("wmode", "transparent");
				        so.addVariable("iconColor", iconColor);
				        so.write(currentFlashHolder);
			        }
			        // NON FLASH
			        if(!$(flashIcon).size()){
			        	$("#" + currentFlashHolder).append("<img width='" + sheetSize + "' src='/cms/lib/NY01914031/Centricity/Template/8/icons/icons-nonflash.gif' />").css({"top":myTopPX,"left":myLeftPX});
			        }
			        var myTopPX = myTop+"px";
				var myLeftPX = myLeft+"px";
				$("#" + currentFlashHolder).css({"top":myTopPX,"left":myLeftPX});
		        }
		        if(myIconType[0] == "custom"){
		        	$("#" + currentFlashHolder).append("<img width='" + iconSize + "' src='/cms/lib/NY01914031/Centricity/Template/8/icons/" + myIconType[1] + "'/>");
		        }
		}
		// FOR SELF-SERVICE
		$("body").append("<div id='global-icon-html' style='display:none'></div>");
		$("a.global-icon:last").clone().appendTo("#global-icon-html");
		var lastIconNum = "number" + numberIcons;
    		var regex = new RegExp(lastIconNum, "g");
		var globalIconHTML = $("#global-icon-html").html();
		globalIconHTML = globalIconHTML.replace(regex,"number99");
		$("#global-icon-html").empty().append(globalIconHTML);	
		$("#icon-number99 .text").html("new");	
		$("#flash-icon-holder-number99").css({"top":"0px","left":"0px"});
	}
	function loadFlash() {
		// NO FLASH
	}
	function templateLibrary(){
		// SHOWS ONLY THE TEMPLATES THATS IN THE SAME CATEGORY AS THE TEMPLATE CURRENTLY VIEWED
        	if(window.location.host == "templatelibrary.schoolwires.net") {
        		var parentAlias = "home";
			if(parentAlias == "silver" || parentAlias == "bronze"){
				$.ajax({
				  url: "/cms/lib2/SW00000032/Centricity/Domain/4/scripts/templateDropdown.js",
				  dataType: "script",
				  success: function() {
				  	modifyDropdown(parentAlias);
				  },
				  error: function() {
				  	alert("An error has occured initializing dropdown modification.");
				  }
				});
			}
		}
	}
	function sameHeight(selector) {
		selector.height(function(){
			$(this).height($(this).parent().height()-20);
		});
		// IF .children() OF SELECTOR RESIZES THEN RESET HEIGHT OF SELECTOR
		selector.children().resize(function(){
			selector.height("auto");
			selector.height(function(){
				$(this).height($(this).parent().height()-20);
			});
		});
	}
	function drawCurve() {
	
	}
</script> 
