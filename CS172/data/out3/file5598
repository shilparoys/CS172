  <script type="text/javascript">
    $(document).ready(function() {
        SetCookie('SWScreenWidth', screen.width);
        SetCookie('SWClientWidth', document.body.clientWidth);

        //If small browser window and responsive template and first load, reload to get the off-canvas menu
        var isResponsiveTemplatePreInit = 'False';
        var isResponsive = 'False';

        if (isResponsiveTemplatePreInit == 'False' && isResponsive == 'True' && document.body.clientWidth < 1024) {
            window.location.reload(true);
        } 

        $("div.ui-article:last").addClass("last-article");
        $("div.region .app:last").addClass("last-app");
    });
