     <div class="post clearfix"> 
      <p><script type="text/javascript"><!--
google_ad_client = "ca-pub-9984698449705657";
/* MCT Header Links */
google_ad_slot = "3479722540";
google_ad_width = 468;
google_ad_height = 15;
