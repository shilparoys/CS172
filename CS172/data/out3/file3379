  <script>
$(function(){
  var frm_cc = $('#contant_contact');
  frm_cc.submit(function () {
      $("#contant_contact_success").hide();
      $("#contant_contact_error").hide();
      $.ajax({
          type: frm_cc.attr('method'),
          url: frm_cc.attr('action'),
          data: frm_cc.serialize(),
          success: function(data) {
              if(data.data.status=="FAIL") {
                $("#contant_contact_error").show();
              } else {
                $("#contant_contact").hide();
                $("#contant_contact_success").show();
              }
          },
          error: function(data) {
              $("#contant_contact_error").show();
          }
      });
      return false;
  });
})
</script> 
