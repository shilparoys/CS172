			jQuery(document).ready(function($){
				$("#jf_tinycarousel").tinycarousel({
					duration:"300",
					intervaltime:10000,
					interval:true,
					axis:"y"
				});
			});
		
			(function($){
				$(window).load(function(){
					$("#jf_nivoslider").nivoSlider({
						effect:"fade",
						slices:15,
						boxCols:8,
						boxRows:4,
						animSpeed:900,
						pauseTime:5000,
						directionNav:true,
						controlNav:true,
						pauseOnHover:true,
						randomStart:false
					})
				})
			})(jQuery);
		
				jQuery(document).ready(function($){
					$(function(){
						var jf_window_h						= $(document).height(),
							jf_waitfor_tag					= $("#rt-transition"),
							jf_middlepanel_btn				= $("#jf_middlepanel_button"),
							jf_preloader					= $("#jf_preloader"),
							jf_indic						= $("#jf_preloader .jf_indicator"),
							jf_logo							= $("#jf_preloader .jf_preloader_logo"),
							jf_progress						= $("#jf_preloader .jf_progress"),
							jf_dur_after					= 1500,
							jf_fadeout_dur					= 500,
							jf_logo_fadein_dur				= 1500;
						jf_waitfor_tag.waitForImages({
							finished:function(){
								jf_middlepanel_btn.css({"left":"-450px"});
								jf_preloader.css({"display":"block","height":jf_window_h}).delay(jf_dur_after).fadeOut(jf_fadeout_dur,function(){
									jf_middlepanel_btn.animate({"left":"-35px"},500);
								});
							},
							each:function(n,c){
								jf_indic.stop(true).animate({width:parseInt(n*100/c)+"%"},"fast");
								jf_logo.fadeIn(jf_logo_fadein_dur);
								jf_progress.text(parseInt(n*100/c)+"%")
							},
							waitForAll:true
						});
					});
				});
			
  </script> 
  <!--[if IE]>
<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
<![endif]--> 
