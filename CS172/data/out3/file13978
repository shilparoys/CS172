  if(window.location.href.search('337224')>=0){ 
    // Remove .active class from the .active menu item
    jQuery('ul#green>li.active').removeClass('active');
    // Add .active class to the Release Notes Tab
    jQuery('ul#green>li.tab_release_notes').addClass('active');
  }
});
</script> 
