  <script id="template-purchase" type="text/template">
<% if(purchase.cards.length > 0 && purchase.use_existing) { %>
  <div class="use-existing purchase-form">
<% } else { %>
  <div class="purchase-form">
<% } %>

  <% if (purchase.amount_due > 0) { %>
    <label class="control-label next-to-stackable cc-info-label">Credit card:</label>

    <div class="controls form-inline stackable">

    <!-- NOTE: There are no name fields on credit card forms.
         This is to ensure no actual credit card data ever hits
         Proximate servers (including logs). This is needed to
         maintain PCI Compliance -->

    <!--// Visibility set via css control of parent -->
    <div class="existing-card clearfix">
      <select id="default-card-selector" class="next-to-options">
        <% for(var i=0; i < purchase.cards.length; i++) {
          card = purchase.cards[i]
          should_use = purchase.current_card == card.id
        %>
          <option value="<%= card.id %>" <%= should_use ? "selected" : "" %>><%= card.type %> ending in <%= card.last4 %></option>

        <% } %>
      </select>
      <% if(purchase.current_card != purchase.default_card) { %>
      <span class="check-area stackable beside-select">
        <a class="minor beside-block use-new-card">Use new card</a>
        <br/>
        <input id="default-card-check"
               type="checkbox"
               <%= purchase.make_default ? "checked" : "" %>
               class="input-default-check-span"><label for="default-card-check" class="minor muted">Make default</label>
      </span>
      <% } else { %>
      <span class="check-area stackable beside-select-single">
        <a class="minor beside-block use-new-card">Use new card</a>
      </span>
      <% } %>
    </div>

    <!--// Visibility set via css control of parent -->
    <div class="new-card clearfix">
      <span class="span3 stackable">
        <label class="mini-label" for="purchase-ccn">Credit card number</label>
        <input type="text"
               id="purchase-ccn"
               class="card-number check-pay-on-enter"
               value="<%= purchase.number %>"
               <% if(_.isPresent(purchase.bg_url)) { %>
               style="background-image: url(<%= purchase.bg_url %>)"
               <% } %>
               autocomplete="on"
               pattern="\d*"
               autocompletetype="cc-number"
               placeholder="•••• •••• •••• ••••" />
      </span>
      <span class="span2 stackable">
        <label class="mini-label" for="purchase-expiration">Expiration</label>
        <input type="text"
               id="purchase-expiration"
               class="card-expiry check-pay-on-enter"
               value="<%= purchase.expiration %>"
               autocomplete="on"
               autocompletetype="cc-exp"
               placeholder="MM / YY" />
      </span>
      <span class="span1 stackable csc-span">
        <label class="mini-label csc-label" for="purchase-cvc">Security Code</label>
        <input type="text"
               id="purchase-cvc"
               class="card-cvc check-pay-on-enter"
               value="<%= purchase.cvc %>"
               autocomplete="off"
               autocompletetype="cc-csc"
               placeholder="•••" />
      </span>

      <% if(purchase.cards.length > 0) { %>
      <span class="check-area stackable">
        <a class="minor use-existing-card">Use existing</a>
        <br/>
        <input id="save-card-check"
               type="checkbox"
