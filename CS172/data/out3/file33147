      <% for(var i=0; i < community.tickets.length; i++) {
      ticket = community.tickets[i]; %>
      <div id="ticket-<%= ticket.id %>-attendee-bar-quantity" class="ticket-quantity"></div>
      <div id="ticket-<%= ticket.id %>-attendee-bar-times-used"
           class="ticket-times-used"
           <% if(ticket.get("quantity") >= MAX_INT) { %>
           title="<%= _.numberFormat(ticket.get("times_used")) %> used"
           <% } else {
             num_left = ticket.get("quantity") - ticket.get("times_used");
             if (num_left === 0) {
              num_left = "<strong>None left</strong>"
             } else if (num_left < 0) {
              num_left = "<strong class='light-red'>Over by "+_.numberFormat(num_left*-1)+"</span>"
             } else {
              num_left = num_left + " remaining"
             }
           %>
           title="<%= _.numberFormat(ticket.get("times_used")) %> of <%= _.numberFormat(ticket.get("quantity")) %> (<%= ticket.percentUsed() %>%) used. <%= num_left %>"
           <% } %>
           ></div>
      <% } %>

      <div class="phantom-tickets"></div>
    </div>
  </div>

