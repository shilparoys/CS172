<div class="answer-choice-admin-wrap
            answer-choice-<%= answer_choice.view_state %>
            <%= answer_choice.more_settings ? "more-settings" : "" %>
           ">
  <div class="answer-choice-basic-admin-controls">
    <div class="basic-answer-choice-adj minor">

    <div id="delete-answer-choice<%= answer_choice.id %>"
         <% if(answer_choice.times_used > 0) { %>
         class="close-btn cant-close delete-answer-choice"
         title="Can't delete. Used <%= answer_choice.times_used %> times. Disable instead."
         <% } else { %>
         class="close-btn can-delete delete-answer-choice"
         title="Delete&nbsp;Discount Code"
         <% } %>
    ><%= _.i("remove") %></div>

      <a href="javascript:void(0);"
         <% if(answer_choice.more_settings) { %>
         class="btn toggled btn-icon-small btn-settings"
         title="Hide additional settings">
         <% } else { %>
         class="btn btn-icon-small btn-settings"
         title="Additional settings">
         <% } %>
         <%= _.i("cog") %>
      </a>

