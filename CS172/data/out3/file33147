  <script type="text/template" id="template-community-overview">

  <%
  now = moment.utc();
  now_day = Math.floor(now.unix() / 86400)

  if (_.isBlank(community.start_date)) {
    start = moment.utc(0);
  } else {
    start = moment.utc(community.start_date);
  }
  start_day = Math.floor(start.unix() / 86400)

  if (_.isBlank(community.end_date)) {
    end = moment.utc(MAX_INT * 1000);
  } else {
    end = moment.utc(community.end_date);
  }
  end_day = Math.floor(end.unix() / 86400)


  day_length = end_day - start_day
  over_year_start = Math.abs(now_day - start_day) >= 356
  over_year_end = Math.abs(now_day - end_day) >= 356

  d = "ddd, MMM Do"
  dy = "ddd, MMM Do YYYY"
  dt = "ddd, MMM Do, h:00 a"
  dta = "ddd, MMM Do @ h:00 a"
  dty = "ddd, MMM Do YYYY, h:00 a"
  dtya = "ddd, MMM Do YYYY @ h:00 a"
  t = "h:00 a"

  if (_.isBlank(community.start_date) && _.isBlank(community.end_date)) {
    date_str = "";
    when_str = "";
    when_class = "invalid-happening"
  } else if (_.isBlank(community.start_date)) {
    if (over_year_end) {
      date_str = "Ends "+end.format(dtya)
    } else {
      date_str = "Ends "+end.format(dta)
    }

    if (end_day == now_day) {
      when_str = "Ending Today"
    } else if (end_day < now_day) {
      when_str = "Ended "+end.fromNow();
    } else if (end_day > now_day) {
      when_str = "Ending "+end.fromNow();
    }

    if (end_day == now_day) {
      when_class = "happening-today"
    } else if (end_day > now_day) {
      if (end_day - now_day > 7) {
        when_class = "will-happen"
      } else if (end_day - now_day <= 7 && end_day - now_day > 1) {
        when_class = "happening-soon"
      } else if (end_day - now_day <= 1) {
        when_class = "happening-tomorrow"
      }
    } else if (end_day < now_day) {
      if (now_day - end_day > 7) {
        when_class = "already-happened"
      } else if (now_day - end_day <= 7 && now_day - end_day > 1) {
        when_class = "recently-happened"
      } else if (now_day - end_day <= 1) {
        when_class = "happened-yesterday"
      }
    }

  } else if (_.isBlank(community.end_date)) {
    if (over_year_start) {
      date_str = "Starts "+start.format(dtya)
    } else {
      date_str = "Starts "+start.format(dta)
    }

    if (start_day == now_day) {
      when_str = "Starting Today"
    } else if (start_day < now_day) {
      when_str = "Started "+start.fromNow();
    } else if (start_day > now_day) {
      when_str = "Starting "+start.fromNow();
    }

    if (start_day == now_day) {
      when_class = "happening-today"
    } else {
      if (Math.abs(start_day - now_day) > 7) {
        when_class = "will-happen"
      } else if (Math.abs(start_day - now_day) <= 7 && Math.abs(start_day - now_day) > 1) {
        when_class = "happening-soon"
      } else if (Math.abs(start_day - now_day) <= 1) {
        when_class = "happening-tomorrow"
      }
    }

  } else {
    if (over_year_start || over_year_end) {
      if (day_length >= 1) {
        date_str = "Start: "+start.format(dty)+"<br/>End: "+end.format(dty)+" "
      } else {
        date_str = ""+start.format(dy)+" from "+start.format(t)+" to "+end.format(t)+" "
      }
    } else {
      if (day_length >= 1) {
        date_str = "Start: "+start.format(dt)+"<br/>End: "+end.format(dt)+" "
      } else {
        date_str = ""+start.format(d)+" from "+start.format(t)+" to "+end.format(t)+" "
      }
    }

    if (start_day <= now_day && now_day <= end_day) {
      when_str = "Happening Today";
    } else if (start_day > now_day) {
      when_str = "Starting "+start.fromNow();
    } else if (end_day < now_day) {
      when_str = "Ended "+end.fromNow();
    }

    if (start_day <= now_day && now_day <= end_day) {
      when_class = "happening-today"
    } else if (start_day > now_day) {
      if (start_day - now_day > 7) {
        when_class = "will-happen"
      } else if (start_day - now_day <= 7 && start_day - now_day > 1) {
        when_class = "happening-soon"
      } else if (start_day - now_day <= 1) {
        when_class = "happening-tomorrow"
      }
    } else if (end_day < now_day) {
      if (now_day - end_day > 7) {
        when_class = "already-happened"
      } else if (now_day - end_day <= 7 && now_day - end_day > 1) {
        when_class = "recently-happened"
      } else if (now_day - end_day <= 1) {
        when_class = "happened-yesterday"
      }
    }
  }
  %>


  <% if(community.enabled == true) { %>
  <div class="community-overview enabled">
  <% } else { %>
  <div class="community-overview">
  <% } %>

    <div class="community-overview-right">
      <div id="community-stats-index-<%= community.id %>" class="community-overview-stats">
        <!--// Rendered by PX.CommunityStatsIndex -->
      </div>

      <div class="community-actions">

        <% if (community.num_users > 0 &&
                !community.options.sample_event) { %>
        <a href="javascript:void(0);"
           class="btn btn-icon community-btn disabled"
           title="Remove all members first or contact support">
           <%= _.i("trash") %>
        </a>
        <% } else { %>
        <a href="javascript:void(0);"
           class="btn btn-icon community-btn btn-delete"
           title="Delete">
           <%= _.i("trash") %>
        </a>
        <% } %>

        <a href="/communities/new?clone_id=<%= community.id %>"
           class="btn btn-icon community-btn btn-clone load-on-click"
           title="Clone">
           <%= _.i("copy") %>
        </a>

        <a href="/<%= community.slug %>#attendees"
           class="btn btn-icon community-btn btn-attendees load-on-click"
           title="Attendees">
           <%= _.i("group") %>
        </a>

        <a href="/<%= community.slug %>#setup"
           class="btn btn-icon community-btn btn-edit load-on-click"
           title="Edit">
           <%= _.i("pencil") %>
        </a>

      </div>
    </div>

    <div class="community-overview-left">
      <div class="community-picture community-info">
        <img src="<%= community.image_thumb_url %>" alt="<%= community.description_short %>">
      </div>
      <div class="<%= when_class %> community-text">
        <div class="community-info">
          <div class="name"><%= community.name %></div>
          <div class="description minor"><%= community.description_short %></div>
        </div>
        <span class="when" title="<%= date_str %>"><%= when_str %></span>
      </div>
    </div>

  </div>
</script> 
  <script type="text/template" id="template-community-region">
<div class="admin-control-wrap <%= community_wrap.admin_panel_visibility_class %>">
  <div id="community-edit" class="community-pane"> <!--// PX.CommunityEdit --> </div>
