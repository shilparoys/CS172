        <span class="editable-content"><%= answer_choice.emails %></span>
      </div>
    </div>
    -->

    <div class="applies-to-wrap half-margin" id="applies-to-<%= answer_choice.id %>">
      <h4>Only applies to:</h4>
      <% for(ticket_id in answer_choice.tickets) {
        tdata = answer_choice.tickets[ticket_id]; %>
        <label class="inline-selectable" for="answer-choice-applies-to-<%= tdata.ticket.id %>">
        <input type="checkbox"
               id="answer-choice-applies-to-<%= tdata.ticket.id %>"
               class="next-to-inline-label answer-choice-applies-to"
               value="<%= tdata.ticket.id %>"
               <%= tdata.checked ? "checked=checked" : "" %>
               data-answer-choice-id="<%= tdata.ticket.id %>">&nbsp;<%= tdata.ticket.get('body') %></label>
      <% } %>
    </div>


  <% } %>

