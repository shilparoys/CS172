           class="editable-area show-hint inline-edit"
           title="<%= answer_choice.usage_info %>">
        <span class="editable-content">
          <% if (_.clearInfinity(answer_choice.quantity) !== "") { %>
            <%= _.numberFormat(_.clearInfinity(answer_choice.quantity)) %>
          <% } else { %>
            <%= "" %>
          <% } %>
        </span>
      </div>

      available.

      <div id="edit-answer-choice-<%= answer_choice.id %>-discount-amount"
           class="editable-area show-hint inline-edit">
        <span class="editable-content">
          <%
            change = 0;
            active = ""
            if(answer_choice.adjustments.length > 0) {
              adj = answer_choice.adjustments.first()
              active = adj.get("active_change")
              if (active == "price_change") {
                change = _.numberFormat(Math.abs(adj.get("price_change")/100), 2);
              } else if (active == "percent_change") {
                change = Math.abs(adj.get("percent_change"));
              } else {
                change = 0 
              }
            }
          %>
          <%= change %>
        </span>
      </div>

      <select class="inline-edit discount-option">
        <option <%= active=="percent_change"?"selected":"" %>>%</option>
        <option <%= active=="price_change"?"selected":"" %>>$</option>
      </select>

      discount.


    </div> <!-- // end basic-answer-choice-adj -->
  </div> <!-- // end answer-choice-basic-admin-controls -->

