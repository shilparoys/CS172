          <script>
function showContent1() {
	document.getElementById("content1").style.display="block";
	document.getElementById("thumb1").className="thumbOn";
	document.getElementById("content2").style.display="none";
	document.getElementById("thumb2").className="";
	document.getElementById("content3").style.display="none";
	document.getElementById("thumb3").className="";
}
function showContent2() {
	document.getElementById("content1").style.display="none";
	document.getElementById("thumb1").className="";
	document.getElementById("content2").style.display="block";
	document.getElementById("thumb2").className="thumbOn";
	document.getElementById("content3").style.display="none";
	document.getElementById("thumb3").className="";
}
function showContent3() {
	document.getElementById("content1").style.display="none";
	document.getElementById("thumb1").className="";
	document.getElementById("content2").style.display="none";
	document.getElementById("thumb2").className="";
	document.getElementById("content3").style.display="block";
	document.getElementById("thumb3").className="thumbOn";
}
showContent3();//set default open content area

</script> 
