        <script type="text/javascript">
function buyMePhotoTellHeightBSI() {
	document.getElementById('buy-me-photo-button-bsi').style.top = buyMePhotoHeight-61 + "px";//adjust bottom positioning here
	document.getElementById('buy-me-photo-button-bsi').style.left = buyMePhotoWidth-89 + "px";//adjust left positioning here
}
</script> 
        <!-- *********************** BEGIN HORIZONTAL ACCORDION *********************** --> 
        <div id="horaccbsi-wrap"> 
         <div id="buy-me-photo-button-bsi"> 
          <script type="text/javascript">
			var buyMePhotoWidth = 576;
			var buyMePhotoHeight = 324;
			var buyMePhotoUrl = '';
		</script> 
          <script type="text/javascript" src="http://grfx.cstv.com/scripts/buy-button-bsi.js"></script> 
         </div>
         <!-- END #buy-me-photo-button-bsi --> 
         <div id="bsi-video-wrap"> 
          <div id="bsi-flash-video"> 
           <script type="text/javascript" language="JavaScript">
				
			var config_url = "http://grfx.cstv.com/schools/psu/data/xml/videolist/config_bsi.xml";
			var vid_width = 574;
			var vid_height = 322;
			var vid_partner = "psu"; //schoolcode

			var vid_adtarget = vid_partner.toUpperCase() + "/FRONTPAGE"; //ad target *****Remove for sport specific (optional) ******
			var vidPlyrVars = "&vidPlayerSchool=" + vid_partner + "&vidPlayerWd=" + vid_width + "&vidPlayerHt=" + vid_height + "&vidAdTarget=" + vid_adtarget + "&loadPlayer=false&configURL=" + config_url + "&"; //full url string - path to swf, passing any vars
			var vidPlyrID = "flvVideoPlayer";
