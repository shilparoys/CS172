/////////////////mediaTypeCheck()///////////////////////////
function mediaTypeCheck() {
	//alert("mediaTypeCheck() fired!!")
	if (bsiCompleteArray[arrayIndex].mediaType == "video") {
		document.getElementById("bsi-video-wrap").style.display = "block";
		document.getElementById("frame_photo"+arrayIndex).style.display = "none";
		document.getElementById('frm'+arrayIndex).style.display = "none";
		document.getElementById("buy-me-photo-button-bsi").style.display = "none";
		if (slideToPanel==1) {
			document.getElementById("bsi-video-wrap").style.left = "71px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}	
		if (slideToPanel==2) {
			document.getElementById("bsi-video-wrap").style.left = "142px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}
		if (slideToPanel==3) {
			document.getElementById("bsi-video-wrap").style.left = "213px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}
		if (slideToPanel==4) {
			document.getElementById("bsi-video-wrap").style.left = "284px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}
		if (slideToPanel==5) {
			document.getElementById("bsi-video-wrap").style.left = "355px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}
		if (slideToPanel==6) {
			document.getElementById("bsi-video-wrap").style.left = "426px";
			document.getElementById("bsi-video-wrap").style.top = "2px";
		}
		loadVideoPlayer();
	} 
	if (bsiCompleteArray[arrayIndex].mediaType == "photo") {
		killVideoPlayer();
		document.getElementById("bsi-video-wrap").style.display = "none";
		document.getElementById("frame_photo"+arrayIndex).style.display = "block";
		document.getElementById('frm'+arrayIndex).style.display = "block";
		document.getElementById("buy-me-photo-button-bsi").style.display = "block";
		if (!document.all){
			flvPlayerLoaded = false;
		}	
	}	
}
////////////////////end mediaTypeCheck()/////////////////////
</script>
