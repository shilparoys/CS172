  <script type="text/javascript">

function MM_jumpMenu(targ,selObj,restore){ //v3.0 
	eval(targ+".location='"+selObj.options[selObj.selectedIndex].value+"'"); 
	if (restore) selObj.selectedIndex=0; 
} 

function theRotator() {
	//Set the opacity of all images to 0
	$('div#rotatorhome ul li').css({opacity: 0.0});
	
	//Get the first image and display it (gets set to full opacity)
	$('div#rotatorhome ul li:first').css({opacity: 1.0});
		
	//Call the rotator function to run the slideshow, set in milliseconds. 1000 = 1 second.
	setInterval('rotate()',3500);
	
}

function rotate() {	
	//Get the first image
	var current = ($('div#rotatorhome ul li.show')?  $('div#rotatorhome ul li.show') : $('div#rotatorhome ul li:first'));

	//Get next image, when it reaches the end, rotate it back to the first image
	var next = ((current.next().length) ? ((current.next().hasClass('show')) ? $('div#rotatorhome ul li:first') :current.next()) : $('div#rotatorhome ul li:first'));	
	
	//Set the fade in effect for the next image, the show class has higher z-index
	next.css({opacity: 0.0})
	.addClass('show')
	.animate({opacity: 1.0}, 1500);

	//Hide the current image
	current.animate({opacity: 0.0}, 3000)
	.removeClass('show');
	};

$(document).ready(function() {		
	//Load the slideshow
	theRotator();  
	
                        var ButtonKeys = { "EnterKey": 13 };
                        
                        $(function()
                        { 
                            $(document).keypress(function(e)
                            {
                                
                                if (e.which == ButtonKeys.EnterKey)
                                {
                                    var q = $(document.Form1.q).val();
                                    var url = "http://www.google.com/u/SBCTC?q=";
                                    //alert(q.length);
                                    if (q.length > 1)
                                    {
                                        //alert(url + q); 
                                        $(location).attr('href',url + q);
                                        return false;
                                    }
                                }
                            });
                        });
});      
</script> 
