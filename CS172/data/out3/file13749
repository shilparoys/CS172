$("#ooh-feat-occupation-default li").hide();
$(document).ready(function(){   
    var length = $("#ooh-feat-occupation-rand li").length;
    var ran = Math.floor(Math.random()*length) + 1;
    $("#ooh-feat-occupation-default li").hide();
    $("#ooh-feat-occupation-rand li:nth-child(" + ran + ")").show();
});

