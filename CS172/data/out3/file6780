                        });

$('#mega-menu-1 > li:has(ul)').each(function(){
    $('#' + $(this).attr('id')).live('focus', function() {
        var subNav = $('.sub',this);
        var subNavDiv = subNav.parent();
        $(this).addClass('mega-hover');
        $(subNav).removeClass("in-active").addClass("active");
        $(subNavDiv).removeClass("in-active").addClass("active");
    });
    $('#' + $(this).attr('id')).live('blur', function() {
        var subNav = $('.sub',this);
        var subNavDiv = subNav.parent();
        $(this).removeClass('mega-hover');
        $(subNav).removeClass("active").addClass("in-active");
        $(subNavDiv).removeClass("active").addClass("in-active");
    });
});

$('#mega-menu-2 > li:has(ul)').each(function(){
    $('#' + $(this).attr('id')).live('focus', function() {
        var subNav = $('.sub',this);
        var subNavDiv = subNav.parent();
        $(this).addClass('mega-hover');
        $(subNav).removeClass("in-active").addClass("active");
        $(subNavDiv).removeClass("in-active").addClass("active");
    });
    $('#' + $(this).attr('id')).live('focusout', function() {
        var subNav = $('.sub',this);
        var subNavDiv = subNav.parent();
        $(this).removeClass('mega-hover');
        $(subNav).removeClass("active").addClass("in-active");
        $(subNavDiv).removeClass("active").addClass("in-active");
    });
});



