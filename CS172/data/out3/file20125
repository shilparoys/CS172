  <script type="text/javascript" charset="utf-8">
     var SlideShow = function(container) {
        this.container = container;
        this.images = [];
        this.$images = [];
        this.delay = 2000;
    };

    SlideShow.prototype = {
      addImage: function(data) {this.images.push(data);},
      ready: function() {
          this.$container = jQuery(this.container);

           jQuery.each(this.images, jQuery.proxy(function(index, img) {
                var $img = jQuery('<img />');
                if (!Modernizr.mq('only all and (max-width: 586px)')) {
                    $img.attr('src', img.src);
                }
                if (img.eventLink) { $img.data('link', img.eventLink); } else { $img.data('link', img.structurLink); }
                $img.data('header', img.dataHeader);
                $img.data('subheader', img.dataSubheader);
                $img.data('text', img.dataText);
                $img.data('background', img.dataBackground);
                if (index == 0) {
                    this.$container.append($img);
                    this.$container.cycle();
                } else {this.$images.push($img);}
            }, this));

            if (Modernizr.mq('only all and (max-width: 586px)')) {
                this.$container.animate({'min-height': '200px', 'max-height': '200px'});
            }
           jQuery(window).load(jQuery.proxy(function() {
               setTimeout(jQuery.proxy(function() {
                  jQuery.each(this.$images, jQuery.proxy(function(index, $img) {
                      this.$container.cycle('add', $img);
                   }, this));

                }, this), this.delay);
           }, this));
      }
    };
    var slideshow = new SlideShow("#homepage-slideshow");
</script> 
  <div id="slideshow" class="wrapper"> 
   <div class="sliderbg"> 
    <div class="container-fluid"> 
     <div id="homepage-header"> 
      <div id="homepage-slideshow" style="min-height: 350px;" class="cycle-slideshow" data-cycle-auto-init="false" data-cycle-fx="fade" data-cycle-log="false" data-cycle-swipe="true" data-cycle-caption-template="<h2>{{header}}</h2><p>{{subheader}}</p><a class='btn primary small' href='{{link}}'>{{text}}</a>" data-cycle-pause-on-hover="true"> 
       <script>
						        var img = {};
                                img.structurLink = "https://hsf.net/en/scholarships/scholarship-application/";
                            	
                                img.src = "//d6a1uqp55pt8u.cloudfront.net/homepage_slider_images/Scholarship-Slider-2.png";
                                img.dataHeader = "Be an HSF Scholar";
                                img.dataSubheader = "Receive an HSF Scholarship!";
                                img.dataText = "Apply Now";
                                img.dataBackground = "";
						        slideshow.addImage(img);
						    </script> 
       <script>
						        var img = {};
                                img.structurLink = "https://hsf.net/en/resources/parents/";
                            	
                                img.src = "//d6a1uqp55pt8u.cloudfront.net/homepage_slider_images/Ad-Council---Slider.png";
                                img.dataHeader = "HSF helps you prepare, plan, and pay for college";
                                img.dataSubheader = "";
                                img.dataText = "Learn More";
                                img.dataBackground = "";
						        slideshow.addImage(img);
						    </script> 
       <script>
						        var img = {};
                                img.structurLink = "https://hsf.net/en/about-hsf/donate-833/";
                            	
