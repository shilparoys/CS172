    var UserCenter = function(container) {
        this.container = container;
    };

    UserCenter.prototype = {
      domReady: function() {
        this.$iAmA = jQuery('#user-center h2');
        this.$container = jQuery(this.container);
        $('#user-tabs a').click(function (e) {
          e.preventDefault();
          var $link = $(e.target);
          if ($link.attr('href') == '#parents' || $link.attr('href') == '#students') {
              this.$iAmA.text(this.$iAmA.data('im_a') + ':').append('<i class="arrow"></i>');
          } else {
              this.$iAmA.text(this.$iAmA.data('im_an') + ':').append('<i class="arrow"></i>');
          }
          $link.tab('show');
        }.bind(this));

        // $('.more-from-hsf .changes-lives .nav-pills a').click(function (e) {
//           e.preventDefault();
//           $(this).tab('show');
//         });

      }
    };
    $(function () {
        // show/hide 3 ways to help today Tabs for phone only
        $('#ways-to-help h4').bind('click', function(e){ // triggered on click
            e.preventDefault();
            if (document.documentElement.clientWidth < 979) {$('#wth-tabs').slideToggle();}
        });
        new UserCenter('#user-center').domReady();
        $('#profile-visibility-toggle').iToggle({
            onClick: function(){}});

     });

	 var tabTimeout = [];
	 var tabClickTimeout = null;
	 var tabPills = $('.changes-lives .nav-pills a');
	 var tabPanes = $('.changes-lives .tab-content');
	 var tabChange = function () {
	     var tabs = $('.changes-lives .nav-pills > li');
	     var active = tabs.filter('.active');
	     var next = active.next('li').length ? active.next('li').find('a') : tabs.filter(':first-child').find('a');
	     // Use the Bootsrap tab show method
	     //next.tab('show');
		 //var target = next.attr('href');
		 var target = next.data('tab');
		 tabPills.parent().removeClass('active');

		 tabPanes.find('.tab-pane').removeClass('in');

		 setTimeout(function(){
			 tabPanes.find('.tab-pane').removeClass('active');
			 next.parent().addClass('active');
			 tabPanes.find(target).addClass('active');
			 setTimeout(function(){tabPanes.find(target).addClass('in');}, 150);

		 }, 150);
		 tabTimeout.push(setTimeout(function(){tabChange();}, 4000));
	 };
	 // Tab Cycle function
	 //var tabCycle = setInterval(tabChange, 4000);
	 tabTimeout.push(setTimeout(function(){tabChange();}, 4000));

	 // Tab click event handler
	 tabPills.on('click', function (e) {
	 	//e.preventDefault(); removed to allow click-through to Changes Lives page
	    //clearInterval(tabCycle);
	    $(this).tab('show');
	    // Start the cycle again in a predefined amount of time
		if (tabClickTimeout != null) {
			clearTimeout(tabClickTimeout);
			$.each(tabTimeout, function(index, timeout){clearTimeout(timeout);});
		}
		tabClickTimeout = setTimeout(function () {
	        tabChange();
			var tabTimeoutLastIndex = tabTimeout.length - 1;
			$.each(tabTimeout, function(index, timeout){
				if (index != tabTimeoutLastIndex) {clearTimeout(timeout);}
			});
			//tabClickTimeout = null;
		//tabCycle = setInterval(tabChange, 4000);
	    }, 4000);
	 });

	 $(window).resize(function() {
		 if ((document.documentElement.clientWidth < 1040) && (document.documentElement.clientWidth > 979)) {
			 var contHeight = $('#homepage-slideshow').height();
			 $('#ways-to-help').height(contHeight).css("margin-top", -(contHeight));
		 }
		 if ((document.documentElement.clientWidth > 1040) || (document.documentElement.clientWidth < 979)) {
			 $('#ways-to-help').removeAttr( 'style' );
		 }
	 });

	 var changeSlideshowBackground = function(slide) { // function to change BG gradient for selected slides
 	    var bgImage = slide.data('background'); // check slide for alt background
  		if (bgImage == '') { // if no alt bg, keep original
			bgImage = "/images/homeslider-bg.jpg";
		} else {
			$('#slideshow .sliderbg').fadeTo('fast', 0.5, function() {
			    $(this).css("background", "url('" + bgImage + "') 0 0 repeat-x");
			}).fadeTo('fast', 1);
		}
	 };

	 $( '#homepage-slideshow' ).on( 'cycle-post-initialize', function(event, optionHash) {
	    changeSlideshowBackground($('#homepage-slideshow .cycle-slide-active').first());
	 });

	$( '#homepage-slideshow' ).on( 'cycle-after', function(event, optionHash, outgoingSlideEl, incomingSlideEl, forwardFlag) {changeSlideshowBackground($(incomingSlideEl));});

	jQuery(function(){
		var tab = jQuery('a[href='+window.location.hash+']');
		if (tab.length > 0) {tab.tab("show");}
		slideshow.ready();
	});


</script> 
  <script charset="utf-8">

var HSFRemoteFreeForm = function(container) {
  this.$container = jQuery(container);
  this.partialURL = this.$container.data("hsf-remote-form-partial");
  this.redirect = this.$container.data("hsf-remote-form-success-redirect");
  this.successMessage = this.$container.data("hsf-remote-form-success-message");
  this.$form = this.$container.find('form:first');
  this.$form.parsley().subscribe('parsley:form:validate', this.handleSubmit.bind(this));
  // this.$form.submit(this.handleSubmit.bind(this));
  this.$button = jQuery("input[type=submit]", this.$form);
  this.$errors = jQuery('#error-messages', this.$form);
  jQuery.get(this.partialURL, this.reloadXID.bind(this));
}

HSFRemoteFreeForm.prototype = {

  reloadXID: function(html) {this.$form.find('input[name="XID"]').val(jQuery(html).find('input[name="XID"]').val());},

  handleSubmit: function(formInstance) {

    formInstance.submitEvent.preventDefault();

    if (formInstance.isValid()) {
      this.showWaiting();
      jQuery.ajax({
        url: this.$form.attr('action'),
        data: this._data(),
        type: 'POST',
        dataType: 'JSON',
        success: this.handleResponse.bind(this)
      });
      return;
    } else {this.animateButton();}
    return;
  },

  animateButton: function() {
    this.$button.one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
      this.$button.removeClass('animated shake');
    }.bind(this));
    this.$button.addClass('animated shake');
  },

  showWaiting: function() {
    this.$button.attr("disabled","disabled").css('background-color', '#F5F5F5').val('Sending...');
  },

  removeWaiting: function() {
    this.$button.removeAttr('disabled').css('background-color', '#7E7E7E').val('Subscribe');
  },

  resetForm: function() {
    $(':input', this.$form)
    .not(':button, :submit, :reset, :hidden')
    .val('')
    .removeAttr('checked')
    .removeAttr('selected');
  },

  showGeneralErrors: function(errors) {
    if (!jQuery.isArray(errors)) {return;}
    this.animateButton();
    this.$errors.empty();
    jQuery.each(errors, function(index, error) {this.$errors.append('<li>'+error+'</li>');}.bind(this));
    this.$errors.show();
  },

  hideGeneralErrors: function() {
    this.$errors.hide();
    this.$errors.empty();
  },

  handleResponse: function(json) {
    this.hideGeneralErrors();
    if (json['success']) {
      this.resetForm();
      this.removeWaiting();

      this.$form.one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function() {
        this.$form.removeClass('animated zoomOut');
        this.$form.empty();
        this.$form.after('<h3 class="animated flipInX" style="color: #fff; text-align: center; font-size: 3em;" id="form-success">'+this.successMessage+'<h3>');
        setTimeout(function() {
          if (this.redirect) { window.location = this.redirect; }
        }.bind(this), 2000);
      }.bind(this));

      this.$form.addClass("animated flipOutX");

    } else {
      this.removeWaiting();
      this.showGeneralErrors(json['errors']);
    }
  },

  _data: function() {
    return this.$form.serialize();
  }
};



  jQuery(function() {
    jQuery('[data-hsf-remote-form]').each(function(index, element) {
      window.form = new HSFRemoteFreeForm(element);
    });
  });

</script> 
