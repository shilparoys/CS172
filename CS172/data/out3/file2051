       <script type="text/javascript">//<![CDATA[
function validate() {
    var valid = true;
                                        var objyour_name = document.getElementById("your_name_err");
                if (document.getElementById("your_name").type == "checkbox")
                {
                    var checkboxStatus = false;
                    var myCheckboxes = document.getElementsByName("your_name[]").length;
                    <!-- Start Todds code here -->
                    if (myCheckboxes > 1){
                        for (var i = 0; i < myCheckboxes; i++){
                            //alert( document.getElementsByName("your_name[]").item(i).checked);
                            if (document.getElementsByName("your_name[]").item(i).checked == true) {
                                checkboxStatus = true;
                            }
                        }
                        if (checkboxStatus == false){
                            objyour_name.innerHTML= 'Your Name is required';
                            alert("Your Name is required");
                            return false;
                        }
                    }
                    else {
                        if (document.getElementById("your_name").checked == false) {
                            alert("Your Name is required");
                            return false;
                        }
                    }
                    <!--End Todds code here -->
                } else {
                    if (document.getElementById("your_name").value == '') {
                        objyour_name.innerHTML= 'Your Name is required';
                        alert("Your Name is required");
                        return false;
                    }
                }
