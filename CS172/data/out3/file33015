  <script type="text/javascript"> 
    $(document).ready(function() {	
	    $("#menu ul li").hover(function(){
		$(this).addClass('hover');
		$(this).children().addClass('hover');
		}, function() {
		$(this).removeClass('hover');
		$(this).children().removeClass('hover');
	    });
    });
    </script> 
