     <section id="chapel" class="chapel"> 
      <figure id="chapvid"> 
       <div class="">
        <div class="video-container">
         <iframe allowfullscreen="true" title="YouTube video player" class="youtube-player" type="text/html" width="980" height="582" src="//www.youtube.com/embed/XbKfp3WNUSs?rel=0&amp;showinfo=0&amp;wmode=transparent" frameborder="0"></iframe>
