          <div id="login-container"> 
           <form action="https://www.rit.edu/academicaffairs/honors/authenticate" id="login-container-form" method="post" accept-charset="utf-8"> 
            <span class="spacer-right">Login</span> 
            <input type="text" name="username" value="" id="username" placeholder="username" class="spacer-right-smallest"> 
            <input type="hidden" name="ref_page" value=""> 
            <input type="password" name="password" value="" id="password" placeholder="password" class="spacer-right-smaller"> 
            <input type="image" src="https://www.rit.edu/academicaffairs/honors/assets/app/images/button-go.png" alt="Submit Form" style="vertical-align: top;"> 
           </form> 
           <script type="text/javascript">
		var u = $('#username');
		(u.val().length == 0) ? u.focus() : $('#password').focus();
	</script> 
          </div>
         </div> 
         <script type="text/javascript">
	var loginForm = $('#login-container-form');
	
	loginForm.on('submit', function(e) {
		var me = $(this),
			par = me.parent(),
			u = me.find('input[name="username"]'),
			p = me.find('input[name="password"]'),
			r = me.find('input[name="ref_page"]').val();
			
		e.preventDefault();
		
		par.disableFormContainer();
		me.find('p.message').remove();
		me.append($('<p class="message spacer-top-small center bold">Attempting login...</p>'));
		
		$.ajax({
			url: loginForm.attr('action'),
			type: 'post',
			data: {username: u.val(), password: p.val()},
			success: function(retData){
				me.find('p.message').remove();
				me.append($('<p class="success-message message spacer-top-small center bold">Success, reloading...</p>'));
				
				if (r.length == 0) {
					window.location.reload();
				} else {
					window.location = r;
				}
			},
			error: function(retData){
				var message = (retData.responseText && retData.responseText.length > 0) ? retData.responseText : 'unknown error';
				me.find('p.message').remove();
				me.append($('<p class="error-message message spacer-top-small center"></p>').html(message)).hide().fadeIn();
			},
			complete: function(retData) {
				par.enableFormContainer();
				(u.val() == '') ? u.focus() : p.focus();
			}
		});
	});
</script> 
