              <script type="text/javascript">
	$K2(document).ready(function(){
		var heightAction = $K2("a.browse").height();
		var heightScroller = $K2("div.scroll_wrapper").height();
		$K2("a.browse").css({
			'top': (heightScroller - heightAction)/2 + 'px'
		});
		var widthAction = $K2("a.browse").width();
		$K2("div.scrollable").css({
			'width': 450 - widthAction*4 + 'px'
		});
		$K2(".key0").addClass('active');
	});
	$K2('.scrollable')
	        .scrollable({ circular: true, mousewheel: false, speed: 1000 })
	        .autoscroll({ interval: 4000, autopause: true, api: true });
	var api = $K2('.scrollable').data('scrollable');
	api.onSeek(function() {
		    // в функции обратного вызова переменная "this" ссылается на API
			$K2(".navi").removeClass('active');
			$K2(".key"+this.getIndex()).addClass('active');
			$K2('#sl-name').text($K2('#sl-name'+this.getIndex()).text());
			$K2('#sl-desc').text($K2('#sl-desc'+this.getIndex()).text());
			$K2('a#sl-link').attr('href', $K2('#sl-link'+this.getIndex()).text());
			if($K2('#sl-target'+this.getIndex()).text() == 1) $K2('a#sl-link').attr('target','_blank');
			else $K2('a#sl-link').attr('target','_self');
		});
</script> 
             </div> 
            </div> 
           </div> 
          </div> 
         </div> 
         <!--******************** END TOP EXTEND ********************--> 
