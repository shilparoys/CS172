	jQuery(".col-sm-5").click(function() {
		var pnm=jQuery(this).data('pname');
		jQuery(".onTop").fadeOut("fast", function() {
		jQuery(".containerslider").slideUp({duration:800, easing: "linear"});
		jQuery(".cornerfold").slideUp("fast");
		document.getElementById('pageTitle').innerHTML =  pnm;
		jQuery(".contentdiv").slideUp({duration:800, easing: "linear"});	
		jQuery('.tier2Img').animate({ width: '90px' });
		jQuery(".onTop").fadeIn("slow");
		for (var i=1; i<=9; i++)
