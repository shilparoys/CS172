var gooMode = 0;
function toggleGooMode() {
  if(gooMode==1) { gooMode=0; } else {gooMode=1;}
  //alert("gooMode is: "+$gooMode);
}

var screenW = 640, screenH = 480;
function init() {
  if (is_nav4up || is_ie4up || is_ie5up ) {
      screenW = screen.width;
      screenH = screen.height;
  }
  document.statusform.tips.value=(tts[1]);
  document.statusform.goocheck.checked=false;
  lastT=1;
}

//put a tooltip msg in statusform.tips field
var lastT;
function tt(t) {
   if(tts[0]<=2) {
     document.statusform.tips.value="warning: no tool tips";
   }
   else {
     if(t>tts[0])  {
       document.statusform.tips.value="warning: tooltips indexing error";
     }
     else {
       document.statusform.tips.value=(tts[t]);
     }
  }
  lastT=t;
  return true;
}

//open a google search result
var gooWindow;
function goo(t ){
   if(gooMode == 0) {
      return;
   }
   if(t<=1) t=lastT;
   if(t<=1) return false;
   ttp=tts[t]; 

   if(tts[0]<=2) {
     document.statusform.tips.value="warning: no tool tips";
   }
   else {
     var width=(3*screenW/4);
     if(width>700){ width=700; }
     var height=3*screenH/4;
     if(height>900){ height=900; }
     var yoff=50;
     var re;

     // edit the Flynn Engdahl region for a better search
     re=/-/gi;
     ttp=ttp.replace(re," ");
     re=/,/;
     ttp=ttp.replace(re," ");
     re=/REGION/gi;
     ttp=ttp.replace(re,"");
     re=/REG\./gi;
     ttp=ttp.replace(re,"");
     re=/N\.Z\./gi;
     ttp=ttp.replace(re,"NEW ZEALAND");
     re=/P\.N\.G\./gi;
     ttp=ttp.replace(re,"PAPUA NEW GUINEA");
     re=/PNG/gi;
     ttp=ttp.replace(re,"PAPUA NEW GUINEA");
     re=/IS\./gi;
     ttp=ttp.replace(re,"ISLANDS");
     re=/PROV\./gi;
     ttp=ttp.replace(re,"PROVINCE");
     // drop NORTH, NORTH CENTRAL, SOUTH CENTRAL, etc:
     re=/THCENT/gi;
     ttp=ttp.replace(re,"TH CENT");
     re=/W\./gi;
     ttp=ttp.replace(re,"WEST");
     re=/ERN /gi;
     ttp=ttp.replace(re," ");
     re=/NORTH/gi;
     ttp=ttp.replace(re," ");
     re=/SOUTH/gi;
     ttp=ttp.replace(re," ");
     re=/EAST/gi;
     ttp=ttp.replace(re," ");
     re=/WEST/gi;
     ttp=ttp.replace(re," ");
     re=/CENTRAL /gi;
     ttp=ttp.replace(re," ");
     re=/ NW /gi;
     ttp=ttp.replace(re," ");
     re=/ SW /gi;
     ttp=ttp.replace(re," ");
     re=/ NE /gi;
     ttp=ttp.replace(re," ");
     re=/ SE /gi;
     ttp=ttp.replace(re," ");
     re=/ARG\./gi;
     ttp=ttp.replace(re,"ARGENTINA");
     re=/OFF COAST OF /gi;
     ttp=ttp.replace(re,"");
     re=/NEAR COAST OF /gi;
     ttp=ttp.replace(re,"");
     re=/NEAR /gi;
     ttp=ttp.replace(re,"");
     re=/OFF /gi;
     ttp=ttp.replace(re,"");
     re=/ OF /gi;
     ttp=ttp.replace(re," ");

     var gooUrl="http://www.google.com/search?hl=en&num=20&q="+escape(ttp+" (EARTHQUAKE OR EARTHQUAKES OR SEISMIC OR SEISMICITY OR TECTONICS) -weather -travel -flinn -score -vacation -deals -team -price");

     var settings="width="+width+",height="+height+",top="+yoff+",left=0"+
         ",screenX=110,screenY="+yoff+",resizable=yes,scrollbars=yes,status=yes,location=yes,toolbar=yes,menubar=yes";

     gooWindow=window.open(gooUrl,  "googleWindow", settings);
     gooWindow.focus();
   }
   return false;
}


// open the event list window, for events near to mouse click
var evnWindow;
function evn(lon,lat){
   var width=950;
   var height=screenH/2+200;
   var yoff=50;
   var xoff=200;

   var rgn="S_Pacific";

   // new event list 
   var url="../eventlist/index.phtml?region="+rgn+"&lon="+lon+"&lat="+lat;
  ;

   var settings="width="+width+",height="+height+
                ",top="+yoff+",left="+xoff+
                ",screenX="+xoff+",screenY="+yoff+
                ",resizable=yes,scrollbars=yes,status=yes,location=yes,toolbar=yes,menubar=yes";

   evnWindow=window.open(url, "eventWindow", settings);
   evnWindow.focus();
   //return true    --don't return anything or it shows up in window
}


</script>
