            <div class="title">
             <a href="/news/articles/2337-all-about-virtual-internships">All About Virtual Internships</a>
            </div> 
           </div> 
          </div> 
          <div class="links bottom"> 
          </div> 
          <script>
  
  var PublishableSlideShow = Class.create({
    position : 0,
    
    initialize: function(elements, children_selector, description_element) {
      this.elements = elements;
      this.children_selector = children_selector;
      this.total = elements.select(children_selector).size();
      this.position = 0;
      this.executer = new PeriodicalExecuter(this.slide_right.bind(this), 8);
      this.description_element = description_element;
    },

    click_left: function() {
      this.executer.stop();
      this.slide_left();
    },
    
    click_right: function() {
      this.executer.stop();
      this.slide_right();      
    },
    
    current_element: function() {
      return this.elements.down(this.children_selector, this.position);
    },
    
    slide_left: function() {
			new Effect.Fade(this.current_element(), {duration:0.2, queue:{position:'end', scope:'scope'}});
			// allows for continuous looping
			this.position = this.get_previous_position();
      new Effect.Appear(this.current_element(), {duration:0.7, queue:{position:'end', scope:'scope'}});
      this.description_element.href = this.current_element().href;
      this.description_element.innerHTML = this.current_element().title;
    },
    
    slide_right: function() {
			new Effect.Fade(this.current_element(), {duration:0.3, queue:{position:'end', scope:'scope'}});
			// allows for continuous looping
			this.position = this.get_next_position();
      new Effect.Appear(this.current_element(), {duration:0.7, queue:{position:'end', scope:'scope'}});
      this.description_element.href = this.current_element().href;
      this.description_element.innerHTML = this.current_element().title;
    },

		get_previous_position: function() {
			if (this.position == 0) {
				return this.total-1;
			} else {
				return this.position-1;
			};
		},
		
		get_next_position: function() {
			if (this.position+1 == this.total) {
				return 0;
			} else {
				return this.position+1;
      };
		}
  
  });
  
  
  var qs = new PublishableSlideShow($$('div.publishables_big_call_to_action_widget div.window div.photos').first(), 'a', $$("div#publishables_big_call_to_action_widget div.controls div.title a").first());
    
</script> 
         </div> 
        </div> 
