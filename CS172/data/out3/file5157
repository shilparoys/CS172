        $('.grid[class*="l-cols-"]').each(function(){
            /* [1] Find out how many columns the user has set by reading the outputted class */
            var userColumns = this.className.match(/l-cols-(\d+)/)[1];
            userColumnsInt = parseInt(userColumns); /* Convert To Integer For The Sake Of Comparing */

            /* [1] Find out how many columns have actually been output */
            var actualColumns = $(this).children().size();
            actualColumnsInt = parseInt(actualColumns); /* Convert To Integer For The Sake Of Comparing */

            /* [3] Test to see if there are less columns than what the user specified */
            if (userColumnsInt > actualColumnsInt) {
                /* If there areâ€¦ */
                $(this).removeClass("l-cols-" + userColumnsInt); /* Remove the user defined column class because it exceeds what's there */
                $(this).each(function(){
                    var columnClassGrid = "l-cols-" + actualColumns;
                    $(this).addClass(columnClassGrid); /* Add a column class for what's really there */
                });
            }
        });


        /* =JFG. GROUP MODERNIZR - Modernizr-like features that every theme should get by default
        =================================================== */
        $('html').removeClass("no-js"); /* Remove the class .no-js since we know the user definitely has JavaScript enabled if this jQuery is executed - related to CC-2934 */


        /* =JFG. GROUP PRODUCT DETAIL
        =================================================== */
        /* CC-2934; review form should be hidden for every theme; not a theme-by-theme basis */
        $("#productDetail .review--product-form").hide();
    });

