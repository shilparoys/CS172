            $(document).ready(function(){

                    //$('#nav > li:last-child a').css("background","none");

                    $('#priorities').cycle({
                            delay:  3000,
                            speed:  500,
                            before: onBefore
                    });

                    function onBefore() {
                            $('#priorities_caption').html(this.alt);
                    };

                    $('img.flash').click(function(){
                            $(this).hide();
                            $(this).next('object').show();
                    });

                    $('a.full_story_link').click(function(){
                            story_id = $(this).attr('rel');
                            $('div.full_story').hide(); // hide others first
                            $('div.excerpt').show(); // show other excerpts that may be hidden
                            $('.full_story_link').show(); // show all links
                            $('#'+story_id+' div.excerpt').hide();
                            $('#'+story_id+' p .full_story_link').hide(); // hide current link
                            $('#'+story_id+' div.full_story').slideDown('slow',fixSidebar);
                            
                            return false;
                    });

            });

        </script> 
