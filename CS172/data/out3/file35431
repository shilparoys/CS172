  <script type="text/javascript">
  function clearTips() {
    $(".joyride-tip-guide").remove();
  }
  $(document).ready( function() {
    $("#tour-link").click(function() {
      clearTips();
      $(window).joyride({
        'postRideCallback': clearTips
      });
    });
    //add tour class to topnav
    $(".nav .dropdown a.dropdown-toggle[href='research']").attr("id","tourstop_mainnav");
    //fix bootstrap dropdown menus in iOS
    $('body').on('touchstart.dropdown', '.dropdown-menu', function (e) {
        e.stopPropagation();
    });
  });
    </script>   
