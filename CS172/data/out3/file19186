       <script type="text/javascript"> 
var iframeURL;
//var layout = Math.floor(Math.random()*(2-1+1)+1);
var layout = 0;

//var container = window.location.pathname.replace(/[\\\/][^\\\/]*$/, '');
var container = window.location.pathname.substring(1);
//alert(container);

if (container.indexOf(".html") != -1) {
    layout = 0;
} else if(container.indexOf("video/") != -1) {
    layout = 0;
} else {
    layout = 1;
}

if (layout == 1) {
	iframeURL = '/right_rail_daily_tech_briefing.html';
	document.getElementById('whitePaperIFrame').setAttribute("src", iframeURL);
	document.getElementById('whitePaperIFrame').setAttribute("height", 810);
}

var updateIframe = false;
var t = 0;

function updateQMPIframe(){
    if (!updateIframe) { 
        if ($('#whitePaperIFrame').contents().find('.asset').length > 0) {
            var height = $('#whitePaperIFrame').contents().find('#assetsListings').height();
            height = height + 25;
            var iframe = document.getElementById('whitePaperIFrame');  
            iframe.setAttribute( 'height', height + 25);
            updateIframe = true;
            clearTimeout(t);
        } else {
            t = setTimeout('updateQMPIframe()', 1000);
        }
    }
}
$(document).ready(function(){
    updateQMPIframe();
});


</script> 
