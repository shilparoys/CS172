     <script type="text/javascript">
			function footerShow() {

				if ($('#footer').is('.pg-footer-hide') ) {

					$('#footer').removeClass('pg-footer-hide').addClass('pg-footer-show');
					$('#glyphicon').removeClass('glyphicon-chevron-up').addClass('glyphicon-chevron-down');
					$('#legal').removeClass('legal-top').addClass('legal-top-full');
						//$(document).scrollTop($(document).height());
						$("html, body").animate({
							scrollTop : $(document).height()
						}, 500);

					} else {
						$('#footer').removeClass('pg-footer-show').addClass('pg-footer-hide');
						$('#glyphicon').removeClass('glyphicon-chevron-down').addClass('glyphicon-chevron-up');
						$('#legal').removeClass('legal-top-full').addClass('legal-top');
					}
				}

				</script> 
     <div class="footer-show" style="z-index:100" onclick="footerShow();"> 
      <span id="glyphicon" class="glyphicon glyphicon-chevron-up" style="color:white;font-size:15px;top:-11px;"></span> 
     </div> 
