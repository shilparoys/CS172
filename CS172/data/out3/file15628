  <!-- feedback scripts --> 
  <script>
    function showHint(str,name,email,message) 
    {
        if (str.length==0) 
        { 
            document.getElementById("txtHint").innerHTML="";
            return;
        }

        var xmlhttp=new XMLHttpRequest();
        xmlhttp.onreadystatechange=function()
     {
        if (xmlhttp.readyState==4 && xmlhttp.status==200)
         {
          document.getElementById("txtHint").innerHTML=xmlhttp.responseText;
         }
     }

      xmlhttp.open("GET","http://www.pdn.ac.lk/uop/feedback/insert.php?name="+name+"&email="+email+"&message="+message,true);
      xmlhttp.send();
    }

    </script> 
  <script>
        function showHintTwo(str,currenturl,otherurl,description) 
    {
        if (str.length==0) { 
        document.getElementById("txtHintTwo").innerHTML="";
        return;
    }

        var xmlhttp=new XMLHttpRequest();
        xmlhttp.onreadystatechange=function() 
    {

        if (xmlhttp.readyState==4 && xmlhttp.status==200)
        {

        document.getElementById("txtHintTwo").innerHTML=xmlhttp.responseText;
        }

    }

        xmlhttp.open("GET","http://www.pdn.ac.lk/uop/feedback/insertproblem.php?currenturl="+currenturl+"&otherurl="+otherurl+"&description="+description,true);
        xmlhttp.send();
    }

    </script> 
  <script type="text/javascript">    

    function disablecurrenturl()
    {    
        if(document.getElementById("checkMe").checked == true)
      {    
          document.getElementById("currenturl").disabled = true;    
      }
        else
      {  
        document.getElementById("currenturl").disabled = false;  
      }    
    }

    </script> 
  <script type="text/javascript">
    function select_all()
    {
    var text_val=eval("document.formurl.type");
    text_val.focus();
    text_val.select();
    }
</script> 
