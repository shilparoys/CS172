          <script type="text/javascript"> 
                             
                                        var imagesList = [
                                        "http://sfedu.ru/www/docs/F2692/2-27-minkin2_md.jpg"
                                        ];
                                        var len = imagesList.length
                                        var aImages = [];
                                        for (var i = 0; i < len; i++) {
                                            aImages[i] = new Image();
                                            aImages[i].src = imagesList[i];
                                            aImages[i].id = 'img_con';
                                            aImages[i].width = 234;
                                            aImages[i].height = 160
                                        }
                                        var k = 0;
                                        var pobj = document.getElementById('img_href');
                                        var obj = document.getElementById('img_con');
                                        
                                        
                                        
                                        
                                        function change(p) {
 
                                           
                                            if (p == 1) {
                                                if (k + 1 < len) {
                                                    k = k + 1
                                                }
                                                else {
                                                    k = 0
                                                }
                                            }
                                            else {
                                                if (k > 0) {
                                                    k = k - 1
                                                }
                                                else {
                                                    k = len - 1
                                                }
                                            }
 
                                            var childobj = pobj.replaceChild(aImages[k], obj);
                                            $("#img_con").attr('width', '234');
                                            $("#img_con").attr('height', '160');
                                            $("#img_con").attr('border', '0');
                                            obj = document.getElementById('img_con');
                                    
                                            $(".widgets-slideshow-txt").children("div").hide();
                                            $(".widgets-slideshow-txt").children("div").eq(k).show();
                                            var ppp = $(".widgets-slideshow-txt").children("div").eq(k).children("div").children("a").attr("href");
                                            $("#img_href").attr("href", ppp);
 
                                            $("#widgets-slideshow-progress").stopTime('Timer1');
                                            $("#widgets-slideshow-progress").everyTime(10000, 'Timer1', function() {
                                                change(1);
                                            });
                                            
                                            $("#widgets-slideshow-progress").stop(false, true);
                                            $("#widgets-slideshow-progress").css("width", "0%");
                                            $("#widgets-slideshow-progress").animate({width:"100%"}, 9970);
                                        }
 
                                        $(document).ready(function() {
                                            $("td.images").eq(1).animate({opacity:"0.2"});
                                            $("td.images").eq(2).animate({opacity:"0.2"});
                                            
                                            // Запуск слайдшоу
                                            change(1);
                                        });
                                        /*********************************************************************************/
                                    </script> 
