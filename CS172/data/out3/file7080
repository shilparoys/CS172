			$(document).ready(function () {
				// Обрезка блока новости
				while (($("ul.nws").height() > (270))) {
					$("ul.nws li:last").remove();
				}
				
				/* deprecated since rector materials are of manual edit
				// Обрезка блока Материалы ректорских совещаний
				$("span.hidem").remove();
				while (($("ul.rectmat").height() > (170+7))) {
					$("ul.rectmat li:last").remove();
				}*/
				
				// serurity patch
				// При внезапном появлении региона с капчей (март 2012) перезагрузить страницу
					$("#unavailable_service").each(function(){
						window.location.reload();
					});
					
				//Ищет картинку для главной с именем search_for и заменяет соответствующий ей пункт меню на баннер src_banner
				// настройки
				var search_for = "glavn_stalingrad";
				var src_banner = "http://sfedu.ru/banner_stalingrad2.jpg";
				// реализация
				var newimg= $('<img width="235px" height="72px" src="'+src_banner+'" style="border: 0px none; margin: 0px 0px 0px -10px;">');
				var imgFullPath = "";
				$("img").each(function(){
					s = $(this).attr("src");
					if (s.lastIndexOf(search_for)>=0) {
						imgFullPath = s;
					}
				});
				
				if (imgFullPath.lastIndexOf(search_for)>=0) {
					$("img[src='"+imgFullPath+"']").eq(1).parent('div').html(newimg);	
				}	
				//----	

				// "100 дней ректора"
