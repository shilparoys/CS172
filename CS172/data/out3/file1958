<!-- (hide from old browsers)
var userpwdFocus = false;
var hasRecovery = false
var recoveryUri = ""
function hadFocus(set)
{
    if (set) userpwdFocus = true;
    return userpwdFocus;
}

function getCookie(name) 
{
    var search = name + "="
    if (document.cookie.length > 0) 
    {
        // if there are any cookies
        offset = document.cookie.indexOf(search)
        if (offset != -1) 
        { // if cookie exists
            offset += search.length // set index of beginning of value
            end = document.cookie.indexOf(";", offset) // set index of end of cookie value
            if (end == -1)
                end = document.cookie.length
            return unescape(document.cookie.substring(offset, end))
        }
    }
}

function clearCache()
{
    var name="usid"
    var value="chocolate-chip-cookies"
    cookie= name + "=" + value + "; path=/; domain=.citadel.edu; expires=" + (new Date(0)).toGMTString();
    if (getCookie (name) != value) document.cookie = cookie;
    doCancel();
}

function doCancel()
{
    document.userid.user.value='';
    document.cplogin.user.value='';
    document.cplogin.pass.value='';
}

function OpenWin(w)
{
    var unDefined;

    if (w == "help")
    {
        stats='resizable=yes,toolbar=no,location=no,directories=no,status=yes,menubar=no,'
        stats += 'scrollbars=yes,width=650,height=475';
        helpWindow = window.open ("http://lesesnegateway.citadel.edu/site/loginhelp.html","helpWindow",stats)
        setTimeout ("helpWindow.focus ()", 500);
    }
}

function xferFocus()
{
    document.getElementById("pass").focus();
}

var clientServerDelta = (new Date()).getTime() - 1425258444455;
function login() 
{
    setQueryAsCookie();
    document.cplogin.user.value=document.userid.user.value;
    if ( document.cplogin.uuid )
    {
        document.cplogin.uuid.value=(new Date()).getTime() - clientServerDelta;
    }
    //document.cplogin.uuid.value="1425258444455";
    document.cplogin.submit();
}
function setQueryAsCookie()
{
    var query = document.location.search;
    if (query.length > 1) {
        document.cookie = "query=" + encodeURIComponent(query) + "; path=/; domain=.citadel.edu;"
    } else {
        // kill any previous query cookie setup
        document.cookie = "query=; path=/; domain=.citadel.edu; expires=" + (new Date(0)).toGMTString();
    }
}

function pageLoad()
{
    if (!hadFocus(false))
    {
        document.getElementById("user").focus();
    }
    checkRecovery();
}

function checkRecovery()
{
	if ( hasRecovery == false ) 
	{
        if (document.getElementById("forgetPass") != null)
	    {
            document.getElementById("forgetPass").style.display="none";
        }
    }
    else
	{
        document.getElementById("recover").setAttribute('href', recoveryUri);
	}
}

var size = "15";
if ( ( is_nav4up || is_ie4up ) && is_mac ) { size = "12"; }
else if ( is_nav4_7up) { size = "10"; }

// === Begin Citadel customization at 1303 on Fri 2009-JUL-31. ===
var xmlhttp01;
function loadXMLDoc01(url) {
  xmlhttp01=null;
  if (window.XMLHttpRequest) {
    // code for Firefox, Opera, IE7, etc.
    xmlhttp01=new XMLHttpRequest();
  } else if (window.ActiveXObject) {
    // code for IE6, IE5
    xmlhttp01=new ActiveXObject("Microsoft.XMLHTTP");
  }
  if (xmlhttp01!=null) {
    xmlhttp01.onreadystatechange=state_Change_01;
    xmlhttp01.open("GET",url,true);
    xmlhttp01.send(null);
  } else {
    ////alert("Your browser does not support XMLHTTP.");
  }
}
function state_Change_01() {
  if (xmlhttp01.readyState==4) {
    // 4 = "loaded"
    if (xmlhttp01.status==200) {
      // 200 = "OK"
      document.getElementById('divPortalStatus01').innerHTML=xmlhttp01.responseText;
    } else {
      ////alert("Problem retrieving data:" + xmlhttp01.statusText);
    }
  }
}
// === End Citadel customization at 1303 on Fri 2009-JUL-31. ===

