function valForm()
{
  if (ttrim(document.frmZip.txtZip.value) == "")
  {
    alert("Please enter the Zip code to proceeed");
    document.frmZip.txtZip.focus();
    return false;
  }
  else
  {
    if (document.frmZip.txtZip.value.length != 5)
    {
      alert("Zip code should be length of 5 numeric digits. Please try again!!!");
      document.frmZip.txtZip.select();
      document.frmZip.txtZip.focus();
      return false;
    }
    if (isNumber(document.frmZip.txtZip.value) == false)
    {
      alert("Only number allowed. Please try again!!!");
      document.frmZip.txtZip.select();
      document.frmZip.txtZip.focus();
      return false;
    }
  }
  document.frmZip.txtFormVal.value = "1";
  document.frmZip.submit();
}
