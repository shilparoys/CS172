    <script language="javascript" type="text/javascript">

  var userId = 0;			// Assign User ID to local JS Variable 
  var access = '';



	
  //<![CDATA[
  // ==================================
  // ====== Gobal variables  ==========
  var map;											 // Google Map Object
  var safodMapProjection;							 // New Map Projection - Using Mercator
  
  var hole_run = "1";
  var tileURL = "hole_e/hole_E_run_1_sec_";
  var selOption = 1;
  var sectionAr = new Array('1','8', '6', '6', '9', '2', '7', '7', '6');
  var sectionCnt = sectionAr[selOption];


  var sampleIdAr = new Array();
  var phaseAr =  new Array();				 
  var sholeAr =  new Array();				 
  var srunAr =  new Array();				 
  var ssectionAr =  new Array();				 
  var intervalAr =  new Array();				 
  var massAr =  new Array();				 
  var pictureAr =  new Array();	
  var piNameAr = new Array();
			
  var shapeTypeAr = new Array();					 // Type of Shape Array  
  var measureAr = new Array();					     // Sample Measurment Text Array
  var reqIdNumAr = new Array();					 // Sample Request ID Num Array
  var shapeAr = new Array();	
  var shapeSizeAr = new Array();					 // Sample Request Shape Object Array
  var shapeMarker = new Array();					 // Array tr Shape Related Info Window Point Coords
  var markerHTML =  new Array();					 // Array of Shape Related HTML for nfoWin display
  var xlylMarkerAr =  new Array();					 // Array of Lower Left Corner Marker
  var xryrMarkerAr =  new Array();					 // Array of Lower Top Right Marker
  var cntrMarkerAr =  new Array();					 // Array of Center Markers for Point/Circles
  var piSampleNumAr =  new Array();				 // PI Sample Number Array


  var req_intervalAr =  new Array();				 
  var req_massAr =  new Array();				 
			
  var req_shapeTypeAr = new Array();					 // Type of Shape Array  
  var req_shapeAr = new Array();	
  var req_shapeSizeAr = new Array();					 // Sample Request Shape Object Array
  var req_shapeMarker = new Array();					 // Array tr Shape Related Info Window Point Coords
  var req_markerHTML =  new Array();					 // Array of Shape Related HTML for nfoWin display
  var req_xlylMarkerAr =  new Array();					 // Array of Lower Left Corner Marker
  var req_xryrMarkerAr =  new Array();					 // Array of Lower Top Right Marker
  var req_cntrMarkerAr =  new Array();					 // Array of Center Markers for Point/Circles
  var req_refNumIdAr = new Array();	 
  var reqIdAr = new Array();	 
  var req_refNumIdAr = new Array();	

  var all_intervalAr =  new Array();				 
  var all_massAr =  new Array();				 
			
  var all_shapeTypeAr = new Array();					 // Type of Shape Array  
  var all_shapeAr = new Array();	
  var all_shapeSizeAr = new Array();					 // Sample alluest Shape Object Array
  var all_shapeMarker = new Array();					 // Array tr Shape Related Info Window Point Coords
  var all_markerHTML =  new Array();					 // Array of Shape Related HTML for nfoWin display
  var all_xlylMarkerAr =  new Array();					 // Array of Lower Left Corner Marker
  var all_xryrMarkerAr =  new Array();					 // Array of Lower Top Right Marker
  var all_cntrMarkerAr =  new Array();					 // Array of Center Markers for Point/Circles
  var all_refNumIdAr = new Array();	 
  var allIdAr = new Array();	 
  var all_refNumIdAr = new Array();	

  // ======================================================================
  // ====== AJAX Function that obtains Core Section data in XML  ==========
  // ====== Utilizes Google GXML Obj from AJAX call to server    ==========
  // ====== Coordinate Data loaded to GLatLng objects for Polygons,   =====
  // ====== Circle, and Point generation in GMap API  =====================
  

   function getSection() {

      shapeAr = new Array();
	  shapeSizeAr = new Array();
	  measureAr = new Array();
	  reqIdNumAr = new Array();
	  shapeTypeAr = new Array();
      piSampleNumAr = new Array();
	  piNameAr = new Array();
   
      sampleIdAr = new Array();
      phaseAr =  new Array();				 
      sholeAr =  new Array();				 
      srunAr =  new Array();				 
      ssectionAr =  new Array();				 
      intervalAr =  new Array();				 
      massAr =  new Array();				 
      pictureAr =  new Array() 
	  
      req_intervalAr =  new Array();				 
      req_massAr =  new Array();				 
			
      req_shapeTypeAr = new Array();					 // Type of Shape Array  
      req_shapeAr = new Array();	
      req_shapeSizeAr = new Array();					 // Sample Request Shape Object Array
      req_shapeMarker = new Array();					 // Array tr Shape Related Info Window Point Coords
      req_markerHTML =  new Array();					 // Array of Shape Related HTML for nfoWin display
      req_xlylMarkerAr =  new Array();					 // Array of Lower Left Corner Marker
      req_xryrMarkerAr =  new Array();					 // Array of Lower Top Right Marker
      req_cntrMarkerAr =  new Array();					 // Array of Center Markers for Point/Circles
      req_refNumIdAr = new Array();	 
      reqIdAr = new Array();	 
      req_refNumIdAr = new Array();	
 
	  
      all_intervalAr =  new Array();				 
      all_massAr =  new Array();				 
			
      all_shapeTypeAr = new Array();					 // Type of Shape Array  
      all_shapeAr = new Array();	
      all_shapeSizeAr = new Array();					 // Sample Request Shape Object Array
      all_shapeMarker = new Array();					 // Array tr Shape Related Info Window Point Coords
      all_markerHTML =  new Array();					 // Array of Shape Related HTML for nfoWin display
      all_xlylMarkerAr =  new Array();					 // Array of Lower Left Corner Marker
      all_xryrMarkerAr =  new Array();					 // Array of Lower Top Right Marker
      all_cntrMarkerAr =  new Array();					 // Array of Center Markers for Point/Circles
      all_refNumIdAr = new Array();	 
      allIdAr = new Array();	 
      all_refNumIdAr = new Array();	
	  
	  	  
      var selectedMapType = map.getMapTypeId();
      hideAllRequest()
	  var section = selectedMapType.substr(4,1); 
	  

	
         // ********    CUSTOMIZED FOR CORE HOLE AND RUN    ***********
		 // ***********************************************************

		switch (hole_run)
		{
			case 'E_1':
  			  var hrTitle = "Hole E - Run 1 - Section ";
			  var tileURL = "hole_e/hole_E_run_1_sec_";
			  var gXMLURL = "E_1_";
			  sectionCnt = 8;
  			break;
			case 'E_2':
  			  var hrTitle = "Hole E - Run 2 - Section ";
			  var tileURL = "hole_e/hole_E_run_2_sec_";
			  var gXMLURL = "E_2_";
			  sectionCnt = 6;
  			break;
			case 'G_1':
  			  var hrTitle = "Hole G - Run 1 - Section ";
			  var tileURL = "hole_g/hole_G_run_1_sec_";
			  var gXMLURL = "G_1_";
			  sectionCnt = 6;
  			break;
			case 'G_2':
  			  var hrTitle = "Hole G - Run 2 - Section ";
			  var tileURL = "hole_g/hole_G_run_2_sec_";
			  var gXMLURL = "G_2_";
			  sectionCnt = 9;
  			break;
			case 'G_3':
  			  var hrTitle = "Hole G - Run 3 - Section ";
			  var tileURL = "hole_g/hole_G_run_3_sec_";
			  var gXMLURL = "G_3_";
			  sectionCnt = 2;
  			break;
			case 'G_4':
  			  var hrTitle = "Hole G - Run 4 - Section ";
			  var tileURL = "hole_g/hole_G_run_4_sec_";
			  var gXMLURL = "G_4_";
			  sectionCnt = 7;
  			break;
			case 'G_5':
  			  var hrTitle = "Hole G - Run 5 - Section ";
			  var tileURL = "hole_g/hole_G_run_5_sec_";
			  var gXMLURL = "G_5_";
			  sectionCnt = 7;
  			break;
			case 'G_6':
  			  var hrTitle = "Hole G - Run 6 - Section ";
			  var tileURL = "hole_g/hole_G_run_6_sec_";
			  var gXMLURL = "G_6_";
			  sectionCnt = 6;
  			break;
   			default:
  			  var hrTitle = "Hole E - Run 1 - Section ";
			  var tileURL = "hole_e/hole_E_run_1_sec_";
			  var gXMLURL = "E_1_";
			  sectionCnt = 8;
  			break;
        }
	
	
	  document.title = hrTitle + section;

	  document.getElementById('holeSecTitle').innerHTML = "Core Viewer Selected Section:  " + hrTitle + section;
	 
	  tile_url = "/images/es_data_page/safod_core_data/" + tileURL + section + "_tiles/";
	  
	  var gURL = "/samples/safod_sample_data/" + gXMLURL + section + "/" + userId + "/" + 0;    <!--  Implement UID/PIGroupID for Authenticated  PI -->
	  
	  var reqURL = "/samples/safod_request_data" + gXMLURL + section + "/" + userId + "/" + 0;    <!--  Implement UID/PIGroupID for Authenticated  PI -->
	  
	  
	  

    map.setCenter(new google.maps.LatLng(0,-25.55));
    map.setZoom(2);
    var newMapTypeId = "sec_1";
    if(section >= 1 && section <= 9) newMapTypeId = "sec_" + section;
    if(map.getMapTypeId() != newMapTypeId) map.setMapTypeId(newMapTypeId);
	  
	    
	
         // ********    END CUSTOMIZATION   ***********
		 // *******************************************

	
      // ==============================================================
      // === Google AJAX Call to URL:  /safod_core/section_data   ======= 

		GDownloadUrl(gURL, function(data, responseCode) {
		
  		var xml = GXml.parse(data);
  		var sectionMarkers = xml.documentElement.getElementsByTagName("marker");
		
  		  for (var i = 0; i < sectionMarkers.length; i++) {
		    var xrLat = parseFloat(sectionMarkers[i].getAttribute("xrlat"));
			var yrLng = parseFloat(sectionMarkers[i].getAttribute("yrlng"));
			var xlLat = parseFloat(sectionMarkers[i].getAttribute("xllat"));
			var ylLng = parseFloat(sectionMarkers[i].getAttribute("yllng"));
			

			

			
			var infoWin = GXml.value(sectionMarkers[i].getElementsByTagName("infowindow")[0]);
			var piNum = sectionMarkers[i].getAttribute("piNum");
			var modified = sectionMarkers[i].getAttribute("modified");
			var shape = sectionMarkers[i].getAttribute("shape");
		    var baseDepth = sectionMarkers[i].getAttribute("baseDepth");
			var measurement = sectionMarkers[i].getAttribute("measurement");
			var piColor	 = sectionMarkers[i].getAttribute("piColor");
			
			var tableData = GXml.value(sectionMarkers[i].getElementsByTagName("tabledata")[0]);
			var shapeSize = sectionMarkers[i].getAttribute("shape_size");
			
			
			if(shapeSize == null) {shapeSize = 9;}
			

	        
			
			sampleIdAr.push(sectionMarkers[i].getAttribute("sampleId"));
			phaseAr.push(sectionMarkers[i].getAttribute("phase"));
			sholeAr.push(sectionMarkers[i].getAttribute("shole"));
			srunAr.push(sectionMarkers[i].getAttribute("srun"));
			ssectionAr.push(sectionMarkers[i].getAttribute("ssection"));
			intervalAr.push(sectionMarkers[i].getAttribute("interval"));
			massAr.push(sectionMarkers[i].getAttribute("mass"));
			pictureAr.push(sectionMarkers[i].getAttribute("picture"));
			piNameAr.push(sectionMarkers[i].getAttribute("piName"));
					
			measureAr.push(sectionMarkers[i].getAttribute("measurement"));
			reqIdNumAr.push(parseFloat(sectionMarkers[i].getAttribute("reqIdNum")));
			shapeTypeAr.push(sectionMarkers[i].getAttribute("shape"));
			shapeSizeAr.push(sectionMarkers[i].getAttribute("shape_size"));
			piSampleNumAr.push(piNum);
			
			
			if(shape != null){
		 	 
		     // ==============================================================
      		 // === Create Shape for request item by Shape Type Value ======= 
			 
		    if(shape == 'R') {
			
			
			    cntrMarkerAr[shapeAr.length] =  null;
			    var markerPoint = new google.maps.LatLng(yrLng, xrLat);
		
		        
				
	            var rectAr =[];
	    		rectAr.push(new google.maps.LatLng(yrLng, xlLat));
	    		rectAr.push(new google.maps.LatLng(ylLng, xlLat));
	    		rectAr.push(new google.maps.LatLng(ylLng, xrLat));	  
	    		rectAr.push(new google.maps.LatLng(yrLng, xrLat));
    		    rectAr.push(new google.maps.LatLng(yrLng, xlLat));	
				
	       		var newObj = drawPolygon(rectAr, piColor, infoWin, markerPoint);
				shapeAr.push(newObj);
				
				

		     }
		    else if(shape == 'C') {
  
	  			xlylMarkerAr[shapeAr.length] =  null;
      			xryrMarkerAr[shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(ylLng, xlLat);
		        var newObj = drawCircle(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), piColor, infoWin);
				shapeAr.push(newObj);
				
				
		     }
		    else{
							  
	  			xlylMarkerAr[shapeAr.length] =  null;
      			xryrMarkerAr[shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(ylLng, xlLat);
		        var newObj = drawPoint(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), piColor, infoWin);
				shapeAr.push(newObj);
		     }		    		
	       
		 
		   // ============================================================================
           // === Create Marker for Info Window Display on Request Item Mouse Over ======= 	
           // === or Shape Mouse Click  ================================================== 	
			
		    shapeMarker[i] = markerPoint;
			markerHTML[i] = infoWin;
		
		   }
			
		  }
		  
	
		 
		 document.getElementById('sectionData').innerHTML = tableData;
		
		
  		});
	
	
    if(!isNaN(userId) && userId > 0  && edit_status != 'R' &&  edit_status != 'E'  )  {
	
	  var reqURL = "/samples/safod_request_data/" + gXMLURL + section + "/" + userId + "/" + 0;    <!--  Implement UID/PIGroupID for Authenticated  PI -->
	  //alert(reqURL);
	   

		GDownloadUrl(reqURL, function(data, responseCode) {
		
  		var xml = GXml.parse(data);
  		var sectionMarkers = xml.documentElement.getElementsByTagName("marker");
		
		
		
  		  for (var i = 0; i < sectionMarkers.length; i++) {
		  
		  
		  
		    var req_xrLat = parseFloat(sectionMarkers[i].getAttribute("xrlat"));
			var req_yrLng = parseFloat(sectionMarkers[i].getAttribute("yrlng"));
			var req_xlLat = parseFloat(sectionMarkers[i].getAttribute("xllat"));
			var req_ylLng = parseFloat(sectionMarkers[i].getAttribute("yllng"));
			var req_piColor	 = sectionMarkers[i].getAttribute("piColor");


			var req_infoWin = GXml.value(sectionMarkers[i].getElementsByTagName("infowindow")[0]);
			var req_modified = sectionMarkers[i].getAttribute("modified");
			var req_shape = sectionMarkers[i].getAttribute("shape");
			
			var req_tableData = GXml.value(sectionMarkers[i].getElementsByTagName("tabledata")[0]);
			var req_shapeSize = sectionMarkers[i].getAttribute("shape_size");
			
			
			if(req_shapeSize == null) {req_shapeSize = 9;}
		
		    reqIdAr.push(sectionMarkers[i].getAttribute("reqId"));		
			req_refNumIdAr.push(sectionMarkers[i].getAttribute("reqRefNum"));
			req_intervalAr.push(sectionMarkers[i].getAttribute("interval"));
			req_massAr.push(sectionMarkers[i].getAttribute("mass"));
			req_shapeTypeAr.push(sectionMarkers[i].getAttribute("shape"));
			req_shapeSizeAr.push(sectionMarkers[i].getAttribute("shape_size"));
			
			
			if(req_shape != null){
			
		     // ==============================================================
      		 // === Create Shape for request item by Shape Type Value ======= 
			 
		    if(req_shape == 'R') {
			
			    req_cntrMarkerAr[req_shapeAr.length] =  null;
			    var markerPoint = new google.maps.LatLng(req_yrLng, req_xrLat);
		
	            var rectAr =[];
	    		rectAr.push(new google.maps.LatLng(req_yrLng, req_xlLat));
	    		rectAr.push(new google.maps.LatLng(req_ylLng, req_xlLat));
	    		rectAr.push(new google.maps.LatLng(req_ylLng, req_xrLat));	  
	    		rectAr.push(new google.maps.LatLng(req_yrLng, req_xrLat));
    		    rectAr.push(new google.maps.LatLng(req_yrLng, req_xlLat));	
				
	       		var newObj = req_drawPolygon(rectAr, req_piColor, req_infoWin, markerPoint);	       		
				req_shapeAr.push(newObj);
				

		     }
		    else if(req_shape == 'C') {
  
	  			req_xlylMarkerAr[req_shapeAr.length] =  null;
      			req_xryrMarkerAr[req_shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(req_ylLng, req_xlLat);
		        var newObj = req_drawCircle(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), req_piColor, req_infoWin);
				req_shapeAr.push(newObj);
				
				
		     }
		    else{
							  
	  			req_xlylMarkerAr[req_shapeAr.length] =  null;
      			req_xryrMarkerAr[req_shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(req_ylLng, req_xlLat);
		        var newObj = req_drawPoint(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), req_piColor, req_infoWin);
				req_shapeAr.push(newObj);
		     }		    		
	
		 
		   // ============================================================================
           // === Create Marker for Info Window Display on Request Item Mouse Over ======= 	
           // === or Shape Mouse Click  ================================================== 	
			
		    req_shapeMarker[i] = markerPoint;
			req_markerHTML[i] = req_infoWin;
			
			}
		  }
		  
		 document.getElementById('digitizedQue').innerHTML = req_tableData;
  		});
		
      }
	  
	  if(!isNaN(userId) && userId > 0) {   //Show - Hide All Sample Reuqests on the core viewer
	 
	  var reqURL = "/samples/safod_allrequest_data/" + gXMLURL + section + "/" + userId + "/" + 0;    <!--  Implement UID/PIGroupID for Authenticated  PI -->
	  
	  

		GDownloadUrl(reqURL, function(data, responseCode) {
		
  		var xml = GXml.parse(data);
  		var sectionMarkers = xml.documentElement.getElementsByTagName("marker");
		
		
		
  		  for (var i = 0; i < sectionMarkers.length; i++) {
		  
		  
		  
		    var all_xrLat = parseFloat(sectionMarkers[i].getAttribute("xrlat"));
			var all_yrLng = parseFloat(sectionMarkers[i].getAttribute("yrlng"));
			var all_xlLat = parseFloat(sectionMarkers[i].getAttribute("xllat"));
			var all_ylLng = parseFloat(sectionMarkers[i].getAttribute("yllng"));
			var all_piColor	 = sectionMarkers[i].getAttribute("piColor");


			var all_infoWin = GXml.value(sectionMarkers[i].getElementsByTagName("infowindow")[0]);
			var all_modified = sectionMarkers[i].getAttribute("modified");
			var all_shape = sectionMarkers[i].getAttribute("shape");
			
			var all_tableData = GXml.value(sectionMarkers[i].getElementsByTagName("tabledata")[0]);
			var all_shapeSize = sectionMarkers[i].getAttribute("shape_size");
			
			
			if(all_shapeSize == null) {all_shapeSize = 9;}
		
		    allIdAr.push(sectionMarkers[i].getAttribute("reqId"));		
			all_refNumIdAr.push(sectionMarkers[i].getAttribute("reqRefNum"));
			all_intervalAr.push(sectionMarkers[i].getAttribute("interval"));
			all_massAr.push(sectionMarkers[i].getAttribute("mass"));
			all_shapeTypeAr.push(sectionMarkers[i].getAttribute("shape"));
			all_shapeSizeAr.push(sectionMarkers[i].getAttribute("shape_size"));
			
			
			
			if(all_shape != null){
			
		     // ==============================================================
      		 // === Create Shape for request item by Shape Type Value ======= 
			 
		    if(all_shape == 'R') {
			
			    all_cntrMarkerAr[all_shapeAr.length] =  null;
			    var markerPoint = new google.maps.LatLng(all_yrLng, all_xrLat);
		
	            var rectAr =[];
	    		rectAr.push(new google.maps.LatLng(all_yrLng, all_xlLat));
	    		rectAr.push(new google.maps.LatLng(all_ylLng, all_xlLat));
	    		rectAr.push(new google.maps.LatLng(all_ylLng, all_xrLat));	  
	    		rectAr.push(new google.maps.LatLng(all_yrLng, all_xrLat));
    		    rectAr.push(new google.maps.LatLng(all_yrLng, all_xlLat));	
				
	       		var newObj = req_drawPolygon(rectAr, all_piColor, all_infoWin, markerPoint);	       		
				all_shapeAr.push(newObj);
				

		     }
		    else if(all_shape == 'C') {
  
	  			all_xlylMarkerAr[all_shapeAr.length] =  null;
      			all_xryrMarkerAr[all_shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(all_ylLng, all_xlLat);
		        var newObj = req_drawCircle(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), all_piColor, all_infoWin);
				all_shapeAr.push(newObj);
				
				
		     }
		    else{
							  
	  			all_xlylMarkerAr[all_shapeAr.length] =  null;
      			all_xryrMarkerAr[all_shapeAr.length] =  null;
				
			    var markerPoint = new google.maps.LatLng(all_ylLng, all_xlLat);
		        var newObj = req_drawPoint(safodMapProjection, markerPoint, sectionMarkers[i].getAttribute("shape_size"), all_piColor, all_infoWin);
				all_shapeAr.push(newObj);
		     }		    		
	
		 
		   // ============================================================================
           // === Create Marker for Info Window Display on Request Item Mouse Over ======= 	
           // === or Shape Mouse Click  ================================================== 	
			
		    all_shapeMarker[i] = markerPoint;
			all_markerHTML[i] = all_infoWin;
			}
			
		  }
		  
		 document.getElementById('otherPIGroups').innerHTML = all_tableData;     //Disabled All Other PI Samples Requests until 1 Aug 2013 end of sample requests
  		});
		
	  }     //Show - Hide All Sample Requests on the Core Viewer
		
   }


  
  
   // ============================================================================
   // === DOJO FORM UPDATE AJAX Call to URL:     ======= 

  function updateCoords() {
  

    if(document.getElementById('updateCoord').reqRefNum.value == '') { alert ('You are only allowed to digitize your own sample requests.'); getSection(); }
	else {

    var bindArgs = {
        url: "/samples/digitizeRequestData",
		mimetype:   "text/plain",
        method: "POST", 
		formNode:document.getElementById('updateCoord'),
		load: function(type,data,evt) {
		  
		}
    };
	
	
	var requestObj = dojo.io.bind(bindArgs);
	
	setTimeout('getSection()', 1000);
	setTimeout('document.getElementById(\'digitize_confirm\').style=\'hidden\'', 3000);
	setTimeout('document.getElementById(\'digitize_confirm\').innerHTML=\'\'', 3100);

    document.getElementById('digitizeForm').style.visibility = 'hidden';
    //document.getElementById('digitizeForm').style.zIndex=-22;
	
	}
   
  }
  
  
  // ===============================================
  // ====== Hide All Request Map Objects    ========	
  
  function hideAllRequest() {
    while(allMarkers.length > 0) allMarkers.pop().setMap(null);
  }
  
  
  
    // ===============================================
  // ====== Show All Request Map Objects    ========	
  
  function showAllRequest() {
 
   var sampleForm = document.getElementById('sampleRecords');

	
   for (i=0; i < sampleForm.elements.length; i++) {
   	   if(sampleForm.elements[i].id != 99 || sampleForm.elements[i].id != 99) { 
           sampleForm.elements[i].checked = true;
	   }
	   
       
    }
   for (i=0; i < shapeAr.length; i++) addToMap(shapeAr[i]);
    
  }
  
  
  
  
  function editVisible() {
	  
	
  var shapeObj = null;
   var sampleForm = document.getElementById('sampleRecords');
   var requestForm = document.getElementById('requestRecords');
   
   var lastSelectedId = 0;
 
   var updateForm = document.getElementById('updateCoord');
   
   document.getElementById('digitizeForm').style.visibility = 'visible';
   document.getElementById('digitizeForm').style.zIndex=1;
   
   updateForm.reset();
   

   

    
   for (i=0; i < req_shapeAr.length; i++) {  
   
  
      shapeObj = req_shapeAr[i];
	  	  
      if(shapeObj.getMap() == map) {
	  
	     if( req_shapeAr[lastSelectedId]  && i > 0) { 
		      
			  removeFromMap(req_shapeAr[lastSelectedId]);
			  
			  requestForm.elements[lastSelectedId].checked = false;      // Change to request Form
			  
			  if(req_xlylMarkerAr[i]) removeFromMap(req_xlylMarkerAr[i]);
			  if(req_xryrMarkerAr[i]) removeFromMap(req_xryrMarkerAr[i]);
			  if(req_cntrMarkerAr[i]) removeFromMap(req_cntrMarkerAr[i]); 		   
		   }
		   
		  lastSelectedId = i;
  		 }
      
   }
     
   
   var t = lastSelectedId;
   
  
   
   //var reqId = reqIdAr[i];                 
   // Add ID Number of Request for Ease of Update 
   //     document.orphanSample.reqNum.value=piReqNum;
	 
   
	
    if(req_xlylMarkerAr[t]!= null) { 
	   
		   var xlylMarker = req_xlylMarkerAr[t];
		   var xryrMarker = req_xryrMarkerAr[t];
		   var reqIdNum = reqIdAr[t];
		   var shpType = req_shapeTypeAr[t];
		   var reqRefId = req_refNumIdAr[t];
	
	  
	
		   updateForm.elements[0].value=xlylMarker.getPosition().lng();
		   updateForm.elements[1].value=xlylMarker.getPosition().lat();
	       updateForm.elements[2].value=xryrMarker.getPosition().lng();
		   updateForm.elements[3].value=xryrMarker.getPosition().lat();
	       updateForm.elements[4].value=reqIdNum;
		   updateForm.elements[5].value=reqRefId;
		   updateForm.elements[7].options[0].selected=true;
		   
		   

		   addToMap(xlylMarker); 
		   google.maps.event.addListener(xlylMarker, "dragend", function() {
		         updateForm.elements[0].value=xlylMarker.getPosition().lng();
		         updateForm.elements[1].value=xlylMarker.getPosition().lat();   
	             updateForm.elements[2].value=xryrMarker.getPosition().lng();
				 updateForm.elements[3].value=xryrMarker.getPosition().lat();
	             updateForm.elements[4].value=reqIdNum;
			     updateForm.elements[5].value=reqRefId;
				 updateForm.elements[7].options[0].selected=true;

				 
				
		     });
			 
		  addToMap(xryrMarker); 
		  google.maps.event.addListener(xryrMarker, "dragend", function() {
		         updateForm.elements[0].value=xlylMarker.getPosition().lng();
		         updateForm.elements[1].value=xlylMarker.getPosition().lat();
				 updateForm.elements[2].value=xryrMarker.getPosition().lng();
				 updateForm.elements[3].value=xryrMarker.getPosition().lat();             
				 updateForm.elements[4].value=reqIdNum;
			     updateForm.elements[5].value=reqRefId;
				 updateForm.elements[7].options[0].selected=true;


		     });    
		}
		
		
		if(req_cntrMarkerAr[t]!= null){ 	
		
		   
		   var cntrMarker = req_cntrMarkerAr[t];
		   var reqIdNum = reqIdAr[t];
		   var shpType = req_shapeTypeAr[t];
		   var reqRefId = req_refNumIdAr[t];
		   
		   var shapesize = req_shapeSizeAr[t];
		   		   
		   updateForm.elements[0].value=cntrMarker.getPosition().lng();
		   updateForm.elements[1].value=cntrMarker.getPosition().lat();
	       updateForm.elements[2].value='0';
		   updateForm.elements[3].value='0';
		   updateForm.elements[4].value=reqIdNum;
		   updateForm.elements[5].value=reqRefId;
		   
		   if(shpType == 'C') {updateForm.elements[6].options[1].selected=true }
		   if(shpType == 'P') {updateForm.elements[6].options[2].selected=true }

		   if(shapesize == 1){updateForm.elements[7].options[0].selected=true }
		   if(shapesize == 3){updateForm.elements[7].options[1].selected=true }
		   if(shapesize == 7){updateForm.elements[7].options[2].selected=true }
		   if(shapesize == 9){updateForm.elements[7].options[3].selected=true }
           if(shapesize == 13){updateForm.elements[7].options[4].selected=true }
		   if(shapesize == 17){updateForm.elements[7].options[5].selected=true }
		   if(shapesize == 21){updateForm.elements[7].options[6].selected=true }
		   if(shapesize == 25){updateForm.elements[7].options[7].selected=true }
		   if(shapesize == 29){updateForm.elements[7].options[8].selected=true }
		   if(shapesize == 33){updateForm.elements[7].options[9].selected=true }
		   if(shapesize == 37){updateForm.elements[7].options[10].selected=true }
		   if(shapesize == 41){updateForm.elements[7].options[11].selected=true }

	

				 		
		   addToMap(cntrMarker); 
		   google.maps.event.addListener(cntrMarker, "dragend", function() {
				 updateForm.elements[0].value=cntrMarker.getPosition().lng();
				 updateForm.elements[1].value=cntrMarker.getPosition().lat();
				 updateForm.elements[4].value=reqIdNum;
		         updateForm.elements[5].value=reqRefId;
			     if(shpType == 'C') {updateForm.elements[6].options[1].selected=true }
		         if(shpType == 'P') {updateForm.elements[6].options[2].selected=true }
				 
		   		if(shapesize == 1){updateForm.elements[7].options[0].selected=true }
		   		if(shapesize == 3){updateForm.elements[7].options[1].selected=true }
		   		if(shapesize == 7){updateForm.elements[7].options[2].selected=true }
		   		if(shapesize == 9){updateForm.elements[7].options[3].selected=true }
           		if(shapesize == 13){updateForm.elements[7].options[4].selected=true }
		   		if(shapesize == 17){updateForm.elements[7].options[5].selected=true }
		   		if(shapesize == 21){updateForm.elements[7].options[6].selected=true }
		   		if(shapesize == 25){updateForm.elements[7].options[7].selected=true }
		   		if(shapesize == 29){updateForm.elements[7].options[8].selected=true }
		   		if(shapesize == 33){updateForm.elements[7].options[9].selected=true }
		   		if(shapesize == 37){updateForm.elements[7].options[10].selected=true }
		   		if(shapesize == 41){updateForm.elements[7].options[11].selected=true }


		  

		    });
	    
		}
		

     var sampleForm = document.getElementById('sampleRecords');

	 
	  for (i=0; i < shapeAr.length; i++) {  
      removeFromMap(shapeAr[i]);
	    sampleForm.elements[i].checked = false;     
	 
    }
  
  } 
  
  
  
  
   function editReq(req_ref_num) {
  
    var bindArgs = {
        url: "/samples/load_update_request_form/" + req_ref_num,
		mimetype:   "text/plain",
        method: "GET", 
		load: function(type,data,evt) {
		
		  document.getElementById('requestUpdate').innerHTML = data;
		  document.getElementById('requestUpdate').style.visibility = 'visible';
		  
		}
    };
	
	
	var requestObj = dojo.io.bind(bindArgs);
	
	
	//clear form
	
	//setTimeout('document.getElementById(\'digitize_confirm\').style=\'hidden\'', 3000);  // Hide Form
	
  
  }

  
  
  
  
  function delReqConfirm(req_ref_num) {
  
  
   if(confirm('Are you sure you want to delete this request permanently')) {
     
    var bindArgs = {
        url: "/samples/safod_request_delete/" + req_ref_num,
		mimetype:   "text/plain",
        method: "GET", 
		load: function(type,data,evt) {
			 document.getElementById('panel3').innerHTML = data;
		  
		}
    };
	
	
	var requestObj = dojo.io.bind(bindArgs);
	
   }
  
 
  }
