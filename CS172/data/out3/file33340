  <script type="text/javascript">

//Initialize our user agent string to lower case.
var uagent = navigator.userAgent.toLowerCase();

//**************************
// Detects if the current device is an iPhone, android or blackberry.
function DetectPhone()
{
   if (uagent.search("iphone") > -1){
	   
      window.location = 'http://www.cmhc.utexas.edu/mobile/select.html';
   
   }else if (uagent.indexOf("android") > -1){
	   
      window.location = 'http://www.cmhc.utexas.edu/mobile/select.html';
   
   }else if (uagent.indexOf("blackberry") > -1){
	   
      window.location = 'http://www.cmhc.utexas.edu/mobile/select.html';
   
   }else if (uagent.indexOf("windows ce") > -1){
	   
      window.location = 'http://www.cmhc.utexas.edu/mobile/select.html';
   
   }else if (uagent.indexOf("opera mini") > -1){
	   
      window.location = 'http://www.cmhc.utexas.edu/mobile/select.html';
   
   }else{
      return false;
	}
}

function checkurl() {
  var url = window.location.href.split('?')[1];
 
  //if url dont have a ? it will detect the phone
  if(!url) {
	  DetectPhone();
  }
}

checkurl();
</script> 
