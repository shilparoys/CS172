  <script type="text/javascript">
	/*Script para los paneles desplegables*/
	jQuery(document).ready(function(){								
		jQuery('.enlaceN1').click(function(){
			var miID = $(this).id;
			var miContenedor = 'cont' + miID.substring(3);
			jQuery('.visor').hide();	
			jQuery('.visorN1').hide();	
			jQuery('.visorN2').hide();	
			jQuery('[id='+miContenedor+']').show();
		});							
		jQuery('.enlaceN2').click(function(){
			var miID = $(this).id;
			var miContenedor = 'cont' + miID.substring(3);
			jQuery('.visor').hide();	
			jQuery('.visorN2').hide();
			jQuery('#temas').show();
			jQuery('[id='+miContenedor+']').show();
		});							
		jQuery('.enlace').click(function(){
			var miID = $(this).id;
			var miContenedor = 'cont' + miID.substring(3);
			var miClase = miID.substring(3);
			jQuery('.visor').hide();
			jQuery('[id='+miContenedor+']').toggle();
			jQuery('#temas').show();
			jQuery('.'+miClase).show();
		});							
	});
</script> 
