    var downloadwindow;
    function OpenDownLoadWindow(pid) {
        var u = "http://www.radiop1.com/p1player/downloadprogram.asp?prgid=" + pid;
        downloadwindow = window.open(u, 'MediaPlayer4', 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=550,height=300');
        if (window.focus) { downloadwindow.focus() }
    }
