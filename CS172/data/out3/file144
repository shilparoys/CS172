								var diversity_feed=new gfeedfetcher("diversity", "diversity_class", "")
								diversity_feed.addFeed("Diversity Calendar", "http://www.greenriver-events.info/events/rss/?l=45") //Specify "label" plus URL to RSS feed
								diversity_feed.displayoptions("title") //show the specified additional fields
								diversity_feed.setentrycontainer("div") //Display each entry as a list (li element)
								diversity_feed.filterfeed(3, "title") //Show 6 entries, sort by title
								diversity_feed.init() //Always call this last
</script> 
