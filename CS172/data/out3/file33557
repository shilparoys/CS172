        function () { 
           $("#home-kudos-area").animate({height: "135px"}, {queue:false, duration: 1200, easing: 'easeOutElastic'}) 
                }, 
                function () { 
          $("#home-kudos-area").animate({height: "0px"}, {queue:false, duration: 1000, easing: 'easeOutBounce'})  
        } 
        
    );
  $("a.home-employement-tab").toggle( 
        function () { 
           $("#home-employment-area").animate({height: "190px"}, {queue:false, duration: 1200, easing: 'easeOutElastic'}) 
                }, 
                function () { 
          $("#home-employment-area").animate({height: "0px"}, {queue:false, duration: 1000, easing: 'easeOutBounce'})  
        } 
    );
        
}); 
