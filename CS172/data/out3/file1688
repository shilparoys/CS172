  <script type="text/javascript">
/*<![CDATA[*/
$(document).ready(function(){
	$("#affiliation input").change(function(){
			if ($("#affiliation input:checked").length>0){$("#affiliation .error").remove();}
			else {$("#affiliation").append("<span class='error f0'>Please select your affiliation to Penn College.<\/span>");}
	});
	$("#agree input").change(function(){
			if ($("#agree input:checked").length>0){$("#agree .error").remove();}
			else {$("#agree").append("<span class='error f0'>Please agree to the Terms of Use/Release Statement.<\/span>");}
	});

	$(".sharexamples").each(function(){
		 if ($(this).height()>300){
			 expand_H=$(this).height();
			 $(this).addClass("too_big").append("<a class='more' id='"+expand_H+"'>Show more hashtag examples <img src='/cgi-bin/images/nav_show.gif'/></a>"); 
		 }
		 
	});
	$("a.more").click(function(){
		$(this).parent("div").removeClass("too_big").css({'height':300}).animate({"height":$(this).attr('id')},$(this).attr('id')*2, function(){$(this).css({'height':'auto'})});
		$(this).fadeOut($(this).attr('id')*2,function(){$(this).remove()})
	});

});
/*]]>*/
</script> 
