  <script type="text/javascript">
    $(document).ready(function() {
    	// Alerts
    	$.jGrowl.defaults.position = "bottom-left";
        $.jGrowl.defaults.life = 90000000;

        $.ajax({
            url: 'http://www.jazanu.edu.sa/_layouts/JazanApp/Alerts/CheckUserToAlert.aspx',
            asyn: true,
            cache: false,
            dataType: 'xml',
            error: function(XMLHttpRequest, textStatus, errorThrown) {

            },
            
            success: function(xml) {
                $(xml).find('row').each(function() {
                    var strDate = ($(this).find('CreatedDate').text());
                    $.jGrowl($(this).find('Message').text(), { header: strDate });
                });
            }
        });
        // Alerts
		

		$('.slides').slides({
				preload: true,
				preloadImage: 'http://static.jazanu.edu.sa/images/portal/NewDesign/loading.gif',
				play: 5000,
				pause: 2500,
				hoverPause: true,
				generatePagination: false
		});
		
	
		$("#example4 ul").zAccordion({
				slideWidth: 750,
				width: 998,
				height: 255,
				timeout: 3000,
				startingSlide: 3,
				slideClass: "frame",
				auto: false, /* Whether or not the slideshow should play automatically. */
				trigger: "mouseover", /* Event type that will bind to the "tab" (click, mouseover, etc.). */
				pause: true, /* Pause on hover. */
				invert: true
		});
		
		$('#scrollbar1').tinyscrollbar();
				
		$('.ln_slides').slides({
				preload: true,
				preloadImage: 'http://static.jazanu.edu.sa/images/portal/NewDesign/loading.gif',
				play: 5000,
				pause: 2500,
				hoverPause: true,
				generatePagination: false
		});
		
		$("ul.colors a").click(function () {
			$.cookie('ColorContainer',$(this).attr('id'), { expires: 365 })
			$("div#ColorContainer").attr('class', $.cookie('ColorContainer'));			
		});
		
		//if($.cookie('ColorContainer')!=undefined){
		//	$("div#ColorContainer").attr('class', $.cookie('ColorContainer'));
		//}
        
	});
	
	function close_pre1() {
				var b_pop_pre_1 = document.getElementById('b_pop_pre');
				b_pop_pre_1.style.visibility = 'hidden';
				}

</script> 
