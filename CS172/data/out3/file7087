      <div class="vote"> 
       <table toplevel="" border="0" cellpadding="0" cellspacing="0" width="100%"> 
        <tbody>
         <tr> 
          <td valign="top">
           <div webpartid="00000000-0000-0000-0000-000000000000" haspers="true" id="WebPartWPQ4" width="100%" class="ms-WPBody" onlyformepart="true" allowdelete="false" style="">
            <div id="ctl00_PlaceHolderMain_WebUserControlLoaderWebPart3"> 
             <script type="text/javascript">


    $(function() {// start
        $("input[type=radio][id*='rbtnAnswers']").prop('onclick', '');
        $("input[type=radio][id*='rbtnAnswers']").click(function() {
       
            $('#ctl00_PlaceHolderMain_WebUserControlLoaderWebPart3_ctl00_jShowCurrentQuestion1_ibtnVote').prop('disabled', '');
        });

        $("#divResults").bind("ajaxStart", function() {
            $(".v_options").hide();
            // put here loading image
            $("#divResults").html('<center><img src="http://static.jazanu.edu.sa/images/portal/ajax-loader.gif"  /></center>');
        });


        // Get resutl button
        $("input[type=submit][id*='btnResult']").click(function() {  // start button get results click
            var guid = '00000000-0000-0000-0000-000000000000';
            $.ajax({ // start ajax
                url: '/_layouts/Jazan/jVoting.aspx?op=GetResults&QuestionId=' + guid,
                asyn: true,
                cache: false,
                dataType: 'xml',
                error: function(XMLHttpRequest, textStatus, errorThrown) {
                    alert(XMLHttpRequest.responseText);
                },
                success: function(xml) {
                    var html = '<table width="100%" border="0" cellspacing="0" cellpadding="0" style="padding-right:7px; padding-left:7px;" >';
                    $(xml).find('Answer').each(function() {
                        html += BuildTableRow(this);
                    });
                    // get the total count form the XML
                    html += '<tr><td  style="width: 100%; font-family: Tahoma; font-size: 10pt;" ><br /><div style="padding-bottom:10px;">مجموع الاصوات ';
                    html += $(xml).find('TotalAnswers').text();
                    html += '</div></td></tr>';
                    html += '<tr><td style="border-top: 1px dashed #dedede; height: 22px; text-align: left;"><a href="#" onclick="return false;" id="hlShowAnswers" class="vote_btn" >تصويت</a>&nbsp;&nbsp;<a href="/Pages/votingArchive.aspx" id="hlShowOldVotes" class="vote_btn" >استبيانات سابقة</a></td></tr>';
                    html += '</table>';
                    $("#divResults").html(html);

                    $("#hlShowAnswers").click(function() {
                        $(".v_options").show();
                        $("#divResults").html('');
                    });
                }
            }); // end ajax
            return false;
        }); // end button get results click


        // Vote Button
        $("input[type=submit][id*='ibtnVote']").click(function() {  // start button vote click
            $("input[type=radio][id*='rbtnAnswers']").each(function() { // start each loop
                if ($(this).prop('checked')) { // start if clause
                    var guid = $(this).prop('value');

                    $.ajax({ // start ajax
                        url: '/_layouts/Jazan/jVoting.aspx?op=AnswerVote&Answerid=' + guid,
                        asyn: true,
                        cache: false,
                        dataType: 'xml',
                        error: function(XMLHttpRequest, textStatus, errorThrown) {
                            alert(XMLHttpRequest.responseText);
                        },
                        success: function(xml) {
                            var html = '<table width="100%" border="0" cellspacing="0" cellpadding="0">';
                            $(xml).find('Answer').each(function() {
                                html += BuildTableRow(this);
                            });
                            // get the total count form the XML
                            html += '<tr><td style="width: 100%; font-family: Tahoma; font-size: 10pt;" ><br /><div style="padding-bottom:10px;" > مجموع الاصوات ';
                            html += $(xml).find('TotalAnswers').text();
                            html += '</div></td></tr>';
                            //html += '<tr><td style="border-top: 1px dashed #dedede; height: 22px; text-align: left;"><a href="/Pages/votingArchive.aspx" id="hlShowOldVots" class="DevHelperVotingbutton" >استبيانات سابقة</a></td></tr>';
                            html += '</table>';
                            $(".v_options").hide();
                            $("#divResults").html(html);
                        }
                    }); // end ajax

                } // end if clause
            }); // end each loop
            return false;
        }); // end button click
    });       // end



    //Build the HTML table row for each answer
    function BuildTableRow(answer) {
        var html = '<tr>\n';
        html += '<td class="DevHelperVotingItem" style="height: 30px">\n';
        html += $(answer).attr('Text');
        html += '</td>\n';
        html += '</tr>\n';

        html += '<tr>\n';
        html += '<td class="resultvoting">\n';
        html += $(answer).attr('Rate') + ' %';
        html += '</td>\n';
        html += '</tr>\n';

        html += '<tr>\n';
        html += '<td>\n';
        html += '<table class="DevHelperVotingRate" cellpadding="0" cellspacing="0">\n';
        html += '<tr>\n';
        html += '<td style="height: 5px; width: 100%;">\n';
        html += '<span style="background-color:' + $(answer).attr('Color') + '; width:' + $(answer).attr('Rate') + '%;display: block; height: 5px;></span>"';
        html += '</td>\n';
        html += '</tr>\n';
        html += '</table>\n';
        html += '</td>\n';
        html += '</tr>\n';

        return html;
    }
               
</script> 
            </div>
           </div></td> 
         </tr> 
        </tbody>
       </table> 
