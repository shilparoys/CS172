      <script language="javascript" type="text/javascript">






$(document).ready(function() {
 //http://www.sohtanaka.com/web-design/simple-tabs-w-css-jquery/comment-page-2/#comment-3615
	//When page loads...
	$(".tab_content").hide(); //Hide all content
	$("ul.tabs li:eq(2)").addClass("active").show(); //Activate first tab. it's eq() = 0,1,2,3
	$(".tab_content#tab3").show(); //Show first tab content. it's #tab = 1,2,3,4  
	$('img#imgtab3').attr({ src: "flash/images/tabIcon_question_hi.png" }); //update this to the correct tab and tab icon image.#imgtab = 1,2,3,4 and tabIcon = _123_hi, _exclamation_hi, _dollar_hi
	
	//On Click Event
	$("ul.tabs li").click(function() {

		$("ul.tabs li").removeClass("active"); //Remove any "active" class
		$(this).addClass("active"); //Add "active" class to selected tab
				
		$(".tab_content").hide(); //Hide all tab content

		var activeTab = $(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
		$(activeTab).fadeIn(); //Fade in the active ID content
				//This changes the icon on the tabs
						if (activeTab == '#tab1'){
						 $('img#imgtab1').attr({ src: "flash/images/tabIcon_123_hi.png" });
						 $('img#imgtab2').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab3').attr({ src: "flash/images/tabIcon_question.png" });
						 $('img#imgtab4').attr({ src: "flash/images/tabIcon_dollar.png" });
				}
				else if (activeTab == '#tab2'){
						 $('img#imgtab1').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab2').attr({ src: "flash/images/tabIcon_123_hi.png" });
						 $('img#imgtab3').attr({ src: "flash/images/tabIcon_question.png" });
						 $('img#imgtab4').attr({ src: "flash/images/tabIcon_dollar.png" });
				}
				else if (activeTab == '#tab3'){
						 $('img#imgtab1').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab2').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab3').attr({ src: "flash/images/tabIcon_question_hi.png" });
						 $('img#imgtab4').attr({ src: "flash/images/tabIcon_dollar.png" });
				}
				else if (activeTab == '#tab4'){
						 $('img#imgtab1').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab2').attr({ src: "flash/images/tabIcon_123.png" });
						 $('img#imgtab3').attr({ src: "flash/images/tabIcon_question.png" });
						 $('img#imgtab4').attr({ src: "flash/images/tabIcon_question_hi.png" });
				}
				//end tab icon change.
		return false;
	});
	// Change tabs on hover instead of click. 
	//	$("ul.tabs li").hover(function() {
	//		$("ul.tabs li").removeClass("active"); //Remove any "active" class
	//		$(this).addClass("active"); //Add "active" class to selected tab
	//		$(".tab_content").hide(); //Hide all tab content
	//
	//		var activeTab = $(this).find("a").attr("href"); //Find the href attribute value to identify the active tab + content
	//		$(activeTab).fadeIn(); //Fade in the active ID content
	//		return false;
	//	}); 
});

</script> 
