      theId = $(this).attr("id");
	
	  $("#dialogData"+theId).dialog({
			bgiframe: true,
			modal: true,
			autoOpen: false,
			/*hide: 'blind',
			show: 'blind',*/
			width: 500
		});
	  
	  $("#dialogData"+theId).dialog('open', 'title');
	  
    })




});





