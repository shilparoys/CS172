var CSMReqs={af:{c:2,e:'amznJQ.AboveTheFold',p:'atf'},cf:{c:2,e:'amznJQ.criticalFeature',p:'cf'}};
function setCSMReq(a){
    a=a.toLowerCase();
    var b=CSMReqs[a];
    if(b&&--b.c==0){
        if(typeof uet=='function'){uet(a);};
        if(b.e){amznJQ.completedStage(b.e);};
        if(typeof P != 'undefined'){P.register(b.p);};
    }
}

