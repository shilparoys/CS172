    $('.clickhere a h3').click(function(e){
    e.preventDefault();
    	if($('.st-content').css('display') === 'none') {
    		$('.st-content').show().animate({height:155}, 200 ,'swing');
    		}
    		else {
    		$('.st-content').animate({height:0}, 200, 'swing').hide(400);
    		}
		});
    </script> 
