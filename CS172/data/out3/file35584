        function popUp(URL) {
            day = new Date();
            id = day.getTime();
            eval("page" + id + " = window.open(URL, '" + id + "', 'toolbar=0,titlebar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width=500,height=300');");
        }

        //calculate total for recurring gifts
        $(function() {
          $.totalFunction = function() {                               

                //find gift amount from other textbox, or radio button amounts
                var giftVal = 0;
                for(i=0;i < 0;i++) {                      
                    if($("#othergiftof" + i).val() === undefined) {
                        giftVal = Number($("#giftof" + i).val()) + Number(giftVal);
                    } else {
                        giftVal = Number($("#othergiftof" + i).val()) + Number(giftVal);
                    }                                          
                }

                var startDayVal=$("#startday").val();
                if (startDayVal != "Day") {
                    $("#enddayDISABLED").val(getEndDay(startDayVal));
                    $("#enddayDISABLED").attr('disabled', true);
                    $("#endday").val(getEndDay(startDayVal));
                    $("#endday").attr('hidden', true);
                }               
                
                var startDate = $("#startmonth").val() + "/" + $("#startday").val() + "/" + $("#startyear").val();
                var endDate = $("#endmonth").val() + "/" +$("#enddayDISABLED").val() + "/" + $("#endyear").val();

                //determine if monthly or annually gift
                var recurringfrequency = $('input:radio[name=recurringfrequency]:checked').val();
                if (recurringfrequency != "annually") {
                    var totalGivingMonths = mydiff(startDate, endDate,"months")  
                    //enable monthly choice, only allow them to change year
                    $("#endmonth").removeAttr('disabled');
                    $("#endmonthHIDDEN").val($("#endmonth").val());
                } else {
                    var totalGivingMonths = mydiff(startDate, endDate,"years") 
                    //disable monthly choice, only allow them to change year
                    $("#endmonth").attr('disabled',true);
                    $("#endmonthHIDDEN").attr('hidden', true);
                    //set to current start month, otherwise "Month" will be selected and won't compute
                    $("#endmonth").val($("#startmonth").val());
                    $("#endmonthHIDDEN").val($("#startmonth").val());
                }

                //count current month
                totalGivingMonths = totalGivingMonths + 1;

                //calculate number of gifts and total amount
                if(totalGivingMonths>0){
                    $("#totalgiftof").val(giftVal*totalGivingMonths);
                    $("#totalmonths").val(totalGivingMonths);
                }     
                                       
          };
          
        });

        function getEndDay(startDayVal) {
            if (parseInt(startDayVal) < 29)
                return startDayVal;
            else {
                var emon = $("#endmonth").val();
                if (emon != "Month") {
                    switch (emon) {
                        case "02":
                            if (IsLeapYear())
                                return 29;
                            else
                                return 28;
                        case "04":
                        case "06":
                        case "09":
                        case "11":
                            if (parseInt(startDayVal) > 30)
                                return 30;
                            else
                                return startDayVal;
                        default:
                            return startDayVal;
                    }
                }
            }
            return startDayVal;
        }

        function IsLeapYear() {
            var year=$("#endyear").val();
            if (year != "Year") {
                return new Date(year, 1, 29).getMonth() == 1;
            }

            return false;
        }

        function mydiff(date1,date2,interval) {
            var second=1000, minute=second*60, hour=minute*60, day=hour*24, week=day*7;
            date1 = new Date(date1);
            date2 = new Date(date2);
            var timediff = date2 - date1;
            if (isNaN(timediff)) return NaN;
            switch (interval) {
                case "years": return date2.getFullYear() - date1.getFullYear();
                case "months": return (
                    ( date2.getFullYear() * 12 + date2.getMonth() )
                    -
                    ( date1.getFullYear() * 12 + date1.getMonth() )
                );
                case "weeks"  : return Math.floor(timediff / week);
                case "days"   : return Math.floor(timediff / day); 
                case "hours"  : return Math.floor(timediff / hour); 
                case "minutes": return Math.floor(timediff / minute);
                case "seconds": return Math.floor(timediff / second);
                default: return undefined;
            }
        }
        //end calculate total for recurring gifts                      
            
             
                        //step1
                        //change text if selecting gift of pledge
                        function pledge(arg) {
                            if(arg == "existingpledge") {                
                                document.getElementById('lblDirect').innerHTML="Select the fund, school or program to which you made your pledge";                    
                                document.getElementById('lblType').innerHTML="pledge";                    
                            } else {
                                document.getElementById('lblDirect').innerHTML = "Please direct my gift to a specific school or program";                    
                                document.getElementById('lblType').innerHTML="gift";                    
                            }   
            
                            //hide specific fund lists if "payroll deduction" radio gift type is checked
                            if(arg == "payroll") {
                                //$('#divSpecific').hide();
                                //$('#divBreakouts').hide();
                                $('.hidePayroll').hide();
                                $('.hideNonPayroll').show();               
                                $('.hideNonPayrollGNC').show();  
                               document.getElementById('searchboxpayroll').style.display = '';  
                                document.getElementById('searchbox').style.display = 'none'; 
                                document.getElementById('lblSearchText').innerHTML="Search all schools and funds available for payroll deduction";  
                            } else {
                                //$('#divSpecific').show();
                                //$('#divBreakouts').show();
                                $('.hidePayroll').show();
                                $('.hideNonPayroll').hide();                
                                $('.hideNonPayrollGNC').hide();  
                                document.getElementById('searchboxpayroll').style.display = 'none'; 
                                document.getElementById('searchbox').style.display = '';              
                                document.getElementById('lblSearchText').innerHTML="Search all schools and funds";
                            }   
                            
                        }
            
        
