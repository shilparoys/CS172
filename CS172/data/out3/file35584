        //Ajax save of step4 data, but not credit card info
        function savedb() {
            $.post("/Step4Ajax?bill_to_forename=" + encodeURIComponent($("#bill_to_forename").val()) + "&bill_to_surname=" + encodeURIComponent($("#bill_to_surname").val()) + "&bill_to_address_line1=" + encodeURIComponent($("#bill_to_address_line1").val()) + "&bill_to_address_line2=" + encodeURIComponent($("#bill_to_address_line2").val()) + "&bill_to_address_city=" + encodeURIComponent($("#bill_to_address_city").val()) + "&bill_to_address_state=" + encodeURIComponent($("#bill_to_address_state").val()) + "&bill_to_address_postal_code=" + encodeURIComponent($("#bill_to_address_postal_code").val()) + "&bill_to_address_country=" + encodeURIComponent($("#bill_to_address_country").val()) + "&matchinggift=" + encodeURIComponent($("#matchinggift").val()));             
        };

        $(document).ready(function () {
            // put all your jQuery goodness in here.
            
            //adds placeholders for IE
            $('[placeholder]').focus(function () {
                var input = $(this);
                if (input.val() == input.attr('placeholder')) {
                    input.val('');
                    input.removeClass('placeholder');
                }
            }).blur(function () {
                var input = $(this);
                if (input.val() == '' || input.val() == input.attr('placeholder')) {
                    input.addClass('placeholder');
                    input.val(input.attr('placeholder'));
                }
            }).blur().parents('form').submit(function () {
                $(this).find('[placeholder]').each(function () {
                    var input = $(this);
                    if (input.val() == input.attr('placeholder')) {
                        input.val('');
                    }
                })
            });

            //Merges the two credit card month/year fields into one hidden input
            $("#card_expiry_date_month").change(function() {
                $('#card_expiry_date').val($('#card_expiry_date_month').val() + '-' + $('#card_expiry_date_year').val());
            });
            $("#card_expiry_date_year").change(function() {
                $('#card_expiry_date').val($('#card_expiry_date_month').val() + '-' + $('#card_expiry_date_year').val());
            });

            //Removes required field from city/state if international
            $("#bill_to_address_country").change(function() {
