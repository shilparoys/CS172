                  <select id="cursoMinLic"> <option>Curso</option> 
                   <!--  <option>ACS</option>--> <option>APA</option> <option>ARQ</option> 
                   <!--  <option>BIO</option>--> 
                   <!--  <option>CCO</option>--> <option>CED</option> 
