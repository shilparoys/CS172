   //animar box
   var out = -155;
   var inn = -170;
   $("#kit").animate({'margin-left': out}, 2000, "swing").fadeOut("slow");
   // listagens
   function hide (){
    $( "#escolhaPraLic, #escolhaMinLic, #escolhaPraMes, #calePmes, #caleMmes" ).hide("slow");
  }
    $('.MindeloLic').click(function(){
        hide();
        $('#escolhaMinLic').slideToggle( "slow");
        $('#caleMmes').slideToggle( "slow");
    });
    $('.PraiaLic').click(function(){
        hide();
        $('#escolhaPraLic').slideToggle( "slow");
    });
    $('.PraiaMes').click(function(){
        hide();
        $('#escolhaPraMes').slideToggle( "slow");
        $('#calePmes').slideToggle( "slow");
    });
    // gerir pdf ------------------------------
function horario (obj) {
    // console.dir(obj);
    var tipo    = $(obj).parent().attr('id').substr(7,6);// retorna PraLic, MinLic, PraMes
    var curso   = $("#curso"+tipo).val();// retorna curso
    var ano     = $("#ano"+tipo).val();// retorna ano

    if(String(tipo) == "PraMes"){curso = curso.toLowerCase();ano ="";}
    if(String(tipo) == "MinLic"){curso = curso.toLowerCase();}
    $('#dialgo .b').appendTo('#escuro');
    $('#' + tipo + curso + ano).appendTo('.PDFS'+tipo);
    console.log("descrisao "+ tipo + curso + ano);
   }
       // clic para abrir dialogos
   $("#cursoPraLic,#anoPraLic").click(function(){horario(this)});
   $("#cursoPraMes,#anoPraMes").click(function(){horario(this)});
   $("#cursoMinLic,#anoMinLic").click(function(){horario(this)});
   $("#ver, #ver1, #ver2").click(function(){horario(this)});
   //-----------------------------------------
   // especifico de telemoveis
 if( !(/Android|webOS|iPhone|iPad|iPod|Mac|mac|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) )) {
       $("#ver, #ver1, #ver2").css('display','none');
}

    // criar dialogos
    $( "#dialgo" ).dialog({ autoOpen: false,modal: true});
    $( "#dialogo_calendario" ).dialog({ autoOpen: false,modal: true });
     $( "#dialogo_mapa" ).dialog({ autoOpen: false,modal: true });

   // $( "#abrir_horario" ).click(function() { $( "#dialgo" ).dialog( "open" ); });
   $( ".abrir_horario" ).click(function() { $( "#dialogo_mapa" ).dialog( "open" ); });
    $( "#abrir_calendario" ).click(function() { $( "#dialogo_calendario" ).dialog( "open" ); });

$(document).ready(function(){
    // animar box
    $("#rolona").hover(function() {
        $("#kit").stop( true, true ).fadeIn("slow").animate({'margin-left': inn}, 2000, "swing");
    },function(){
       $("#kit").stop( true, true ).animate({'margin-left': out}, 2000, "swing").fadeOut("slow");
    });
 // activar banner
   $('.slide').css('display', '');
});
  </script> 
