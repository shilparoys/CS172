     <script type="text/javascript">
	$(document).ready(function(e) {
    /* start dropdown */
		$('#dropdown ul').bind('mouseleave', function(){
				dropdown_show_hide();
		});

		$('#dropdown ul').bind('click', function(){
				dropdown_show_hide();
		});

		$('.rdropdown').bind({
			click: function(){
				dropdown_show_hide();
			}
		});
		// end dropdown
	});

	function dropdown_show_hide(){
		if ( $('#dropdown').css('display') == 'none'){
			$('#dropdown').show();
		}
		else{
			$('#dropdown').hide();
		}
	}
</script> 
