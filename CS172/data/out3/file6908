            function redirectSP() {
                var agent = navigator.userAgent;
                var isSP = false;
                if (agent.search(/iPhone/) != -1) {
                    isSP = true;
                } else if(agent.search(/iPod/) != -1) {
                    isSP = true;
                } else if(agent.search(/Android.*Mobile/) != -1) {
                    isSP = true;
                } else if(agent.search(/Windows Phone/) != -1) {
                    isSP = true;
                }
                if (isSP == true) {
                    var pcSiteView = 0;
                    if (document.cookie.search(/pc\_site\_view=1/) != -1) {
                        pcSiteView = 1;
                    }
                    
                    if (pcSiteView == 0) {
                        location.href = "http://www.kanazawa-it.ac.jp/sp/index.html";
                    }
                }
            }
            setTimeout("redirectSP()", 0);
        </script> 
