  <script language="javascript" type="text/javascript">
    function onBefore(){
        comps = this.alt;
        comp_parts = comps.split('_');
        $('.staCaption').html('<p><span class="staCode">'+comp_parts[0]+' '+comp_parts[1]+'</span>&nbsp;&nbsp;&nbsp;'+comp_parts[2]+'</p>')
    }
    $(document).ready(function() {

        // {{{ Dynamic station list
        $.ajax({
            type: "GET",
            url: "/cachejson/stations/stalist.json",
            dataType: "json",
            success: function(data) {
                $.each(data, function(i,val) {
                    $("#suggeststa").autocomplete(this) ;
                });
            },
            error: function() {
                alert( "Could not load station list for form autocomplete" ) ;
            }
        });
        // }}} Dynamic station list

        $('.staIm').cycle({
            delay:2000,
            speed:500,
            before: onBefore
        });
        $('.spevents').cycle({
            delay:1000,
            speed:700
        });

    });
    </script> 
