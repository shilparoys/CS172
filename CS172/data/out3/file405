function getAppVersion(){
	appname= navigator.appName;
	appversion = navigator.appVersion;
	majorver = appversion.substring(0, 1);
	if ( (appname == "Netscape") && ( majorver >= 3 ) ) return 1;
	if ( (appname == "Microsoft Internet Explorer") && (majorver >= 4) ) return 1;
	return 0;
}

function chgImg(myNum, myName, logoNum) {
	if (getAppVersion()) {
		document[myName].src = img[myNum].src;
		
		}
}

imgsrc = new Array();


imgsrc[6] = "nimages/academic_off.gif";
imgsrc[7] = "nimages/academic_on.gif";

imgsrc[8] = "nimages/home_off.gif";
imgsrc[9] = "nimages/home_on.gif";

imgsrc[10] = "nimages/admission_off.gif";
imgsrc[11] = "nimages/admission_on.gif";

imgsrc[12] = "nimages/administration_off.gif";
imgsrc[13] = "nimages/administration_on.gif";

imgsrc[14] = "nimages/news_off.gif";
imgsrc[15] = "nimages/news_on.gif";

imgsrc[16] = "nimages/search_off.gif";
imgsrc[17] = "nimages/search_on.gif";

imgsrc[18] = "nimages/alumni_off.gif";
imgsrc[19] = "nimages/alumni_on.gif";


if (getAppVersion()){
	img = new Array();
	for (i = 0; i < imgsrc.length; i++){
		img[i] = new Image();
		img[i].src = imgsrc[i];
	}
}
</script> 
