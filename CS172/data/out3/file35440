  <script type="text/javascript">
	/*@cc_on
	  @if (@_jscript_version == 10)
	    document.write(' <link type= "text/css" rel="stylesheet" href="http://nurses.keckmedicine.org/wp-content/themes/Avada_3/css/ie10.css" />');
