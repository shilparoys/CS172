     <td align="middle" nowrap valign="top" width="550" height="1"> <font class="navtext" color="#FDE39B" style="font-size: 7pt">|&nbsp; </font> <font class="navtext" color="#F0ECE0" style="font-size: 7pt"> <a href="location_map.htm"><font color="#FDE39B">Location Map</font></a><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font> <a href="news_events.htm"><font color="#FDE39B">News &amp; Events</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font></font> <a href="search.htm"><font color="#FDE39B">Search</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font></font> <a href="contact_us.htm"><font color="#FDE39B">Contact Us</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> |</font></font><a href="credit.htm"><font color="#FDE39B">Credit</font></a></font><font class="navtext" color="#FDE39B" style="font-size: 7pt"> |</font><font face="Verdana" color="#FDE39B"><span style="font-size: 7pt">
        <!--webbot bot="HTMLMarkup" startspan --><script language="JavaScript">

function getAppVersion(){
	appname= navigator.appName;
	appversion = navigator.appVersion;
	majorver = appversion.substring(0, 1);
	if ( (appname == "Netscape") && ( majorver >= 3 ) ) return 1;
	if ( (appname == "Microsoft Internet Explorer") && (majorver >= 4) ) return 1;
	return 0;
}

function chgImg(myNum, myName, logoNum) {
	if (getAppVersion()) {
		document[myName].src = img[myNum].src;
		
		}
}

imgsrc = new Array();


imgsrc[6] = "nimages/academic_off.gif";
imgsrc[7] = "nimages/academic_on.gif";

imgsrc[8] = "nimages/home_off.gif";
imgsrc[9] = "nimages/home_on.gif";

imgsrc[10] = "nimages/admission_off.gif";
imgsrc[11] = "nimages/admission_on.gif";

imgsrc[12] = "nimages/administration_off.gif";
imgsrc[13] = "nimages/administration_on.gif";

imgsrc[14] = "nimages/news_off.gif";
imgsrc[15] = "nimages/news_on.gif";

imgsrc[16] = "nimages/search_off.gif";
imgsrc[17] = "nimages/search_on.gif";

imgsrc[18] = "nimages/alumni_off.gif";
imgsrc[19] = "nimages/alumni_on.gif";


if (getAppVersion()){
	img = new Array();
	for (i = 0; i < imgsrc.length; i++){
		img[i] = new Image();
		img[i].src = imgsrc[i];
	}
}
</script> 
