  <script>
		function getRemaining( slotitemID, field, qtyfield, commentfield ){
			$.ajax({
				url: 'model/service/remoteService.cfc',	
				type: 'get',
				dataType: 'JSON',
				data: {
					method: 'getRemaining',	
					dsn: 'SUGeniusSQLProd',
					slotitemID: slotitemID
				},
				success: function( data ){
					field.val('');
					field.attr( 'placeholder', data.PLACEHOLDER );
					field.attr( 'max', data.REMAINING );
					field.val( 1 );
					qtyfield.html( '(' + data.PLACEHOLDER + ')' );
					commentfield.text( data.ITEMCOMMENT );
				},
				error: function ( err, message, detail ){
					console.log( message, detail );
				}
			});
		}
		$(document).on('pageshow', '#goNonSSL', function(event, ui){
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=members.myaccount&loadMobileTemplate';
		});
		$(document).on('pageshow', '#goNonSSLNoLogin', function(event, ui){
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=main.default&loadMobileTemplate&nossl';
		});
		$(document).on('pageshow', '#goNonSSLSummary', function(event, ui){
			var sugToken = $('#sugToken').val();
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=signups.signupSummary&sugToken='+sugToken+'&mode=signup&loadMobileTemplate&nossl';
		});
		$(document).on('pageshow', '#goNonSSLForm', function(event, ui){
			var sugToken = $('#sugToken').val();
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=signups.signupForm&sugToken='+sugToken+'&mode=signup&loadMobileTemplate&nossl';
		});
		$(document).on('pageshow', '#reloadThankYou', function(event, ui){
			var sugToken = $('#sugToken').val();
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=signups.thankyou&sugToken='+sugToken+'&loadMobileTemplate';
		});
		$(document).on('pageshow', '#reloadSignupPage', function(event, ui){
			var sugToken = $('#sugToken').val();
			window.location.href = 'http://www3.signupgenius.com/mobile/index.cfm?action=signups.signup&sugToken='+sugToken+'&jqPageID=reloaded&loadMobileTemplate';
		});
	</script> 
