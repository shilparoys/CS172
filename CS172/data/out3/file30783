  <script language="javascript">
function classChange(id,newclass) {
	var element = document.getElementById(id);
	element.className = newclass;
}


function save_remove_ad(id) {
	var adIds = readCookie('ad_id');
	var buttonSaveClass = 'buttonSave';
		var buttonRemoveClass = 'buttonSaveRemove';

	var buttonAnchorObj = document.getElementById(id);

	if (adIds.indexOf(id) > -1) {
		RemoveCookieItem ('ad_id',id);
		classChange(id,buttonSaveClass);
		buttonAnchorObj.title = "Save Listing";
	} else {
		AppendCookie ('ad_id',id);
		classChange(id,buttonRemoveClass);
		buttonAnchorObj.title = "Remove Listing";
	}
	see_saved_ads();
}

function check_saved_ads(id) {
	var adIds = readCookie('ad_id');
		var buttonRemoveClass = 'buttonSaveRemove';
	if (adIds.indexOf(id) > -1) {		
		classChange(id,buttonRemoveClass);
	}
}


function see_saved_ads(){
	var w = document.getElementById('see_saved_ads_a');
	var x = document.getElementById('see_saved_ads_img_a');
	var y = document.getElementById('see_saved_ads_b');
	var z = document.getElementById('see_saved_ads_img_b');
	var adIds = readCookie('ad_id');
	var buttonSeeSaveImgGrey = 'http://media.nj.com/static/classifieds/merchandise/images/grayseesavedlistings_button.gif';
	var buttonSeeSaveImg = 'http://media.nj.com/static/classifieds/merchandise/images/btn_seesavedlistings_blue.gif';

	if (adIds.length > 0) {		
	 	if (x) x.src = buttonSeeSaveImg;
		if (w) {
			var wHref = '?property=nj.com&temp_type=clipboard&category=results&tp=ME_nj&classification=classifieds';
			w.href = wHref;
		}
	 	if (z) z.src = buttonSeeSaveImg;
		if (y) y.href = "?property=nj.com&temp_type=clipboard&category=results&tp=ME_nj&classification=classifieds";
	}else{
	 	if (x) x.src = buttonSeeSaveImgGrey;
		if (w) w.href = "javascript:void(0);";
	 	if (z) z.src = buttonSeeSaveImgGrey;
		if (y) y.href = "javascript:void(0);";
	}
}

function verifyAdv(f) {
	 var newPg = parseInt(f.page.value,10);
	 var finalPg = parseInt(f.finalPg.value,10);
	 if ((newPg > finalPg) || (newPg < 1) || (isNaN(newPg))) {
	 	f.page.value = f.thisPg.value;
	}else{
		f.page.value = newPg;
	}
}
$(document).ready(function() {
	$('#keywordSubmit').click(function() {
		var keywords = $('#keywords').val();
		if (keywords == '') {
			alert('Please enter a keyword and then hit the search button.');
		} else {
			$('#keywordFormID').submit();
		}
		return false;
	});
});
