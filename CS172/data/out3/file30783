      <script type="text/javascript">
		function check_yellowbot() {
			if ($("#yellowbot_q").val() == "Keyword or Category" || !$("#yellowbot_q").val()) {
				$("#yellowbot_q").val("Business");
			}
			if ($("#yellowbot_place").val() == "City or Zip") {
				$("#yellowbot_place").val("");
			}
			return true;
		}
	</script> 
      <div class="bl_main"> 
       <span>Find A Local Business</span> 
       <div class="bl_form"> 
