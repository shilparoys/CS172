  <script language="javascript">
	//globals
	var maj_count_is_one=false ;

	$(document).ready(function() {
		see_saved_ads();
		if ($('#categ_col').length > 0){
			var listring = '';
			var print_once = 'false';
			var cur_cat = '';
			var minor_select = '';


			jQuery.getJSON("/?orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Pets&AD_IMAGE_EXISTS=&orig_cat=&keywords=&date_created=&tl=2", function(jx) {
				if(jx.majors.length == 1) maj_count_is_one=true;
				$.each(jx.majors,function(i,item) {
					if(item.minors.length > 1 || jx.majors.length > 1){
						if(print_once == 'false'){
							$('#categ_col').append('<p class="narrow_by">Narrow By Category</p>');
							print_once = 'true';
							if(jx.majors.length > 1){
								cur_cat = '';
								if ("Pets" != "classifieds") {
									$('#categ_col').append('<p class="major_cat"><a href="?property=nj.com&orig_prop=nj.com&ads_per_page=20&temp_type=browse&category=results&tp=ME_nj&classification=classifieds&">All Categories</a> (' + jx.ad_count + ')</p>');
								}else{
									$('#categ_col').append('<p class="major_cat_gold">All Categories (' + jx.ad_count + ')</p>');
								}
							}
						}
						if ((item.major == "Pets" ) && (true) ){
							$('#categ_col').append('<p class="major_cat_gold">' + item.major + ' (' + item.ad_count + ')</p>');
						} else {
							var majorURL = '?cur_cat=&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj';
							majorURL += '&classification=' + item.major;
							$('#categ_col').append('<p class="major_cat"><a href="' + majorURL + '&ads_per_page=20&orderby=feature_value(featured_listing):nl,date_created:d">' + item.major + '</a> (' + item.ad_count + ')</p>');
						}
						listring = '';
						 $.each(item.minors,function(i,item2){
							if (item2.cat_no == '') {
								//place active selection at the top of the list
								listring = "<li class='active'>"+item2.minor+" ("+item2.ad_count+")</li>" + listring;
							} else {
								var item2URL = '?cur_cat=' + item2.cat_no + '&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj';
								item2URL += '&classification=' + item.major;
								listring = listring + '<li><a href="' + item2URL + '&ads_per_page=20&orderby=feature_value(featured_listing):nl,date_created:d">' + item2.minor + '</a> (' + item2.ad_count + ')</li>';
							}
				 		});
						$('#categ_col').append('<ul class="minor_cat">' + listring + '</ul>');
					}
				});
				var img_exists = '';
					img_exists = readCookie('ad_image_exists' );
				if(!img_exists){
					if(print_once == 'true'){
						$('#categ_col').append('<hr />');
					}
					print_once = 'true';

					$('#categ_col').append('<p class="narrow_by">Narrow By Ads With Photos</p>');
					listring = '';
						var photosURL = "?property=nj.com&orig_prop=nj.com&ads_per_page=20&temp_type=browse&category=results&tp=ME_nj&classification=Pets&ad_image_exists=PHOTO1";
						listring = '<li><a title="Show Only Ads With Photos" href="' + photosURL +'">Show Only Ads With Photos</a></li>';
					$('#categ_col').append('<ul class="minor_cat">' + listring + '</ul>');
				}

				var orig_prop = 'nj.com';
				var property = 'nj.com';
				var affiliateHost = 'nj.com';
				var affiliateHostData = 'nj.com';

				if (orig_prop != property || (property == affiliateHost || property == affiliateHostData) ) {

				jQuery.getJSON("/?property=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Pets&AD_IMAGE_EXISTS=&cur_cat=&keywords=&date_created=&tl=3", function(jp) {
					if(jp.properties.length > 1){
						if(print_once == 'true'){
							$('#categ_col').append('<hr/>');
						}
						$('#categ_col').append('<p class="narrow_by">Narrow By Source</p>');
						if (property == affiliateHost || property ==affiliateHostData ) {		
							$('#categ_col').append('<p class="major_cat_gold">All Sources</p>');
						} else {
							var allSourceURL = '?cur_cat=&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj';
							allSourceURL += "&classification=Pets";
							$('#categ_col').append('<p class="major_cat"><a href="' + allSourceURL + '&ads_per_page=20&orderby=feature_value(featured_listing):nl,date_created:d">All Sources</a></p>');
						}	
						listring = '';
						$.each(jp.properties,function(i,item) {
							if(item.property =='nj.com'){		
								listring = "<li class='active'>"+item.name+"</li>" + listring;
							} else {
								var itemURL = "?cur_cat=&orig_cat=&orig_prop=nj.com&property="+item.property+"&temp_type=browse&category=results&tp=ME_nj";
								itemURL += "&classification=Pets";
								listring = listring + '<li><a href="' + itemURL + '&ads_per_page=20&orderby=feature_value(featured_listing):nl,date_created:d">' + item.name + '</a></li>';
							}
						});
						$('#categ_col').append('<ul class="minor_cat">' + listring + '</ul>');	
					}
				});

				}
				if ($('#categ_col').children().length == 0){
					$('#categ_col').remove();
				}
			});

		}
	});


function changeSort(sorterId){
	var sortElement = document.getElementById(sorterId);
	var url = "?property=nj.com&orig_prop=nj.com&ads_per_page=20&temp_type=browse&category=results&tp=ME_nj&classification=Pets";
	url += '&orderby=' + sortElement.options[sortElement.selectedIndex].value;
	location.href=url;
}

function changeCount(countId){
	var sortElement = document.getElementById(countId);
	var url = "?property=nj.com&orig_prop=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Pets";
	if (sortElement.options[sortElement.selectedIndex].value > 10) {
		url += '&ads_per_page=' + sortElement.options[sortElement.selectedIndex].value;
	}
	location.href=url;
}

function orig_search_button(){	
		var img_exists = readCookie('ad_image_exists' );
		var dt_created = readCookie('date_created');
		//var adspp = readCookie('ads_per_page');
		//var orderby = '';

	if (((false) || maj_count_is_one) ||(false) || (false) || img_exists != '' || dt_created !='') {
		var str = '<a href="?cur_cat=&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj';
			str += "&classification=Pets";
		if (img_exists) str += '&ad_image_exists=' + img_exists;
		if (dt_created) str += '&date_created=' + dt_created;
		str +=  '"><img align="absmiddle" alt="Return to original search results" src="http://media.nj.com/static/classifieds/merchandise/images/btn_backtooriginal.gif" /></a>';

		document.getElementById('back_to_original').innerHTML = str;
	}
}

$(document).ready(function() {
		$('a.buttonSave').each( function() {
			var listingId = this.id;
			check_saved_ads(listingId);
		});
});

