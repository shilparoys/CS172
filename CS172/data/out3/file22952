            var v = setInterval(showChat, 250);
            var it = 0;
            function showChat() {
                it = it + 1;
                if (it > (4 * 5)) {
                    if (v && null != v) {
                        clearInterval(v);
                        v = null;
                    }
                }
                else {
                    $('#scjnRy a').each(function () {
                        if (/live_chat.png$/i.test($(this).children('img').attr('src'))) {
                            $('#scjnRy').show();
                            $('#divchatoffline').hide();
                        }
                        if (v && null != v) {
                            clearInterval(v);
                            v = null;
                        }
                    });
                }
            }
