  <script type="text/javascript">
	/* Javascript specific to only this page (not site) */

function printDate() {
	var months=new Array(13);
	months[1]="jan";
	months[2]="feb";
	months[3]="mar";
	months[4]="apr";
	months[5]="may";
	months[6]="june";
	months[7]="july";
	months[8]="aug";
	months[9]="sep";
	months[10]="oct";
	months[11]="nov";
	months[12]="dec";
	var currentTime=new Date();
	var month=months[currentTime.getMonth() + 1];
	var day=currentTime.getDate();
	var year=currentTime.getFullYear();
	document.write(day +" "+ month +" "+ year);
}





// Permalinks option
function flashPutHref(href) { location.href = href; }

// SWFObject embed
var flashvars = {
	paramXMLPath: "/affiliate/rghs/_site_files/slideshow/param.xml",
	initialURL: escape(document.location)
}
var params = { 
	base: ".",
	bgcolor: "#121212",
	allowfullscreen: "true"
}                
var attributes = {}
swfobject.embedSWF("/affiliate/rghs/_site_files/slideshow/slideshowpro.swf", "flashcontent", "650", "285", "9.0.0", false, flashvars, params, attributes);




