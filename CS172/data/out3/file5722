    <section class="bodee"> 
     <link rel="stylesheet" href="/www.idge.ctw/css/features.css"> 
     <section id="p100-detail-content" class="p100-content"> 
      <div class="ad"> 
       <div id="topimu" class="adunit"> 
        <script type="text/javascript">
						IDG.GPT.addDisplayedAd("topimu", "true");
