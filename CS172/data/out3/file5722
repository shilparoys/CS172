	console.log("moduleTrack()");
	s = s_gi(g_sOmnitureReportSuite);
	s.eVar42=moduleName;
	s.eVar44=loc;
	s.linkTrackEvents=s.events='event31';
	s.list3=s.prop42+","+s.eVar42;
	s.tl(true, 'o', 'Module click');
}
