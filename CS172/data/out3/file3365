  <script type="text/javascript">
        /* Get or create the MOB namespace */
        var MOB = MOB || {};
        MOB.current_test = 0;
        /* Choose an abtest case to try by number. If a test with that number doesn't exist, falls back on the default (0). */
        MOB.abtest = function(casenumber) {
            MOB.current_test = casenumber;
            $('.abtest').hide();
            if ($('.abtest-' + casenumber)[0])
            {
                $('.abtest-' + casenumber).show();
            } else {
                $('.abtest-0').show();
            }
            $("select[class*='abtest-']").change(function(){
                var that = this
                var name = this.name.replace(/-[0-9]+$/, '');
                $("select[name*=" + name + "]").val(that.value);
            });
        };
        $(document).ready(function()
        {
            window.scrollTo(0, 1);
            $("a.totoplink").click(function()
            {
                window.scrollTo(0, 1);
                return false;
            });

            var menuStatus;
            $("#menubutton").click(function(){
                if(menuStatus != true){
                    $("#maincontent").animate({
                        marginLeft: "280px",
                        marginRight: "-280px",
                    }, 300, function(){menuStatus = true});
                    return false;
                } else {
                    $("#maincontent").animate({
                        marginLeft: "0px",
                        marginRight: "0px",
                    }, 300, function(){menuStatus = false});
                    return false;
                }
            });

            $("a.accordion-toggle").click(function()
            {
                if ($(this).hasClass("expanded")) {
                    $("a.accordion-toggle").removeClass("expanded");
                } else {
                    $("a.accordion-toggle").removeClass("expanded");
                    $(this).addClass("expanded");
                }
            });

            $("#locationbar").click(function ()
            {
                if (menuStatus != true)
                {
                    $("#maincontent").animate({
                        marginLeft:"280px",
                        marginRight:"-280px"
                    }, 300, function ()
                    {
                        $("#maincontent").addClass("toside");
                        menuStatus = true
                    });
                    return false;
                }
                else
                {
                    $("#maincontent").removeClass("toside");
                    $("#maincontent").animate({
                        marginLeft:"0px",
                        marginRight:"0px"
                    }, 300, function ()
                    {
                        menuStatus = false
                    });
                    return false;
                }
            });

            


        });


    </script> 
