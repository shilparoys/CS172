                    $('a.moreevents').live("click", function (event)
                    {
                        var thisTarget = $(this).attr('href');
                        $(this).parent().addClass('spinning');
                        $(this).parent().html('<img src="/styles/tmus/polling3.gif" alt="wait">');
                        $.ajax({
                            type: 'Get',
                            url: thisTarget,
                            success: function (data)
                            {
                                // successful request; do something with the data
                                $(".spinning").remove();
                                $("#moreevents").append(data)

                            }

                        });
                        return false

                    });

