    <script type="text/javascript">
$(document).ready(function() {
  $('a.lightbox').lightBox({fixedNavigation:true}); // Select all links with lightbox class

//subscribe box ad 12.17.14 Beth objects/article1.pbo for html
var d = new Date();
var n = d.getMinutes();
var floatingBox = $('#floating-box'); 
var isShowing = 0;
var closeClicked = 0;
var syncLoggedIn = 'notLoggedIn';
if(sync_isLoggedIn()) {var syncLoggedIn = 'LoggedIn';}
else {var syncLoggedIn = 'notLoggedIn';}
//below tracks whether or not the user is logged into syncronex
ga('send', 'event', 'ArticleLoggedIn', 'view', syncLoggedIn);
if ($(window).width() > 1560 && syncLoggedIn == 'notLoggedIn') {
	$(window).bind('scroll', function() {
	    if(($(window).scrollTop() >= $('#article').offset().top + $('#article').outerHeight() - window.innerHeight) && isShowing == 0 && closeClicked == 0){
	      isShowing=1;
	      var url = 'https://services.buffalonews.com/cgi/cgiip.wsc/custservice/web/addrfind.html?stateInfo=Cbkdjddljbzdealb3035|3&closebutton=yes';
	      if (n%2==0){
	      	//alert('current minutes are even! Ad will display at the top!!');
	      	var data = '&version=a';
	      	floatingBox.css({"margin-top": "-300px","display":"block","top":"25px"});
	        floatingBox.animate({ marginTop: '0px'}, 1000); 
	        $('a.subscribe-button').attr('href', url + data);
	      }
	      else {
	      	//alert('current minutes are odd! Ad will display at the bottom!!');
	      	var data = '&version=b';
	      	floatingBox.css({"margin-bottom": "-300px","display":"block","bottom":"25px"});
	      	floatingBox.animate({ marginBottom: '0px'}, 1000); 
	      	$('a.subscribe-button').attr('href', url + data);
	      }
	    }        
	});
}
$( ".close-subscribe-left" ).click(function() {
	isShowing = 0;
	closeClicked = 1;
	if (n%2==0){
	  	floatingBox.animate({ marginTop: '-300px'}, 1000);
  	}
  	else {
  		floatingBox.animate({ marginBottom: '-300px'}, 1000);
  	}
});

});
</script> 
