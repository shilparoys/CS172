         <script type="text/javascript">
	var startURL = 'http://ezproxy.drew.edu/login?url=';
    
	var defaultSearchText_1 = 'Quick Search';

	jQuery(document).ready(function(){

	// Caching 
	var autoC = jQuery('#quick_search');
	
	// Defining a placeholder text; first check if the text is already set
	// then fallback to default
	if (window.defaultSearchText_1 == null) {
		var defaultSearchText_1 = 'Start typing . . .';
	} else {
		var defaultSearchText_1 = window.defaultSearchText_1;
	}

	
	
	// Using jQuery UI's autocomplete widget:
	autoC.autocomplete({
		minLength	: 3,
		source		: 'http://walter.drew.edu/splus/subjects/includes/autocomplete_data.php?collection=databases',
		focus: function(event, ui) {
			return;
		},
		select: function(event, ui) {
            
			location.href = startURL + ui.item.value;
		}
	});
        autoC.defaultText(defaultSearchText_1);
	});
	</script> 
