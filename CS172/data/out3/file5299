//  img_two.src = '/Resources/Images/Rates_OffBtn_Two.jpg';
//  img_three.src = '/Resources/Images/Rates_OffBtn_Three.jpg';
  img_one.src = '/Resources/Images/Rates_OnBtn_One.gif';
  img_two.src = '/Resources/Images/Rates_OffBtn_Two.gif';
  img_three.src = '/Resources/Images/Rates_OffBtn_Three.gif';
 } else if (objName == 'hptwo') {
  layer_one.style.display = 'none';
  layer_two.style.display = 'block';
//  layer_three.style.display = 'none';
//  img_one.src = '/Resources/Images/Rates_OffBtn_One.jpg';
//  img_two.src = '/Resources/Images/Rates_OnBtn_Two.jpg';
//  img_three.src = '/Resources/Images/Rates_OffBtn_Three.jpg';
  img_one.src = '/Resources/Images/Rates_OffBtn_One.gif';
  img_two.src = '/Resources/Images/Rates_OnBtn_Two.gif';
  img_three.src = '/Resources/Images/Rates_OffBtn_Three.gif';
 } else {
  layer_one.style.display = 'none';
  layer_two.style.display = 'none';
  layer_three.style.display = 'block';
