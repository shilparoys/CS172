			$('.majordetails').toggle();
			t = $('#majordetailslink').text();
			if(t.match(/hide/i)) t = t.replace('Hide', 'Show');
			else t = t.replace('Show', 'Hide');
			$('#majordetailslink').text(t);
			return false;
		});
		$(".tabs>ul>li>a").each(function(){
			$t = $(this);
			if ($t.text() == "Faculty" || $t.text() == "faculty") $t.parent().remove();
		});
	});
</script> 
