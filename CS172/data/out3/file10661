       <form action="http://vfw1741.us2.list-manage.com/subscribe/post?u=b4c8d59d6d70a2cc88deac5e7&amp;id=d00fa27d63" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank"> 
