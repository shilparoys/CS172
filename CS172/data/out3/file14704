  var month=new Array('gener','gener', 'febrer','febrer', 'març','març', 'abril','abril', 'maig','maig', 'juny','juny', 'juliol','juliol', 'agost','agost', 'setembre','setembre', 'octubre','octubre', 'novembre','novembre', 'desembre','desembre');
