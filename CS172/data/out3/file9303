        <div class="controls"> 
         <select name="quality" id="quality" class="required"> <option value="B" selected>B - Best Data</option> <option value="E">E - Everything</option> <option value="Q">Q - Data tagged as QC'd</option> <option value="D">D - Data of indeterminate quality</option> <option value="R">R - Raw Data</option> </select> 
        </div> 
       </div> 
      </fieldset> 
