      <!--
      function popup(mylink, windowname) {
        if (! window.focus) return true;
        var href;
        if (typeof(mylink) == 'string')
          href=mylink;
        else
          href=mylink.href;
        window.open(href, windowname, 'width=800,height=300,scrollbars=yes');
        return false;
      }
      //-->

      $(document).ready(function(){
        $.validator.addMethod("injectioncheck", function(value, element, param) {
          if(value.match(new RegExp("^"+param+"$"))) return false;
          return true
        });
        $("#webreq").validate({
          rules: {
            name: { injectioncheck: "[<>!\\?\\*\/].*" },
            inst: { injectioncheck: "[<>!\\?\\*\/].*" },
            address: { injectioncheck: "[<>!\\?\\*\/].*" },
          },
          messages: {
            name: "Required. Characters banned: <>=!?*/",
            inst: "Required. Characters banned: <>=!?*/",
            address: "Required. Characters banned: <>=!?*/",
          },
          highlight: function(label) {
            $(label).closest('.control-group').addClass('error');
          },
          success: function(label) {
            label.addClass('valid').closest('.control-group').addClass('success');
          }
        });
      });
    </script> 
