										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData6177313').appendTo('div#metaInfoData6177313');
											$('#shareData6177313').appendTo('#moreInfoData6177313');
											$(".shareData6177313").click(function(event){
												if ($('#moreInfoData6177313').is(':visible') && $('#shareData6177313').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData6177313").hide();
												    $("#shareData6177313").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData6177313").show();
												    $("#shareData6177313").show();
												    $("#uploadData6177313").hide();
												}
											});
										   $('#closeUData6177313').click(function(event){				   
												$("#moreInfoData6177313").hide();				    
										   });
										   $('#closeSData6177313').click(function(event){				   
												$("#moreInfoData6177313").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData6177313').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData6177313').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData6177313').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
