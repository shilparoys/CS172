           <script>
										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData3177313').appendTo('div#metaInfoData3177313');
											$('#shareData3177313').appendTo('#moreInfoData3177313');
											$(".shareData3177313").click(function(event){
												if ($('#moreInfoData3177313').is(':visible') && $('#shareData3177313').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData3177313").hide();
												    $("#shareData3177313").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData3177313").show();
												    $("#shareData3177313").show();
												    $("#uploadData3177313").hide();
												}
											});
										   $('#closeUData3177313').click(function(event){				   
												$("#moreInfoData3177313").hide();				    
										   });
										   $('#closeSData3177313').click(function(event){				   
												$("#moreInfoData3177313").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData3177313').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData3177313').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData3177313').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
									</script> 
