           <script>
										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData1177313').appendTo('div#metaInfoData1177313');
											$('#shareData1177313').appendTo('#moreInfoData1177313');
											$(".shareData1177313").click(function(event){
												if ($('#moreInfoData1177313').is(':visible') && $('#shareData1177313').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData1177313").hide();
												    $("#shareData1177313").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData1177313").show();
												    $("#shareData1177313").show();
												    $("#uploadData1177313").hide();
												}
											});
										   $('#closeUData1177313').click(function(event){				   
												$("#moreInfoData1177313").hide();				    
										   });
										   $('#closeSData1177313').click(function(event){				   
												$("#moreInfoData1177313").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData1177313').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData1177313').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData1177313').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
									</script> 
