// Weather script
$(document).ready(function() {  
  getWeather(); //Get the initial weather.
  setInterval(getWeather, 900000); //Update the weather every 15 minutes.
});

function getWeather() {
  $.simpleWeather({
    zipcode: '98104',
    woeid: '12798948',
    unit: 'f',
    success: function(weather) {
      //html = '<h3 style="margin: 0; text-transform: none;">'+weather.city+', '+weather.region+'</h3>';
	  //html = '<h2 style="background-image: url('+weather.image+'); background-position: 0 -10px; background-repeat: no-repeat; padding-top: 120px;"><span><strong>Right now</strong> <em style="text-transform: lowercase; font-style: normal; font-size: 11px;">(updated every 15 minutes)</em></span>'+weather.temp+'&deg;'+weather.units.temp+'</h2>';
	  html = '<h2 style="background-image: url('+weather.image+'); background-position: 0 -10px; background-repeat: no-repeat; margin-top: -10px;padding-top: 120px;"><span><strong>Currently: '+weather.currently+'</strong></span>'+weather.temp+'&deg;'+weather.units.temp+'</h2>';
	  html += '<ul><li class="currently">Today: '+weather.high+'&deg;, '+weather.forecast+'</li>';
	  html += '<li>Tomorrow: '+weather.tomorrow.high+'&deg;, '+weather.tomorrow.forecast+' </li></ul>';
	  //html += '<ul><li class="currently">Today: '+weather.high+'&deg; '+weather.units.temp+', '+weather.forecast+'</li>';
	 // html += '<li>Tomorrow: '+weather.tomorrow.high+'&deg; '+weather.units.temp+', '+weather.tomorrow.forecast+' </li></ul>';
	  //html += '<li><a href="'+weather.link+'">See the Forecast</a> &raquo;</li></ul>';
      //html += '<h2>'+weather.temp+'&deg;'+weather.units.temp+'</h2>';
      //html += '<ul><li>'+weather.city+', '+weather.region+'</li>';
      //html += '<ul><li class="currently"><img style="float: left; margin-top: -10px;" src="'+weather.thumbnail+'"/ > '+weather.currently+'</li></ul>';
	  //html += '<p style="text-transform: none;"><strong>Tomorrow\'s Forecast</strong></p><ul><li>'+weather.tomorrow.high+'&deg; '+weather.units.temp+', '+weather.tomorrow.forecast+' </li></ul>';
      //html += '<li>'+weather.tempAlt+'&deg;C</li></ul>';
  
      $("#weather").html(html);
    },
    error: function(error) {
      $("#weather").html('<p>'+error+'</p>');
    }
  });
}

  // Hover panels
  
  $('.home').hover(
	function(){
		$(this).find('.slide').animate({top:"34px"}, 'fast');			
	}, 
	function(){
		$(this).find('.slide').animate({top:"200px"}, 'fast');
	}		
	);
	
  // remove leading date from news
  
	$('#delicious li a').each(function(){
	 var c = $(this).text().split(":");
		$(this).html( '<span>' + c[0] + ':</span><strong>' + c[1] + '</strong>');
	});

});



