function OtomasyonAc(adres)
{    if (top.window.document.location.href.indexOf(adres)!=-1)
    { top.window.document.location.href=top.window.document.location.href;
    } else if (top.window.document.location.href!=document.location.href)
        { top.window.document.location.href=document.location.href;
        }
        else
        {   var sHeight,sWidth; sHeight=715;  sWidth = 1015;
            mywindow=window.open(adres,"newWindow","Height=" + sHeight + ", width=" + sWidth + ",toolbar=0,scrollbars=0,location=0,resizeble=no")
            window.opener = top;
            if (mywindow!=null)
            {     mywindow.moveTo(0,0);

            }
      }
