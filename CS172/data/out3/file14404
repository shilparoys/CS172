  <script type="text/javascript">
	jQuery(document).ready(function(){
		jQuery("#loginForm").validate({
			rules: {
				l:  {
					required: true,
					defaultInvalid_login: "Usuario"
				},
				p:  {
					required: true,
					defaultInvalid_login: "Clave"
				}
			}
		}),
		jQuery.validator.addMethod('defaultInvalid_login', function(value,element){
			switch(element.name){
				case "l":
					if(value == "Usuario"){
						return false;
					}
				case "p":
					if(value == "Clave"){
						return false;
					}
				default:
					return true;
			}
		}),
		jQuery(".login_campus_opener a").click(function(){
