//<![CDATA[

	jQuery.noConflict();
	jQuery(document).ready(function($) {
		
		// Handle the ENTER key in the Search box
    	jQuery("#navQuery").keypress(function(event) {
			if (event.keyCode == 13) {
				JavaScript:Search('nav'); 
				return false;
			}
    	});
    	
    	// Handle clicking the Search button
    	jQuery("#navSearchRef").click(function() {
    		JavaScript:Search('nav');
    		return false;
    	});
    	
    	// Add Webtrends functionality on Search box and button
       	jQuery("#navQuery").webtrends.assignSearchInputBox();
       	jQuery("#navSearchRef").webtrends.assignSearchButton();
    	
       	jQuery("#navQuery").help.addHelpSearchInputBehaviors();

	}); // end jQuery(document).ready
	//]]>
</script> 
