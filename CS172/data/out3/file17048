      <script type="text/javascript">
function slide() {
    // slides up or down and sets the new image source
    if($('#extendedfooterlinks').hasClass("invisible")) {
        $('#extendedfooterlinks').slideDown('slow');
        $('#extendedfooterlinks').removeClass("invisible");
        $('#opencloseImg').attr("src", "/content/lfw/en/index/_jcr_content/extendedfooter/extendedfooter_0/closeimage.img.jpg");
    } else {
        $('#extendedfooterlinks').slideUp('slow');
        $('#extendedfooterlinks').addClass("invisible");
        $('#opencloseImg').attr("src", "/content/lfw/en/index/_jcr_content/extendedfooter/extendedfooter_0/openimage.img.jpg");
    }
}
</script> 
