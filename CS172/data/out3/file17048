            <script type="text/javascript">
        var obj_leftcolumn_standard_6 = {
            setTCValues : function() {
                var tcTagStr = '';
                var tcTagArr = tcTagStr.split("/");
                if (tcTagArr != null && tcTagArr != "") {
                    for(var i=1;i<=3;i++) {
                        var tcTag = (i <= tcTagArr.length) ? tcTagArr[i-1] : "";
                        $.cookie('TC'+i, tcTag, { path : '/' });
                    }
                }
            },
            showRandomRows : function() {
                var allIndices = _.range(5);
                var indicesToShow = _.sample(allIndices, 0);
                if (false) {
                    _.forEach(indicesToShow, function(idx) {
                        $('#leftcolumn_standard_6 tbody').append($('#leftcolumn_standard_6_agg_row_' + idx).clone().removeClass("aggregator-random-hidden-row"));
                    });
                } else {
                    _.forEach(indicesToShow, function(idx) {
                        $('#leftcolumn_standard_6_agg_row_' + idx).removeClass("aggregator-random-hidden-row");
                    });
                }
            }

        };
        $(document).ready(function(){
            $('#leftcolumn_standard_6 a').click(function(e) {
                obj_leftcolumn_standard_6.setTCValues();
            });
            
        });
    </script> 
