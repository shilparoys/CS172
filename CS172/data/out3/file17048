         <script type="text/javascript">
        function validateSearchInput(name) {
            if (navigator.appName == 'Microsoft Internet Explorer') {
                return true; // just skip further checks
            }
            var keyword = $(name).val();
            return keyword[0] != null && keyword[0] != "";
        }
    </script> 
