        $('#ctl00_cphMain_ctl02_btnSubmit').click(function(e) {
            $.fancybox.resize();
        });
        function reSize(e) {
            var key = e ? e.which : event.keyCode;
            if (key == 13) {
                $.fancybox.resize();
            }
        }
    </script> 
