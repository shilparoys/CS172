   <div id="content"> 
    <h2>Faculty Support</h2> 
    <!-- Start Sidebar --> 
    <div id="sidebar"> 
     <div class="nav"> 
      <h3 class="accessibility">Main Navigation</h3> 
      <ul> 
       <li><a href="/" title="Home">Home<br><span></span></a></li>
       <li class="nav_inline_last"><h3>Help &amp; Support</h3>
        <ul>
         <li class="currentpage"><h3><dfn>Current page is: </dfn>Faculty</h3></li>
         <li>&nbsp;| <a href="/staff.html" title="Staff">Staff</a></li>
         <li>&nbsp;| <a href="/students.html" title="Students">Students</a></li>
        </ul></li>
       <li><a href="/diversity.html" title="Commitment to Diversity">Commitment to Diversity</a></li>
       <li><a href="/about_us.html" title="About Us">About Us</a></li>
