       <script type="text/javascript">

            var sejnRy = document.createElement("script");
            sejnRy.type = "text/javascript";
            var sejnRys = (location.protocol.indexOf("https") == 0
                    ? "https" : "http") + "://image.providesupport.com/js/ges_candice/safe-standard.js?ps_h=jnRy&ps_t="
                    + new Date().getTime()
                    + '&online-image=/includes/hosted/images/live_chat.png' + '&offline-image=/includes/hosted/images/live_chat_offline.png';
            setTimeout("sejnRy.src=sejnRys;document.getElementById('sdjnRy').appendChild(sejnRy)", 1);
            var v = setInterval(showChat, 250);
            var it = 0;
            function showChat() {
                it = it + 1;
                if (it > (4 * 5)) {
                    if (v && null != v) {
                        clearInterval(v);
                        v = null;
                    }
                }
                else {
                    $('#scjnRy a').each(function () {
                        if (/live_chat.png$/i.test($(this).children('img').attr('src'))) {
                            $('#scjnRy').show();
                            $('#divchatoffline').hide();
                        }
                        if (v && null != v) {
                            clearInterval(v);
                            v = null;
                        }
                    });
                }
            }
        </script> 
