        <!-- include jsplayer --> 
        <script src="http://rc.rit.edu//inc/jwplayer/jwplayer.js" type="text/javascript"></script> 
        <script type="text/javascript">
				//this script is ment to be used with live.php to let
// the server know the user is watching
var Timer;
var url
function start(x) {
url = x;
sendget();
Timer = setInterval("sendget()", '300000');
}

//code from w3c tutorial on sending get requests w javascript
function sendget() {
var xmlhttp;
if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
xmlhttp = new XMLHttpRequest();
} else {// code for IE6, IE5
xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
}
xmlhttp.open("GET", url, true);
xmlhttp.send();
}

function d(x){console.log(x);}

function init() {
var elem = document.getElementById('mediaspace');
var h = elem.clientHeight;
var w = elem.style.clientWidth;

jwplayer('mediaspace').setup({
'flashplayer' : 'http://rc.rit.edu//inc/jwplayer/player.swf',
'file' : 'live/Polisseni_Center_2.sdp',
'image': 'http://rc.rit.edu//images/Poli1.png',
'provider' : 'rtmp',
'streamer' : 'rtmp://wowza.rc.rit.edu/live',
'controlbar' : 'none',
'dock' : 'true',
'autostart' : 'false',
'width' : w,
'height' : h
	});
	}

	document.write("<div id='mediaspace'>  </div>");
	window.onload = function()
	{
		init();
	};

			</script> 
        <a href="live-video.html"> Live Video Page </a> 
