        var sumallAB = { active : false,
            addCSSRule: function(sheet, selectorText, value) {
                if (sheet.insertRule) sheet.insertRule(selectorText + ' {' + value + '}', sheet.cssRules.length);
                else if (sheet.addRule) sheet.addRule(selectorText, value, sheet.rules.length);
            },
            cookie: {
                get: function(n){var a = document.cookie.split(';');for(var i=0;i<a.length;i++){var t=a[i].split('=');if(n==t[0].replace(/^\s+|\s+$/g,'')){if(t.length==1)return ""; return t[1];}}return null;},
                set:function(n,v,exp,p,d) {document.cookie=n+"="+v+(exp?"; expires="+exp.toGMTString():"")+(p?"; path="+p:"; path=/")+(d?"; domain="+d:"");}
            },
            parseQS: function(qs) {var d=decodeURIComponent;var r={};var p=qs.split('&');for(var i in p){var v=p[i].split('=');v[0]=d(v[0]);v[1]=d(v[1]);if (r[v[0]]==undefined)r[v[0]]=[v[1]];else r[v[0]].push(v[1]);}return r;}
        };

        // fetch cookie if not primed by server
        if (sumallAB.bucketing == undefined || sumallAB.bucketing.length <= 0) {
            var c = sumallAB.cookie.get("abExpBkt");
            if (c && c.length > 0) {
                sumallAB.bucketing = JSON.parse(c);
            }
        }

        // default empty obj
        if (sumallAB.bucketing == undefined) {
            sumallAB.bucketing = {};
        }


        // a way to define a landing/injecting set of experiments and variations
        var injectAB = {};
        var docLocation = (document.location+'');
        if (docLocation.indexOf('?') > -1) {
            var abQueryString = docLocation.substring(docLocation.indexOf('?')+1);

            // check for AB test id injection and retrieve
            var qsAB = sumallAB.parseQS(abQueryString)['abtst'];
            if (qsAB != undefined) {
                for (var i in qsAB) {
                    var pos = qsAB[i].indexOf('_')
                    var e = qsAB[i].substring(0,pos);
                    var v = qsAB[i].substring(pos+1).split(',');
                    injectAB[e] = v;
                }
            }
        }

        // apply any id injection from the landing URL
        for (var i in injectAB) {
            sumallAB.bucketing[i] = injectAB[i];
        }

        // set a cookie with current bucketing data
        sumallAB.cookie.set('abExpBkt', JSON.stringify(sumallAB.bucketing));

        // set bucketing information into Optimizely's API
        if (sumallAB.bucketing != undefined) {
            window['optimizely'] = window['optimizely'] || [];

            for (var expId in sumallAB.bucketing) {
                var varIds = sumallAB.bucketing[expId];
                for (var k in varIds) {
                    window['optimizely'].push(["bucketVisitor", expId, varIds[k]]);
                }
            }
        }
