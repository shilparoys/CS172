  function ShwAQt() {
	var n = $('#shadowbox').children('p').length;
	n = Math.floor(Math.random()*n);
	$('#shadowbox').children('p:eq('+n+')').removeClass('NoShw').addClass('Shw');
/*	var d = $('#shadowbox').children('p:eq('+n+')');
	d.removeClass('NoShw');
	d.addClass('Shw'); */
  }
</script>
