      <script type="text/javascript">
var prev_dest='';
var prev_dest_inside='';
 var req = null;
var prev_main_div='';
var prev_span='';

function GetJobsInv(destination,span_d) {
		if (document.getElementById(destination).style.display=='none'){
    		document.getElementById(destination).style.display='inline';
    		if(prev_main_div!=''&&prev_main_div!=destination) document.getElementById(prev_main_div).style.display='none';
    		prev_main_div=destination;
			 
			document.getElementById(span_d).className='branch_invert';
			if(prev_span!=''&&prev_span!=span_d) document.getElementById(prev_span).className='branch';
			prev_span = span_d;
    	}else{
			 document.getElementById(destination).style.display='none';
			  document.getElementById(span_d).className='branch';
		}	 
}
function popDivOver(dest_div,dest,dest_img){
	document.getElementById(dest_div).style.display='block';
	document.getElementById(dest).style.background=''; 
	
}
function popDivOut(dest_div,dest,dest_img){
	document.getElementById(dest_div).style.display='none';
	document.getElementById(dest).style.background=''; 
}

function changejobdiv(div){
	if(div == 'cat'){
		document.getElementById('get_jobs_by_type').style.display="block";
		document.getElementById('get_jobs_by_region').style.display="none";
		document.getElementById('get_jobs_by_comp').style.display="none";
		document.getElementById('cat').className="selected";
		document.getElementById('reg').className="";
		document.getElementById('comp').className="";
	}else if(div == 'reg'){
		document.getElementById('get_jobs_by_type').style.display="none";
		document.getElementById('get_jobs_by_region').style.display="block";
		document.getElementById('get_jobs_by_comp').style.display="none";
		document.getElementById('cat').className="";
		document.getElementById('reg').className="selected";
		document.getElementById('comp').className="";
	}else if(div == 'comp'){
		document.getElementById('get_jobs_by_type').style.display="none";
		document.getElementById('get_jobs_by_region').style.display="none";
		document.getElementById('get_jobs_by_comp').style.display="block";
		document.getElementById('cat').className="";
		document.getElementById('reg').className="";
		document.getElementById('comp').className="selected";	
	}
}
</script> 
