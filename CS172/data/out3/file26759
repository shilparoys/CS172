       <script type="text/javascript">
var res=new Array();

String.prototype.trim = function()
	{
	    return this.replace(/(^\s*)|(\s*$)/g, "");
	}


function Jopa(id,vis){

	if (vis==1){
		if (id=='job_category[0]_content'){
			
			document.getElementById('country').style.display='none';
			document.getElementById('region').style.display='none';
			document.getElementById('city').style.display='none';
		}	
	}else{
		if (id=='job_category[0]_content'){
			
			document.getElementById('country').style.display='inline';
			document.getElementById('region').style.display='inline';
			document.getElementById('city').style.display='inline';
		}
	}
	
}

function InputTemplate(t){
 // t.className=/\D/.test(t.value)?'err_sal':'str';
  
  if(/\D/.test(t.value)){
  	t.value=t.defaultValue;
  		
	}
  else {
  	t.defaultValue=t.value;
  	
  	return true;
  }
  		
}	

function setInput(obj, default_txt) {

 if(obj.value == "") {
 obj.value = default_txt;
 } else {
 if(obj.value == default_txt){
 obj.value = "";
 }
 }
}

function KeywordOnBlur(obj,default_txt){


 if(obj.value == "") {
 	obj.value = default_txt;
 } else {
	 if(obj == default_txt){
	 obj.value = "";
	 }
 }
}

function DoInvisible(dest,par){
	if (document.getElementById(dest).style.display=='none'){
		document.getElementById(dest).style.display=par;
		hell=dest;
		Jopa(dest,'1');
	}else{
		hell='';
		document.getElementById(dest).style.display='none';
		Jopa(dest,'2');
	}	
}
function DoInvisible2(dest,par){
	if (document.getElementById(dest).style.display=='none'){
		document.getElementById(dest).style.display=par;
		
		document.getElementById(dest+'_img').src='templates/default_theme/images/hell_menu/treeview-expanded-sign.png';
	}else{
		document.getElementById(dest).style.display='none';
		
		document.getElementById(dest+'_img').src='templates/default_theme/images/hell_menu/treeview-collapsed-sign.png';
	}	
}

function SelectAllSub(frm,arr,qwant,itm,itm_ch,lim,cpt,get_div){
	var re= /(\d+)/;
	if(document.getElementById(cpt).innerHTML=='Select Job Category'){
		//276 276 0
		
		res[0]=276;
			cnt=0;
	}else{
			res = re.exec(document.getElementById(cpt).innerHTML);			
			if (res==null) cnt=lim;
			else{			
				if (res[0]==lim) cnt=0; else cnt=res[0];
			}
	}
	if(cnt == lim)
		cnt=0;
	zzz=0;
	elem =document.forms[frm].elements[arr+"[]"];
	//checked
		for (var i=0; i < elem.length; i++){ 
		
			res=re.exec(elem[i].value);
			if(itm.value== res[0]){				
				if(itm_ch){ 					
					if (cnt<lim){						
						if(elem[i].checked == false){
							cnt++;	
							zzz++;
							elem[i].checked=true;							
							if (cnt<lim) {
								document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
								zzz = cnt;
							}
							if (cnt==lim){							
								document.getElementById(cpt).innerHTML=cnt +'&nbsp;Maximum categories selected';
							}
						} 
					}else{						
						elem[i].checked=false;
						document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
						document.getElementById(cpt).innerHTML=cnt +'&nbsp;Maximum categories selected';
					}
				}else{//unchecked					
					cnt--;
					elem[i].checked=false;
					if (cnt>0) {
						document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
					}else{
						document.getElementById(cpt).innerHTML='Select Job Category';
					}
					
				}
			}
	
		}
		if(itm_ch && zzz<qwant) 
			itm.checked=false;
		
		document.getElementById(get_div).style.display="block";
		document.getElementById(get_div+'_img').src="templates/default_theme/images/hell_menu/treeview-expanded-sign.png";
	
}
function SelectCh(itm,itm_ch,lim,cpt,mn_ch){

	if(document.getElementById(cpt).innerHTML=='Select Job Category'){ 
		//276 276 0
		res[0]=276;
		cnt=0;
	}else{
		res = /(\d+)/.exec(document.getElementById(cpt).innerHTML);
		if (res==null) {
			nt_chk = 0;
			cnt=lim;
		}else{
			if (res[0]==lim) 
				cnt=0; 
			else 
				cnt=res[0];
		}
	}
	
	if(itm_ch){
	cnt++;	
		if (cnt<=lim){
			itm.checked=true;
			if (cnt<lim)
			document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
			if (cnt==lim)
			document.getElementById(cpt).innerHTML=cnt+'&nbsp;Maximum categories selected';
		}
		else{	
			
				if(nt_chk > cnt){
					itm.checked=false;
					document.getElementById(cpt).innerHTML='Maximum categories selected';
				}else{
					cnt = 1;
					itm.checked=true;
					document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
				}
		
			
		}
	}else{
		
		cnt--;
		itm.checked=false;
		document.getElementById(mn_ch).checked=false;
		if (cnt>0) {
			
		
			document.getElementById(cpt).innerHTML=cnt+'&nbsp;categories selected';
		}else{
			document.getElementById(cpt).innerHTML='Select Job Category';
		}
		
	}	
	//itm.checked=par;
/*	//var arr=
	if (document.getElementById(dest).style.display=='none'){
		document.getElementById(dest).style.display=par;
	}else{
		document.getElementById(dest).style.display='none';
	}	
	*/
}
function UncheckAll(frm,arr,lim,cpt)
{
	elem =document.forms[frm].elements[arr+"[]"];
	for (var i=0; i < elem.length; i++) elem[i].checked = false;
	elem =document.forms[frm].elements[arr+"[opt][]"];
	for (var i=0; i < elem.length; i++) {
		elem[i].checked = false;
	}
	document.getElementById(cpt).innerHTML='Select Job Category';
}

</script> 
       <!--END OF QUICK SEARCH CONTENT --> 
