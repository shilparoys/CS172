      <div id="block-views-features-block" class="block block-views"> 
       <div class="content"> 
        <script type="text/javascript">

jQuery(document).ready(function($) {
	
	var highlight_next = function() {
		clearTimeout(switch_timer);
		var count	= $('#highlights-featured .views-row').length-1;
		var index	= $('#highlights-featured .views-row:visible').index()-1;
		var next	= (index==count) ? 0:index+1;
		$('#highlights-featured .views-row').eq(index).fadeOut('slow');
		$('#highlights-featured .views-row').eq(next).fadeIn('slow');
		switch_timer = setTimeout(function(){ highlight_next(); },6000);
	};
	
	var highlight_prev = function() {
		clearTimeout(switch_timer);
		var count	= $('#highlights-featured .views-row').length-1;
		var index	= $('#highlights-featured .views-row:visible').index()-1;
		var next	= (index==0) ? count:index-1;
		$('#highlights-featured .views-row').eq(index).fadeOut('slow');
		$('#highlights-featured .views-row').eq(next).fadeIn('slow');
		switch_timer = setTimeout(function(){ highlight_next(); },6000);
	};
	
	$('#highlights-featured .views-row').hide().first().show();
	var switch_timer = setTimeout(function(){ highlight_next(); },6000);
	
	$('#highlights-featured .arrow.right').click(function(){ highlight_next(); });
	
	$('#highlights-featured .arrow.left').click(function(){ highlight_prev(); });
	
});

</script> 
