                 <button class="ProfileTweet-actionButton  js-actionButton js-actionRetweet js-tooltip" title="Retweeten" data-modal="ProfileTweet-retweet" type="button"> <span class="Icon Icon--retweet"></span> <span class="u-hiddenVisually">Retweeten</span> <span class="ProfileTweet-actionCount"> <span class="ProfileTweet-actionCountForPresentation" aria-hidden="true">3</span> </span> </button>
                 <button class="ProfileTweet-actionButtonUndo js-actionButton js-actionRetweet js-tooltip" data-modal="ProfileTweet-retweet" title="Retweet ongedaan maken" type="button"> <span class="Icon Icon--retweet"></span> <span class="u-hiddenVisually">Geretweet</span> <span class="ProfileTweet-actionCount"> <span class="ProfileTweet-actionCountForPresentation" aria-hidden="true">3</span> </span> </button> 
                </div> 
                <div class="ProfileTweet-action ProfileTweet-action--favorite js-toggleState"> 
                 <button class="ProfileTweet-actionButton js-actionButton js-actionFavorite js-tooltip" title="Favoriet" type="button"> <span class="Icon Icon--favorite"></span> <span class="u-hiddenVisually">Favoriet</span> <span class="ProfileTweet-actionCount"> <span class="ProfileTweet-actionCountForPresentation" aria-hidden="true">1</span> </span> </button>
