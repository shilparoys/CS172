     <script type="text/javascript">
$(document).ready(function() {
  $( "#calendar" ).datepicker({
  onSelect: function(dateText, inst)
  {
    //alert(d.getFullYear()+"-"+(parseInt(d.getMonth())+1));
     ShowDateEvent(dateText);
  }
  ,
  onChangeMonthYear: function(year, month, inst)
  {
    CheckMonthActivity(year,month);
  }
  });
  $("#dialogEvent").dialog({autoOpen:false,modal:true,width:600,height:600,zIndex:1000});

$("#carousel").carouFredSel({
	responsive	: true,
	auto		: {
		timeoutDuration	: 6000
	},
	scroll		: {
		fx			: "crossfade",
	},
	items		: {
		visible		: 1,
		width		: 960,
		height		: 320
	},
	pagination  : "#carousel_pag"

});


});
function CheckMonthActivity(year,month)
{
  $.get("/pws/CheckMonthActivity.php?year="+year+"&month="+month, function(data) {
//alert(data);
       $("#calendar a").each(function(index){
           if(!isNaN($(this).html()))
           {
             if(data.indexOf("|"+$(this).html()+"|")!=-1)
             {
               //alert($(this).html());
               $(this).css("font-weight","bold");
               $(this).css("color","#0b3d4c");
             }
           }
       });
    });
}
function ShowDateEvent(date)
{
  $("#dialogEvent").dialog("open");

  var content="";
  var objDate=new Date(date);
  var months=new Array("January","February","March","April","May","June","July","August","September","October","November","December");
  var weeks=new Array("Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday");

  var dateString=weeks[objDate.getDay()]+", "+months[objDate.getMonth()]+" "+objDate.getDate()+", "+objDate.getFullYear();
  
content+="<div><b>"+dateString+"</b></div><br>";
  var url="/pws/getEvent.php?date="+date;
  //alert(date);
  $.get("/pws/getEvent.php?date="+date, function(data) {
    content+="<div>"+data+"</div>";
    $("#dialogEvent").html(content);
    var d=new Date(Date.parse(date));
    CheckMonthActivity(d.getFullYear(),parseInt(d.getMonth())+1);
  });
}
</script> 
