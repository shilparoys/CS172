  function CheckFocus(val)
  {
    if(document.frmGoogleSearch.q.value==val)
    {
      document.frmGoogleSearch.q.value="";
    }
  }
  function CheckBlur(val)
  {
    if(document.frmGoogleSearch.q.value=="")
    {
      document.frmGoogleSearch.q.value=val;
    }
  }
  </script> 
