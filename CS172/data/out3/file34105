                var mapOptions = {
                    center: new google.maps.LatLng(33.7764, -84.3874),
                    zoom: 13,
                    mapTypeControl: false,
                    zoomControl: true,
                    tilt: 0,
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                };
                var infowindow = new google.maps.InfoWindow({
                    content: '<strong>Georgia Institute of Technology<br/>Scheller College of Business</strong><br/>800 West Peachtree, NW<br/>Atlanta, Georgia 30308'
                });
                    
