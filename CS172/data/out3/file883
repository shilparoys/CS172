       <!--
<script type="text/javascript" src="/js/jquery/jquery-1.9.0.js"></script>--> 
       <script type="text/javascript">	

	/* VERIFY THAT REQUIRED FIELDS ARE COMPLETED */


		function validateForm(){
				var i;
		var i2;
		var i3;
		var i4;
		var iTotal;
		
			/* CHECK IF SID IS FILLED */
			if (document.getElementById('txtSID').value == ""){
				document.getElementById('show-sid-msg').innerHTML = "Please enter your SID";
				i= 1;
				document.getElementById('btnLogin2').disabled = true;
				
				return (false);
			}
			else{
				i = 0;
				document.getElementById('btnLogin2').disabled = false;
			}
					
			/* CHECK IF EMAIL IS FILLED */
			if (document.getElementById('txtEmail').value == ""){
				document.getElementById('show-email-msg').innerHTML = "Please enter a valid email address";
				i2= 1;
				document.getElementById('btnLogin2').disabled = true;
				
				return (false);
			}
			else{
				i2 = 0;
				document.getElementById('btnLogin2').disabled = false;
			}
			
			/* CHECK IF FIRST NAME IS FILLED */
			if (document.getElementById('txtFirstName').value == ""){
				document.getElementById('error3').innerHTML = "Please provide your first name.";
				i3= 1;
				document.getElementById('btnLogin2').disabled = true;
				
				return (false);
			}
			else{
				i3 = 0;
				document.getElementById('btnLogin2').disabled = false;
			}
			
			/* CHECK IF LAST NAME IS FILLED */
			if (document.getElementById('txtFamilyName').value == ""){
				document.getElementById('error4').innerHTML = "Please provide your family name.";
				i4= 1;
				document.getElementById('btnLogin2').disabled = true;
				
				return (false);
			}
			else{
				i4 = 0;
				document.getElementById('btnLogin2').disabled = false;
			}
			 
			iTotal = i + i2 + i3 + i4;
			if (iTotal > 0){
				return (false);
				//alert(iTotal);
			}
			else{
				//alert(iTotal);
				return (true);
			}
			
		}
</script>   
      </div> 
