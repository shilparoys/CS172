  <script type="text/javascript">

$(document).ready(function(){

//console.log($('div:last').attr('id'));
// This fixed the responsive bug when EE Debugger is visible

if($('div:last').attr('id') == undefined)
	{
		$('div:last').parent().css('margin','0px auto');
		$('div:last').parent().css('width','80%');
		//$('div:last').parent().css('max-width','360px');
		$('div:last').parent().css('overflow','hidden');
		$('div:last').parent().css('word-wrap','break-word');
	}

});

</script> 
  <script type="text/javascript">
$(document).ready(function(){

// THIS IS DONE == ONLY == ON THE HOMEPAGE
// TO BRING UP THE 2 SOCIAL BUTTONS ABOVE THE NEWS/EVENTS MODULE

// DEFAULT LOAD RESPONSIFICATION
if($(document).width() > 767)
	{
		$('.specialMove').insertAfter('.main-navi');
	}
else
	{
		$('.specialMove').insertBefore('#mid-column');
	}

// WHILE RESIZING RESPONSIFICATION
$(window).on('resize', function () 
	{
		if($(document).width() > 767)
			{
				$('.specialMove').insertAfter('.main-navi');
			}
		else
			{
				$('.specialMove').insertBefore('#mid-column');
			}
	});


});
</script> 
