        <!-- // rotator_box --> 
        <script type="text/javascript">
$(document).ready(function(){

	// This is done to prevent loading rotators if a mobile website is loaded
	// Degault load incase desktop is loaded first
	if($(document).width() > 767)
		{
			$('#homerotator_1 img').attr('src', RotatorIMGS[0]);
			$('#homerotator_2 img').attr('src', RotatorIMGS[1]);
			$('#homerotator_3 img').attr('src', RotatorIMGS[2]);
			$('#homerotator_4 img').attr('src', RotatorIMGS[3]);
			$('#homerotator_5 img').attr('src', RotatorIMGS[4]);
		}
	// Responsive load, incase someone starts mobile but ends up desktop
	$(window).on('resize', function () 
		{
			if($(document).width() > 767)
				{
					$('#homerotator_1 img').attr('src', RotatorIMGS[0]);
					$('#homerotator_2 img').attr('src', RotatorIMGS[1]);
					$('#homerotator_3 img').attr('src', RotatorIMGS[2]);
					$('#homerotator_4 img').attr('src', RotatorIMGS[3]);
					$('#homerotator_5 img').attr('src', RotatorIMGS[4]);
				}
		});

});
</script> 
