						var time = null;
						$("#comunidad_dropdown").click( function() { 
							$(".comunidad_links").fadeIn();
							time = setTimeout(function(){ $(".comunidad_links").fadeOut("slow"); }, 1000);
						});
						
						$(".comunidad_links").hover(function(){clearTimeout(time);}, function () {
							time = setTimeout(function(){ $(".comunidad_links").fadeOut("slow"); }, 500);
						});
					});
