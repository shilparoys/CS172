        YUI.namespace('Env.Af.settings').beacon = {
            batch: true,
            sampleSize: 20,
            beaconUncaughtErr: true,
            pathPrefix: '/' + App.Cache.magazine + '/_td_api/beacon',
            meta: {
                site: App.Cache.magazine
            }
        };
        var deps = ['yosaic-app'];
        if (YMedia.UA.ie && YMedia.UA.ie <= 9) {
            deps.unshift('yosaic-ie-compat');
        }
        if (
            (YMedia.UA.safari || YMedia.UA.ios) ||
            (YMedia.UA.gecko || YMedia.UA.gecko < 29) ||
            (YMedia.UA.ie || YMedia.UA.ie < 11) ||
            (YMedia.UA.chrome || YMedia.UA.chrome < 24) ||
            (YMedia.UA.opera && YMedia.UA.opera < 15)
        ) {
            deps.unshift('intl-locale-loader');
        }

        YMedia.use(deps, function (Y) {
            if (YMedia.UA.gecko) {
                Y.one('body').addClass('firefox');
            }

            var yosaicApp = new Y.Yosaic.App({
                container: 'body',
                viewContainer: '#mainview',
                magazine: '',
                moneyballStreamAdData: moneyballStreamAdData,
                moneyballArticleAdData: moneyballArticleAdData
            });
            Yosaic.app = yosaicApp;
            Yosaic.app.customDispatch();
