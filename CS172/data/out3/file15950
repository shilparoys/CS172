        <!-- // rotator_box --> 
        <script type="text/javascript">
$(document).ready(function(){

	// This is done to prevent loading rotators if a mobile website is loaded
	// Degault load incase desktop is loaded first
	if($(document).width() > 767)
		{
			$('#channel_parents_1 img').attr('src', RotatorIMGS[0]);
			$('#channel_parents_2 img').attr('src', RotatorIMGS[1]);
			$('#channel_parents_3 img').attr('src', RotatorIMGS[2]);
		}
	// Responsive load, incase someone starts mobile but ends up desktop
	$(window).on('resize', function () 
		{
			if($(document).width() > 767)
				{
					$('#channel_parents_1 img').attr('src', RotatorIMGS[0]);
					$('#channel_parents_2 img').attr('src', RotatorIMGS[1]);
					$('#channel_parents_3 img').attr('src', RotatorIMGS[2]);
				}
		});

});
</script> 
