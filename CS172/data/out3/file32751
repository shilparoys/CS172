     <h1>
      <!-- InstanceBeginEditable name="Page Title" -->History of Stanford
      <!-- InstanceEndEditable --></h1> 
     <div class="row"> 
      <div id="sidebar-first" class="col-md-3" role="navigation">
       <!-- InstanceBeginEditable name="sidebar-first" --> 
       <h2 class="hidden">CONTENTS</h2> 
       <ul class="nav" role="navigation"> 
