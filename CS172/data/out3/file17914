          <script type="text/javascript">
    function gfunBuildPopOverHTML() {
        var objPopOver = gfunGetObject("MYC-LoginPopover");
        if (!objPopOver) {
            // Add the Pop-over HTML to the document
            objPopOver = document.createElement("div");
            objPopOver.id = "MYC-LoginPopover";
            document.body.appendChild(objPopOver);
            gfunSetClass(objPopOver, "MYC-SecurePopover");
            objPopOver.innerHTML = '<div id="MYC-LoginPopover-Dimmer" class="MYC-SecurePopover-Dimmer"><div id="MYC-LoginPopover-Box" class="MYC-SecurePopover-Box"><div class="MYC-SecurePopover-SecureHeader"><span class="MYC-SecurePopover-Close"><a href="#" onclick="return funMYCCloseSecurePopover(event);"><img src="http://buffalonews.mycapture.com/global/images/icon-popover-close.png" alt="Close" height="25" width="25" /></a></span><h3 id="MYC-SecurePopover-Title">Secure Login</h3><div class="MYCclear"></div></div><div id="MYC-SecurePopover-Container" class="MYC-SecurePopover-iFrameContainer"><iframe id="MYC-SecurePopover-Frame" width="680" height="500" frameborder="0" src=""></iframe></div></div></div>';
        }
    }

    function funMYCShowLogin(evt) {
        gfunBuildPopOverHTML();
        gfunGetObject("MYC-SecurePopover-Frame").src = "https://secure.secondstreetmedia.com/secureLogin.asp?affiliate=BFLO";
        gfunSetClass("MYC-SecurePopover-Container", "MYC-SecurePopover-iFrameContainer MYC-PopoverSmall");
        gfunUpdateHeight("MYC-SecurePopover-Frame", 350);
        gfunDisplay("MYC-LoginPopover");
        funMYCSetPopoverMargin();
        gfunSetFullSizeOfDocument("MYC-LoginPopover-Dimmer");
        return gfunCancelEvent(evt);
    }

    function funMYCShowResetPass(evt, sCpc) {
        gfunBuildPopOverHTML();
        gfunGetObject("MYC-SecurePopover-Frame").src = "https://secure.secondstreetmedia.com/secureLogin.asp?affiliate=BFLO&cpc=" + sCpc;
        gfunSetClass("MYC-SecurePopover-Container", "MYC-SecurePopover-iFrameContainer MYC-PopoverSmall");
        gfunUpdateHeight("MYC-SecurePopover-Frame", 350);
        gfunDisplay("MYC-LoginPopover");
        funMYCSetPopoverMargin();
        gfunSetFullSizeOfDocument("MYC-LoginPopover-Dimmer");
        return gfunCancelEvent(evt);
    }

    function funMYCShowRegistration(evt) {
        gfunBuildPopOverHTML();
        gfunGetObject("MYC-SecurePopover-Frame").src = "https://secure.secondstreetmedia.com/secureRegistration.asp?affiliate=BFLO";
        gfunSetClass("MYC-SecurePopover-Container", "MYC-SecurePopover-iFrameContainer");
        gfunUpdateHeight("MYC-SecurePopover-Frame", 500);
        gfunDisplay("MYC-LoginPopover");
        funMYCSetPopoverMargin();
        gfunSetFullSizeOfDocument("MYC-LoginPopover-Dimmer");
        return gfunCancelEvent(evt);
    }

    function funMYCShowCheckout(cartid, evt) {
        gfunBuildPopOverHTML();
        gfunGetObject("MYC-SecurePopover-Frame").src = "https://secure.secondstreetmedia.com/secureAddressInfo.asp?affiliate=BFLO&cartid=" + cartid;
        gfunSetClass("MYC-SecurePopover-Container", "MYC-SecurePopover-iFrameContainer");
        gfunUpdateHeight("MYC-SecurePopover-Frame", 500);
        gfunDisplay("MYC-LoginPopover");
        funMYCSetPopoverMargin();
        gfunSetFullSizeOfDocument("MYC-LoginPopover-Dimmer");
        return gfunCancelEvent(evt);
    }

    function funMYCCloseSecurePopover(evt) {
        gfunCollapse("MYC-LoginPopover");
        gfunGetObject("MYC-SecurePopover-Frame").src = "";
        return gfunCancelEvent(evt);
    }

    function funSSMLoginComplete(bIsLoggedIn, bIsFullyRegistered) {
        gfunBuildPopOverHTML();
        funMYCCloseSecurePopover();
        if (bIsLoggedIn) {
            if (!bIsFullyRegistered) {
                gfunGetObject("MYC-SecurePopover-Frame").src = "https://secure.secondstreetmedia.com/secureRegistration.asp?affiliate=BFLO";
                gfunDisplay("MYC-LoginPopover");
            }
            else {
        window.location.href = window.location.href;
            }
        }
        else {
            alert("We're sorry, but there was an error logging in. Please try again."); 
        }
    }

    function funMYCReceiveFromSecure(sData) {
        if (sData.substring(0, 7) == "#title=") {
            gfunGetObject("MYC-SecurePopover-Title").innerHTML = sData.substring(7, sData.length);
        }
    }

    function funMYCSetPopoverMargin() {
        var obj = gfunGetObject("MYC-LoginPopover-Box");
        //var style = (window.getComputedStyle) ? window.getComputedStyle(obj, null) : null;
        var obj = gfunGetObjectStyle(obj);
        if (obj) obj.marginTop = (gfunGetScrollValueY() + 100) + "px";
    }

    /********************************/
    /*      LIBRARY ITEMS           */
    /********************************/
    // Returns a reference to an object
    function gfunGetObject(sObj) {
        var obj = sObj;
        if (typeof sObj == "string") {
            if (document.getElementById) obj = document.getElementById(sObj);
            else if (document.all) obj = document.all[sObj];
            else if (document.layers) obj = document.layers[sObj];
            else {
                try {
                    obj = eval("document." + sObj);
                }
                catch (e) {
                    obj == null;
                }
            }
        }

        return obj;
    }
    
    // Returns a reference to an object's style
    function gfunGetObjectStyle(sObj) {
        var obj = gfunGetObject(sObj);
        if (obj != null) {
            if (obj.style != null) return obj.style;
            else if (obj.css != null) return obj.css;
            else return obj;
        }
        return null;
    }

    function gfunCancelEvent(evt) {
        evt = (evt) ? evt : ((window.event) ? window.event : null);
        if (evt) {
            if (evt.cancelBubble != null) evt.cancelBubble = true;
            if (evt.stopPropagation != null) evt.stopPropagation();
        }
        // Return False to stop events
        return false;
    }
    
    // Display an inline element
    function gfunDisplay(sObj) {
        var obj = gfunGetObjectStyle(sObj);
        if (obj != null) obj.display = "block";
    }

    // Hide an inline element
    function gfunCollapse(sObj) {
        var obj = gfunGetObjectStyle(sObj);
        if (obj != null) obj.display = "none";
    }

    // Update the Class of an element
    function gfunSetClass(sObj, sClass) {
        var obj = gfunGetObject(sObj);
        if (obj != null) {
            if (obj.setAttribute != null) obj.setAttribute("class", sClass);
            if (obj.className != null) obj.className = sClass;
        }
    }

    // Update the Height of an element
    function gfunUpdateHeight(sObj, height) {
        var obj = gfunGetObject(sObj);
        if (obj != null) {
            if (obj.setAttribute != null) obj.setAttribute("height", height);
            if (obj.height != null) obj.height = height;
            if (obj.style) {
                if (obj.style.pixelHeight != null) obj.style.pixelHeight = height;
                if (obj.style.height != null) obj.style.height = height + "px";
            }
        }
    }
    
    // Update the Width of an element
    function gfunUpdateWidth(sObj, width) {
        var obj = gfunGetObject(sObj);
        if (obj != null) {
            if (obj.setAttribute != null) obj.setAttribute("width", width);
            if (obj.width != null) obj.width = width;
            if (obj.style) {
                if (obj.style.pixelWidth != null) obj.style.pixelWidth = width;
                if (obj.style.width != null)  obj.style.width = width + "px";
            }
        }
    }

    // Get the Height of an element
    function gfunGetHeight(sObj) {
        var obj = gfunGetObject(sObj);
        var height = 0;
        if (obj != null) {
            if (obj.offsetHeight != null) height = obj.offsetHeight;
            if (parseInt(height) == 0 && obj.clip && obj.clip.height) height = obj.clip.height;
            if (parseInt(height) == 0 && obj.currentStyle != null && obj.currentStyle.pixelHeight != null) height = obj.currentStyle.pixelHeight;
            if (parseInt(height) == 0 && obj.style != null && obj.style.height != null) height = obj.style.height;
            if (parseInt(height) == 0 && obj.style != null && obj.style.pixelHeight != null) height = obj.style.pixelHeight; 
            if (parseInt(height) == 0 && obj.height != null) height = obj.height;
        }
        return parseInt(height);
    }

    // Get the Width of an element
    function gfunGetWidth(sObj) {
        var obj = gfunGetObject(sObj);
        var width = 0;
        if (obj != null) {
            if (obj.offsetWidth != null) width = obj.offsetWidth;
            if (parseInt(width) == 0 && obj.clip && obj.clip.width) width = obj.clip.width;
            if (parseInt(width) == 0 && obj.currentStyle != null && obj.currentStyle.pixelWidth != null) width = obj.currentStyle.pixelWidth;
            if (parseInt(width) == 0 && obj.style != null && obj.style.width != null) width = obj.style.width;
            if (parseInt(width) == 0 && obj.style != null && obj.style.pixelWidth != null) width = obj.style.pixelWidth; 
            if (parseInt(width) == 0 && obj.width != null) width = obj.width;
        }
        return parseInt(width);
    }

    // Set the Size of an element to the full size of the document
    function gfunSetFullSizeOfDocument(sObj) {
        var obj = gfunGetObject(sObj);
        if (obj != null) {
            var nWidth = gfunGetWidth(document.body);
            var nHeight = gfunGetHeight(document.body);
            if (nWidth > gfunGetWidth(obj)) gfunUpdateWidth(obj, nWidth);
            if (nHeight > gfunGetHeight(obj)) gfunUpdateHeight(obj, nHeight);
        }
    }

    // Returns the height in pixels that a page is scrolled down
    function gfunGetScrollValueY() {
        if (document.documentElement) if (document.documentElement.scrollTop) return document.documentElement.scrollTop;
        if (document.body) return document.body.scrollTop;
        else return null;
    }

</script> 
