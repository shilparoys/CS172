		function loadBackground()
		{			
			var imgBGObject = new Array();
			var preloadIndex = 0;
			imgBGObject[imgBGObject.length] = '/webadmin/images/crabapple_1.jpg';
					imgBGObject[imgBGObject.length] = '/webadmin/images/clocktower_1.jpg';
					imgBGObject[imgBGObject.length] = '/webadmin/images/science-background.jpg';
					
			
			if(imgBGObject.length > 0) {				
				var randomIndex = Math.floor(Math.random() * (imgBGObject.length));	
				$.backstretch(imgBGObject[randomIndex], {centeredY: false});
			}			
		}
		if(typeof randomBackgroundDisabled === 'undefined' || randomBackgroundDisabled != true){
			loadBackground();
 		}
