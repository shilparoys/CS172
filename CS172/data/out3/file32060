       <script type="text/javascript">
	jQuery(function() {
		var methodPath = '/ADF/apps/wunderground/webservices/Main.cfc?subsiteURL=/sunypotsdam/&method=GetCurrentConditions';
		
		$.getJSON(methodPath, function(data) {
			if(data.STATUS == "SUCCESS") {
				$("#wundergroundConditions .location").text(data.LOCATION);
				$("#wundergroundConditions .temperature").html(data.TEMP + "Â°");
				
				$("<img/>").attr("src", '/ADF/apps/wunderground/images/' + data.ICONPATH + '/' + data.ICON + ".png")
						   .attr("alt",data.ICON)
						   .addClass("icon")
						   .appendTo("#wundergroundConditions");
						   
				$("#wundergroundConditions").fadeIn();
			}
		});
	});
</script> 
