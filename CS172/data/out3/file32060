			function setBackground(index) {			
				$.backstretch(imgBGObject[index], {speed: 400, centeredY: false});					
			}
			
			function preloadImages() {
				for(index = 0; index < imgBGObject.length; index++) {
					new Image().src = imgBGObject[index];
				}
			}
			
			setTimeout(preloadImages, 1000);
			
			var $slider = jQuery('#home_blurb_366582 .slideContainer').bxSlider({
				  infiniteLoop: true,
				  speed: 3000,
				  controls: false,
				  pagerSelector: '#home_blurb_366582 .pagerContainer',
				  onSlideBefore: function($slideElement, oldIndex, newIndex){
					setBackground(newIndex);
					setTimeout(function(){$(".backstretch").first().remove();},500);					
				  },
				  auto: true,
				  pause: 20000
			});
			
			setBackground(0);
			
