  <script>

/**************************************************************************\
	Display Org Info
\**************************************************************************/
$('.org').click(function(e){
	var link = $(this).attr('data-link') + ".ajax";
	e.preventDefault();
	var info = $('#org-' + $(this).attr('data-pk'));

	// close current window, or open a new (and close all others)
	if(info.hasClass('open')){ info.removeClass('open'); return; }
	$('.open').removeClass('open'); info.addClass('open');

	// load org info
	if(info.hasClass('loaded')) return;
	info.addClass('loading');
	$.ajax({
		url: link,
		dataType: 'html',
		success: function(data){
			info.html(data);
			info.removeClass('loading');
			info.addClass('loaded');
		}
	});
	return false;
});

/**************************************************************************\
	onload
\**************************************************************************/

$(document).ready(function(){
	//scroll to org
	var org = $('#org-' + 'null');
	if(org.length){
		var scroll = $(org).offset().top - 26;
		$('html,body').animate({scrollTop: scroll}, 'slow');
	}

	new CampusMap({
		urls : {
			static   : "/static/",
			location : "/locations/%s/.json"
		},
		simple              : true,
		canvas_id           : 'canvas',
		pan_control         : false,
		zoom_control        : false,
		street_view_control : false,
		map_type_control    : false,
		infobox_location_id : "18",
		illustrated         : true
	})
});

</script>   
