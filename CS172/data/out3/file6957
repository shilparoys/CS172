   <script type="text/javascript">
    $(document).ready(function () {
        //        var maxHeight = Math.max.apply(null, $("div.FooterSitemapCol").map(function () {
        //            return $(this).height();
        //        }).get());
        //        $("div.FooterSitemapCol").css("height", maxHeight + "px");

        var nb_index_map = $("#nb_index_map").val();
        for (var i = 1; i <= nb_index_map; i++) {           
            var maxHeight = Math.max.apply(null, $("div.FooterSitemapCol.fmap" + i).map(function () {
                return $(this).height();
            }).get());            
            $("div.FooterSitemapCol.fmap" + i).css("height", maxHeight + "px");
        }
    });
</script> 
