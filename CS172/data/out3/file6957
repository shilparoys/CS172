         $(".HsliderTabs a").css("width", ItemW);

     });

     function BeforeHome(currSlideElement, nextSlideElement, options, forwardFlag) {

         var fullW = $(".HsliderTabs").outerWidth(true) - 158;
         if ($.browser.chrome) {
             var fullW = $(".HsliderTabs").outerWidth(true) - 157;
         }
         if ($.browser.msie && parseInt($.browser.version, 10) === 7) {
             var fullW = $(".HsliderTabs").outerWidth(true) - 155;
         }
         if ($.browser.msie && parseInt($.browser.version, 10) === 8) {
             var fullW = $(".HsliderTabs").outerWidth(true) - 155;
         }
         if ($(window).width() < 640) {
             var fullW = $(".HsliderTabs").outerWidth(true) - 160;
         }
         var nbrOItems = $(".HsliderTabs a").length - 1;
         var ItemW = (fullW / nbrOItems) - 1;
         $(".HsliderTabs a").css("width", ItemW);
         //console.log(currSlideElement)
         $(currSlideElement).find(".rightMoving").animate({ "right": "-267px", "opacity": "0" }, 700);
         $(currSlideElement).find(".leftMoving").animate({ "left": "-235px", "opacity": "0" }, 700);
     }
     function afterHome(currSlideElement, nextSlideElement, options, forwardFlag) {
         //console.log(currSlideElement)
         $(this).find(".rightMoving").animate({ "right": "0", "opacity": "1" }, 700);
         $(this).find(".leftMoving").animate({ "left": "0", "opacity": "1" }, 700);
         var $ht = $(this).height() + 10;

         //set the container's height to that of the current slide
         $(this).parent().animate({ height: $ht });
         $(this).parents(".homeSlider").animate({ height: $ht });
     }
 </script> 
