             //                 after: afterHome
             //             });
             //         }

             var fullW = $(".HsliderTabs").outerWidth(true) - 158;
             if ($.browser.chrome) {
                 var fullW = $(".HsliderTabs").outerWidth(true) - 157;
             }
             if ($.browser.msie && parseInt($.browser.version, 10) === 7) {
                 var fullW = $(".HsliderTabs").outerWidth(true) - 155;
             }
             if ($.browser.msie && parseInt($.browser.version, 10) === 8) {
                 var fullW = $(".HsliderTabs").outerWidth(true) - 155;
             }

