     $(window).load(function () {

         $(".homeImages").cycle({
             timeout: 5000,
             speed: 1000,
             width: '100%',
             slideResize: true,
             containerResize: true,
             fit: 1,
             pager: '.HsliderTabs',
             before: BeforeHome,
             after: afterHome
         });

