        <script>
$(function() {
    $('#summon-search-field').blur(
        function() {
            if ($(this).val() == '') {
                $(this).val('Buscar artículos, libros y mucho más');
            }
        }                   
    );      

    $('#summon-search-field').focus(
        function() {
            if ($(this).val() == 'Buscar artículos, libros y mucho más') {
                $(this).val('');
            }
        }                   
    );      

    $('#summon-submit').click(
        function() {
            $('#search-box form').submit();
        }
    );
});
</script> 
