       <!-- **************************** START PAGE TITLE ************************************ --> 
       <h1>National Veterans Summer Sports Clinic</h1> 
       <!-- **************************** END PAGE TITLE ************************************** --> 
      </div> 
      <!--End main content wrapper --> 
      <div id="tier2gallery">
       <!-- begin apple images gallery--> 
       <div id="gallery">
        <!-- begin apple images gallery--> 
        <img src="http://www.va.gov/opa/speceven/ssc/slider/images/image-slider-1.jpg" width="740" height="370" alt="Image of Veteran at the Summer Sports Clinic"> 
       </div>
       <!--close gallery--> 
