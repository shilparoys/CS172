  <script type="text/javascript">

        $(document).ready(function () {
            var options = { replacementTargetSelector: "#pjaxReplaceTarget" };

            $.ajax({
                type: "GET",
                url: "http://www.dell.com/support/incidents-online/us/en/04/supportrequest",
                header: 'Access-Control-Allow-Credentials: true',
                data: null,
                contentType: "text/html",
                dataType: "html",
                crossDomain: 'true',
                success: function (response) {

                    $('#SH_placeHolder').html(response);
                },
                error: function (err) {
                    var errDiv = '<div class="hidden"><p>Exception :<br/>' + err.Message + '</p></div>';
                    $('#SH_placeHolder').html('<div class="col-xs-12 col-md-6 col-sm-6"><p>' + '' + '</p></div>');
                    $('#SH_placeHolder').append(errDiv);
                }

            });
            Dell.MySupport.Modules.Pjax.bindPjax(options);
            var invalidServiceTag = 'False';
            if (invalidServiceTag == 'True') {
                if ($("#showCaptchaMessage").length == 0) {
                    $("#entryError").removeClass("hidden");
                }
            }
        });


        if (!Dell.MySupport.Selection) {
            Dell.MySupport.Selection = {
                isTag: "True",
                productCode: "",
                productName: "",
                selection: ""
            };
        }
        // Page scoped Drivers Change Product identifier.
        DCP = "";


        var stickyNavBarIsvisible = function (index, value) {
            var el = document.getElementById(value);
            if (el != null) {
                var top = el.getBoundingClientRect().top, rect, el = el.parentNode;
                do {
                    rect = el.getBoundingClientRect();
                    if (top <= rect.bottom === false)
                        return false;
                    el = el.parentNode;
                } while (el != document.body);
                var producselRect = document.getElementById("product-support").getBoundingClientRect();
                var topsolRect = document.getElementById("common-issues").getBoundingClientRect();
                var cusSupRect = document.getElementById("customer-support").getBoundingClientRect();
                var isSelectPSStickyNavBar = false;
                if ($("#collapse-1").hasClass("collapse in")) {
                    var pjaxRect = document.getElementById("pjaxReplaceTarget").getBoundingClientRect();

                    if ((pjaxRect.top <= 200 && pjaxRect.top >= -150) ||
                        (pjaxRect.bottom < pjaxRect.height && pjaxRect.bottom > 0)) {
                        isSelectPSStickyNavBar = true;
                    }                  
                }
                if (isSelectPSStickyNavBar ||
                    (producselRect.bottom < producselRect.height && producselRect.bottom > 0) ||
                    (topsolRect.bottom < topsolRect.height && topsolRect.bottom > 0)) {
                    top = 100;
                    index = (topsolRect.bottom < topsolRect.height && topsolRect.bottom > 0) ? (cusSupRect.top <= 150 && cusSupRect.top >= -150) ? 2 : 1 : 0;
                 
                    //This two lines added for when tiles tile is more than 1 line
                    if (!isSelectPSStickyNavBar)
                        index = (topsolRect.top <= 260 && topsolRect.top >= -260) ? 1 : 2;
                    index = (producselRect.bottom > 15) ? 0 : index;
                }
                // Check its within the document viewport
                var bottomHeight = $("#merchandizing-banner").children().length > 0 ?390 : 275;
                if ((top <= 200 && top >= -150) || (el.getBoundingClientRect().bottom - document.documentElement.clientHeight) <= bottomHeight) {
                    index = (el.getBoundingClientRect().bottom - document.documentElement.clientHeight) <= bottomHeight ? 2 : index;
                    index = index > 2 ? 0 : index;
                    $("#support-sub-nav").find("li:eq(" + (index) + ")").css("background-color", "#CCE7F3");
                    return false;
                }
            }
        };

        function fireIfElementVisible(el, callback) {
            if ($("#support-sub-nav").is(":visible")) {
                return function () {
                    $("#support-sub-nav").find("li").each(function () { $(this).css("background-color", "white"); });
                    $.each("product-support,common-issues,customer-support,product-suggestions-block,collapse-1,pjaxReplaceTarget".split(','), function (index, value) {
                        return stickyNavBarIsvisible(index, value);
                    });
                }
            }
        }
        var handler = fireIfElementVisible(stickyNavBarIsvisible);
        $(window).on('DOMContentLoaded load resize scroll', handler);
    </script> 
