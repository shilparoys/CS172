    <script type="text/javascript">

    $(document).ready(function () {
        var contactsliderrect = document.getElementById("contactslidertitle").getBoundingClientRect();
        var contactusdetailsrect = document.getElementById("ContactUsDetails").getBoundingClientRect();
        var styles = "top:" + (Math.floor(contactusdetailsrect.top + (contactusdetailsrect.top - contactsliderrect.top)) - 45) + "px";
        $("#contactslidertitle").attr("style", styles);
        //console.log(contactsliderrect.right + "==" + contactsliderrect.left);
    });
    function toggleContactUs() {

        if ($(".contact-us").hasClass('open')) {
            $(".contact-us").animate({
                "right": "-245px"
            }, 200);
            $(".contact-us").removeClass('open');

        }
        else {
            $(".contact-us").animate({
                "right": "0"
            }, 200);
            $(".contact-us").addClass('open');
        }
        //TODO should we have a delayed automatic close? or if you start interacting with the masthead should this collapse?
    }
    function LoadContactsDetails() {
        var params = new Object();
        params.country = 'us';
        params.lang = 'en';
        params.seg = '04';

        if (!$(".contact-us").hasClass('open')) {
            $.ajax({
                url: '/support/home/us/en/04/Home/ContactUsDetails',
                type: "GET",
                cache: true,
                data: params,
                success: function (data) {
                    toggleContactUs();
                    $('#ContactUsDetails').html(data);

                },
                error: function (jqXHR, textStatus, errorThrown) {
                    var err = eval("(" + jqXHR.responseText + ")");

                }
            });
        }
        else {
            toggleContactUs();
        }

    }
</script> 
