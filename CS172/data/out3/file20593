       <script type="text/javascript">
    var recentProducts = [];
    var MPSProducts = [];
    var isUserAuthenticated = 'False';
    var profileId = '';
    var regionName = 'US';
    $.recentViewedProducts.configPsAngularSection();
    var inputText = 'ABCD123 or 1234567890';

    function ServicetagPopover() {
        $('#servicetagpopover').popover('show');
        $('.popover').addClass("support-dashboard");
    }
    // to close popovers when clicking anywhere
    $('body').on('click', function (e) {
        $('[data-toggle="tagPopover"]').each(function () {
            if (!$(this).is(e.target) && $(this).has(e.target).length === 0 && $('.popover').has(e.target).length === 0) {
                $(this).popover('hide');
                $(this).removeClass("support-dashboard");
            }

        });
    });
    $(document).ready(function () {
        $('#servicetagpopover').popover({
            html: true,
            content: '<i class=" close icon-ui-close pull-right" data-dismiss="popover" onclick="$(&quot;#servicetagpopover&quot;).popover(&quot;hide&quot;);"></i><h4>Where is my Service Tag or Express Service Code? </h4><p>Both product IDs are located on the back or bottom of your device.</p><a href="http://www.dell.com/support/contents/category/Product-Support/Self-support-Knowledgebase/locate-service-tag">See details</a>'
        });
        $("#collapse-1").on('show.bs.collapse', function () {
            $("#browseProductBorderHighlighter").addClass("open");
        });
        $("#collapse-1").on('hide.bs.collapse', function () {
            $("#browseProductBorderHighlighter").removeClass("open");
        });
        //To remove example text only for tablet
        if (screen.width <= 1024 && screen.width >= 768) {
            $('#inputServiceTag').removeAttr('placeholder');
        }

        if (window.innerWidth <= 1024 && window.innerWidth >= 768) {
            $('#inputServiceTag').removeAttr('placeholder');
        }

        // Hiding  invalid service tag error message if user comes from cold product page
        var isservicetagcall = GetQueryStringParams()["IsTag"];
        if (isservicetagcall == "False") {
            // $("#entryError").addClass("hidden");
            $('#entryError').css("display", "none");

        }


        Dell.MySupport.Modules.ProductSelectorRuntime.bindToForm('/support/home/us/en/04/product-support/servicetag', '/support/home/us/en/04/ProductSelector/Home');

        if (Dell.MySupport.Selection != null) {
            if (Dell.MySupport.Selection.isTag == 'True') {
                $('#inputServiceTag').val(Dell.MySupport.Selection.selection)
                if ($('#inputServiceTag').val().length) {
                    $('#btnSubmit').removeClass('disabled');
                }
                $('#hidSubmitValue').val('true');
            }
        }

        if ($("#btnSubmitCaptcha").length > 0) {

            $('#btnSubmit').addClass('disabled');
            var servicetagvalue = $("#inputServiceTag").val();
            if (servicetagvalue.length == 0) {
                $('#btnSubmitCaptcha').addClass('disabled');
            }
        }
        if (screen.width <= 767) {
            $('.LBD_CaptchaDiv').removeAttr("width").removeAttr("height").css({ width: "", height: "" });
            $('.LBD_CaptchaImageDiv').removeAttr("width").removeAttr("height").css({ width: "", height: "" });
            $('.LBD_CaptchaIconsDiv').removeAttr("width").removeAttr("height").css({ width: "", height: "" });
            $('#captchaContainer').removeClass("pull-left");
        }

    });
    function openServiceTagModal() {
        $('#service_tag_location_modal').modal('show');
    }

    // Hiding invalid service tag error message on home page ,if user comes from  invalid product code


    function GetQueryStringParams() {
        var vars = [], hash;
        var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
        for (var i = 0; i < hashes.length; i++) {
            hash = hashes[i].split('=');
            vars.push(hash[0]);
            vars[hash[0]] = hash[1];
        }
        return vars;
    }

    // hiding dell account text if five cookie


    if ($("#fivecookiemyproductlistlink").length) {

        $("#HaveDellAccountText").hide();

    }

</script> 
