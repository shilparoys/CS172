  <script type="text/javascript">
        $(function () {
            if(typeof iPerceptions !== 'undefined')
            {
                if (iPerceptions) {
                    var randomNumber = Math.floor((Math.random() * 100));
                    if (randomNumber < iPerceptions.invitationRate) {
                        $.getScript(iPerceptions.surveyJsPath);
                    }
                }
            }
        });        
