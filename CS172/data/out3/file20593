       <script>
        $('body').scrollspy({ target: '.scroll-spy-nav' });

        $('.secondary-nav').affix({
            offset: {
                top: function () {
                    return $('.main-nav').offset().top + 87;
                }
            }
        });

        $(".secondary-nav a[href^='#']").on('click', {}, pageScroller);
        $(".main-nav a[href^='#']").on('click', {}, pageScroller);

        function pageScroller(e) {
            // prevent default anchor click behavior
            e.preventDefault();

            // store hash
            var hash = this.hash;

            // animate
            $('html, body').animate({
                scrollTop: $(this.hash).offset().top - ($('.secondary-nav').outerHeight() + 15)
            }, 300, function () {
            });
        }

        $('[scroll-spy="scroll"]').each(function () {
            var $spy = $(this).scrollspy('refresh')
        })
       
        $('#biganchor_ps').click(function () {
            var metricval = { 'appcode': '900.100.009', 'tabname': 'product_support' };
            $('#biganchor_ps').data("metrics", metricval);
        });
        $('#biganchor_ts').click(function () {
            var metricval = { 'appcode': '900.200.001', 'tabname': 'top_resources' };
            $('#biganchor_ts').data("metrics", metricval);
        });
        $('#biganchor_os').click(function () {
            var metricval = { 'appcode': '900.300.001', 'tabname': 'orders_support_request' };
