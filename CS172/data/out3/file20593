             <div> 
              <h3 id="OS_Header">Order support</h3> 
              <p id="OS_Detail">View your order history or get information about the ordering and delivery process.</p> 
              <a href="http://www.dell.com/support/contents/category/eSupport-Order-support" id="OS_ViewAll_SOCTA">View all order support</a> 
              <h5 class="top-offset-30"><strong id="OS_OStatus">Check order status</strong></h5> 
              <div class="row"> 
               <div class="col-xs-12"> 
                <label class="form-label" for="service-tag" id="OS_OLabel">Order number:</label> 
               </div> 
              </div> 
              <div class="row"> 
               <div class="col-md-6 col-xs-12 md-no-pad-right"> 
                <input type="text" class="form-control bottom-offset-10 " id="ordernumbertxtbox" placeholder="123456789"> 
               </div> 
               <div class="col-md-6 col-xs-12"> 
                <a class="btn btn-primary" id="ordersupportsubmitbutton">Submit</a> 
               </div> 
              </div> 
             </div> 
            </div> 
            <script type="text/javascript">

    $("#ordernumbertxtbox").keypress(function (e) {
        if (e.keyCode == 32) {
            return false;
        } else if (e.which == 32) {

            return false;
        }
    });

    $("#ordernumbertxtbox").on('paste', function (e) {
        e.preventDefault();
        var pastedVal = "";
        var userAgent = navigator.userAgent.toString().toLowerCase();
        if ((userAgent.indexOf('msie') != -1) || !!navigator.userAgent.match(/Trident.*rv\:11\./)) {
            pastedVal = window.clipboardData.getData("Text");
        }
        else {
            pastedVal = e.originalEvent.clipboardData.getData('Text');
        }

        var trimmed = myTrim(pastedVal);
        $("#ordernumbertxtbox").val(trimmed);

    });


    EnableandDisablesubmitbutton();
    $("#ordersupportsubmitbutton").click(function () {
        
        // check Text box has some value or else stop submitting
        if ($("#ordernumbertxtbox").val() != null && $("#ordernumbertxtbox").val() != '') {           
            var ordernumber = $("#ordernumbertxtbox").val();
            var url = 'https://www.dell.com/support/orderstatus?orderNum=' + ordernumber;
            window.location = url;
        }
            return false;
    });

    $("#ordernumbertxtbox").bind('keyup', function () {
        
       
       EnableandDisablesubmitbutton();
    });

    $("#ordernumbertxtbox").bind('blur', function () {
        EnableandDisablesubmitbutton();
    });
   


    function EnableandDisablesubmitbutton() {
      
        if ($("#ordernumbertxtbox").val() != null && $("#ordernumbertxtbox").val() != '' && $("#ordernumbertxtbox").val() != "123456789") {
           
            $('#ordersupportsubmitbutton').removeClass('disabled');
        }
        else {
            $('#ordersupportsubmitbutton').addClass('disabled');
           
        }


    }

</script> 
