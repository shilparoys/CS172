                 <script type="text/javascript">

    $(function () {

        if ('true' === 'false') {
            if (''.toLowerCase().length > 0) {

                // $('#accordion1 .accordion-toggle').click();
                $("#browseProductBorderHighlighter").addClass("open");
                $("#accordion1 .accordion-toggle").removeClass("collapsed");
                $('#collapse-1').collapse('show');          
            }
        }
        var canShowAjaxAlert = 'false'
        var canHidePsHomeAlert = 'false'

        if (canHidePsHomeAlert == 'true') {
            $('#ps500').addClass('hidden');
        }
        else {
            if (closep500Alert == 'true') {
                $('#ps500').addClass('hidden');
            }
            else {
                $('#ps500').removeClass('hidden');
            }
        }

        if (canShowAjaxAlert == 'true') {
            $('#ps500in').removeClass('hidden');
            $('#pssitewidein').addClass('hidden');
        } else {
            $('#ps500in').addClass('hidden');
            $('#pssitewidein').removeClass('hidden');

        }
        // added to remove the ps 500 when clicks on browse product/ catalog details
        if (canHidePsHomeAlert != 'true' && closep500Alert == 'true' && bCloseAllAlerts == 'true') {
            $("#homePageAlerts").addClass("hidden");
        }
        else if (canHidePsHomeAlert == 'true' && bCloseAllAlerts == 'true' && closep500Alert == 'false') {
            $("#homePageAlerts").addClass("hidden");
        }

    });

</script> 
