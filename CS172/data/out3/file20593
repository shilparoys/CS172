        (function (Dell) {
            // dont delete this onerror hook, it may cause improper UI product entry section when JS error throws before executes angular JS part
            window.onerror = function () {
                try {
                    $.recentViewedProducts.configPsAngularSection();
                } catch (e) {
                    console.log("Global JS Error =", e)
                    console.log("Global JS Error Message =", e.message)
                }
            };
            Dell.Global = Dell.Global || {};
            Dell.MySupport = Dell.MySupport || {};
            Dell.MySupport.Modules = Dell.MySupport.Modules || {};
            Dell.Global.Context = {
                c: 'us',
                l: 'en',
                s: 'bsd',
                cs: '04'
            }
