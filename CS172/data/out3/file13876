   <script type="text/javascript">
	$('#q').attr('autocomplete', 'off');
	function verifyForm(textBox)
	{
		var strQuery = $('#q').val();
		if (strQuery != "Search/Busqueda" && strQuery.trim() != ""){
			return true;
		} else {
			alert("Please enter a search term.");
			return false;
		}
	}
	$('#q').autocomplete("/scripts/search-suggest.php");
	</script> 
