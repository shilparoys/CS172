      <div class="content"> 
       <h3 style="margin-bottom: 3px">I64A</h3> Boothbay, ME, USA 
       <br>
       <br> 
       <img style="width: 175px" class="photoCenter" src="http://anf.ucsd.edu/cacheimages/station_photos/I64A_top_pick_9934359756.jpg" alt="Station Photo"> 
       <img class="photoCenter" src="http://maps.google.com/maps/api/staticmap?center=43.9197,-69.6347&amp;zoom=4&amp;size=175x175&amp;maptype=roadmap&amp;markers=color:blue|43.9197,-69.6347&amp;sensor=false&amp;key=ABQIAAAAP28NUn0El9u6czmlS4XN1RRhiHKqT7kxkRg1cRlrAHTEBMYvwhQKsTVk872sFonjAOiVXly18iF0Sw" alt="Station Location"> 
      </div> 
     </div>
     <!-- end featuredStation --> 
     <div class="separator">
      <img src="/static/images/backgrounds/separator.gif" alt="">
     </div> 
    </div>
    <!-- end colLeft --> 
    <div id="colRight"> 
     <!--[if IE 6]>
      <div id="ie6warning">
