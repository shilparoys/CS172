         <style>
		#log22 {
			overflow: auto;
			min-height:188px;
		}
		 
		#log22.ajax-loading {
			/*margin-top:50px;*/
			background: url(images/loadingx3.gif) no-repeat center;
			min-height:188px;
		}
		</style> 
         <div id="log22">
          &nbsp;
         </div> 
         <script type="text/javascript" language="javascript">
		var log = $('log22').empty().addClass('ajax-loading');
		function loading() {
			log = $('log22').empty().addClass('ajax-loading');
			setTimeout("loader()",1400);
		}
		
		function loader() {
			/*var url = "/modules/mod_randomimage/getmainidea.php?lang=ms";
			new Ajax(url, {
				method: 'get',
				onComplete: function() {
					log.removeClass('ajax-loading');
					setTimeout("loading()",10000 )
				},
				onCancel: function() {
					log.removeClass('ajax-loading'); 
				},
				update: $('log')
			}).request();
			*/
			
			jQuery.ajax({
				url: "modules/mod_randomimage/getmainidea.php?lang=ms",
				type:"GET"
			}).done(function(msg) {
				
				$('log22').empty().removeClass('ajax-loading');
				jQuery('#log22').html(msg);
				setTimeout("loading()",10000 )
			});
		}
		function starthere() {
			loading();
		}
		var k = starthere();
		</script> 
