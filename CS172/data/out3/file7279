  <script type="text/javascript">

function displayResult1(radio1choice)
 {
 document.getElementById("pcccClasses").value=radio1choice;
 }
 
 function displayResult2(radio2choice)
 {
 document.getElementById("helpful").value=radio2choice;
 }

function checkall(formname,checkname,thestate){
var el_collection=eval("document.forms."+formname+"."+checkname)
for (c=0;c<el_collection.length;c++)
el_collection[c].checked=thestate
}

function clearallcomments(){
document.getElementById("limitedtextarea").value="";
document.getElementById("charleft").innerHTML=500;

checkall('commentsSubmit','radio1',false);
checkall('commentsSubmit','radio2',false);
document.getElementById("pcccClasses").value="";
document.getElementById("helpful").value="";
}

function hideallcomments(){

var xmlhttp;
inputstr="";
radpccc="";
radhelp="";

inputstr=document.getElementById("limitedtextarea").value;
radpccc=document.getElementById("pcccClasses").value;
radhelp=document.getElementById("pcccClasses").value;

var str = inputstr.replace(/[`~!@#$%^&*()_|+\-=?;:'",<>\{\}\[\]\\\/]/gi, ''); 
 if (str=="" && radhelp=="" && radpccc=="")
   {
   alert("Nothing to Submit!");
   return;
   }

var url=("submitcomments.php?mycomment="+str +"&pcccClass="+radpccc+"&pagehelpful="+radhelp);
 if (window.XMLHttpRequest)
   {// code for IE7+, Firefox, Chrome, Opera, Safari
   xmlhttp=new XMLHttpRequest();
   }
 else
   {// code for IE6, IE5
   xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
   }
 xmlhttp.onreadystatechange=function()
   {
   if (xmlhttp.readyState==4 && xmlhttp.status==200)
     {
    document.getElementById('allcomments').style.display='none';
	document.getElementById('thankyou').style.display='block';
    document.getElementById("limitedtextarea").value="";
	 }
   }
 xmlhttp.open("GET",url,true);
 xmlhttp.send();
 }

function limitText(limitField, limitCount, limitNum) {
	if (limitField.value.length > limitNum) {
		limitField.value = limitField.value.substring(0, limitNum);
	} else {
		limitCount.value = limitNum - limitField.value.length;
	document.getElementById("charleft").innerHTML=limitNum - limitField.value.length;
	}
}

function NewWindow(mypage){
myWindow=window.open(mypage,'','resizable=yes,fullscreen=no,scrollbars=yes');
myWindow.focus();
}

function openWin(anslink)
{
myWindow=window.open(anslink,'','resizable=yes,fullscreen=yes,scrollbars=yes');
myWindow.focus();
}

function newPopup(url) {
popupWindow = window.open(url,'popUpWindow','height=300,width=500,left=10,top=10,resizable=yes,scrollbars=yes,toolbar=no,menubar=no,location=no,directories=no,status=yes')
popupWindow.focus();
}


</script> 
