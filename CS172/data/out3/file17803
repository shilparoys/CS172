				var tier1test = '';
				if (tier1test.length > 0) {
					ctxt_ad_source = 'npc_buffalonews_buffalonews_t1_ctxt';
					ctxt_ad_id = tier1test;
					ctxt_ad_type = tier1test;
				}
				//Context Parameters
				ctxt_ad_url = window.location.href;
				var pgsection = 'Latest_Local_News';
				ctxt_ad_section = pgsection;
				var keywords = '';
				if (keywords.length > 0) {
					ctxt_ad_keywords = keywords; // OPTIONAL: keywords (URL-safe vals only)
				}
				//OPTIONAL PARAMETERS
				ctxt_ad_oe = 'utf8'; //latin1 or utf8
			ctxt_ad_css = 'http://static.buffalo.com/vendors-2010/css/text-ads.css';
				var adcss = '';
				if (adcss.length > 0) {
					ctxt_ad_css = adcss;
				}
				ctxt_ad_newwin = 1; // new window for results
			//-->
			</script> 
        <div style="text-align: center;"> 
         <script type="text/JavaScript" src="http://cm.npc-buffalonews.overture.com/partner/js/ypn.js"></script> 
        </div> 
       </div> 
       <!-- textad --> 
      </div> 
