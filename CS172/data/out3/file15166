           <script>
function sendBusqueda(){
	var urlStr = "http://www.unav.edu/web/vida-universitaria/noticias?p_p_id=buscadornoticias_WAR_buscadoravanzadoportlet_INSTANCE_4bZ8&p_p_lifecycle=0&p_p_state=exclusive&p_p_col_id=column-3&p_p_col_pos=3&p_p_col_count=7&_buscadornoticias_WAR_buscadoravanzadoportlet_INSTANCE_4bZ8_jspPage=%2FWebContent%2FbuscadorNoticias.jsp";
	var namespace = "_buscadornoticias_WAR_buscadoravanzadoportlet_INSTANCE_4bZ8_";
	var palabras = document.getElementById("evento").value;
	//setCookie("palabras",palabras,0);
	//var donde2 = "11318";
	var donde2Div = document.getElementById("donde-2BusquedaAvanzada");
	var tipo = document.getElementById("tipoBusquedaNoticias").value;
	var desdeDia = document.getElementById(namespace + "desdeDiaBusquedaAvanzada").value;
	var desdeMes = document.getElementById(namespace + "desdeMesBusquedaAvanzada").value;
	var desdeAno = document.getElementById(namespace + "desdeAnoBusquedaAvanzada").value;
	var hastaDia = document.getElementById(namespace + "hastaDiaBusquedaAvanzada").value;
	var hastaMes = document.getElementById(namespace + "hastaMesBusquedaAvanzada").value;
	var hastaAno = document.getElementById(namespace + "hastaAnoBusquedaAvanzada").value;
	var categoryIdDiv = document.getElementById("categoryIdBusquedaAvanzada");
	AUI().use("liferay-portlet-url", function(a) {
		var portletURLJS = Liferay.PortletURL.createRenderURL();
		portletURLJS.setPortletId("buscadornoticias_WAR_buscadoravanzadoportlet_INSTANCE_VJt6");
		portletURLJS.setPlid("501754");
		portletURLJS.setWindowState("normal");
		portletURLJS.setPortletMode("view");
		portletURLJS.setParameter("resultados","true");
		portletURLJS.setParameter("jspPage","/WebContent/buscadorNoticias.jsp");
		portletURLJS.setParameter("palabras", palabras);
		portletURLJS.setParameter("desdeDia", desdeDia);
		portletURLJS.setParameter("desdeMes", desdeMes);
		portletURLJS.setParameter("desdeAno", desdeAno);
		portletURLJS.setParameter("hastaDia", hastaDia);
		portletURLJS.setParameter("hastaMes", hastaMes);
		portletURLJS.setParameter("hastaAno", hastaAno);
		if(categoryIdDiv!=null){
			var categoryId = categoryIdDiv.value;
			portletURLJS.setParameter("categoryId", categoryId);
		}
		if(donde2Div!=null){
			var donde2 = donde2Div.value;
			portletURLJS.setParameter("donde2",donde2);
		}
		portletURLJS.setParameter("tipo",tipo);
		window.location.href = portletURLJS;
	});	
}

</script> 
