            showVersionsHistoryDlg = function() {
                var dialog = Knowledge.Dialog.getInstance({
                    id: 'articleVersionListDlg_KnowledgeDialog',
                    accessibleMode: false,
                    componentId: 'articleVersionListDlg',
                    title: 'Version History',
                    width: 600,
                    height: 400,
