                    dialog.show();
                } else {
                    alert("Error: Implementation error. Show() function not implemented");
                }
            }
             //workaround for pages without header (when showheader="false")
            if ((window.sfdcPage) && (sfdcPage.executeOnloadQueue)) sfdcPage.executeOnloadQueue();
        </script> 
