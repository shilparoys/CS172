            });
            
            if(false && true && false) {
             if(false) {
              toolbar.add('<div class="toolbarPlaceHolder"><p>&#160;</p></div>');
             } else {
              return;
             }
            }
                toolbar.add(Ext.fly('j_id0:j_id1:j_id4:j_id5:votingComponent').dom.innerHTML);
           
            if(false) {
             theTopicCP = toolbar.add('');
            }
           
            toolbar.addFill();

            if(true){
             toolbar.add(Ext.fly('j_id0:j_id1:j_id4:j_id5:propertiesPanelLabel').dom.innerHTML);
            }
            toolbar.doLayout();
            
           if(false) {
             var topicInToolBar = document.getElementById(theTopicCP.id);
             topicInToolBar.appendChild(document.getElementById('knowledgeTopic'));
      }
