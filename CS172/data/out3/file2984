    mode: 'thumbnails-a',
    container: 'taboola-sc-below-article-thumbnails',
    placement: 'SC Below Article Thumbnails RSX',
    target_type: 'mix'
  });
</script> 
          </section> 
          <section class="blogroll main_content_thumb_title_roll clearfix size4"> 
           <h3>Around the Web</h3> 
           <div id="taboola-exchange-below-article-thumbnails"></div> 
           <script type="text/javascript">
  window._taboola = window._taboola || [];
  _taboola.push({
    mode: 'organic-thumbnails-b',
    container: 'taboola-exchange-below-article-thumbnails',
