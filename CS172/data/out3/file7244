    $j(function() {
        var itsTrigger = ".text-search-bkg > div > input[type=\"text\"].search-query ";
        var divcontainer = ".text-search-bkg";

        $j(itsTrigger).focus(function() {

            $j(divcontainer).addClass("search-query-focus");
            $j(itsTrigger).css("color", "#53A853");

        });
        $j(itsTrigger).focusout(function() {

            $j(divcontainer).removeClass("search-query-focus");
            $j(itsTrigger).css("color", "#999");

