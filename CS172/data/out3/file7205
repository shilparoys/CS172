          function trackFunction(_gaq) {
            _gaq.push(['w._setAccount', 'UA-10286486-5']);
            _gaq.push(['w._setCustomVar',1,'VisitorType','CommonApp', 1]);
            _gaq.push(['w._trackPageview']);
            return;
          }
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-10286486-4']);
  _gaq.push(['_setDomainName', 'wheelock.edu']);
  _gaq.push(['_trackPageview']);
// New tracker code for consolidated view
  _gaq.push(['w._setAccount', 'UA-10286486-5']);
  _gaq.push(['w._setDomainName', 'wheelock.edu']);
  _gaq.push(['w._trackPageview']);
