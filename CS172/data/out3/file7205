  <script type="text/javascript">
            $(function(){
			var randomSlide = Math.floor((Math.random() * 5) + 1)
            $('#valueProp ul.carousel').anythingSlider({
            autoPlay: false,
            hashTags: false,
            expand: true,
            resizeContents: false,
            buildStartStop: false,
            buildArrows: true,
            easing: 'easeOutQuint',
            appendNavigationTo: $('#valueProp .navContainer'),
            showMultiple: 2,
            easing: 'easeOutQuint',
            animationTime: 750,
            delay: 5000,
			startPanel: randomSlide,
            onInitialized: function(e, slider){
            $(".carousel").show();
            $('#valueProp .thumbNav span').fadeTo(0, 0.3).eq(randomSlide - 1).fadeTo(0, 0.01);
            $('#valueProp .thumbNav span').bind('mouseenter', function(){
            $(this).stop().fadeTo(250, 0.01);
            });
            $('#valueProp .thumbNav span').bind('mouseleave', function(){
            if ($(this).parent().hasClass('cur')) return false;
            $(this).stop().fadeTo(250, 0.3);
            });
            $('#valueProp .foreground').css('left', '-1500px');
            $('#valueProp .copy').hide();
            $('#valueProp .more').hide();
            setTimeout(function(){
            animateSlideIn(randomSlide);
            }, 250);
            },
            onSlideBegin: function(e, slider) {
            animateSlideOut(slider.currentPage);
            animateSlideIn(slider.exactPage);
            $('#valueProp .thumbNav .cur span').stop().fadeTo(250, 0.3);
            },
            onSlideComplete: function(slider) {
            $('#valueProp .thumbNav .cur span').stop().fadeTo(0, 0.01);
            }
            });
            function animateSlideOut(i) {
            $('#valueProp .foreground').eq(i).stop().animate({left: -1500}, 1500, 'easeOutQuint');
            $('#valueProp .copy').eq(i).fadeOut(750);
            $('#valueProp .more').eq(i).fadeOut(750);
            }
            function animateSlideIn(i) {
            if (i == 0) i = 5;
            if (i > 5) i = 1;
            $('#valueProp .foreground').eq(i).stop().animate({left: 0}, 1500, 'easeOutQuint');
            $('#valueProp .copy').eq(i).fadeIn(750);
            $('#valueProp .more').eq(i).fadeIn(750);
            }
            });
            $(function(){
            var duration = 8000;
            var speed = 1500;
            var i = 0;
            var n = $('.slideshow .body li').length;
            var timer = null;
            function play() {
            clearTimeout(timer);
            nextIndex = i + 1;
            if (i == (n - 1)) nextIndex = 0;
            $('.slideshow .titleBar li').removeClass('on').eq(i);
            $('.slideshow .body li').eq(i).fadeOut(speed);
            $('.slideshow .body li').eq(nextIndex).fadeIn(speed, function(){
            $('.slideshow .titleBar li').eq(nextIndex).addClass('on');
            timer = setTimeout(play, duration);
            });
            i++;
            if (i == n) i = 0;
            }
            var timer = setTimeout(play, duration);

            $('.slideshow .titleBar a').map(function(index){
            $(this).bind('click', function(){
            if (index == i) return false;
            clearTimeout(timer);
            $('.slideshow .body li').eq(i).stop().fadeOut(speed);
            $('.slideshow .body li').eq(index).stop().fadeIn(speed);
            $('.slideshow .titleBar li').removeClass('on').eq(index).addClass('on');
            i = index;
            return false;
            });
            });

            $('.slideshow .photo a.spotlightVideo').map(function(index){
            $(this).bind('click', function(){
            //alert("WORKED!");
            //if (index == i) return false;
            clearTimeout(timer);
            $('.slideshow .body li').eq(i).stop().fadeOut(speed);
            $('.slideshow .body li').eq(i).stop().fadeIn(speed);
            $('.slideshow .titleBar li').eq(i).addClass('on');
            //i = index;
            return false;
            });
            });
            });
          </script>
