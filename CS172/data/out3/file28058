var NYTD = NYTD || {};
NYTD.TJ = NYTD.TJ || {};
NYTD.TJ.Ads = {};
function TJAdsCallback( data ) {
    NYTD.TJ.Ads = data.ads;
    jQuery(document).ready(function() {
        if ( ( typeof NYTD.TJ.Ads !== "undefined" ) ) {
            if ( typeof NYTD.TJ.Ads.Position1 !== "undefined" ) {
                jQuery("div.ad.Position1").html( NYTD.TJ.Ads.Position1.creative ).removeClass("hidden");
            } else {
                jQuery("div.the-new-york-times").removeClass("hidden");
            }
            
            if ( typeof NYTD.TJ.Ads.PaidPost !== "undefined" ) {
                jQuery("li.ad.PaidPost").html( NYTD.TJ.Ads.PaidPost.creative ).removeClass("hidden");
            }
        }
    });
}
