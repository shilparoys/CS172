$(document).ready(function(){

 $("div.left_236_title").click(function () { 
	var idname = $(this).attr("id");
	 $("#"+idname+"_body").toggle("slow");
    });

$('.toggle2').hide();

$(".toggle1").click(function () { 
	$(this).next(".toggle2").slideToggle();
});
 	

	
	$("div.item").click(function () { 
	$("div.item").css({'background-image' : 'url(../img/grey_bg_off.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	$("div#go1").css({'background-image' : 'url(../img/grey_bg_off_big.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	$("div.item_down").css({'background-image' : 'url(../img/grey_bg_down_off.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	if($(this).attr("id")=='go1'){$(this).css({'background-image' : 'url(../img/grey_bg_big.jpg)', 'font-size' : '12pt', 'color' : '#cdaa3e'});}
	else
	{$(this).css({'background-image' : 'url(../img/grey_bg.jpg)', 'font-size' : '12pt', 'color' : '#cdaa3e'});}
	var idname2 = $(this).attr("id");
	$("div#go1_body,div#go2_body,div#go3_body,div#go4_body,div#go5_body,div#go8_body,div#go9_body,div#go10_body,div#go12_body,div#go13_body,div#go14_body").slideUp("700");
	$("#"+idname2+"_body").slideDown("200");
	});

	$("div.item_down").click(function () { 
	$("div.item").css({'background-image' : 'url(../img/grey_bg_off.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	$("div#go1").css({'background-image' : 'url(../img/grey_bg_off_big.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	$("div.item_down").css({'background-image' : 'url(../img/grey_bg_down_off.jpg)', 'font-size' : '11pt', 'color' : '#06275d'});
	$(this).css({'background-image' : 'url(../img/grey_bg_down.jpg)', 'font-size' : '12pt', 'color' : '#cdaa3e'});
	var idname2 = $(this).attr("id");
	$("div#go1_body,div#go2_body,div#go3_body,div#go4_body,div#go5_body,div#go8_body,div#go9_body,div#go10_body,div#go12_body,div#go13_body,div#go14_body").slideUp("700");
	$("#"+idname2+"_body").slideDown("200");
	});

$("div.item,div.item_down").hover(function () {$(this).css({'font-size':'12pt', 'cursor':'pointer'});},
	function () {$(this).css({'font-size':'11pt'});});

$(function() {
   $('a[rel=full]').lightBox({
	overlayBgColor: '#000',
	overlayOpacity: 0.7,
	imageLoading: 'http://e-u.in.ua/img/lightbox-ico-loading.gif',
	imageBtnClose: 'http://e-u.in.ua/img/lightbox-btn-close.gif',
	imageBtnPrev: 'http://e-u.in.ua/img/lightbox-btn-prev.gif',
	imageBtnNext: 'http://e-u.in.ua/img/lightbox-btn-next.gif',
	containerResizeSpeed: 350,
	txtImage: 'Фото',
	txtOf: 'з'
   });

    });
	
	
	var p = $("#left_top");
	var position = p.position();
	//$("p:last").text( "left: " + position.left + ", top: " + position.top );
	$('#blue').css({"left":position.left+106,"top":position.top}).delay(1000).slideDown("Slow");
	
	var p = $("#right");
	var position = p.position();

	$('#blue2').css({"left":position.left+90,"top":position.top+5}).delay(1200).slideDown("Slow");
	$('#blue2_close').click(function() {
		 $('#blue2').slideUp("Fast");
	});

/*
	$('#blue3').css({"left":position.left+110,"top":position.top+200}).delay(1200).slideDown("Slow");
	$('#blue3_close').click(function() {
		$('#blue3').slideUp("Fast");
	});
*/
	
	$('#blue').click(function() {
		$('#blue').hide();
	});

});
