   <script type="text/javascript">
	(function($){
        /**
		Mobile Search Button
		*/
		var shown = false;
		$('.mobile-search').click(function() {
			var windowWidth = $(window).width();
			var buttonWidth = $('.mobile-search').innerWidth();
			var animateWidth = -windowWidth + buttonWidth;
			
			if(!shown)
			{
				$('.search-container,.mobile-search').css({
					'-webkit-transform':'translateX(' + animateWidth + 'px)',
					'-moz-transform':'translateX(' + animateWidth + 'px)',
					'transform':'translateX(' + animateWidth + 'px)'
				});
				shown = true;
				
			}
			else
			{
				$('.search-container,.mobile-search').css({
					'-webkit-transform':'translateX(0)',
					'-moz-transform':'translateX(0)',
					'transform':'translateX(0)'
				});
				shown = false;
			}
		});
		
		resizeSearch = function(){
			var windowWidth = $(window).width();
			var buttonWidth = $('.mobile-search').innerWidth();
			var animateWidth = -windowWidth + buttonWidth;
			$('.search-container,.mobile-search').css({
				'-webkit-transform':'translateX(' + animateWidth + 'px)',
				'-moz-transform':'translateX(' + animateWidth + 'px)',
				'transform':'translateX(' + animateWidth + 'px)'
			});
		}
		
		resetSearch = function(){
			$('.search-container,.mobile-search').css({
				'-webkit-transform':'translateX(0)',
				'-moz-transform':'translateX(0)',
				'transform':'translateX(0)'
			});
			shown = false;
		}
	})(jQuery);
</script>
