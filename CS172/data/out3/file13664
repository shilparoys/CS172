  <script src="//ziplookup.googlecode.com/git/public/ziplookup/zipLookup.min.js" type="text/javascript"></script> 
  <script type="text/javascript">
  
    /* Using zipLookup.min.js */
    $(document).ready(function() {    
        // Check zip
        function checkZip(){
            $.zipLookup(                                        // Do a Zip code Lookup
                $('input[name=zip-code]').val(),                // Zip code Field Value
                function(cityName, stateName, stateShortName){  // If Successful,
                    //$(this).val(stateName);                   // Set State abbreviation
                    //$(this).val(stateShortName);              // Set State name
                    window.location = stateName + ".htm";      // Redirect
                },
                function(errMsg){                               // If Zip couldn't be found,
                    $("label[for='zip-code'].error").show();    // Add an error message
                }
            );       
        }
        // On submit run function
        $(".zip form").submit(function(){
            checkZip();
            return false;
        });
    });
</script>   
