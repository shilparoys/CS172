    $(document).ready(function () {
        var ver = getIEVersion();
        var txt = "";
        if (ver == 8.0) {
            txt += "<div style='background-color:#f4f4f4;border:1px solid #990000; padding:10px;'><font color=red><b> For Internet Explorer 8 Browser (IE8) users, please go to [Tools] menu and select [Compatibility View Settings] to configure the Internet Explorer 8.0's Compatibility View Settings to (Display all websites in Compatibility View).</b></font></div>";
            document.getElementById("IE8Msg").innerHTML = txt;
        }
    });
    
</script> 
