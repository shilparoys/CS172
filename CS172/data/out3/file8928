  <script type="text/javascript">
    function debug (msg) {
      if (typeof console === "undefined") return;
         console.log(msg);
    }
    </script> 
  <script type="text/javascript">

        $(document).ready(function () {
            // call the tablesorter plugin
            $("table").tablesorter({

                // enable sorting by date, by implementing custom text
                // extraction method. see tablesorter docs.
                textExtraction: function (node) {
                    // extract data from markup and return it;
                    // NOTE: this is called for ALL columns

                    var colStr = new String(node.childNodes[0].nodeValue);
                    colStr = $.trim(colStr); // only FF seems to have a leading space here!

                    if (colStr.indexOf(":") > 0) {

                        // if this is the date column, return a UNIX epoch value
                        // and note date functions differ in browsers so although some could
                        // handle our date exactly as is, FF doesn't handle months JAN, FEB, MAR, etc!

                        // 05-DEC-2012 17:08:12  --> 12/5/2012 17:08:12
                        // see http://dygraphs.com/date-formats.html

                        var evtXBrowsStr = colStr;
                        // break out the month  e.g. JAN to get as digits
                        var dateAry = evtXBrowsStr.split(' ');
                        var dayAry = dateAry[0].split('-');
                        var month = dayAry[1];
                        var monDigits; // 01 02 etc
                        switch (month) {
                            case "JAN" :
                            case "ENE" :
                                monDigits = "01";
                                break;
                            case "FEB" :
                                monDigits = "02";
                                break;
                            case "MAR" :
                                monDigits = "03";
                                break;
                            case "APR" :
                            case "ABR" :
                                monDigits = "04";
                                break;
                            case "MAY" :
                                monDigits = "05";
                                break;
                            case "JUN" :
                                monDigits = "06";
                                break;
                            case "JUL" :
                                monDigits = "07";
                                break;
                            case "AUG" :
                                monDigits = "08";
                                break;
                            case "SEP"  :
                                monDigits = "09";
                                break;
                            case "OCT" :
                                monDigits = "10";
                                break;
                            case "NOV" :
                                monDigits = "11";
                                break;
                            case "DEC" :
                            case "DIC" :
                                monDigits = "12";
                                break;
                            default:
                                monDigits = "ER";
                                debug('error: bad value in event date: ' + colStr);
                        }

                        // 05-DEC-2012 17:08:12  --> 12/5/2012 17:08:12
                        evtXBrowsStr = monDigits + '/' + dayAry[0] + '/' + dayAry[2] + ' ' + dateAry[1];
                        var d = Date.parse(evtXBrowsStr);
                        //debug('' + evtXBrowsStr + ' parsed d = ' + d); // note: IE hasn't got this
                        return(d);

                    } // end if this is a date column
                else {
                    if (colStr === "" && node.className === " region")
                        return(node.innerText);
                    else
                        // slow:   debug('returning val: '+colStr);// note: IE hasn't got this
                        return(colStr);
                }
            } // end text extraction func

            // right paren below closes call to tablesorter constructor;
            // so we are binding to the tablesorter object here:

            // after every sort re-color row backgrounds alternating
        }).bind("sortEnd", function () {
                    $('tr:even').removeClass('even odd');
                    $('tr:even').removeClass('even odd');
                    $('tr:even').addClass('even');
                    $('tr:odd').addClass('odd');
                });  // end bind

            // now color the rows
            $('tr:even').removeClass('even odd');
            $('tr:even').removeClass('even odd');
            $('tr:even').addClass('even');
            $('tr:odd').addClass('odd');

        }); // end document ready

    </script> 
 </head> 
