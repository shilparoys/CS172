  <script>$(window).on("resize", function () { chooseBG("/~/media/global/heroimages/hero-trail-sm.ashx", "/~/media/global/heroimages/hero-trail-lg.ashx", "/~/media/global/heroimages/hero-trail-xlg.ashx") }).resize();</script> 
