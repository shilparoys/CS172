  <script type="text/javascript">
YUI().use('node', 'io', 'event', 'animation','node-load','node-event-simulate', function (Y) {

	var genct=1;
	var bpos = "";
	Y.all('.mnu li b').each(function (node)
	{
	bpos = node.getStyle('backgroundPosition');
	if(bpos == "0px 0px") {

	node.addClass("top_gen" + genct);
	  genct++;
     if(genct==13) {genct=1;}
	}
	});

	if(Y.all('#top_menu li').size()< 4){
		Y.one('#topics_more').hide();
	}
Y.one('#topics_more').on('click',function() {
	Y.one('#smp').show();
	Y.one('#topics_more').hide();
});
 var url  = document.URL;
 var tags = url.split("#");
 if(tags[1] != "undefined"){
     if(tags[1]=="prods_more" || tags[1]=="daily_prods_more"){
        Y.one('#prod_tab a').simulate('click');
     }
     if(tags[1]=="daily_prods_more"){
        Y.one('#prods_more a').simulate('click');
     }
     if(Y.one('#'+tags[1]+' a') ){
     	Y.one('#'+tags[1]+' a').simulate('click');
    	// Y.one('#'+tags[1]).focus();
     } 
}

});
