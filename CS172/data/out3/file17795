   <form id="tbn-nl-form" action="index.php" method="post"> 
    <input type="email" name="email" class="email" placeholder="Email"> 
    <p class="error-message email-error">Please enter a valid email address</p> 
    <ul class="dropdown"> 
     <li> <input type="checkbox" checked name="select-all" class="select-all" value="select-all"> <label for="select-all"> All Newsletters</label> <a href="#" class="dropdown-link">
       <div class="dropdown-icon"></div></a> </li> 
     <div class="selections"> 
      <li> <input type="checkbox" checked name="tbn-nls" class="selection" value="1"> <label class="tooltip" title="When major stories break, the news will come straight to your inbox." for="breaking-news"> Breaking News</label> </li> 
