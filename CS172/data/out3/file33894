$(document).ready(function(e) {


			
							$(".fancyGalleryEvent").fancybox({
								
									afterClose: function(){
										
											setTimeout(function(){ $.fancybox( $(lastFancyBoxEvent).parents('div:eq(0)').find(".mercuryContentEvent").html() ) },200);
										}
								
								});
			
});
	



$(document).on("click",".showMercuryEvent_4", function(){
		
		var $this = $(this);
		var mercuryContent = $($this).parents('div:eq(0)').find(".mercuryContentEvent").html();
		lastFancyBoxEvent = $this;
		
		
		
		$.fancybox(mercuryContent,{
				
				fitToView	: false,
				width		: '90%',
				height		: '90%',
				autoSize	: false,
				closeClick	: false,
				openEffect	: 'fade',
				closeEffect	: 'elastic',
				openSpeed: '1500'
				
			});
		

	});



</script> 
