     <script type="text/javascript">
	function calculateCost(){
	var total=0.0;
	var single_room_cost=500.0;
	var your_name = document.form1.your_name;
	var guest1 = document.form1.guest1;
	var guest2 = document.form1.guest2;
	var guest3 = document.form1.guest3;
	
	var single_room_number = document.form1.single_room_number;
	var double_room_number = document.form1.double_room_number;

	var deposit = document.form1.deposit;
	var first = document.form1.first;
	var last = document.form1.last;
	
	var count = 0;
	if(your_name && your_name.value) count++;
	if(guest1 && guest1.value) count++;
	if(guest2 && guest2.value) count++;
	if(guest3 && guest3.value) count++;
	
	if(deposit.checked) total = total + deposit.value*count;
	if(first.checked) total = total + first.value*count;
	if(last.checked) total = total + last.value*count;


	total = total + single_room_number.value*single_room_cost;

	//if(room_type[0].checked) total = total + 500;
	
	total = looksLikeMoney(total);
	
	loadTextInToElement(total, "total_cost");
	}

	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}
	function looksLikeMoney(total) {
		var total = (parseInt((total+.001)*100))/100;
		var niceTotal = "$"+total;
		if(niceTotal.substring(niceTotal.length-2,niceTotal.length-1)=="."){
			niceTotal=niceTotal+"0";
		}
 		return niceTotal;
	}

	</script> 
     <script type="text/javascript">
	function makeSubmitEnabled(){
		document.getElementById('submit_form').disabled='';
	}
</script> 
     <h1>Westmont Alumni and Parent Trip to Mexico</h1> 
     <form class="forms" id="form1" name="form1" method="post" action="https://secure.westmont.edu/forms/alumni/mexico_trip/registration_processor.php" onsubmit="document.getElementById('submit_form').disabled = 1;"> 
      <fieldset> 
       <legend>Event Information</legend> 
       <b> Date: June 27th – July 6th, 2015 <br> Total trip cost: $2550 <br>Payment Due Dates: (all are per person) </b> 
       <ul> 
        <li> $500 deposit: with registration (non refundable) </li>
        <li> $1000: February 13th, 2015 </li>
        <li> $1050: March 16, 2015( or payment in full if no prior payments) </li>
       </ul> 
       <p> For trip details and terms and conditions, see the <a href="http://www.westmont.edu/alumni/documents/Westmont-Alumni-Parent-Mexico-Trip-2015.pdf"> brochure</a>. As with all travel, insurance is recommended for this tour. </p>
       <p> By enclosing the above payment for this tour, I hereby confirm that I have read, understood and accept the Terms and Conditions as stated in the tour information. <input type="text" name="initials" class="small required"> (Please initial here as your signature) </p> 
      </fieldset> 
      <fieldset> 
       <legend>Attendee Information (name(s) exactly as they appear on passport)</legend> 
       <ol> 
        <li> <label>Your Name:</label> <input name="your_name" class="required large" onkeyup="calculateCost()"> </li> 
        <li> <label>Guest 1: (if applicable)</label> <input name="guest1" class=" large" onkeyup="calculateCost()"> </li> 
        <li> <label>Guest 2: (if applicable) </label> <input name="guest2" class="large" onkeyup="calculateCost()"> </li> 
        <li> <label>Guest 3: (if applicable)</label> <input name="guest3" class=" large" onkeyup="calculateCost()"> </li> 
        <li> <label>Address:</label> <input name="address" class=" large"> </li> 
        <li> <label>City/State/Zip:</label> <input name="city" class=" ">/ <input name="state" class="small ">/ <input name="zip" class=" "> </li> 
        <li> <label>Email:</label> <input name="email" class=" large required"> </li> 
        <li> <label>Phone:</label> <input name="phone" class=" large"> </li> 
       </ol> 
      </fieldset> 
      <fieldset> 
       <legend>Room Information</legend> 
       <ol> 
        <li> <label>Room Type:</label> <input name="single_room_number" class=" small" onkeyup="calculateCost()"> Single room number (extra charge $500 each) <br> <label></label> <input name="double_room_number" class=" small" onkeyup="calculateCost()"> Double room number </li> 
       </ol> 
      </fieldset> 
      <fieldset> 
       <legend> Pay Now </legend> 
       <ol> 
