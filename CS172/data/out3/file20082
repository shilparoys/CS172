         <a id="main-content"></a> 
         <h1 class="title" id="page-title">Executive Council</h1> 
         <div class="tabs"> 
         </div> 
         <div class="region region-content"> 
          <div id="block-system-main" class="block block-system"> 
           <div class="content"> 
            <div id="node-235408" class="node node-page clearfix" about="/about/staff/executive-council" typeof="foaf:Document"> 
             <span property="dc:title" content="Executive Council" class="rdf-meta element-hidden"></span>
             <span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span> 
             <div class="content"> 
              <div class="field field-name-body field-type-text-with-summary field-label-hidden">
               <div class="field-items">
                <div class="field-item even" property="content:encoded"> 
                 <style type="text/css">
    div.dir_box {
        height: 270px;
    }

    div.dir_box_content img {
        width: 135px;
        height: 185px;
    }
</style> 
                 <div class="dir_box"> 
                  <div class="dir_box_content">
                   <img alt="Dr. Rod A. Risley" src="/sites/default/files/images/about/bod_rod_risley.jpg"> 
                   <ul> 
                    <li><span class="dir_name">Dr. Rod A. Risley</span></li> 
                    <li><span class="dir_title">Executive Director and CEO</span></li> 
                    <li><a href="#ec-bio1" class="cbox-bio internal">Read Rod's Biography</a></li> 
                   </ul> 
                  </div> 
                 </div> 
                 <div class="dir_box"> 
                  <div class="dir_box_content">
                   <img alt="Deidra Daws" src="/sites/default/files/images/about/daws_deidra14.jpg"> 
                   <ul> 
                    <li><span class="dir_name">Deidra Daws</span></li> 
                    <li><span class="dir_title">Chief of Staff</span></li> 
