  <!-- Remove this credit if you want, but if you leave it in I will appreciate it! -->  
  <script type="text/javascript">
    	var timeID = null;
    	
    	$("document").ready(function(){

    		
    		var count = $("ul#spotlights li").children().get().length;
    		$("ul#spotlights").css({display:"block",height:count * 143 + "px"});
    		
			for(i=0;i<count-1;i++)
			{
				if(i==0)
				{
					$('<a class="navigator-item navigator-item-active"></a>').attr("page",i).click(NavigatorStep).bind("mouseover",function(){$(this).addClass("navigator-item-hover");}).bind("mouseout",function(){$(this).removeClass("navigator-item-hover");}).appendTo($("#navigator"));
				}				
				else
				{
					$('<a class="navigator-item"></a>').attr("page",i).click(NavigatorStep).bind("mouseover",function(){$(this).addClass("navigator-item-hover");}).bind("mouseout",function(){$(this).removeClass("navigator-item-hover");}).appendTo($("#navigator"));
				}
			}
    		timeID = window.setTimeout(SpotlightSlide,5000);
    		
    		////////////////////////////////////////////////////
    		

    	});
    	
    	function NavigatorStep(clear)
    	{
    		window.clearTimeout(timeID);    		
    		$("ul#spotlights").stop(true,false);
    		
    		var elm = $(this); 
    		$("a.navigator-item-active").removeClass("navigator-item-active");
    		elm.addClass("navigator-item-active"); 
    		var page = elm.attr("page");
    		var nextTop = -1 * page * 143;

    		$("ul#spotlights").animate({top:nextTop + "px"},function(){
    			timeID = window.setTimeout(SpotlightSlide,5000);
    		});
  		
    	}
    	function SpotlightSlide()
    	{
    		
    		    $("ul#spotlights").stop(true,false);
    		    var top = $("ul#spotlights").css("top");
    			var height = $("ul#spotlights").height();
    			var odd = $("ul#spotlights").attr("odd");    			
    			var nextTop = parseInt(top,10) - 143;
    			page = -1 * nextTop/143;
    			$("a.navigator-item-active").removeClass("navigator-item-active");
    			$("a.navigator-item[page="+page+"]").addClass("navigator-item-active");
    			
    			if((-1 * nextTop) >= height - 143 )
    			{
    				$("div#navigator").attr("disabled","disabled");
    				$("ul#spotlights").fadeOut("slow",function(){    					
						$("ul#spotlights").css({top:0}).fadeIn("slow",function(){
							$("div#navigator").removeAttr("disabled");
							$("a.navigator-item-active").removeClass("navigator-item-active");
			    			$("a.navigator-item[page=0]").addClass("navigator-item-active");    			

  						timeID = window.setTimeout(SpotlightSlide,5000);    						
    					});
    				});
    			
    			}else
    			{
    				$("ul#spotlights").animate({top:nextTop + "px"},function(){
    					timeID = window.setTimeout(SpotlightSlide,5000);
    				});    				
    			}
    	}
    </script> 
  <script type="text/javascript">
                               (function(){
                                  // if firefox 3.5+, hide content till load to prevent FOUT
                                  var d = document, e = d.documentElement, s = d.createElement('style');
                                  if (e.style.MozTransform === ''){ // gecko 1.9.1 inference
                                    s.textContent = 'body{visibility:hidden}';
                                    e.firstChild.appendChild(s);
                                    function f(){ s.parentNode && s.parentNode.removeChild(s); }
                                    addEventListener('load',f,false);
                                    setTimeout(f,1000);
                                  }
                                })();
               </script> 
