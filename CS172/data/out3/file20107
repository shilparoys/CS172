  <script type="text/javascript">
        function validateAndSubmit(username,password) {

                jQuery("#submitBtn").hide();
                jQuery("#workingBtn").show();
                jQuery.ajax({
                    type:"POST",
                    url:"login",
                    data:"username=" + username + "&password=" + password,
                    dataType:"text",
                    success:function (response) {
                        if (response == "success")
                            doCAS(username,password);
                        else if (response == "registrationRequired")
                            doRegistration();
                        else if (response != "falseFacebookLogin") {
                            if (response == "messageLoginInvalid")
                                jQuery("#errorMessage").text("Invalid username or password.");
                            else if (response == "messageLoginSynch")
                                jQuery("#errorMessage").text("Your Phi Theta Kappa account has not been synchronized with CollegeFish. Please contact Key Services at 800.946.9995 or ptk.org/contact.");
                            else if (response == "messageLoginLocked")
                                jQuery("#errorMessage").text("Your account has been locked. Please use the Forgot Password link to reset your password.");
                            else if (response == "unameOrPassInvalidPatternMbr") {
                                jQuery("#errorMessage").empty();
                                jQuery("#errorMessage").append("Special characters are not allowed in usernames or passwords. Please click <a href=  'https://www.ptk.org/loginmanager/?action=change&backLink=https://www.collegefish.org/transferfish/login.do'  >here</a> to change your log in.");
                            }
                            else if (response == "unameOrPassInvalidPatternNonMbr") {
                                jQuery("#errorMessage").empty();
                                jQuery("#errorMessage").append("Special characters are not allowed in usernames or passwords. Please click <a href='forgottenUsernamePassword.vm'>here</a> to change your log in.");
                            }
                            jQuery("#errorDialog").dialog("open");	//non-blocking
                            jQuery("#submitBtn").show();
                            jQuery("#workingBtn").hide();
                        }
                    },
                    error:function () {
                        jQuery("#errorMessage").text("Oops. An error occurred. Please try your request again.");
                        jQuery("#errorDialog").dialog("open");	//non-blocking
                        jQuery("#submitBtn").show();
                        jQuery("#workingBtn").hide();
                    }
                });
            
            return false;	//ajax callback will submit the form after retrieving the login ticket (assuming no errors)
        }

        // forward to registration process for additional information
        function doRegistration() {
            window.location.href = "/loginmanager/registration";
        }

        function doFalseLogin() {
            jQuery("#form1").get(0).setAttribute('action', '/loginmanager/new.go');
            jQuery("#form1").submit();
        }

        function initDialogs() {
            jQuery("#errorDialog").dialog({
                autoOpen:false,
                modal:true,
                buttons:{
                    'OK':function () {
                        jQuery(this).dialog("close");
                    }
                }
            });
        }

        function init() {
            initDialogs();
  			            jQuery("#formTable").show();
        }
    </script> 
 </head> 
