	function makeArray(len) {
	    for (var j = 0; j < len; j++) this[j] = null;
	this.length = len;
	}

	// This is where the array of text/images/sounds is created.

	ideas = new makeArray(5);
	
	ideas[0] = "&quot;The quality of our thoughts is bordered on all sides by our facility with language.&quot; <BR>J. Michael Straczynski"
	ideas[1] = "&quot;Thought is the blossom; language the bud; action the fruit behind it.&quot; <BR>Ralph Waldo Emerson"
	ideas[2] = "&quot;A special kind of beauty exists which is born in language, of language, and for language.&quot; <BR>Gaston Bachelard"
	ideas[3] = "&quot;To have another language is to possess a second soul.&quot;<BR> Charlemagne"
	ideas[4] = "&quot;Those who know nothing of foreign languages know nothing of their own.&quot;<BR> Johann Wolfgang Von Goethe"
	ideas[5] = "&quot;Language is the blood of the soul into which thoughts run and out of which they grow.&quot;<BR> Oliver Wendell Holmes"
	// The random number generator.

	function rand(n) {
	seed = (0x015a4e35 * seed) % 0x7fffffff;
	return (seed >> 16) % n;
	}
        
	var now = new Date()
	var seed = now.getTime() % 0xffffffff

