<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
 <head> 
  <meta charset="UTF-8"> 
  <meta name="Keywords" content="Video, multimedia, media, live events, videos"> 
  <meta name="Description" content="Online videos produced at Illinois' flagship public university, offering information for current and prospective students, alumni, and parents from a world leader in research, teaching, and public engagement."> 
  <title>Here &amp; Now: Videos | Illinois</title> 
  <noscript> 
   <!-- if scripts are disabled, go ahead this way. --> 
   <meta http-equiv="REFRESH" content="0; url=https://ensemble.illinois.edu/app/Portal/default.aspx?PortalID=5e85bc3f-11b0-46c8-8c95-e26c85f536f3"> 
  </noscript> 
  <script type="text/javascript">
// Get the parameters from the URL
var paramstr = document.location.search;
	var ensembleURL = 'https://ensemble.illinois.edu/app/Portal/default.aspx?PortalID=5e85bc3f-11b0-46c8-8c95-e26c85f536f3'
if (paramstr) {
	// If there are any, go to the right portal page to load the content requested
	if (paramstr.match(/contentid/i)) {
		document.location = ensembleURL + 'video.aspx' + paramstr + '&sharing=true&credits=&attach=&links=&stats=&embed=&play=&hc=&cc=';
	} else if (paramstr.match(/categoryid/i)) {  
		// Format is important here...
		var catid = paramstr.split(/categoryid=/i)[1].split('&')[0];
		document.location = ensembleURL + 'categories.aspx?CategoryID=' + catid;
	} else { // there's nothing usable in the URL, so go to index
		document.location = ensembleURL;
	}
} else {
	// Otherwise just go to the index page
	document.location = ensembleURL;
}
</script> 
 </head> 
