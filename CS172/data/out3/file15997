     <script language="javascript" type="text/javascript">
$(document).ready(function(){
	
	// Initial Setup for Tabs
	$("#TabContent1").css("display","block");
	$("#Tab1").css("border-bottom","1px solid #FFFFFF");
	$("#Tab1").css("background-color","#FFFFFF");
	
	// If Tab 1 is clicked do the following
	$("#Tab1L").click(function(){
		//Setup Tab 1
		$("#TabContent1").css("display","block");
		$("#Tab1").css("border-bottom","1px solid #FFFFFF");
		$("#Tab1").css("background-color","#FFFFFF");
		
		//Reset all other tabs
		$("#TabContent2").css("display","none");
		$("#Tab2").css("border-bottom","1px solid #999");
		$("#Tab2").css("background-color","#fff989");
		$("#TabContent3").css("display","none");
		$("#Tab3").css("border-bottom","1px solid #999");
		$("#Tab3").css("background-color","#fff989");
		$("#TabContent4").css("display","none");
		$("#Tab4").css("border-bottom","1px solid #999");
		$("#Tab4").css("background-color","#fff989");
	});
	
	// If Tab 2 is clicked do the following
	$("#Tab2L").click(function(){
		//Setup Tab 1
		$("#TabContent2").css("display","block");
		$("#Tab2").css("border-bottom","1px solid #FFFFFF");
		$("#Tab2").css("background-color","#FFFFFF");
		
		//Reset all other tabs
		$("#TabContent1").css("display","none");
		$("#Tab1").css("border-bottom","1px solid #999");
		$("#Tab1").css("background-color","#fff989");
		$("#TabContent3").css("display","none");
		$("#Tab3").css("border-bottom","1px solid #999");
		$("#Tab3").css("background-color","#fff989");
		$("#TabContent4").css("display","none");
		$("#Tab4").css("border-bottom","1px solid #999");
		$("#Tab4").css("background-color","#fff989");
	});

	// If Tab 3 is clicked do the following
	$("#Tab3L").click(function(){
		//Setup Tab 1
		$("#TabContent3").css("display","block");
		$("#Tab3").css("border-bottom","1px solid #FFFFFF");
		$("#Tab3").css("background-color","#FFFFFF");
		
		//Reset all other tabs
		$("#TabContent1").css("display","none");
		$("#Tab1").css("border-bottom","1px solid #999");
		$("#Tab1").css("background-color","#fff989");
		$("#TabContent2").css("display","none");
		$("#Tab2").css("border-bottom","1px solid #999");
		$("#Tab2").css("background-color","#fff989");
		$("#TabContent4").css("display","none");
		$("#Tab4").css("border-bottom","1px solid #999");
		$("#Tab4").css("background-color","#fff989");
	});
	
	// If Tab 4 is clicked do the following
	$("#Tab4L").click(function(){
		//Setup Tab 1
		$("#TabContent4").css("display","block");
		$("#Tab4").css("border-bottom","1px solid #FFFFFF");
		$("#Tab4").css("background-color","#FFFFFF");
		
		//Reset all other tabs
		$("#TabContent1").css("display","none");
		$("#Tab1").css("border-bottom","1px solid #999");
		$("#Tab1").css("background-color","#fff989");
		$("#TabContent2").css("display","none");
		$("#Tab2").css("border-bottom","1px solid #999");
		$("#Tab2").css("background-color","#fff989");
		$("#TabContent3").css("display","none");
		$("#Tab3").css("border-bottom","1px solid #999");
		$("#Tab3").css("background-color","#fff989");
	});
	

	$("#articles_in_database_dd").change(function(){
		var goToURL = $("#articles_in_database_dd").val();
		//alert("got here");
		window.location = goToURL;
	});
	
});
</script> 
