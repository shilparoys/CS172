  <script language="javascript" type="text/javascript">
function fbs_click() 
	{
		u=location.href;
		t=document.title;
		window.open('http://www.facebook.com/sharer.php?u='+encodeURIComponent(u)+'&t='+encodeURIComponent(t),'sharer','toolbar=0,status=0,width=1024,height=768');
		return false;
	}
</script> 
