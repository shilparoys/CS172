      <div id="search"> 
       <script language="javascript" type="text/javascript">
	
	function show_select_box(){
		document.getElementById("search_select_container").style.display = "block";
		//alert("HI");
	}
	
	function show_select_box_google(){
		document.getElementById("search_select_container_google").style.display = "block";
	}

	function show_select_box_community(){
		document.getElementById("search_select_container_community").style.display = "block";
	}

	
	function set_search_val(val){
		var siteObj;
		var googleObj;
		//alert(val);
		//val = document.getElementById("search_select_element").value;
		
		if(val == 1){
			//if selects default search
			//only close the select Element containing div
			document.getElementById("search_form").style.display = "block";
			
			//make sure Google search form is hidden
			//since it shares the same search form
			document.getElementById("search_form_google").style.display = "none";			
			document.getElementById("search_form_community").style.display = "none";			
						
		}else if(val == 2){
			//Since Google searchis selected
			//Show the Google search form
			document.getElementById("search_form_google").style.display = "block";
		
			//Since Google searchis selected
			//Hide the default search form			
			document.getElementById("search_form").style.display = "none";	
			document.getElementById("search_form_community").style.display = "none";		
		}else if(val == 3){
			//Since Google searchis selected
			//Show the Google search form
			document.getElementById("search_form_community").style.display = "block";
		
			//Since Google searchis selected
			//Hide the default search form			
			document.getElementById("search_form").style.display = "none";	
			document.getElementById("search_form_google").style.display = "none";		
		}
		
		//Close the Select Box of the default select
		document.getElementById("search_select_container").style.display = "none";
	}
	
	function set_search_val_google(val){
		//var val;
		var siteObj;
		var googleObj;
		/*val = document.getElementById("search_select").selectedIndex;*/
		//val = document.getElementById("search_select_element_google").value;
		
		if(val == 1){
			//Since Google searchis selected
			//Show the Google dearch form
			document.getElementById("search_form").style.display = "block";
		
			//Since Google searchis selected
			//Hide the default search form			
			document.getElementById("search_form_google").style.display = "none";	
			document.getElementById("search_form_community").style.display = "none";
		
						
		}else if(val == 2){
			//if selects default search
			//only close the select Element containing div
			document.getElementById("search_form_google").style.display = "block";
			
			//make sure Google search form is hidden
			//since it shares the same search form
			document.getElementById("search_form").style.display = "none";	
			document.getElementById("search_form_community").style.display = "none";		
					
		}else if(val == 3){
			//if selects default search
			//only close the select Element containing div
			document.getElementById("search_form_community").style.display = "block";	
			
			
			//make sure Google search form is hidden
			//since it shares the same search form
			document.getElementById("search_form").style.display = "none";	
			document.getElementById("search_form_google").style.display = "none";	
					
		}
		
		//Close the Select Box of the default select
		document.getElementById("search_select_container_google").style.display = "none";
	}
	
	
	function set_search_val_community(val){
		//var val;
		var siteObj;
		var googleObj;
		/*val = document.getElementById("search_select").selectedIndex;*/
		//val = document.getElementById("search_select_element_google").value;
		
		if(val == 1){
			//Since Google searchis selected
			//Show the Google dearch form
			document.getElementById("search_form").style.display = "block";
		
			//Since Google searchis selected
			//Hide the default search form			
			document.getElementById("search_form_google").style.display = "none";	
			document.getElementById("search_form_community").style.display = "none";
		
						
		}else if(val == 2){
			//if selects default search
			//only close the select Element containing div
			//document.getElementById("search_select_container_google").style.display = "none";
			document.getElementById("search_form_google").style.display = "block";
			
			//make sure Google search form is hidden
			//since it shares the same search form
			document.getElementById("search_form").style.display = "none";		
			document.getElementById("search_form_community").style.display = "none";	
					
		}else if(val == 3){
			//if selects default search
			//only close the select Element containing div
			document.getElementById("search_form_community").style.display = "block";
			
			//make sure Google search form is hidden
			//since it shares the same search form
			document.getElementById("search_form").style.display = "none";	
			document.getElementById("search_form_google").style.display = "none";			
					
		}
		
		//Close the Select Box of the default select
		document.getElementById("search_select_container_community").style.display = "none";
	}
	
	
	
	function disappear_search_box(){
		document.getElementById("search_select_container").style.display = "none";
	}
	
	function appear_search_box(){
		document.getElementById("search_select_container").style.display = "block";
	}

	function disappear_search_box_google(){
		document.getElementById("search_select_container_google").style.display = "none";
	}
	
	function appear_search_box_google(){
		document.getElementById("search_select_container_google").style.display = "block";
	}

	function disappear_search_box_community(){
		document.getElementById("search_select_container_community").style.display = "none";
	}
	
	function appear_search_box_community(){
		document.getElementById("search_select_container_community").style.display = "block";
	}
	
	function appear_search_box_background(linkItem){
		if(linkItem == 1){
			document.getElementById("search_site_link_first").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_first").style.color = "#FF9300";
			
			document.getElementById("search_site_link_first_google").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_first_google").style.color = "#FF9300";
			
			document.getElementById("search_site_link_first_community").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_first_community").style.color = "#FF9300";

		}else if(linkItem == 2){
			document.getElementById("search_site_link_second").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_second").style.color = "#FF9300";
			
			document.getElementById("search_site_link_second_google").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_second_google").style.color = "#FF9300";
			
			document.getElementById("search_site_link_second_community").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_second_community").style.color = "#FF9300";

		}else if(linkItem == 3){
			document.getElementById("search_site_link_third").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_third").style.color = "#FF9300";
			
			document.getElementById("search_site_link_third_google").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_third_google").style.color = "#FF9300";
			
			document.getElementById("search_site_link_third_community").style.backgroundColor = "#1c6595";
			document.getElementById("search_site_link_third_community").style.color = "#FF9300";

		}
	}
	
	function disappear_search_box_background(linkItem){
		if(linkItem == 1){
			document.getElementById("search_site_link_first").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_first").style.color = "#2765AA";
			
			document.getElementById("search_site_link_first_google").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_first_google").style.color = "#2765AA";
			
			document.getElementById("search_site_link_first_community").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_first_community").style.color = "#2765AA";

		}else if(linkItem == 2){
			document.getElementById("search_site_link_second").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_second").style.color = "#2765AA"
			
			document.getElementById("search_site_link_second_google").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_second_google").style.color = "#2765AA"
			
			document.getElementById("search_site_link_second_community").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_second_community").style.color = "#2765AA"
		}else if(linkItem == 3){
			document.getElementById("search_site_link_third").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_third").style.color = "#2765AA"
			
			document.getElementById("search_site_link_third_google").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_third_google").style.color = "#2765AA"
			
			document.getElementById("search_site_link_third_community").style.backgroundColor = "#ffffff";
			document.getElementById("search_site_link_third_community").style.color = "#2765AA"
		}
	}
</script> 
