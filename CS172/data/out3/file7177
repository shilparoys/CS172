//<![CDATA[
window.addEvent('domready', function() {
	var walkers = document.getElements('#handlers98 span');
	var spSlide98 = new slideshow_sp2($('sp-slide98'), {
		transition: 'cover-inplace-fade',
		size: {width: 700, height: 300},
		interval: 5000,
		fxOptions: {duration:  500, transition: Fx.Transitions.Sine.easeOut},
		onWalk: function (i, j) {
			walkers[i].addClass('active');
			walkers[j].removeClass('active');
		},
		onInitialized: function () {
			walkers[0].addClass('active');
		}		
	});

	var images = ([
		'http://www.knlu.kiev.ua/images/knlu/about/slides/slide2.jpg','http://www.knlu.kiev.ua/images/knlu/about/slides/slide3.jpg','http://www.knlu.kiev.ua/images/knlu/about/slides/slide1.jpg','http://www.knlu.kiev.ua/images/knlu/about/slides/slide4.jpg'	
	]);
	var images_elements = document.getElements('#sp-slide98 img');

	var loader = Asset.images(images, {
		onProgress: function (i) {
			images_elements[i-1].src = images[i-1];
		},		
		onComplete: function () {
			document.getElement('.preload').style.visibility='hidden';
			spSlide98.play();
		}
	});
			spSlide98.addPlayerControls('previous', [$('prev98')]);
		spSlide98.addPlayerControls('next', [$('next98')]);
		
	spSlide98.addItemWalkers(walkers);		
	
});
//]]>
</script> 
