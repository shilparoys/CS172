  <script type="text/javascript">
// <![CDATA[
var flags = new Array();
//setUpFlagArray initializes array for flags based on size of database
function setUpFlagArray(){
flags[0] =0;flags[1] =0;flags[2] =0;flags[3] =0;flags[4] =0;flags[5] =0;flags[6] =0;flags[7] =0;flags[8] =0;flags[9] =0;}
//handleUpdateBuildCode updates map location from coordinates from ajax call
function handleUpdateBuildCode(){
	var http = buildTimer.getHttpObj();
	if (http.readyState == 4) {
		if (http.responseText.indexOf('invalid') == -1) {
			var data = http.responseText;
			if(data != 'none'){
				var temp = data.split('&&&');
				var coords = temp[2].split(',');
				moveToBuild(coords[0],coords[1]);
				updateTitle(temp[0],temp[1],temp[3]);
			}
			else{
				alert('Invalid building code.');
			}
    	}
	}
}
//updateBuildingCode retrieves building coordinates from database
function updateBuildingCode(){
	var code = document.getElementById('buildCode').value;
	if(code != ''){
		code = escape(code);
		var url = "ajaxBuildCode.php?myCode="+code;
		buildTimer.initTimer(url,handleUpdateBuildCode);
		buildTimer.startTimer();
//		http.open("GET", url, true);
//   		http.onreadystatechange = handleUpdateBuildCode;
//   		http.send(null);
	}
}
//updateBuildingView sends ajax call to building database
function updateBuildingView(){
	//database call to retrieve building coords
	var index = document.getElementById('buildID').value;
	if(index != 'blank'){
		var url = "ajaxBuildData.php?index="+index;
		buildTimer.initTimer(url,handleHttpResponseInfo);
		buildTimer.startTimer();
//		http.open("GET", url, true);
//   		http.onreadystatechange = handleHttpResponseInfo;
//   		http.send(null);
	}
	else{
	var oldZoom = zoom;
		zoom = 0;
		updateZoom(oldZoom,0,0);
	}
}
//handleHttpResponseInfo gets coordinates of building from database
function handleHttpResponseInfo(){
	var http = buildTimer.getHttpObj();
	if (http.readyState == 4) {
		if (http.responseText.indexOf('invalid') == -1) {
			var data = http.responseText.split('&&&');
			var coords = data[2].split(',');
			moveToBuild(coords[0],coords[1]);
			updateTitle(data[0],data[1],data[3]);
    	}
	}
}
function hideTitle(){
	document.getElementById('buildTitle').innerHTML = ' ';
}
function updateTitle(name,number,abrev){
	var title = document.getElementById('buildTitle');
	title.innerHTML = number+' - '+name+'  ('+abrev+')';
}
function setBuild(){
}
// ]]>
</script> 
