  <script type="text/javascript">
			var availableDates = [];
var events = [];

function showCalendar(spsId, siteId, name){
	$(".slidingDiv").slideUp(function(){
		$('#locationName').text(name);
	});
	$("#error-message").slideUp();
	$("#availableTimes").slideUp();

	$.getJSON("./getevents?spsId=" + spsId + "&siteId=" + siteId
		).done(function(eventList){
			//Success
			if(eventList.Error){
				$("#error-message").slideDown();
				return;
			}
			availableDates = [];
			events = [];
			for(var i = 0; i < eventList.Events.length; i++){
				availableDates[i] = eventList.Events[i].EventStart;
				events[i] = eventList.Events[i];
			}
			if(availableDates.length > 0){
				$('#inLineCal').datepicker('option', 'minDate', getFirstAvailable(availableDates[0]));
				$('#inLineCal').datepicker('setDate', getFirstAvailable(availableDates[0]));
				var firstDate = getFirstAvailable(availableDates[0]);
				showAppointments(getjQDate(firstDate), {
					selectedDay: firstDate.getDate(),
					selectedMonth: firstDate.getMonth(),
					selectedYear: firstDate.getFullYear()});
			}
			$('#inLineCal').datepicker("refresh");
			$(".slidingDiv").slideDown(function(){
				$('#inLineCal').datepicker("refresh");
			});
			
		}).fail(function(){
			//Failed to load
			$("#error-message").slideDown();
	});
}

function getFirstAvailable(firstDate) {
	var split = firstDate.split('-');
	var firstAvailable = new Date(split[2], split[1] - 1, split[0]);
	return firstAvailable;
}

function available(date) {
  var dayOfMonth = date.getDate();
  if(dayOfMonth < 10){
	  dayOfMonth = "0" + dayOfMonth;
  }
  var month = date.getMonth()+1;
  if(month < 10){
	  month = "0" + month;
  }
  dmy = dayOfMonth + "-" + month + "-" + date.getFullYear();
  if ($.inArray(dmy, availableDates) == -1) {
    return [false, "","Unavailable"];
  } else {
    return [true,"","Available"];
  }
}

function getjQDate(date) {
	var dayOfMonth = date.getDate();
	if(dayOfMonth < 10){
		dayOfMonth = "0" + dayOfMonth;
	}
	var month = date.getMonth()+1;
	if(month < 10){
		month = "0" + month;
	}
	dmy = month + "/" + dayOfMonth + "/" + date.getFullYear();
	return dmy;
}

function showAppointments(dateText, inst){
	$("#availableTimes").fadeOut(function(){
		$("#availableTimes tr:gt(1)").not(':last').remove();
		$("#scheduleDate").text(dateText);
		var dayOfMonth = inst.selectedDay;
		if(dayOfMonth < 10){
			dayOfMonth = "0" + dayOfMonth;
		}
		var month = inst.selectedMonth + 1;
		if(month < 10){
			month = "0" + month;
		}
		var chosenDate = dayOfMonth + "-" + month + "-" + inst.selectedYear;
		var eventIndex = $.inArray(chosenDate, availableDates);
		if (eventIndex > -1) {
			var availableTimesTable = $("#availableTimes");
		    var event = events[eventIndex];
		    var table = $("#appointmentsTable");
		    var half_way = Math.ceil(event.Appointments.length / 2);
		    for(var i = 0, k = half_way; i < half_way; i++){
		    	var appt = event.Appointments[i];
		    	var fortURL = '<a href="#" onClick="window.open(\'https://fort.fidelity.com/fortpros?PageDest=register';
		    	var appointment1 = fortURL + '&clientId=' + event.ClientId + '&eventTypeId=1&SiteId=' + appt.SiteId + '&PsEventId=' + event.EventId + '&PsEventTs=' + appt.AppointmentStartDay + '\');">' + appt.AppointmentBegin + ' - ' + appt.AppointmentEnd + '</a>';
		    	var appointment2 = '';
		    	if(k < event.Appointments.length){
			    	var appt2 = event.Appointments[k];
			    	var appointment2 = fortURL + '&clientId=' + event.ClientId + '&eventTypeId=1&SiteId=' + appt2.SiteId + '&PsEventId=' + event.EventId + '&PsEventTs=' + appt2.AppointmentStartDay + '\');">' + appt2.AppointmentBegin + ' - ' + appt2.AppointmentEnd + '</a>';
		    	}
		    	k++;
		    	table.append('<tr><td class="active-time">' + appointment1 + '</td><td class="active-time">' + appointment2 + '</td></tr>') ;
		    	table.append('<tr><td colspan="2"><div class="calendar-divider"></div></td></tr>') ;
		    	
		    }
		}
	});
	$("#availableTimes").fadeIn();
}

$(function(){
	$(".slidingDiv").hide();
	$("#error-message").hide();
	$("#availableTimes").hide();
	$('#inLineCal').datepicker({ beforeShowDay: available, onSelect: showAppointments });
})
		</script> 
