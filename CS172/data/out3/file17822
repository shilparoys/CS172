        <script type="text/javascript">
$(document).ready(function(){
	$("#emailSignupForm").submit(function(e){
		var selectedValues = [];
		$('.newsletter-item input:checked').each(function(i,input) {
			if($(input).val() !="more" && $(input).val() !="all"){
		    	selectedValues.push($(input).val().trim());
		    }
		});
		var selectedValues = selectedValues.join(',');
		console.log(selectedValues);
		var data = "txtAudienceGUID="+selectedValues+"&txtEmailAddress="+$("input[name='txtEmailAddress']").val();
		$.ajax({
			crossDomain:true,
			data:data,
			type: "POST",
			cache: false,
			url: "http://newsletters.buffalonews.com/templates/addToAudience.php",
			//error: function(xhr, status, error) {
			 // $("#display-response").html(error);
			//},
			success: function (msg){   
				if (msg == '<p>Fail: Invalid Email Address.</p>') {
					$("#display-response").html('<p class="nl-message nl-error">Please enter a valid email address.</p>');
				} else if (msg == '<p>Fail: Invalid AudienceGUID.</p>'){
					$("#display-response").html('<p class="nl-message nl-error">Please check the newsletters you\'d like.</p>');
				} else {
					$("#display-response").html('<p class="nl-message nl-success">You\'ve been subscribed.</p>');
				}
			}
		});
		return false;
	});
    $('#show-more-newsletter').change(function(){
        if(this.checked)
            $('#newsletter-more').fadeIn('fast');
        else
            $('#newsletter-more').fadeOut('fast');
    });
    $("#all-newsletter").click(function () {
		$(".newsletter-item input").prop('checked', $(this).prop('checked'));
	});
});
</script>
