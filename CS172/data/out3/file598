          <h3>Library Hours</h3> 
          <div id="success2"></div> 
          <div id="error2"></div> 
          <script type="text/javascript">
$(function(){ 
var webpath = window.location.href.replace("/academics/library.htm","").replace("#tab4","");

                $("#success2").load(webpath + "/datasets/hours/main-campus-hours.htm #library_hours", function(response, status, xhr) {
                  if (status == "error") {
                                    var msg = "Sorry but there was an error: ";
                                $("#error2").html(msg + xhr.status + " " + xhr.statusText);
                 }
});
});
</script> 
          <h3>Media Services Hours</h3> 
          <div id="success3"></div> 
          <div id="error3"></div> 
          <script type="text/javascript">
$(function(){ 
var webpath = window.location.href.replace("/academics/library.htm","").replace("#tab4","");
$("#success3").load( webpath + "/datasets/hours/main-campus-hours.htm #media_center_hours", function(response, status, xhr) {
						  if (status == "error") {
						    var msg = "Sorry but there was an error: ";
						    $("#error3").html(msg + xhr.status + " " + xhr.statusText);
						  }
						});
					});
</script> 
