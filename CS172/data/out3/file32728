       <div id="responsive-calendar"></div> 
       <script type="text/javascript">
     var today = new Date();
     var lowDate = new Date();
     var highDate = new Date();
     lowDate.setFullYear(lowDate.getFullYear(), lowDate.getMonth() - 24);
     highDate.setFullYear(highDate.getFullYear(), highDate.getMonth() + 24);
     var metaDate = $('meta[name=application-name]').attr("data-date");
     if (metaDate != null && metaDate.length == 10) {
         metaDate = metaDate.replace(/-/g,'');
     }
     $("#date-input").val("");
     
     datePickerController.createDatePicker({
     // Associate the text input to a DD/MM/YYYY date format                            
     formElements: { "date-input": "%m/%d/%Y" },
     noFadeEffect: true,
     labelText: "Go to date",
     cursorDate: metaDate,
     staticPos: true,
     fillGrid: false,
     constrainSelection: false,
     positioned: "responsive-calendar",
     callbackFunctions:{"datereturned":[gotoDate]}
        });

     datePickerController.setRangeLow("date-input", lowDate);
     datePickerController.setRangeHigh("date-input", highDate);


      function gotoDate(argObj) {
         var url = datePickerController.printFormattedDate(argObj.date, "/%Y/%F/%j", false);
         window.location = url;
      }
</script> 
