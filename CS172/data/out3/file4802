    $(document).ready(function () {
        $("#global_search_input_container").hoverIntent({
            interval: 0,
            over: revealFilter,
            timeout: 2000,
            out: hideFilter
        });

        /* FADE IN/OUT */
        function revealFilter() {
            $("#global_search_form").addClass("filter_active");
            $("#filter").fadeTo("fast", 1.0);
        }

        function hideFilter() {
            $("#global_search_form").removeClass("filter_active");
            $("#filter").fadeTo("fast", 0.0)
        }
    });
</script> 
          <!-- gubata added 1-17-14 had been removed for some reason --> 
         </div> 
         <!-- END SEARCH --> 
        </div> 
       </div> 
       <!-- END HEADER --> 
