     <script>
		start = 25;
		last = 37;
		obj = document.getElementById("photoSlider");
		setInterval(function() {slideShow()}, 5000);
		function slideShow(){
			if(obj){
				obj.src = "http://ikiu.ac.ir/images/homepage/ikiu_"+start+".jpg";
				start++;
				if(start > last)
					start = 1;
			}
		}
