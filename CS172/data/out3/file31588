   <script>$('.booknow-btn').on('click', function(event) {
                var $qRes = $(this).parents('.quickres-content');
                
                var d1 = $qRes.find('input[name="date1"]').val();
                    d1 = $.datepicker.formatDate('yy-mm-dd', $.datepicker.parseDate(dateFormat, d1));
                var d2 = $qRes.find('input[name="date2"]').val();
                    d2 = $.datepicker.formatDate('yy-mm-dd', $.datepicker.parseDate(dateFormat, d2));
    
                var parm = '&ArrivalDate=' + d1 + '&DepartureDate=' + d2 + '&PromoCode=' + $qRes.find("input[name='PromotionalCode']").val() +"&NumberOfRooms=1" + "&SubSource=361" + "&NumberOfAdults=1"
                    parm = parm.replace(/=undefined/g, '=');
                var tUrl = "https://be.cohores.net/10206?NextPage=room_list" + parm;
                
                $(this).attr('href', tUrl);
            });
            $('.booknow-btn').attr('href','https://be.cohores.net/10206?NextPage=room_list');

        </script> 
