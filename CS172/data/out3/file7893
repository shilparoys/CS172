  <script src="//search-platform.libapps.com/prepare.js?token=a8adc4e5e56c1a769ee839dc9cc96a7c"></script> 
  <script src="/web/search/underscore.js"></script> 
  <script src="/web/search/backbone.js"></script> 
  <script src="/web/search/search.js"></script> 
