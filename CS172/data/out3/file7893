  <script>
				 // Spring Tracking init
				springTrack.init({_st_site_id: '148'});
				solr_conf = {
					site_id: "148",
					guide_id: "0",
					solrUrl: "search-platform.libapps.com",
					solrIndex: "lg2",
					facets: {
						group: [],
						tag: [],
						content_type: [],
						guide_type: [],
						subject: []
					},
					page: 1,
					sort: { field: "score", dir: "desc" },
					lang: {
						Srch_NotAsked: "No results found.",
						Srch_Update: "Updated",
						Srch_SyntaxError: "Syntax Error: Quotation marks can only be used for phrase searching and must be in pairs.",
						Srch_ResRange: "Showing %1 (of %2)",
						Srch_PageLabel: "Pagination of search results",
						Srch_FilterSubject: "Assigned Subjects",
						Srch_FilterGroup: "In Group",
						Srch_FilterGType: "Guide Type",
						Srch_FilterCType: "Contains Content Type",
						Srch_FilterTag: "Assigned Tags",
						Srch_FacetNone: "None within current search terms.",
						Srch_FilterCurr: "Current Filters",
						Srch_FacetRemoveAll: "Remove All",
						Srch_FacetAll: "Viewing all",
						Srch_SortLabelRel: "Relevancy",
						Srch_SortLabelDate: "Date",
						Srch_SortHead: "Sort By",
						Srch_Subject: "Subjects",
						Srch_BoxHead: "results for: %1",
						Srch_By: "by"
					},
					perpage: 10,
					profile_url: "//libguides.camdencc.edu/prf.php?account_id=",
					//searchwt: { guide: 6, page: 5, subject: 4, tag: 4, allInOne: 1 },
					group_data: []
				}
				
				//set-up the search model
				var searchinterface = new SearchInterface(solr_conf);
				
				(function(window,undefined){
					var History = window.History; // Note: We are using a capital H instead of a lower h	
					// Bind to State Change
					History.Adapter.bind(window, "statechange", function(){
						//var State = History.getState(); // Note: We are using History.getState() instead of event.state
						//History.log("statechange:", State.data, State.title, State.url);
						searchinterface.retrieve();
					});
				})(window);
				
				jQuery(document).ready(function(){
					//set up all the views
					var sresults = new ResultsList({ model: searchinterface, el: jQuery("#s-lg-srch-results") });
					var sform = new SearchForm({ model: searchinterface, el: jQuery("#s-lg-srch-form") });
					var sheader = new SearchHeader({ model: searchinterface, el: jQuery("#s-lg-srch-header") });
					var sfacet_cur = new CurrentParamsView({  model: searchinterface, el: jQuery("#s-lg-srch-facet-current") });
					var sfacet_sort = new SortView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-sort") });
					var sfacet_g = new FacetView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-group"), facetType: "group" });
					var sfacet_t = new FacetView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-tag"), facetType: "tag" });
					var sfacet_ct = new FacetView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-content_type"), facetType: "content_type" });
					var sfacet_gt = new FacetView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-guide_type"), facetType: "guide_type" });
					var sfacet_s = new FacetView({ model: searchinterface, el: jQuery("#s-lg-srch-facet-subject"), facetType: "subject" });
					//retrieve the results
					if (true) {
						searchinterface.retrieve();
					}	
				});
				
				loadLibGuidesSearch=function() {
					//var q = "?q=" + jQuery("#s-lg-srch-cols iframe").contents().find("input[name=q]").val();
					document.location.href="/srch.php";
				}
				
				loadLibAnswersSearch=function(id) {
					jQuery(".s-lg-srch-nav-btn").removeClass("active");
					jQuery("#s-lg-srch-site-" + id + "-btn").addClass("active");
					jQuery("#s-lg-srch-local").toggle(false);
					jQuery("#s-lg-srch-cols").html("<div class=\"bold s-lib-color-lt-grey pad-top-med text-center\">Loading...</div>");
					
					xhr = jQuery.ajax({
						url: "srch_process.php",
						type: "POST",
						dataType: "json",
						data: {
							action: 570,
							site_id: id, 
							query: jQuery("#s-lg-srch-input-q").val()
						},
						success: function(response, textStatus) {
							if ( response.errCode == 200 ) {
								jQuery("#s-lg-srch-cols").html(response.data.html);
							} 
						},
						error: function (jqXHR, textStatus, errorThrown) {
							alert("Oops, sorry! Something unexpected happened: " + errorThrown + " \n\nThat might not mean much to you, but it probably does " + 
							"to the Springy Techs...you can let them know at support@springshare.com.");
						}
					});	
				}
			</script> 
