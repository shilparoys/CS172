		table#mainTable td#footer a:link, table#mainTable td#footer span.notlink {
			font-family: Arial, Helvetica, sans-serif;
			font-size: 0.8em;
			color: #FFFFFF;
			font-weight: normal;
			text-decoration: none;
		}
