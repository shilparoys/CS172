            <script type="text/javascript">
				//<![CDATA[
					function SetChecked(val) {
						dml= document.frmOpenReq;
						len = dml.elements.length;
						var i=0;
						for (i=0; i<len; i++) {
							if (dml.elements[i].name=='chkCategoryID') {
								dml.elements[i].checked=val;
								}
							}
					 	}
					function checkCategories() {
						var isChecked = 0;

						if (!document.frmOpenReq.chkCategoryID.length) {
							if (document.frmOpenReq.chkCategoryID.checked == true) {
								isChecked = 1;
							}
						} else {
							for (i=0; i<document.frmOpenReq.chkCategoryID.length; i++) {
								if (document.frmOpenReq.chkCategoryID[i].checked == true) {
									isChecked = 1;
									i = document.frmOpenReq.chkCategoryID.length;
								}
							}
						}
						if (isChecked == 0) {
							alert("Please choose at least one category!");
							return false;
						} else {
							return true;
						}
					}
				//]]>
			</script> 
