        <script type="text/javascript">
			function launchWindow(u) {
			document.getElementById("bug-options-mkt").action = u;
			document.getElementById("bug-options-mkt").submit();
			}
			$(".market-motion").click(
				function(event) {
					event.preventDefault();
					_gaq.push(['_trackEvent', 'Mkt Widget - Simple', 'Click', $(event.target).text()]);
					var id = $(event.target).attr("href").substring(1);
					$(".large-holder").hide();
					$("#"+id).show();
					$("#container-holder-long").animate(
						{
						"left" : "-300px" 
						}, 600
					);
				}
			);
			$(".close-it").click(
				function() {
					$("#container-holder-long").animate(
						{
							"left" : "0"		
						}, 600
					);
				}
			);
			$('.click_this').click(function() {
				_gaq.push(['_trackEvent', 'Mkt Widget - Simple', 'Click', 'Bug cats'])
				$('#bug-market-categories').slideToggle('slow');
				return false;
			});
			$('.close-it').click(function() {
				$('#bug-market-categories').slideUp('slow');
				return false;
			});
			</script> 
