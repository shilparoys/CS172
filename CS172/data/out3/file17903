         <div id="defaultCommunityControl" style="display:none;"> 
          <input type="checkbox" id="defaultCommunity" name="defaultCommunity" value="defaultCommunity"> 
          <label style="display:inline;font-style:italic;color:#9E0700;" for="defaultCommunity">Make my default community.</label> 
         </div> 
        </form> 
        <script type="text/javascript">
		// if select changed
        $('#communities').change(function() {
			if($('select#communities').val() == 'select') {
				$('#defaultCommunityControl').hide();
				$('.stories').hide();
			} else {
				$('#defaultCommunityControl').show();
				$('.stories').show();
			}
			var tbn_community = $.cookies.get('tbn_community'); 
			if(tbn_community == $('#communities').val()) {
				$('#defaultCommunity').attr('checked','checked');
				$("label[for='defaultCommunity']").text("Default community");
			} else {
				$('#defaultCommunity').removeAttr('checked');
				$("label[for='defaultCommunity']").text("Make my default community");
			}
			// get the value and load headlines via AJAX
            var profileID = $('select#communities').val();
            if(profileID != 'select') {
                $.ajaxSetup({cache: false})
				var ajax_load = "<img src='/images/ajax-loader.gif' alt='loading...' />";  
				var loadUrl = '/section/communityWidgetHeadlines/?profileID=' + profileID;  
				$(".stories").html(ajax_load).load(loadUrl);  
            }
        });
		// handle default community checkbox click
		$("#defaultCommunity").click(function() {
			//test if browser is accepting cookies
			if(!$.cookies.test())
			{
			  alert('You must have cookies enabled in order to use this feature.');
			}
			var community = $('select#communities').val();
			var communityName = community.substring(15);
			communityName = communityName.replace('%20', ' ');
			// exit if user tries to set select as default community
			if(communityName == '') {
				return false; // exits callback function
			}
			// set or delete cookie
			if($(this).is(":checked")) {
				var options = {
					path: '/',
					expiresAt: new Date( 2099, 1, 1 )
  				}
				$.cookies.set('tbn_community', community, options);
				$("label[for='defaultCommunity']").text("Default community");
			} else {
				$.cookies.del('tbn_community');
				$("label[for='defaultCommunity']").text("Make my default community");
			}
		});
		$(document).ready(function() {
			// check for tbn_community cookie
		  	var tbn_community = $.cookies.get('tbn_community'); 
			if(tbn_community != null) {
				$.ajaxSetup({cache: false})
				var ajax_load = "<img src='/images/ajax-loader.gif' alt='loading...' />";  
				var loadUrl = '/section/communityWidgetHeadlines/?profileID=' + tbn_community;  
				$(".stories").html(ajax_load).load(loadUrl); // load headlines
				$('#defaultCommunity').attr('checked','checked'); 
				$("label[for='defaultCommunity']").text("Default community");
				$('#communities').val(tbn_community);
			}  
			if($('select#communities').val() == 'select') {
				$('#defaultCommunityControl').hide();
			} else {
				$('#defaultCommunityControl').show();
			}
		});
	</script> 
        <div class="stories"> 
        </div> 
