    <script type="text/javascript">
(function() {
  var gRecordFormName = 'record';

  

  var gLengthVerifier = function(e) {
    var result = check4KLengths(gRecordFormName);
    if (! result && e && e.preventDefault) {
      e.preventDefault();
    }
    return result;
  };

  var gLengthInit = function() {
    var form = document.forms[gRecordFormName];
    if (form) {
      if (form.addEventListener) {
        form.addEventListener("submit", gLengthVerifier, true);
      } else if (form.attachEvent) {
        form.attachEvent("onsubmit", gLengthVerifier);
      }
    }
  }

 if (window.addEventListener) {
    window.addEventListener('load', doFormLoad, false);
    window.addEventListener('load', gLengthInit, false);
  } else if (window.attachEvent) {
    window.attachEvent('onload', doFormLoad);
    window.attachEvent('onload', gLengthInit);
  }
})();
</script> 
