  <script type="text/javascript">
// JavaScript Document
$(document).ready(function() {
    equalHeight($("#sideNav,#content"));

});

function equalHeight(group) {
    tallest = 0;
    group.each(function() {
        thisHeight = $(this).height();
        if(thisHeight > tallest) {
            tallest = thisHeight;
        }
    });
    group.height(tallest);
}


</script> 
