//setup DFP ads

window.location.getParam = function (key) {
    var vars = [], hash;
    var q = this.search.substring(1).split('&');

    for (var i=0; i< q.length; i++) {
        hash = q[i].split('=');
        vars[hash[0]] = hash[1];
    }
    return vars[key];
};


if (typeof googletag === 'undefined') {

    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    (function() {
        var gads = document.createElement('script');
        gads.async = true;
        gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') +'//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();

}

var adSlots = {}, __ad_server = "8264";

googletag.cmd.push(function() {


    if (interstitial) adSlots.interstitial = googletag.defineOutOfPageSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, interstitial_id).addService(googletag.pubads());
    if (leader_plus_top && page_ptid_nodeid == '2000:1') { adSlots.leader_plus_top = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [[728,90],[970,66],[970,250]], leader_plus_top_id).addService(googletag.pubads()).setTargeting("pos", "top")};

    if (leader_top) { adSlots.leader_top = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [[728,90],[970,66]], leader_top_id).addService(googletag.pubads()).setTargeting("pos", "top")};if (marquee_top) { adSlots.marquee_top = adSlots["marquee_top"] = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [234, 60], marquee_top_id).addService(googletag.pubads()).setTargeting("pos", "top")};if (mpu_top) { adSlots.mpu_top = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [300, 250], mpu_top_id).addService(googletag.pubads()).setTargeting("pos", "top")}; if (mpu_plus_top) { adSlots.mpu_plus_top = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [[300,250],[300,600],[300,1050]], mpu_plus_top_id).addService(googletag.pubads()).setTargeting("pos", "top")};
    if (skin_top) { adSlots.skin_top = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [1600,1000], skin_top_id).addService(googletag.pubads()).setTargeting("pos", "top")};

    //front door override

    if (leader_middle) { adSlots.leader_middle = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [970, 66], leader_middle_id).addService(googletag.pubads()).setTargeting("pos", "middle")};

    if (magnet_bottom) { adSlots.magnet_bottom = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [468, 60], magnet_bottom_id).addService(googletag.pubads()).setTargeting("pos", "bottom")};    if (mpu_bottom) { adSlots.mpu_bottom = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [300, 250], mpu_bottom_id).addService(googletag.pubads()).setTargeting("pos", "bottom")};if (leader_bottom) { adSlots.leader_bottom = googletag.defineSlot("/" + __ad_server + "/aw-cbs/" + utag_data.path, [[728,90],[970,66]], leader_bottom_id).addService(googletag.pubads()).setTargeting("pos", "bottom")};

    googletag.pubads().setTargeting("ptype", DW_ptid.toString());
    googletag.pubads().setTargeting("node", onidFinal.toString());
    googletag.pubads().setTargeting("session", getAdvCookie('CBS_ADV_VAL'));
    googletag.pubads().setTargeting("sb", "0");
    googletag.pubads().setTargeting("vguid", DW.pageParams.pguid);
    googletag.pubads().setTargeting("firstpg", ((document.cookie.indexOf("first_page_today") == -1 || (typeof firstpgvar != 'undefined' && firstpgvar == 1))?"1":"0") );

    
    try {

        if (window.location.getParam('ftag')) {
            googletag.pubads().setTargeting("ftag", window.location.getParam('ftag'));
        } else {
            if (window.location.hash) {
                if (location.hash.split('ftag=') != "") {
                    googletag.pubads().setTargeting("ftag", window.location.hash.split('ftag=')[1].split('&')[0]);
                }
            }
        }

    } catch (e) {
    }



    
    if ((!displayUnitsEnabled) || disableInitialLoadFlag) googletag.pubads().disableInitialLoad();

    googletag.pubads().enableAsyncRendering();
    googletag.pubads().enableSingleRequest();
    googletag.pubads().collapseEmptyDivs(true);
    googletag.enableServices();
});

    function blankAdCallback(evt) {
        if (evt === '234' && adChunk2.length > 0) {
            googletag.pubads().refresh(adChunk2);
        }
    };

    var adChunk1 = [];
    var adChunk2 = [];

    googletag.cmd.push(function () {

        if (disableInitialLoadFlag) {

            if (interstitial) {
                adChunk1.push(adSlots['interstitial']);
            }

            if (leader_top) adChunk1.push(adSlots['leader_top']);
            if (leader_plus_top) adChunk1.push(adSlots['leader_plus_top']);
            if (marquee_top) adChunk1.push(adSlots['marquee_top']);
            if (mpu_top) {
                adChunk1.push(adSlots['mpu_top']);
            }
            if (mpu_plus_top) adChunk1.push(adSlots['mpu_plus_top']);
            if (skin_top) adChunk1.push(adSlots['skin_top']);
            if (leader_middle) adChunk1.push(adSlots['leader_middle']);
            if (magnet_bottom) adChunk1.push(adSlots['magnet_bottom']);
            if (mpu_bottom) adChunk2.push(adSlots['mpu_bottom']);
            if (leader_bottom) adChunk2.push(adSlots['leader_bottom']);

            if(adChunk1.length>0) googletag.pubads().refresh(adChunk1);
        }


    });


    if (interstitial) googletag.cmd.push(function() {googletag.display(interstitial_id);});
    if (leader_top) googletag.cmd.push(function() { googletag.display(leader_top_id);});
    if (leader_plus_top) googletag.cmd.push(function() { googletag.display(leader_plus_top_id);});
    if (marquee_top) googletag.cmd.push(function() { googletag.display(marquee_top_id);});
    if (mpu_top) googletag.cmd.push(function() { googletag.display(mpu_top_id);});
    if (mpu_plus_top) googletag.cmd.push(function() { googletag.display(mpu_plus_top_id);});
    if (skin_top) googletag.cmd.push(function() {googletag.display(skin_top_id);});

    if (leader_middle) googletag.cmd.push(function() { googletag.display(leader_middle_id);});
    if (magnet_bottom) googletag.cmd.push(function() {googletag.display(magnet_bottom_id);});

    if (mpu_bottom) googletag.cmd.push(function() { googletag.display(mpu_bottom_id);});
    if (leader_bottom) googletag.cmd.push(function() { googletag.display(leader_bottom_id);});

    googletag.cmd.push(function () {
        if (isRecommendedGalleries) {
            if (adChunk2.length > 0) {
                googletag.pubads().refresh(adChunk2);
                adChunk2 = [];
            }
        }
    });


    if (leader_top) {
        $("#leader-top").css("padding","10px 0")
    }

