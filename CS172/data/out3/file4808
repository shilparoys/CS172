                 </tbody>
                </table> 
                <input name="ctl00$PCSearchPart$SearchBox$ctl05" type="hidden" id="ctl00_PCSearchPart_SearchBox_ctl05" value="0"> 
               </div>
              </div>
             </div></td> 
           </tr> 
          </tbody>
         </table> 
        </div> 
        <!-- add in   --> 
        <!-- FILTER BOX --> 
        <script type="text/javascript">
    $(document).ready(function () {
        $("#global_search_input_container").hoverIntent({
            interval: 0,
            over: revealFilter,
            timeout: 2000,
            out: hideFilter
        });

        /* FADE IN/OUT */
        function revealFilter() {
            $("#global_search_form").addClass("filter_active");
            $("#filter").fadeTo("fast", 1.0);
        }

        function hideFilter() {
            $("#global_search_form").removeClass("filter_active");
            $("#filter").fadeTo("fast", 0.0)
        }
    });
</script> 
       </div> 
      </div> 
