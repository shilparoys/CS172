             <div class="panel-pane pane-node"> 
              <h2 class="pane-title">About Campus Advisories</h2> 
              <div class="pane-content"> 
               <div class="node node-general-content node-teaser clearfix" about="/about-campus-advisories" typeof="sioc:Item foaf:Document" id="node-742"> 
                <div class="node-content clearfix "> 
                 <div class="field field-name-body field-type-text-with-summary field-label-hidden prose">
                  <div class="field-items">
                   <div class="field-item even" property="content:encoded">
                    <p>Maintained by the Office of Emergency Management, Campus Advisories is the university's primary website used for communicating emergency preparedness and incident-related information to the GW community.</p> 
                   </div>
                  </div>
                 </div> 
                </div> 
                <div class="node-links clearfix"> 
                </div> 
               </div> 
              </div> 
             </div> 
             <div class="panel-separator"></div>
             <div class="panel-pane pane-custom pane-5"> 
              <h2 class="pane-title">Metro Status</h2> 
              <div class="pane-content"> 
               <style type="text/css">
<!--/*--><![CDATA[/* ><!--*/

  #metro {
    font-family:"Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 18px;
    width: 215px;
  }
  #metro .wrap {
    margin-bottom: 15px;
  }
  #metro ul.INlist, #metro ul.Ellist {
    list-style-type: none;
    padding: 0px;
    margin: 0px;
    margin-bottom: 10px;
  }
  #metro ul.INlist li.lab, #metro ul.Ellist li.lab {
    width:140px;
    line-height: 18px;
    text-align: left;
    font-weight: bold;
    color:#666;
    font-size: 12px;
  }
  #metro ul.INlist li.items, #metro ul.Ellist li.items {
    font-size: 12px;
    color:#666;
    padding:4px 0px;
    width:100%;
  }
  #metro ul, p {
    font-size: 12px;
  }
  #metro .stationdetails a {
    display: inline-block;
    width: 100%;
    font-size: 12px;
    font-style: italic;
  }
  #metro .asof, #metro #timeNowMetro {
    font-size:11px;
    font-style: italic;
    color:#666;
  }
  #metro h5 {
    width: 100%;
    line-height: 22px;
    color: #2972A7;
    margin-bottom: 3px;
    font-size: 12px;
    font-weight: bold;
  }
  #metro h6 {
    width: 100%;
    line-height: 20px;
    color: #666;
    padding: 4px 0px;
    margin-bottom: 0px;
    font-size: 12px;
    font-weight: normal;
  }

/*--><!]]>*/
</style>
               <script type="text/javascript">
<!--//--><![CDATA[// ><!--

  function rightNowM() {
    var dm = new Date();
    var nt = dm.toString();
    jQuery('#timeNowMetro').html(nt);
  };

  function formatJsonDate(jsonDate) {
    return (new Date(parseInt(jsonDate.substr(6))));
  }

  function getIncidentsLine(indata) {
    var line = [];
    jQuery.each(indata, function() {
      jQuery.each(this, function(p) {
        searchline = this.LinesAffected;
        if (searchline.search("BL") >= 0 || searchline.search("OR") >= 0) {
          line.push(p);
        };
      });
    });
    return line;
  };

  function jsonToHtml(eldata) {
    var item = [];
    if (eldata.ElevatorIncidents.length > 0) {
      jQuery('#noEldata').hide();
      jQuery.each(eldata, function() {
        jQuery.each(this, function(i) {
          item.push('<ul class="Ellist">' + '<li class="lab">Date Updated<li class="items">' + formatJsonDate(this.DateUpdated) + '' + '<li class="lab">Equipment Type<li class="items">' + this.UnitType + '' + '<li class="lab">Location Description<li class="items">' + this.LocationDescription + '' + '<li class="lab">Reason<li class="items">' + this.SymptomDescription + '<\/ul>');
        });
      });
      jQuery('#yesEldata').html(item.join(''));
    }
  }

  function getElevatorData() {
    jQuery.ajax({
      url: 'http://api.wmata.com/Incidents.svc/json/ElevatorIncidents?StationCode=C04&api_key=nssnqudys4g27vhcjgrkda4u',
      dataType: 'jsonp',
      jsonpCallback: 'jsonToHtml',
      data: 'eldata',
      success: function() {},
      error: function() {}
    });
  }

  function jsonToHtmlIncident(indata) {
    var tstation = [];
    var tline = [];
    var it = getIncidentsLine(indata);
    if (it.length > 0) {
      for (var x = 0; x < it.length; x++) {
        p = it[x];
        if ((indata.Incidents[p].IncidentType == "Station Incident") && (indata.Incidents[p].StartLocationFullName == "Foggy Bottom" || indata.Incidents[p].EndLocationFullName == "Foggy Bottom")) {
          jQuery('#noIncidentStationData').hide();
          tstation.push('<ul class="INlist">');
          tstation.push('<li class="lab">Date Updated<li class="items">' + formatJsonDate(indata.Incidents[p].DateUpdated) + '' + '<li class="lab">Description<li class="items">' + indata.Incidents[p].Description + '');
          if (indata.Incidents[p].EmergencyText != null) {
            tstation.push('<li class="lab">EmergencyText<li class="items">' + indata.Incidents[p].EmergencyText + '');
          }
          tstation.push('<\/ul>');
        } else {
          jQuery('#noIncidentLineData').hide();
          tline.push('<ul class="INlist">' + '<li class="lab">Date Updated<li class="items">' + formatJsonDate(indata.Incidents[p].DateUpdated) + '' + '<li class="lab">Incident Type<li class="items">' + indata.Incidents[p].IncidentType + '');
          if (indata.Incidents[p].DelaySeverity != null) {
            tline.push('<li class="lab">Delay Severity<li class="items">' + indata.Incidents[p].DelaySeverity + '');
          }
          if (indata.Incidents[p].PassengerDelay > 0) {
            tline.push('<li class="lab">Passenger Delay<li class="items">' + indata.Incidents[p].PassengerDelay + '');
          }
          tline.push('<li class="lab">Description<li class="items">' + indata.Incidents[p].Description + '');
          if (indata.Incidents[p].EmergencyText != null) {
            tline.push('<li class="lab">EmergencyText<li class="items">' + indata.Incidents[p].EmergencyText + '');
          }
          if (indata.Incidents[p].StartLocationFullName != null) {
            tline.push('<li class="lab">Start Location<li class="items">' + indata.Incidents[p].StartLocationFullName + '');
          }
          if (indata.Incidents[p].EndLocationFullName != null) {
            tline.push('<li class="lab">End Location<li class="items">' + indata.Incidents[p].EndLocationFullName + '');
          }
          tline.push('<\/ul>');
        }
      }
      jQuery('#yesIncidentStationData').html(tstation.join(''));
      jQuery('#yesIncidentLineData').html(tline.join(''));
    }
  }

  function getIncidentData() {
    jQuery.ajax({
      url: 'http://api.wmata.com/Incidents.svc/json/Incidents?api_key=nssnqudys4g27vhcjgrkda4u',
      dataType: 'jsonp',
      jsonpCallback: 'jsonToHtmlIncident',
      data: 'indata',
      success: function() {},
      error: function() {}
    });
  }

//--><!]]>
</script>
               <div id="metro"> 
                <div class="wrap"> 
                 <h5>Foggy Bottom Metro Station</h5> 
                 <div id="noIncidentStationData"> 
                  <h6>No station incidents at this time.</h6> 
                 </div> 
                 <div id="yesIncidentStationData"></div> 
                 <p><span class="stationdetails"><a target="_blank" href="http://www.wmata.com/rail/station_detail.cfm?station_id=40">Station Details</a></span> </p>
                </div> 
                <div class="wrap"> 
                 <h5>Blue/Orange lines</h5> 
                 <div id="noIncidentLineData"> 
                  <h6>There are no line incidents reported at this time.</h6> 
                 </div> 
                 <div id="yesIncidentLineData"></div> 
                </div> 
                <div class="asof">
                 This Information is current as of:
                </div> 
                <div id="timeNowMetro"></div> 
                <div class="asof">
                 Updated at 10 min interval.
                </div> 
               </div> 
               <script type="text/javascript">
<!--//--><![CDATA[// ><!--

  $(document).ready(function() {
    var updateDateM = rightNowM();

    var indata = getIncidentData();
  });

//--><!]]>
</script>
               <script type="text/javascript">
<!--//--><![CDATA[// ><!--

  var timerM = setInterval(function() {
    updateDateM = rightNowM();

    indata = getIncidentData();
  }, 590000);

