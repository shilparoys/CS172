             <div class="panel-pane pane-custom pane-2"> 
              <h2 class="pane-title">Weather Alerts</h2> 
              <div class="pane-content"> 
               <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script> 
               <script type="text/javascript">
function rightNow() {
  var d = new Date();
  var z = d.toString();
  jQuery('#timeNowa').html(z);
}
</script> 
               <script type="text/javascript">
(function (e) {
    e.fn.weatherAlert = function (t) {
        var n = {
            FeedUrl: "https://ajax.googleapis.com/ajax/services/feed/load?v=1.0",
            MaxCount: 5,
            ShowDesc: true,
            ShowPubDate: true,
            CharacterLimit: 0,
            TitleLinkTarget: "_blank"
        };
        if (t) {
            e.extend(n, t)
        }
        var r = e(this).attr("id");
        var i;
        var noalert = new Boolean(false);
        e("#" + r).empty().append('<div style="padding:3px;"><img src="/files/image/loader.gif" /></div>');
        e.ajax({
                url: "https://ajax.googleapis.com/ajax/services/feed/load?v=1.0&num=" + n.MaxCount + "&output=json&q=" + encodeURIComponent(n.FeedUrl) + "&hl=en&callback=?",
                dataType: "json",
                success: function (t) {
                    e("#" + r).empty();
                    var s = "";
                    e.each(t.responseData.feed.entries, function (e, t) {
                    if (t.title.indexOf("There are no") != -1) {
                     noalert = true;
                    };
                        s += '<a href="' + t.link + '" target="' + n.TitleLinkTarget + '" >' + t.title + '<\/a>';
                        i = new Date(t.publishedDate);
                        s += '<div class="itemDate">' + i + '<\/div>';
                    });
                    if (noalert == true ) {
                         s = '<span class="noalert">No alerts at this time.</span>';
                    }
                     e("#" + r).append('<div class="itemList">' + s + '<\/div>');
                }
            })
    }
})
(jQuery);
</script> 
               <script type="text/javascript">
    jQuery(document).ready(function () {
       updateDate = rightNow();
         jQuery('#feed1').weatherAlert({
            FeedUrl: 'https://alerts.weather.gov/cap/wwaatmget.php?x=DCC001&y=0',
            MaxCount: 100,
            ShowDesc: false
        });
         jQuery('#feed11').weatherAlert({
            FeedUrl: 'https://alerts.weather.gov/cap/wwaatmget.php?x=DCZ001&y=0',
            MaxCount: 100,
            ShowDesc: false
        });
         jQuery('#feed2').weatherAlert({
            FeedUrl: 'https://alerts.weather.gov/cap/wwaatmget.php?x=VAC107&y=0',
            MaxCount: 100,
            ShowDesc: false
        });
        

    });
    
    
</script> 
