        <script>
  $("input#resume:file").change(function (){
    $('#contact_create_form').ajaxSubmit({
      type: "POST",
      url: "/slick",
      dataType: "json",
      beforeSend: function(){
        $('.upload_resume_slick_apply, .mobileApplyHeader').fadeOut('fast', function() {
            $("#first_progress_bar").fadeIn();
            $('.progress .progress-bar').progressbar({display_text: 'center'});
            setTimeout(function() {
              $(".progressbar-front-text").replaceWith("<i class='fa fa-spin fa-spinner blackText font15 mt7'></i>");
            }, 1300);
        });
      },
      success: function(res, statusText, xhr, jqForm) {
        $('#name').val(res.name);
        $('#email_address').val(res.email);
        $('#phone_number').val(res.phone);
        if (res.name != '' && res.email != '' && res.phone != '') {
          window.supressSpinner = true;
          $('#progress_message').html('Finishing up...');
          $('input[name=submitted]').click();
        }
        else {
          $('input[name=name],input[name=email_address],input[name=phone_number]').valid();
          $("#first_progress_bar").fadeOut("fast", function(){
            $('.finish_header').show();
            $('#contact_create_form').show();
            
              $('.terms_copy').show();
                        $('#contactInformationWrap').show();
            $('#contact-submit-block').show();
            $('#photoWrap').show();
          });
        }
      },
      error: function(error) {
        console.log(error);
      }
    });

    var fileName = $(this).val();
    $(".filename").html(fileName);
  });

    function callPop() {
    setTimeout(function() {
      $(function() {
        $('body').mousemove(function( event ) {
          var clientY = event.clientY;
          var clientX = event.clientX;
          if ( clientY < 8 && ! $.cookie('test_3000')) {
          $( "#createAlerteLink" ).trigger( "click" );
            $.cookie('test_3000', 'XasS8gQ', { path: '/' });
          } else {

          }
        });
      });
    }, 1000);
  };
          function callPopImmediate() {
        $(function() {
          setTimeout(function() {
            if (! ($.cookie('previously_registered_pop') || $.cookie('immediate_pop') == 'dhjSJdhsk' || $.cookie('job-seeker-overlay-test') == 'active')) {
              $("#createAlerteLink").trigger("click");
              $.cookie('immediate_pop', 'dhjSJdhsk', { expires: 1, domain: 'ziprecruiter.com', path: '/' });
            }
          }, 1000);
        });
      };
              callPopImmediate();
              //Allow us to trigger 3rd party btns (called within each include)
  function getClickables(container) {
    var allElems = $(container).find('*'),
    elems = allElems.filter(function(index){
      var events = $._data(this, "events");
      return events && (events.click || events.mousedown);
    });
    return elems;
  }

