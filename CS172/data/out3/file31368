             <script>
window.condition_queue = {
  queue: [],
  timeout: 250,
  limit: 100,
  register: function(condition, callback, limit) {
    limit = limit ? limit : this.limit;
    if (typeof(condition) === 'function' && typeof(callback) === 'function') {
      this.queue.push([condition, callback, limit, 0]);
      clearInterval(this.interval_id);
      var queue = this.queue;
      var interval_id = setInterval(function() {
        _queue = [];
        _.each(queue, function(condition_array) {
          condition_array[3]++;
          if (condition_array[0]()) {
            condition_array[1]();
          } else {
            if (condition_array[2] > condition_array[3]) {
              _queue.push(condition_array);
            }
          }
        });
        queue = _queue;
        if (!queue.length) {
          clearInterval(interval_id);
        }
      }, this.timeout);
      this.interval_id = interval_id;
    }
  }
}
</script> 
