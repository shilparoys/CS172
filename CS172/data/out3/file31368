$('body').on('click', '#steptwo', function(e) {
  e.preventDefault();
  
  if ($('input[name=name],input[name=email_address],input[name=phone_number]').valid()) {
      MobileApplyManager.triggerSection(EXPERIENCE_SECTION);
      $.ajax({
        url: '/mobile_apply_job_alert/35d80230',
        datatype: 'json',
        type: 'POST',
        data: {
          name: $('input[name=name]').val(),
          email_address: $('input[name=email_address]').val(),
          phone_number: $('input[name=phone_number]').val()
        }
      });
      if (window.ga) {
        ga('send', {
          'hitType': 'event',
          'eventCategory': 'jobseeker-mobile',
          'eventAction': 'application-step2'
        });
      }
  }
  return false;
});

$('body').on('click', '#add-another-position', function(e) {
  e.preventDefault();
  MobileApplyManager.triggerSection(EXPERIENCE_SECTION);
});

$('body').on('click', '.last-step', function(e) {
  e.preventDefault();
  if (window.ga) {
    ga('send', {
      'hitType': 'event',
      'eventCategory': 'jobseeker-mobile',
      'eventAction': 'application-step3'
    });
  }
  if(MobileApplyManager.triggerSection(EXPERIENCE_SECTION, {lastSection: true}) ) {
    MobileApplyManager.triggerSection(EDUCATION_SECTION);
  }
});

$('body').on('click', '#add-another-school', function(e) {
  e.preventDefault();
  MobileApplyManager.triggerSection(EDUCATION_SECTION);
});




