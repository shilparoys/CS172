             <script>
function show_upload() {
  $('#upload_resume').show();
  $('#text_resume').hide();
}

function show_text() {
  $('#upload_resume').hide();
  $('#text_resume').show();
}

function init_resume_fields() {
  $('#text_resume_link').on('click',function() {
    show_text();
    $('#attach_resume_label').hide();
    $('#text_resume_label').show();
  });
  $('#upload_resume_link').on('click',function() {
    show_upload();
    $('#text_resume_label').hide();
    $('#attach_resume_label').show();
  });
}


var _MobileApplyManager = function(formSelector){
  var sectionRegistry = {},

  execSectionTransition = function mobileTrans() {
    $('html,body').scrollTop($(formSelector).offset().top);
  },

  saveEntry = function(section) {
    var fields = section.fields,
    saveObj = {};
    for (var i=0, fieldsLen = fields.length; i < fieldsLen; i++) {
      var fieldName = fields[i].name;
      saveObj[fieldName] = $('#'+fieldName).val();
    }
    section.contentArr.push(saveObj);
  },

  sectionIsEmpty = function(section){
    var fields = section.fields;

    for (var i=0, fieldsLen = fields.length; i < fieldsLen; i++) {
      var fieldName = fields[i].name;
      if ($('#'+fieldName).val() != "") {
        return false;
      }
    }
    return true;
  },

  genConcatSectionText = function(s) {
    if (!s) {
      throw "MobileApplyManager->genConcatSectionText: No section type registered for " + s.name;
      return;
    }
    var text = "\n" + s.name + ":\n\n";

    var allContent = s.contentArr;

    for (var i=0, allContentLen = allContent.length; i < allContentLen; i++) {
      var secObj = allContent[i];
      for (prop in secObj) {
        var val = secObj[prop];
        if (val == "") {
          continue;
        }

        if (s.custom_output && typeof s.custom_output[prop] == "function") {
          text += s.custom_output[prop](val);
        }
        else {
          text += val + "\n";
        }
      }
      text += "\n";
    }
    text += "\n";
    return text;
  };

  return {
    createSectionType: function(params) { // {name, fieldArr, uiUpdateFct}
      var s = sectionRegistry[params.name] = {};// should fix this to use params object and canonical names

      s.name        = params.name;
      s.contentArr  = [];
      s.fields      = params.fieldArr;
      s.uiUpdate    = params.uiUpdateFct;
      s.count       = 0;
      s.validate    = params.validate;
      s.uiUpdateFct = params.uiUpdateFct;
      s.custom_output      = {};

      var fields = s.fields;

      for (var i=0; i < fields.length; i++) {
        var field = fields[i];
        if (field.custom_output && typeof field.custom_output == "function") {
          s.custom_output[field.name] = field.custom_output;
        }
      }
      return params.name;
    },

    triggerSection: function(type, params) { //params->{lastSection}
        var s = sectionRegistry[type],
        params = params || {},
        last = params.lastSection;
        if (!s) {
          throw "MobileApplyManager->triggerSection: No section type registered for " + type;
          return false;
        }

        var no_validate = (s.count == 0) || (sectionIsEmpty(s) && last && s.count > 1);

        if (!no_validate && !s.validate()) {
          return false;
        }
        s.count++;
        if (s.count > 1) {
          saveEntry(s);
        }

        if (!params.lastSection) {
          execSectionTransition();
          s.uiUpdateFct(s);
        }
        return true;
    },

    getTextResume: function() {
      var text = "";
      text += $('#name').val() + "\n" + $('#email_address').val() + "\n" + $('#phone_number').val() + "\n\n";

      var summary = $('#summary_text').val();
      if (summary != "") {
        text += "\nExecutive Summary:\n\n" + summary + "\n\n";
      }

      for (var s in sectionRegistry) {
        text += genConcatSectionText(sectionRegistry[s]);
      }

      return text;

    }
  }
};
var MobileApplyManager = new _MobileApplyManager('#inlineApplyForm');


var EXPERIENCE_SECTION = MobileApplyManager.createSectionType({
  name: "Experience",
  fieldArr: [
    {name: "companyname"},
    {name: "jobtitle"},
    {name: "startdatemonth", custom_output: function(val){
      return "Dates: " + val + "/";
    }},
    {name: "startdateyear", custom_output: function(val){
      return val + " to ";
    }},
    {name: "enddatemonth", custom_output: function(val){
      return val + "/";
    }},
    {name: "enddateyear"},
    {name: "job_desc", custom_output: function(val){
      return "\nSummary: " + val + "\n";
    }}
  ],
  uiUpdateFct: function(section) {
    var count = section.count;
    $('#work-history').fadeOut(function(){
      if (count > 1) {
        $('#summary_header').html("<i class='fa fa-briefcase greenText'></i> Work History - Job #" + count);
        if (count == 2) {
          $('#summary_block').fadeOut(100);
          $('#position_header').html("Add Position:");
        }
        $('.work').val('');
        $('#status_msg').html('<i class="fa fa-check greenText"></i> Job saved. Add another job.').show();
        $('#work-history').fadeIn(100);
      }
      else {
        $('#status_msg').hide().html('');
        $('#quickApplyStep1,#mobile-indeed').fadeOut(function() {
          $('#work-history').fadeIn(100);
          $('.mobileApplyHeader').hide();
          $('.termsLink').hide();
          $('.topApplyButtons').hide();
        });
      }
    });
  },
  validate: function() {
    return $('input[name=jobtitle],input[name=companyname],select[name=startdatemonth],select[name=startdateyear],select[name=enddatemonth],select[name=enddateyear]').valid();
  }
});


var EDUCATION_SECTION = MobileApplyManager.createSectionType({
  name: "Education",
  fieldArr: [
    {name: 'schoolname'},
    {name: 'degree'},
    {name: 'yearcompleted', custom_output: function(val){
      return "Degree date: " + val + "\n";
    }},
    {name: 'honors_text'}
  ],
  uiUpdateFct: function(section) {
    var count = section.count;
    $('#edu-history').fadeOut(function(){
      if (count > 1) {
        $('.edu').val('');

        $('#edu_title').html("<i class='fa fa-book greenText'></i> Education - School #" + count);
        if (count == 2) {
          $('#edu_header').html("Add School");
        }
        $('#status_msg').html('<i class="fa fa-check greenText"></i> School saved. Add another school.').show();
        $('#edu-history').fadeIn(100);

      }
      else {
        $('#status_msg').hide().html('');
        $('#work-history').fadeOut(function() {
          $('#edu-history').fadeIn(100);
          $('#contact-submit-block').fadeIn(100);
          $('.mobileApplyHeader').hide();
          $('.termsLink').removeClass('dNonePhone dNoneTablet dNoneTabletSmall').show();
        });
      }
    });
  },
  validate: function() {
    return $('input[name=degree],input[name=schoolname],input[name=yearcompleted]').valid();
  }
});
