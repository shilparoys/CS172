  <script type="text/javascript">
$(function() {
  var launch_job_alert_popup = function(e) {
  var ajax_params = {'ajax': 1, 'modal': 1};
  if ($(this).hasClass('candidate_pop_012414')) {
    ajax_params['variation'] = 'candidate_pop_012414';
  } else if ($(this).hasClass('pop_man_with_phone_1')) {
    ajax_params['variation'] = 'pop_man_with_phone_1';
  } else if ($(this).hasClass('pop_man_with_phone_2')) {
    ajax_params['variation'] = 'pop_man_with_phone_2';
  } else if ($(this).hasClass('pop_woman_with_phone')) {
    ajax_params['variation'] = 'pop_woman_with_phone';
  } else if ($(this).hasClass('pop_letter')) {
    ajax_params['variation'] = 'pop_letter';
  } else if ($(this).hasClass('pop_phone')) {
    ajax_params['variation'] = 'pop_phone';
  } else if ($(this).hasClass('ziprecruiter_logo_header')) {
    ajax_params['variation'] = 'ziprecruiter_logo_header';
  } else if ($(this).hasClass('ziprecruiter_logo')) {
    ajax_params['variation'] = 'ziprecruiter_logo';
  }
  $.get($(this).attr('href'), ajax_params, function(raw_response) {
    parseAjaxResponse(raw_response);
  });
  return false;
};
$('#createAlerteLink').on('click', launch_job_alert_popup);

  

  
  // validate form on keyup and submit
  $("#contact_create_form").validate({
    rules: {
      resume: {
        required: true,
        extension: 'pdf|ps|doc|docx|ppt|xls|xlxs|odt|odp|ods|txt|rtf'
      },
      resume_text: { required: true },
      resume_url: {
        required: false,
        complete_url: true
      },
      photo: {
	        required: false,
	        accept: "jpg|gif|jpeg|png"
      }
    },
    messages: {
      resume: 'Resume file type must be .pdf,.ps,.doc,.docx,.ppt,.xls,.xlxs,.odt,.odp,.ods,.txt or.rtf',
      email_address: "Please enter a valid email address.",
      phone_number: "Please enter phone number (with area code).",
      resume_url: "Please enter a URL like http://www.linkedin.com/in/username",
      name: "Please enter your name.",
      photo: "Please upload only .gif, .jpg or .png files"
    }
  });

