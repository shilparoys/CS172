  <script type="text/javascript">
	 // DOM ready
	 $(function() {
	   
      // Create the dropdown base
      $("<select />").appendTo("mobilemenu1");
      
      // Create default option "Go to..."
      $("<option />", {
         "selected": "selected",
         "value"   : "",
         "text"    : "Ask IT Links..."
      }).appendTo("mobilemenu1 select");
      
      // Populate dropdown with menu items
      $("mobilemenu1 a").each(function() {
       var el = $(this);
       $("<option />", {
           "value"   : el.attr("href"),
           "text"    : el.text()
       }).appendTo("mobilemenu1 select");
      });
      
	   // To make dropdown actually work
	   // To make more unobtrusive: http://css-tricks.com/4064-unobtrusive-page-changer/
      $("mobilemenu1 select").change(function() {
        window.location = $(this).find("option:selected").val();
      });
	 
	 });
	</script> 
  <script type="text/javascript">
	 // DOM ready
	 $(function() {
	   
      // Create the dropdown base
      $("<select />").appendTo("mobilemenu2");
      
      // Create default option "Go to..."
      $("<option />", {
         "selected": "selected",
         "value"   : "",
         "text"    : "Go to UW pages..."
      }).appendTo("mobilemenu2 select");
      
      // Populate dropdown with menu items
      $("mobilemenu2 a").each(function() {
       var el = $(this);
       $("<option />", {
           "value"   : el.attr("href"),
           "text"    : el.text()
       }).appendTo("mobilemenu2 select");
      });
      
	   // To make dropdown actually work
	   // To make more unobtrusive: http://css-tricks.com/4064-unobtrusive-page-changer/
      $("mobilemenu2 select").change(function() {
        window.location = $(this).find("option:selected").val();
      });
	 
	 });
	</script> 
