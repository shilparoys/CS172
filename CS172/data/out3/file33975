   <script type="text/javascript">
//<![CDATA[

jQuery(document).ready(function(){
	

// group latest buzz boxes into rows of 4
var buzzBoxes = jQuery(".featureItem");
for(var i = 0; i < buzzBoxes.length; i+=4) {
  buzzBoxes.slice(i, i+4).wrapAll("<div class='featureRow'></div>");
}

// add alpha/omega classes to landing boxes
jQuery(".featureRow").each(function() {
	jQuery(this).find(".featureItem:first").addClass("alpha");
    jQuery(this).find(".featureItem:last").addClass("omega");
    
    equalHeight(jQuery(this).find(".featureItem"));
});

jQuery(window).resize(function() {

    // add alpha/omega classes to landing boxes on/during window resize;
    jQuery(".featureRow").each(function() {
        equalHeight(jQuery(this).find(".featureItem"));
    });

});

// Initialize BX Slider (MOBILE ONLY)
jQuery('.homeRotator .bx_slide').wrapAll('<div class="bx_slider"></div>');
   jQuery('.bx_slider').bxSlider({
		auto: 			true,		// Starts paused (autoplay off
      //  preloadImages:	'visible',
        pause:			6000,
        mode:			'fade',
        nextText:		'<span>Next Slide</span>',
        prevText:		'<span>Last Slide</span>',
		pager: 			false	// Builds navigation links/buttons and arrows
  });
  jQuery(".bx-controls-direction a.bx-prev").prepend('<i class="fa fa-chevron-circle-left"></i>');
  jQuery(".bx-controls-direction a.bx-next").append('<i class="fa fa-chevron-circle-right"></i>');


// tabs
	//When page loads...
	jQuery(".tab_content").hide(); //Hide all content
	jQuery(".tabNav a:first").addClass("active").show(); //Activate first tab
	jQuery(".tab_content:first").show(); //Show first tab content

	//On Click Event
	jQuery(".tabNav a").click(function() {

		jQuery(".tabNav a").removeClass("active"); //Remove any "active" class
		jQuery(this).addClass("active"); //Add "active" class to selected tab
		jQuery(".tab_content").hide(); //Hide all tab content

		var activeTab = jQuery(this).attr("href"); //Find the href attribute value to identify the active tab + content
		jQuery(activeTab).fadeIn(); //Fade in the active ID content
		return false;
	});
	


});
