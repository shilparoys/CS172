       <script type="text/javascript">

Cufon.replace('.singleBox .text ul li a', { fontFamily: 'Gotham Book' });
Cufon.replace('.text p ul a', { fontFamily: 'Gotham Book' });
Cufon.replace('.caption', { fontFamily: 'Gotham Book' });
			//image rollovers
			$(document).ready(function() {
			  $('#saMenu a', this).hover(function() {
				//mouseover
				$(".caption", this).animate({
					height: "60px" 
				}, 200);
			  }, function() {
				//mouseout
				$(".caption", this).animate({
					height: "20px" 
				}, 200);
			  });
			});
		      
			//random images
			function chooseImage(loc,numOfPics) {
				numOfPics = numOfPics - 1;
				var pics=numOfPics; //number of images
				var num;
				var path;
				num = Math.round(Math.random() * pics) + 1;
				path = "http://www.nyu.edu/content/dam/nyu/studyAbroad/images/" + loc + num + ".jpg";
				document.write('<img src="' + path + '" width="178" height="178" alt="' + loc + '" />');
			}
		</script> 
       <style type="text/css">
