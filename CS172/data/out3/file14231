    <script>
        /* jQuery Initialization */
        $(document).ready(function () {

            // Fix for Blank video in Firefox.
            $('iframe').each(function () {
                var url = $(this).attr("src");
                if (url != null && url != undefined) {
                    var result = url.indexOf('?');
                    if (result != -1) {
                        $(this).attr("src", url + "&wmode=transparent");
                    }
                    else {
                        $(this).attr("src", url + "?wmode=transparent");
                    }
                }
            });

            // Show the slider HTML before loading.
            $('.bfw-showcase').show();

            // Load the Slider.
            var slider = $('.bxslidermain').bxSlider({
                touchEnabled: $('.bxslidermain li').length > 1 ? true : false,
                useCSS: false,
                adaptiveHeight: true,
                captions: true,
                video: true,
                autoHidePager: true,
                auto: false,
                speed: 1500,
                pause: 7000,
                autoHover: true,
                pager: $('.bxslidermain li').length > 1 ? true : false
            });
        });
    </script> 
