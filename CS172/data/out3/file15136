             <div class="portlet-dropzone portlet-column-content portlet-column-content-last" id="layout-column_column-2"> 
              <div id="p_p_id_56_INSTANCE_ZI6l_" class="portlet-boundary portlet-boundary_56_  portlet-static portlet-static-end portlet-journal-content "> 
               <a id="p_56_INSTANCE_ZI6l"></a> 
               <div class="portlet-borderless-container" style=""> 
                <div class="portlet-body"> 
                 <div class="journal-content-article" id="article_10136_11306_226456_2.0">   
                  <meta content="text/html; charset=ISO-8859-1" http-equiv="content-type">   
                  <!-- DIV QUE CONTIENE TODA LA PAGINA --> 
                  <div id="main" name="main" class="main"> 
                   <h2 style="font-size:2.1em;">San Josemaría y la Universidad de Navarra</h2> 
                   <img style="height: 15px;width: 100%;padding-top: 10px;margin-bottom: -10px; " src="/html/images/shadow-701px.gif"> 
                  </div>   
                 </div> 
                </div> 
               </div> 
