   <section name="waypoint_section" id="how_it_works"> 
    <div class="container"> 
     <a name="share"></a> 
     <div class="share-site"> 
      <div id="share_main" class="addthis_toolbox btn-tab secondary"> 
       <span>Share</span> 
       <small>this</small> 
       <span>Site:</span> 
       <a href="javascript:sharePop('https://www.facebook.com/sharer/sharer.php?s=100&amp;p[title]=Take+the+first+step+to+restore+The+Colorado+River+Basin%2C+pledge+to+add+1%2C000+gallons&amp;p[url]=http%3A%2F%2Fchangethecourse.us%2Farchives%2F1')" class="social-share-link socialico socialico-facebook"></a>
       <a href="javascript:sharePop('http://twitter.com/intent/tweet?text=Help+to+put+1%2C000+gallons+back+in+the+Colorado+River+%40TakePart')" class="social-share-link socialico socialico-twitter"></a>
       <a class="addthis_button_email addthis_button_preferred_3 at300b social-share-link icon icon-email"></a> 
