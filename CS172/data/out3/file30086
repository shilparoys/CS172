      <div class="widget"> 
       <script type="text/javascript">

    $(document).ready(function () {

        btnPracticeAreaOverview = $('#ButtonPracticeAreaOverView');
        ddlPracticeAreaOverview = $('#DropDownListPracticeAreas');

        btnPracticeAreaOverview.attr('Enabled', false);

        ddlPracticeAreaOverview.change(function () {

            if (ddlPracticeAreaOverview.val() != '') {

                btnPracticeAreaOverview.attr('Enabled', true);
                btnPracticeAreaOverview.removeClass('disabled');

            } else {

                btnPracticeAreaOverview.attr('Enabled', false);
                btnPracticeAreaOverview.addClass('disabled');

            }

        });

        btnPracticeAreaOverview.click(function (e) {

            if (ddlPracticeAreaOverview.val() == '') {

                e.preventDefault();

            }

        });

    });

</script> 
