  <script language="JavaScript">
function selectMonthByYear() {
    var date = new Date();
    var curr_year = date.getFullYear();
    if((document.eventsearch.year.value != curr_year) && (document.eventsearch.month.value.length == 0)) {     
        document.eventsearch.month.options[0].selected = true;
    }
}

function selectYearByMonth() {
    var date = new Date();
    var curr_year = date.getFullYear();
    if(document.eventsearch.month.value.length == 0) {
        document.eventsearch.year.value = curr_year;
    }
}
</script> 
