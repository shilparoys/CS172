      <h3>Search News</h3> 
      <div class="search"> 
       <link rel="stylesheet" href="/common/css/search.css" type="text/css" media="screen"> 
       <link rel="stylesheet" href="/common/css/news.css" type="text/css" media="screen"> 
       <link rel="stylesheet" href="/common/css/jquery-ui-1.7.1.custom.css" type="text/css" media="screen"> 
       <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/jquery-ui.min.js" type="text/javascript"></script> 
       <script src="/common/js/jquery.ui.datepicker.js" type="text/javascript"></script> 
       <script type="text/javascript">
$(function() {
    jQuery.fn.toggleText = function(a, b) {
        return this.each(function() {
            jQuery(this).text(jQuery(this).text() == a ? b : a);
        });
    };
    
    $("#searchBox label.fromDate input").datepicker({maxDate: '-1'});
    $("#searchBox label.toDate input").datepicker({maxDate: new Date()});
    
    if (!$(".news.search").length) {
        $("#searchBox .advanced").hide();
        $("#searchBox .advanced-toggle").click(function() {
            $(this).toggleClass("toggled");
            $(this).toggleText("Simple Search", "Advanced Search")
            $("#searchBox .advanced").slideToggle();
        });     
    }
});
</script> 
