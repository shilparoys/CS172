	//setPageHeight();
});
jQuery(window).scroll(function($) {
    
});
jQuery('#page').resize(function($) {
	//setPageHeight();
});


var sitemap_init = false;

function initSitemap(){

		jQuery(function($) {
		
		if(!sitemap_init){

		sitemap_init = true;
		//alert($("html").attr("lang"))
		if($("html").attr("lang") != "en-US") return;
		var pos_map = [0, 770, 757, 755];

		var left = $("#menu-item-761").position().left - $("#menu-item-763").position().left
		$("#menu-item-763 ul li:nth-last-child(4),#menu-item-763 ul li:nth-last-child(3),#menu-item-763 ul li:nth-last-child(2),#menu-item-763 ul li:nth-last-child(1)").each(function(i){

			var top =  $("#menu-item-762").position().top - $("#menu-item-763").position().top + (pos_map[i]?$("#menu-item-"+pos_map[i]).position().top:0)
			$(this).css('position','absolute').css('top',top+"px").css('left',left+"px")
		})		

		}

		})
}
initSitemap()

function toggleSitemap(){

	
	jQuery(function($) {

		if($("#menu-item-30717").height() > 40){
			$("#menu-item-30717").css('width','200px');
		}
		if($("#menu-item-6633").height() > 20){
			$("#menu-item-6633").css('width','220px');
		}
		if($("#menu-item-15539").height() > 20){
			$("#menu-item-15539").css('width','220px');
		}
	
		if($("#menu-item-759 ul.sub-menu").height() > 580){
			$("#menu-item-759 ul.sub-menu li").css('width','200px');
		};
		//alert($( '#sitemap-footer' ).height());
		if( Math.abs($('#sitemap-footer' ).height()) > 5 ){
			$( '#sitemap-footer' ).animate({
				height: '0px'
			}, 500);
		}else{
			
			if ($( '#sitemap-footer' ).hasClass('sitemap-footer-en')){
				//if ( $( 'html' ).hasClass('ie') ){ height='630'; }else{ height='700'; }
				height='720'
			}else{
				//if ( $( 'html' ).hasClass('ie') ){ height='540'; }else{ height='610'; }
				height='630'
			}
			$( '#sitemap-footer' ).animate({
				height: height+'px'
			}, 300);
		}

		$( '#footer-sitemap-arrow' ).slideToggle(300);
	});
}
function setPageHeight(){
	jQuery(function($) {
		var windowH = getWindowHeight();
		var wrapperH = $('#page').height();
		if(windowH > wrapperH) {                            
			$('#page').css({'min-height':windowH+'px'});
		}
	});
}
function getWindowHeight() {
		var windowHeight = 0;
		if (typeof(window.innerHeight) == 'number') {
			windowHeight = window.innerHeight;
		}
		else {
			if (document.documentElement && document.documentElement.clientHeight) {
				windowHeight = document.documentElement.clientHeight;
			}
			else {
				if (document.body && document.body.clientHeight) {
					windowHeight = document.body.clientHeight;
				}
			}
		}
		return windowHeight;
};
