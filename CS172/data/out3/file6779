jQuery( function() {

var link =['http://www.ust.hk/cover-stories/hkust-attains-highest-proportion-internationally-excellent-research-work-ugc-research-assessment-exercise/','http://www.ust.hk/cover-stories/playing-god-redesigned-genes/','http://www.ust.hk/cover-stories/beam-construction-industry-advanced-aluminum-composite-wide-applications/','http://www.ust.hk/cover-stories/wonderful-world-5-g-wireless-systems-2/','http://www.ust.hk/cover-stories/hkust-graduates-ranked-among-worlds-employable/'];

 jQuery.vegas('slideshow', {
  delay:7000,
  backgrounds:[{ src:'http://www.ust.hk/wp-content/uploads/2015/01/CoverPage_PR2015_eng.jpg', fade:800 },{ src:'http://www.ust.hk/wp-content/uploads/2015/01/CoverPage_SyntheticBiology_eng.jpg', fade:800 },{ src:'http://www.ust.hk/wp-content/uploads/2015/01/CoverPage_Aluminium-EN3.jpg', fade:800 },{ src:'http://www.ust.hk/wp-content/uploads/2015/01/Cover_5GWirelessSystem_Eng.jpg', fade:800 },{ src:'http://www.ust.hk/wp-content/uploads/2014/12/CoverPage_16thWorldwide_Eng.jpg', fade:800 }]
})('overlay', {
   src:'' 
})

jQuery(document).ready(function($) {

$('body').bind('vegaswalk',function(e, bg, step) {
	//alert("step:"+step)
	if(link[step] != ""){
		$("#cover_link").attr('href',link[step]).show();
	}else{
		$("#cover_link").attr('href','#').hide();
	}
});
});

});

