	});

			jQuery("#header-search-submit").click(function(){

				if(jQuery('input[name=sopt]:checked', '#header-search-form').val() == 'publications'){
					var q = jQuery("#header-search-query").val();
					//window.open("http://repository.ust.hk/ir/Search/Results?lookfor="+ encodeURIComponent (q)+"&type=AllFields&submit=Find");
					window.open("http://repository.ust.hk/ir/Search/Results?lookfor="+ encodeURIComponent (q)+"&sp=1");
				}else{
					jQuery("#header-search-form").submit();

				}

				event.preventDefault();
				return false;
			})

