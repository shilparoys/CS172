        $(document).ready(function () {

//            //initialize
            $('#divDidYouKnowAnswer').hide();
            $('#divDidYouKnowClose').hide();

            //events
            $('#divDidYouKnowClose').click(function () {
                $('#divDidYouKnowAnswer').hide('fast');
                $('#divDidYouKnowClose').hide('fast');
            });

            $('#radDidYouKnowTrue').click(function () {
                DisplayAnswer("True");
            });

            $('#radDidYouKnowFalse').click(function () {
                DisplayAnswer("False");
            });
                
            //rotator
            var xmlDoc = loadXMLDoc("rotator.xml");
            items = xmlDoc.getElementsByTagName("ITEM");

            RotateOnTimer(index);

        });

        function DisplayAnswer(selectedAnswerText){
            var correctAnswer = $('[id*="hdnDidYouKnowAnswer"]').val();
            if (selectedAnswerText == correctAnswer) {
                $('#h2DidYouKnowAnswer').html("<em>Correct!</em>");
            }
            else {
                $('#h2DidYouKnowAnswer').html("<em>The correct answer is<br />" + correctAnswer + "</em>");
            }

            $('#divDidYouKnowAnswer').show('fast');
            $('#divDidYouKnowClose').show('fast');
        }
    </script> 
