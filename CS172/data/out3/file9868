        /* =   -  -   -  =  -  - -   -   =   -  -   -  =  -  - -   -  =   -  -   -  =  -  - -   -
		 Load  Class Information
         =   -  -   -  =  -  - -   -   =   -  -   -  =  -  - -   -  =   -  -   -  =  -  - -   -
		*/			
			$(document).ready(function(){
			$("tr:odd").addClass("odd");

				$(".moreinfo").each(function(){
					var t;
					var isanimated = false;
					var isvisible = false;
					var popup = $(".floatingpanel",this);
					var popup2 = $(".floatingpanel2",this);
					$(this).mouseover(function(){
						if (t) clearTimeout(t);
						if (!isvisible && !isanimated){
							isanimated = true;
							popup.css({opacity:0,display:"block",marginTop:15}).animate({marginTop:0,opacity:1},500, function(){isanimated=false;isvisible=true;});
							popup2.css({opacity:0,display:"block",marginTop:15}).animate({marginTop:0,opacity:1},500, function(){isanimated=false;isvisible=true;});

						} else {
							return;
						}
					}).mouseout(function(){
						if (t) clearTimeout(t);
						if (!isanimated && isvisible){
							t = setTimeout(function(){isanimated=true;popup.animate({marginTop:-10,opacity:0},500, function(){popup.css({display:"none"});isvisible=false;isanimated=false;});}, 500);
						} else {
							return;
						}
					});					
				});									   
			});
			
            $(function() {

         	   $("#dvDefaultView").show();	
				$("#dvListing").css('filter','alpha(opacity=100)').fadeIn();	
		
			var deptName0 = document.getElementById('ctl00_MainContent_dropInstructionalPrograms').value;
			if (deptName0 == "-1"){
	        	$.ajax({
	   				type: "get",
	   				url: "class-schedule-result.aspx", 
		   			data: "name=read&option=d&qtr=" + document.getElementById('rdoQtr1').value,
			 	   success: function(html){
	   	     	   $(html).appendTo("#dvListing");
					$("#dvListing > table").addClass('tablesorter');	    
			 	   $("#dvListing > div").attr('id','pager');	
			 	   $("#dvListing > div").addClass('pager');	
					$("#dvListing > table.tablesorter")
	  		 	   .tablesorter({widthFixed: true, widgets: ['zebra']})
					.tablesorterPager({container: $("#pager")});
					$("#dvListing > input[type=text]").addClass('pagedisplay');	
					$("#dvListing select").addClass('pagesize');	
					       	   $("#dvDefaultView").hide();	
		 	  	}
		 		});
			}
			else{

	        	$.ajax({
	   				type: "get",
