				 });
			});

			$("#cboEnum").click(function(){

			         	  $("#dvDefaultView").show();	
				document.getElementById('dvListing').innerHTML = '<img src="/class-finder/img/loading-results.gif" alt="Loading Classes"/>';
				$.ajax({
		   			type: "get",
		   			url: "class-schedule-result.aspx", 
		   			

		   			data: "name=" + document.getElementById('cboEnum').value + "&option=b&qtr=" +  $("input[@name=rdoQtr1]:checked").val(),
				    success: function(html){
				    document.getElementById('dvListing').innerHTML = '';

		   	        $(html).appendTo("#dvListing");
					$("#dvListing > table").addClass('tablesorter');	    
				    $("#dvListing > div").attr('id','pager');	
				    $("#dvListing > div").addClass('pager');	
					$("#dvListing > table.tablesorter")
		  		    .tablesorter({widthFixed: true, widgets: ['zebra']})
					.tablesorterPager({container: $("#pager")});
					$("#dvListing > input[type=text]").addClass('pagedisplay');	
					$("#dvListing select").addClass('pagesize');
										       	   $("#dvDefaultView").hide();		
				   	}
				 });
			});
