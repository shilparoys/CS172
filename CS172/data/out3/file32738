  <script>

$(window).load(function(){
	$('#page-loader').fadeOut(500);	
			
	$(".fade-in").velocity({ translateY: 300 }, {
		duration: 1000,
		easing: [ .42, 0, .58, 1 ]
	});
				
});

$(function () {

	Highcharts.theme= {
		colors: ['#0098DB', '#007C92', '#009B76', '#B26F16', '#E98300', '#EAAB00'],
		chart:{
			backgroundColor: '#FFF'
		}
	}

	Highcharts.setOptions(Highcharts.theme);
	

    $('#chart-container').highcharts({
        chart: {
            plotBackgroundColor: null,
            plotBorderWidth: null,
            plotShadow: false,
        },
		credits:{
			enabled:false	
		},
        title: {
            text: 'Distribution of Funding Across Disciplines',
			style:{
				color: '#8C1515'	
			}
        },
		subtitle:{
			text: 'Total Research Proposals Funded: 986<br/>Independent Student-Authored Proposals Funded: 263',
			style:{
				color: '#4d5357'
			}
		},
        tooltip: {
            pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
        },
        plotOptions: {
            pie: {
                allowPointSelect: true,
                cursor: 'pointer',
				animation:false,
                dataLabels: {
                    enabled: true,
                    format: '<b>{point.name}</b>:<br>{point.percentage:.1f} %',
                    style: {
                        color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                    },
                    connectorColor: 'silver'
                }
            }
        },
        series: [{
            type: 'pie',
            name: 'Funding share',
            data: [
                ['Humanities & Fine Arts',   11.0],
                ['Social Sciences',       18.0],
                ['Natural Sciences',    22.0],
                ['Engineering',    31.0],
                ['Interdisciplinary Programs',     18.0],
            ]
        }]
    });
	
	    $('#chart-container2').highcharts({
        chart: {
            plotBackgroundColor: null,
            plotBorderWidth: null,
            plotShadow: false
        },
		credits:{
			enabled:true,
			href: "",
			position: {
				verticalAlign: 'bottom',
				align: 'right',
				y:-10	
			},
			style:{
				color: '#4d5357'
			},
			text: '* includes Eastern Europe, the Middle East, and Australia & the Pacific Islands'
		},
        title: {
            text: 'Foreign Countries Visited for Research',
			style:{
				color: '#8C1515'	
			}
        },
		subtitle:{
			text: 'Students Conducting Independent Research Overseas: 89<br/>Foreign Countries Visited for Research: 41',
			style:{
				color: '#4d5357'
			}
		},
		legend: {
			align: 'right',
			verticalAlign: 'bottom',
			x:0,
			y:100,
			title: 'other'	
		},
        tooltip: {
            pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'
        },
        plotOptions: {
            pie: {
                allowPointSelect: true,
                cursor: 'pointer',
				animation:false,
                dataLabels: {
                    enabled: true,
                    format: '<b>{point.name}</b>:<br>{point.percentage:.1f} %',
                    style: {
                        color: (Highcharts.theme && Highcharts.theme.contrastTextColor) || 'black'
                    },
                    startAngle: -90,
					endAngle: 90,
					center: ['50%', '75%']
                }
            }
        },
        series: [{
            type: 'pie',
            name: 'Visited',
			innerSize: '50%',
            data: [
                ['Africa',       13.0],
				['Western Europe',   39.0],
                ['*Other',    13.0],
				['Asia',    24.0],
				['Latin America', 11.0]
            ]
        }]
    });
	
	if (ga) {
		$(".ga").click(function(ev){
		  var $this = $(this)
			, category = $this.data('ua-category') || 'feature'
			, action   = $this.data('ua-action')   || 'click'
			, label    = $this.data('ua-label')    || $this.attr('href')
			;
		  ga('send', 'event', category, action, label);
		});
  }
	
	
});
