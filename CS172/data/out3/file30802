      <script>
            var z = 0;
            var carouselPlacement = null;
            $('.scoreboard-slider ul li').each(function(){
                if ($(this).hasClass('firstfavorite')) {
                    carouselPlacement = z;
                }
                z++;
            });
            if (carouselPlacement == null) {
                carouselPlacement = 4;
            }
            if (document.documentElement.clientWidth < 600) { 
                scrollValue = 1; 
            } else { 
                scrollValue = 2;
            } 
            var initScoreboards = function() {
                $('#scoreboards .carousel').jCarouselLite({
                autoCSS: true,
                scroll: scrollValue,
                visible: 2,
                start: carouselPlacement,
                circular: false,
                speed: 500,
                responsive: true,
                swipe: true,
                btnNext: '.slider-nav.next',
                btnPrev: '.slider-nav.prev'
                });
            };
            $(function() {

                // (2014-9-24) quickfix for HSSN-2526
                //Advance.MT.util.attachEventHandler('personalizationInitialized', initScoreboards);
                initScoreboards();

                $('#see-more-scores').show();
            });
        </script> 
