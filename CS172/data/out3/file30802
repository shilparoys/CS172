    // brightcove event handler
    function templateLoaded(experienceID) {
        adv_player = brightcove.api.getExperience(experienceID);
        adv_modVP = adv_player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
    }

    // brightcove event handler
    function templateReady(evt) {
        adv_modVP.loadVideoByID(video_id);
    }

    // keep loadVideo() from being called multiple times
    var loaded = false;
    function loadVideo() {

        // legacy pages only fire $(window).load(), but new pages fire
        // $(document).ready() which is faster; both are used but this handler
        // should only be run once
        if (loaded) {
            return;
        } else {
            loaded = true;
        }

        var playerID;

        function createPlayer() {

            // insert player's <object> markup into page DOM
            var brightcoveMarkup = $("#brightcove-template").text();
            $(".BCLvideoWrapper").append($(brightcoveMarkup));

            // activate brightcove
            $(".BrightcoveExperience").parents(".embeddedMedia").css('display', 'block');
            $("#myExperience").children("[name='playerID']").val(playerID);
            brightcove.createExperiences();
        }

        var timesNotFound = 0;
        function successCallback(data) {

            // check if brightcove found the video. this callback gets called twice
            // for two different brightcove accounts, and it's only a complete
            // failure if both searches turn up nothing
            if(data === null || data.code === 210) {
                timesNotFound++;
                if (timesNotFound == 2) {
                    console.warn("Brightcove did not find the video.");
                }
                return;
            }

            video_id = data.id;

            // get brightcove player ID based on publisher ID
            playerID = videoid_mapping[data.thumbnailURL.split("pubId=")[1]];

            // AdvAsnycLoader is undefined on legacy pages so avoid calling it if
            // possible
            if (typeof brightcove == 'undefined') {
                AdvAsyncLoader.require('http://admin.brightcove.com/js/BrightcoveExperiences.js',
                                       createPlayer);
            } else {
                createPlayer();
            }
        }

        // we have no idea if the token should be the production or the community token, so let's try both!!
        function searchForVideoUsing(token) {
            var apiUrl = "http://api.brightcove.com/services/library?command=find_video_by_reference_id&reference_id=hssn-3656426&video_fields=id,thumbnailurl&token=" + token;
            $.ajax({
                url: apiUrl,
                dataType: 'jsonp',
                success: successCallback
            });
        }
        searchForVideoUsing(adv_token);
        searchForVideoUsing(comm_token);
    }

    return {
        loadVideo: loadVideo,
        templateLoaded: templateLoaded,
        templateReady: templateReady
    };

}());

// video initializer
jQuery(document).ready(HSSNVideo.loadVideo);
jQuery(window).load(HSSNVideo.loadVideo);
</script> 
