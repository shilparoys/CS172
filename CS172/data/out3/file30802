        <script type="text/javascript">

var HSSNVideo = (function() {
    $ = jQuery;

    var adv_player;
    var adv_modVP;
    var video_id = 0;
    var adv_token = "LaSy7_vKMpoPlgEH_Prv3O2_mbz-ciL5FbGUOIG2g_ySWBjj5zjfew..";
    var comm_token = "sgCR0qjPOvGRo4V0d2eH9rF7gVB-DdYS7M2wMWO7XDh3dlxjk0NGew..";
    var videoid_mapping = JSON.parse("{\u0022260701618001\u0022: \u00221949050399001\u0022, \u0022108289219001\u0022: \u00221950981412001\u0022, \u0022268012953001\u0022: \u00221949044327001\u0022, \u0022207391983001\u0022: \u00221949030299001\u0022, \u0022260701696001\u0022: \u00221949044313001\u0022, \u0022275353187001\u0022: \u00221949030302001\u0022, \u0022275353186001\u0022: \u00221949050409001\u0022, \u0022275353219001\u0022: \u00221949050400001\u0022, \u0022260701695001\u0022: \u00221950981419001\u0022, \u0022260701670001\u0022: \u00221950981436001\u0022, \u0022207397228001\u0022: \u00221950981417001\u0022, \u0022268012954001\u0022: \u00221949044325001\u0022, \u0022207391980001\u0022: \u00221949050398001\u0022, \u0022275353222001\u0022: \u00221949044321001\u0022, \u0022275353218001\u0022: \u00221949055954001\u0022, \u0022260825824001\u0022: \u00221949030301001\u0022, \u0022275353221001\u0022: \u00221949030307001\u0022, \u0022275353189001\u0022: \u00221949030308001\u0022, \u0022109377666001\u0022: \u00221949044318001\u0022, \u0022268012955001\u0022: \u00221949055966001\u0022, \u0022109377664001\u0022: \u00221950981416001\u0022, \u0022260825825001\u0022: \u00221949030305001\u0022, \u0022260701700001\u0022: \u00221949044328001\u0022, \u0022207391981001\u0022: \u00221949030306001\u0022, \u002224489607001\u0022: \u00221949030298001\u0022, \u0022108289215001\u0022: \u00221949044315001\u0022, \u0022268012957001\u0022: \u00221949044324001\u0022, \u0022275353185001\u0022: \u00221949055965001\u0022, \u0022207397229001\u0022: \u00221949055962001\u0022, \u0022275353220001\u0022: \u00221949044317001\u0022, \u0022108289214001\u0022: \u00221836693922001\u0022, \u0022107767378001\u0022: \u00221949050395001\u0022, \u0022108289220001\u0022: \u00221949050387001\u0022, \u0022109377665001\u0022: \u00221950981405001\u0022, \u0022108289216001\u0022: \u00221949030304001\u0022, \u0022260701698001\u0022: \u00221949055961001\u0022, \u0022665003308001\u0022: \u00221950981437001\u0022, \