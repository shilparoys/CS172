          <link rel="stylesheet" type="text/css" href="http://cdn.gurl.com/wp-content/plugins/feature_slider/carousel.css"> <script type="text/javascript">

	jQuery(document).ready(function() {
		  var feature_slider_carousel = jQuery("#feature_slider_carousel"); 

		  feature_slider_carousel.jcarousel({
				vertical: true,
				auto: 0,
				wrap: "both"
		  });


		  feature_slider_carousel.find("img").bind("carousel_mouseover",  function() {                             
				var element = jQuery(this);
				handleCarouselMouseOver(element);         
		  });   

		  // if( typeof window.orientation == 'undefined' ) {

			  feature_slider_carousel.find("img").mouseover(function() {                             
					var element = jQuery(this);
					handleCarouselMouseOver(element);         
			  });   
		  		
			  feature_slider_carousel.mouseover(function() {     
					clearInterval(slideShowTimer);
			  });   
					
			  var slideShowActiveIndex = 1; 
			  var slideShowTimer=self.setInterval( function() {                 
					if (slideShowActiveIndex<4) {
						  slideShowActiveIndex = slideShowActiveIndex + 1;
					} else {
						  slideShowActiveIndex = 1;
					}                             
					feature_slider_carousel.find("img:eq(" + (slideShowActiveIndex-1) + ")").trigger("carousel_mouseover");                                                       
			  }, 5000);
						  
			  feature_slider_carousel.find("img:first").trigger("carousel_mouseover");               

		  // if( typeof window.orientation == 'undefined' ) {
				jQuery('li a', feature_slider_carousel).click(function(e) {      
					e.preventDefault();
					clearInterval(slideShowTimer);
					var element = jQuery(this).find('img');
					handleCarouselMouseOver(element);
					return false;         
				});   
		  //}


	  	var $swipeHolder = jQuery('#feature_slider_narrow_box');
	  	var $swipeHandle = jQuery('<div style="position:absolute; width:100%; height: 100%; top: 0; left: 0; z-index: 10; cursor: pointer;"></div>');

	  	$swipeHolder.find('.jcarousel-skin-teen').css( {'z-index': 11, 'position': 'relative', 'display': 'block'} );
		$swipeHandle.appendTo($swipeHolder);
		$swipeHandle.swipe( {

			swipeLeft:function(){
				var $current_slid = feature_slider_carousel.find('a.active').parent().prev();
				if( $current_slid.index() < 0 ) {
					$current_slid = feature_slider_carousel.find('li').last();
				}

				var element = $current_slid.find('img');
				handleCarouselMouseOver(element);
			},

			swipeRight:function(){
				var $current_slid = feature_slider_carousel.find('a.active').parent().next();
				if( $current_slid.index() < 0 ) {
					$current_slid = feature_slider_carousel.find('li').first();
				}

				var element = $current_slid.find('img');
				handleCarouselMouseOver(element);
			},

/*			tap:function(event, target) {
				console.log(event);
				alert(this.directions);
				var url = jQuery(target).parent().find('a.slideshow_link').attr('href');
				if(url) window.location.href = url;
			},*/
			pageScroll: 'auto',
			threshold: 75
		});

		$swipeHandle.click(function(){ 
			var url = jQuery(this).parent().find('a.slideshow_link').attr('href');
			if(url) window.location.href = url;
		});


		function handleCarouselMouseOver(element) {
		
			  var   slideshow = jQuery("#feature_slider_narrow"),                     
					link = jQuery("#feature_slider_data a.slideshow_link").clone(),
					large_image = link.find("img");     
					info_wrapper = link.find(".info-wrapper");      
			  if (jQuery("#feature_slider_narrow").find("a").attr("href") !== element.attr("link") ) {  

					if( typeof window.orientation == 'undefined' ) {
						slideshow.find("img").fadeOut("slow");
					}
					// else {
					//	slideshow.find("img").hide();
					//}

					link.attr("href", element.attr("link") ); 
					large_image.attr("src", element.attr("large_image") ); 
					large_image.attr("alt", element.attr("alt") ); 
					info_wrapper.find(".title").html(element.attr("promotion_title"));     
					info_wrapper.find(".info").html(element.attr("promotion_description")); 

					slideshow.html("").append(link);
					
					if( typeof window.orientation == 'undefined' ) {
						link.fadeIn("slow");
					}
					else {
						link.show();
					}

					// the image is active, for now
					jQuery("#feature_slider_carousel a").removeClass("active");              
					element.parent('a').addClass("active");
			  }                 
		}

	});
	
		/* ]]> */
		</script> 
