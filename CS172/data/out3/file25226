    <script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery.fn.search = function() {
			return this.focus(function() {
				if( this.value == this.defaultValue ) {
					this.value = "";
				}
			}).blur(function() {
				if( !this.value.length ) {
					this.value = this.defaultValue;
				}
			});
		};
		jQuery("#s").search();

		jQuery("#search_button").hover(function() {
			jQuery(this).attr("src","http://cdn.gurl.com/wp-content/themes/gurl2011/images/btn-search_RO.png");
				}, function() {
			jQuery(this).attr("src","http://cdn.gurl.com/wp-content/themes/gurl2011/images/btn-search.png");
		});

		jQuery("#nav .boards").hover(function() {
			jQuery(this).find(".nav_mygurl").attr("src","http://cdn.gurl.com/wp-content/themes/gurl2011/images/link-nav_mygurl_hover.png");
				}, function() {
			jQuery(this).find(".nav_mygurl").attr("src","http://cdn.gurl.com/wp-content/themes/gurl2011/images/link-nav_mygurl.png");
		});
	});
</script>
