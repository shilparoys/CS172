										  $(document).ready(function(){
												var u = new Date();
												var u1 = u.getFullYear();
												var u2 = u.getWeekNumber();
												$('#central').attr("src", "http://www.ub.edu/dret/calendari/"+u1+"_"+u2+".html");	  
											});
										
								  function dateChanged(calendar) {
									// Beware that this function is called even if the end-user only
									// changed the month/year.  In order to determine if a date was
									// clicked you can use the dateClicked property of the calendar:
									/*if (calendar.dateClicked) {
									  // OK, a date was clicked, redirect to /yyyy/mm/dd/index.php
									  var w = calendar.date.getWeekNumber();
									  var y = calendar.date.getFullYear();
									  var m = calendar.date.getMonth();     // integer, 0..11
									  var d = calendar.date.getDate();      // integer, 1..31
									  // redirect...
									  parent.central.location = "http://www.ub.edu/dret/calendari/"+y+"_"+w+".html"; 
									}*/
									
									$('.day').click(function(event){
										var w = calendar.date.getWeekNumber();
									  var y = calendar.date.getFullYear();
									  var m = calendar.date.getMonth();     // integer, 0..11
									  var d = calendar.date.getDate();      // integer, 1..31
									  //$('#central').attr("src", "http://www.ub.edu/dret/calendari/"+y+"_"+w+".html");
									  $('#central').remove();
									  var dades = "<iframe src='http://www.ub.edu/dret/calendari/"+y+"_"+w+".html' id='central' width='100%' height='250px' frameborder='0'></iframe>";
									  $('#iframe').append(dades);
									});
								  };
								
								  Calendar.setup(
									{
									  flat         : "calendar-container", // ID of the parent element
									  flatCallback : dateChanged           // our callback function
									}
								  );
								  </script> 
