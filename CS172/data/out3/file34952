          jQuery(document).ready(function() {
		      jQuery('#search-block-form .custom-search-box').addClass('faded'); 
		      jQuery('#search-block-form .custom-search-box').blur(function (){ if (jQuery(this).value==""){ jQuery(this).value = jQuery(this).defaultValue; } jQuery(this).removeClass('focused');jQuery(this).addClass('faded'); } ); 
		      jQuery('#search-block-form .custom-search-box').focus(function(){ if (jQuery(this).defaultValue==jQuery(this).value) jQuery(this).value = ""; jQuery(this).removeClass('faded');jQuery(this).addClass('focused'); });  
	 
          });

		function focusSearch(formfield){ if (formfield.defaultValue==formfield.value) formfield.value = ""; formfield.className = "focused"; }
		function blurSearch(formfield){ if (formfield.value==""){ formfield.value = formfield.defaultValue; formfield.className = "faded";} }
