         <div class="DnnModule DnnModule-DNN_HTML DnnModule-6039">
          <a name="6039"></a> 
          <div class="LimeGreen06_style"> 
           <div class="LimeGreen06_header"> 
            <div class="c_icon LimeGreen06_top_height"> 
            </div> 
            <div class="c_title LimeGreen06_top_height"> 
             <h2 class="c_titles"> <span id="dnn_ctr6039_dnnTITLE_titleLabel" class="c_title_white">Welcome to Allegany!</span> </h2> 
            </div> 
            <div class="LimeGreen06_c_menu"> 
            </div> 
            <div class="LimeGreen06_c_help"> 
            </div> 
            <div class="LimeGreen06_l"></div> 
            <div class="LimeGreen06_r"></div> 
            <div class="clear_float"></div> 
           </div> 
           <div class="c_content_style"> 
            <div id="dnn_ctr6039_ContentPane" class="LimeGreen06_content">
             <!-- Start_Module_6039 -->
             <div id="dnn_ctr6039_ModuleContent" class="DNNModuleContent ModDNNHTMLC"> 
              <div id="dnn_ctr6039_HtmlModule_lblContent" class="Normal"> 
               <div style="display: inline-block; padding: 2px 0px 0px; text-align: center;">
                <a href="http://www.elocallink.tv/clients3/ny/greateralleganycountycoc/tourplay.php?movie=gralcococny12_wel_rev2_iwd&amp;spon=welcome" target="_blank"><img alt="" src="/portals/28/Images/Welcome.png" style="width: 100%; height: 100%; border: 1px solid #68a253;"></a>
               </div> 
              </div> 
             </div>
             <!-- End_Module_6039 -->
            </div> 
           </div> 
           <div class="c_footer"> 
            <div class="c_footer_l"> 
            </div> 
            <div class="c_footer_r"> 
