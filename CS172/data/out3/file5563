             <!-- slider markup --> 
             <ul id="unoslider6007" class="Theme_13_Inline"> 
              <li style="display:none;"> 
               <div class="unoslider_caption"> 
                <p style="font-weight: bold; color: #205867;">Fly Fishing in the Genesee River</p> 
               </div> <a target="_self" href="http://www.discoveralleganycounty.com/ThingsToDo/OutdoorActivities/HuntingFishing.aspx" title="Fly Fishing"><img src="/Portals/28/Images/Fishing.png" alt="Fly Fishing"></a> </li> 
              <li style="display:none;"> 
