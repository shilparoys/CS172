      <script type="text/javascript">
function getDate(){
var months=new Array(13);
months[1]="Jan";
months[2]="Feb";
months[3]="Mar";
months[4]="Apr";
months[5]="May";
months[6]="Jun";
months[7]="Jul";
months[8]="Aug";
months[9]="Sep";
months[10]="Oct";
months[11]="Nov";
months[12]="Dec";
var time=new Date();
var lmonth=months[time.getMonth() + 1];
var date=time.getDate();
var year=time.getYear();
if (year < 2000){
year = year + 1900;
}
return(lmonth + "&nbsp;" + date + ",&nbsp;" + year);
}

            </script> 
      <ul id="search_nav_RIGHT"> 
       <li><a href="http://illinois.edu/find/calendars.html"><script type="text/javascript">document.write(getDate());</script> </a>&nbsp;&nbsp;</li> 
       <li class="bullet"><a href="http://www.atmos.illinois.edu/weather/index.php">weather</a></li> 
      </ul> 
