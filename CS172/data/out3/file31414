   <script type="text/ng-template" id="/menu/modifiers/modifier-directive.html">
    <div class="menu--modifiers-group">
        <div class="group-title" ng-class="{'group-hasError': !isValid()}">
            <div class="title-s">
                {{modifier.name}}
            </div>
            <span ng-if="!isValid()" class="title-required">Required</span>

            <div class="clearfix"></div>
            <span class="text-muted" ng-if="modifier.instruction">{{modifier.instruction}}</span>
        </div>

        <div class="group-items" ng-if="modifier.max_selection_points == 1 && modifier.min_selection_points == 1">
            <div ng-repeat="item in modifier.modifier_items" class="modifier-item">
                <label for="{{ modifier.id + '_' + item.id }}" ng-click="modifierClicked(item)">
                    <input id="{{ modifier.id + '_' + item.id }}"
                           class="radio-branded"
                           type="checkbox"
                           class="modifier-checkbox"
                           name="{{ modifier.name }}"
                           ng-model="item.selected"
                           title="{{item.name }}"
                           value="{{ item.id }}"
                           ng-disabled="!item.available"
                           ng-class="{'not-available': !item.available, 'show-ie': isLtIe9}"
                           ng-change="itemSelected(modifier, item)">
                    <span class="checkbox" ng-if="!isLtIe9"></span>
                    <span>{{item.name}}</span>
                </label>
            </div>
        </div>

        <div class="group-items"
             ng-if="modifier.max_selection_points > 1 || (modifier.max_selection_points == 1 && modifier.min_selection_points == 0)">
            <div ng-repeat="item in modifier.modifier_items" class="modifier-item multiple">
                <label for="{{ modifier.id + '_' + item.id }}" ng-click="modifierClicked(item)">
                    <input id="{{ modifier.id + '_' + item.id }}"
                           class="checkbox-branded"
                           type="checkbox"
                           name="{{item.name}}"
                           ng-model="item.selected"
                           ng-disabled="!item.available"
                           ng-class="{'not-available': !item.available}"
                           title="{{item.name}}"
                           value="{{ item.id }}"
                           ng-click="modifierClicked(item)"
                           ng-change="modifierSelected(item, false)">
                    <span>{{item.name}}</span>
                </label>

                <div class="counter-control-horizontal" ng-if="item.selected && modifier.allow_multiple_same_item">
                    <div class="controls">
                        <button class="control ctrl-down" ng-click="decrement(item)">
                            &ndash;
                        </button>
                        <button class="control ctrl-up" ng-click="increment(item)">
                            +
                        </button>
                    </div>
                    <input type="text" ng-model="item.quantity" class="no-ui" readonly>
                </div>
            </div>
        </div>
    </div>
</script> 
