        <div class="ngdialog-inner" >
            <div class="row-fluid form-row">
                <div class="col-12">
                    <label for="modal-search-postcode" class="label-pre">Postcode</label>
                    <input type="text" ng-model="ctrl.delTimes.delInfo.postCode" id="modal-search-postcode" placeholder="e.g. W1T 6PZ" required roo-post-code maxlength="9">
                </div>
            </div>
            <div class="row-fluid form-row">
                <div class="col-6">
                    <label for="modal-search-day" class="label-pre">Day</label>

                    <div class="select-branded select-small">
                        <select id="modal-search-day" ng-model="ctrl.delTimes.delInfo.delDay">
                            <option value="today">Today</option>
                            <option value="tomorrow">Tomorrow</option>
                        </select>
                    </div>
                </div>
                <div class="col-6">
                    <label for="modal-search-time" class="label-pre">Time</label>

                    <div class="select-branded select-small">
                        <select id="modal-search-time"
                                ng-model="ctrl.delTimes.delInfo.selectedTime"
                                ng-change="ctrl.delTimes.timeChange()"
                                ng-options="times.name for times in ctrl.delTimes.getDeliveryTimes() track by times.value">
                        </select>
                    </div>
                </div>
            </div>
            <div ng-if="ctrl.message" class="alert alert-warning">
                <div class="alert-inner">{{ ctrl.message }}</div>
            </div>
        </div>
        <div class="ngdialog-actions">
            <input type="submit" formnovalidate class="btn btn-primary btn-fixed-small pull-right" value="Find Food">
        </div>
    </form>
</script> 
