     <script type="text/javascript">
    function doSearch(e) {
        var searchURL = "/search?searchtext=" + encodeURIComponent(document.getElementById("header-search-field").value);
        window.location = searchURL;
        if (e != null)
            e.preventDefault(); // sometimes useful
        return false;
    }

    document.getElementById("header-search-field").addEventListener("keydown", function (e) {
        if (!e) { var e = window.event; }

        // Enter is pressed
        if (e.keyCode == 13) {
            doSearch(e);
        }

        return true;
    }, false);
</script> 
