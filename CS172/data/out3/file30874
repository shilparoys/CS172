   <script type="text/javascript">
	function redirectRadioButtons() {
		$(".ylist input[type='radio']").each(function() {
			if ($(this).attr('checked')) {
				window.location.href = this.value;
			}
		});
	}
	function redirectBRadioButtons() {
		$(".brlist input[type='radio']").each(function() {
			if ($(this).attr('checked')) {
				window.location.href = this.value;
			}
		});
	}
</script> 
