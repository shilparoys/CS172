  <script type="text/javascript">

    $(function(){
        $("div#collegesDropdown").click(function(){
          $(".collegesList").slideToggle('slow'); // .toggle() if you need it to close with next click, or .show() for one-shot
          var self = $(this); //cache jQuery object
          if (self.hasClass("arrow-up")) {
              self.removeClass("arrow-up").addClass("arrow-down");
          } else {
              self.removeClass("arrow-down").addClass("arrow-up");
              $('html, body').animate({ scrollTop: $('#collegesDropdown').offset().top }, "slow");
          }
    });
});

$(function(){
        $("div#sectionsDropdown").click(function(){
          $(".sectionsList").slideToggle('slow'); // .toggle() if you need it to close with next click, or .show() for one-shot
          var self2 = $(this); //cache jQuery object
          if (self2.hasClass("arrow2-up")) {
              self2.removeClass("arrow2-up").addClass("arrow2-down");
          } else {
              self2.removeClass("arrow2-down").addClass("arrow2-up");
              $('html, body').animate({ scrollTop: $('#sectionsDropdown').offset().top }, "slow");
          }
    });
});

$(function(){
        $("div#newsDropdown").click(function(){
          $(".column-one").slideToggle('slow'); // .toggle() if you need it to close with next click, or .show() for one-shot
          var self3 = $(this); //cache jQuery object
          if (self3.hasClass("arrow2-up")) {
              self3.removeClass("arrow2-up").addClass("arrow2-down");
          } else {
              self3.removeClass("arrow2-down").addClass("arrow2-up");
              $('html, body').animate({ scrollTop: $('#newsDropdown').offset().top }, "slow");


          }
    });
});


</script> 
