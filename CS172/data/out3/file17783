          <script type="text/javascript">// <![CDATA[
function getInput(obj, default_txt) {                   if (obj.value == default_txt) {                                 return "";                      }                       return obj.value;               }               function clearInput(obj, default_txt) {                         if (obj.value == default_txt) {                                 obj.value = "";                         }               }               function isHjChannel() {             return ch != "";         }                 function appendQueryString(url, queryString) {             if (url.indexOf("?") > 0)
                return url + "&" + queryString;
            else
                return url + "?" + queryString;
        }

        function appendTracking(obj, tracking) {
            obj.href = appendQueryString(obj.href, tracking);
        }

                function enterKey(e) {
                        var keynum;

                        if (window.event) {
                                keynum = e.keyCode;
                        }
                        else if (e.which) {
                                keynum = e.which;
                        }

                        if (keynum == 13) {
                                MNSwdgDoSearch(document.getElementById('MNSwdgFormAction').href);
                                return false;
                        }
                        else return true;
                }

                function MNSwdgDoSearch(url) {
                        var skillskeywords;

                        if (isHjChannel()) {
                                skillskeywords = "&q=" + getInput(document.getElementById("MNSwdgSkillsKeywords"), "");
                        }
                        else {
                                skillskeywords = "?q=" + getInput(document.getElementById("MNSwdgSkillsKeywords"), "");
                        }

                        var location = "";
                        var city = getInput(document.getElementById("MNSwdgLocation"), "");
                        if (city != "") {
                                location = "&where=" + city;
                        }

                        document.location.href = url + skillskeywords + location + "&re=130&cy=us&brd=1&rad=" + defaultradius;
                }

                 function replaceDomains(links) {
            for (var i = 0; i < links.length; i++) {                 if (isHjChannel()) {                     links[i].href = appendQueryString(links[i].href, "ch=" + ch);                 }                 else {                     var domainReplacement = rootDomain;                     /* use standard Monster domain for Hiring */                     if (links[i].href.indexOf("hiring.local-jobs.monster.com") > 0) {
                        domainReplacement = ".monster.com";
                    }

                    links[i].href = links[i].href.replace(".local-jobs.monster.com", domainReplacement);
                }
            }
        }

                /* add default location to search box, and add channel id to links and search button*/
                document.getElementById("MNSwdgLocation").value = defaultlocation;
                document.getElementById("MNSwdgSkillsKeywords").value = defaultkeywords;

                /* add tracking codes */
                if (isHjChannel()) {
                    appendTracking(document.getElementById("MNSwdgFormAction"), "wt.mc_n=hjnpsearch");
                }

                var MNSwdg = document.getElementById("lp640_cntr");
                MNSwdg.links = MNSwdg.getElementsByTagName("A");
        replaceDomains(MNSwdg.links);
// ]]></script> 
