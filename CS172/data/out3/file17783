    var adSlot12= googletag.defineSlot(targetingString, [300, 90], 'div-gpt-ad-boxsmall-2');
    var adSlot13= googletag.defineSlot(targetingString, [300, 90], 'div-gpt-ad-boxsmall-3');
    
    

	//Add custom targeting value, key, key with multiple values, or multiple keys
	adSlot2.setTargeting('POS', ['ATF']);
	adSlot6.setTargeting('POS', ['ATF']);


	googletag.pubads().addEventListener('slotRenderEnded', function(event) {
		var slotType = event.slot.b.d;
		//alert(slotType);

		var slotToCheck="div-gpt-ad-slider";

		if(slotType==slotToCheck){
			if(event.isEmpty){
				//$(".ad-li").hide();
				//$(".slide-4").show();
			}
		}
		//alert('Creative with id: ' + event.creativeId + ' is rendered to slot of size: ' + event.size[0] + 'x' + event.size[1] + ' Slot: ' + event.slot);
	});

	adSlot1.addService(googletag.pubads());
	adSlot2.addService(googletag.pubads());
	adSlot3.addService(googletag.pubads());
	
	adSlot5.addService(googletag.pubads());
	adSlot6.addService(googletag.pubads());
	adSlot7.addService(googletag.pubads());
	
	
	adSlot10.addService(googletag.pubads());
	adSlot11.addService(googletag.pubads());
		adSlot12.addService(googletag.pubads());
	adSlot13.addService(googletag.pubads());
	
	

		googletag.pubads().setTargeting('Section','Primary_Front');
		//googletag.pubads().enableSingleRequest();
	googletag.pubads().collapseEmptyDivs();
	googletag.pubads().enableSyncRendering();
	googletag.enableServices();
});
