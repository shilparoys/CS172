  <script>
jQuery(function(){

used = Array();
function getrandom(l){
	n = false;
	while (n == false){
		rnd = (Math.floor(Math.random()*(l-1))) + 1 ;
		if(jQuery.inArray(rnd,used) === -1) n = true;
	}
	used.push(rnd);
	return rnd;
}
function getaos(){
	used = Array();
	$("#SurpriseMe .columns").html('');
	for (i=0; i<3; i++){
		rnd = getrandom(aos.length);
		$("#SurpriseMe .columns").append(
			 '<div class="column grid_1">'
			+'	<header><h2><a href="'+aos[rnd].url+'">'+aos[rnd].title+'</a></h2></header>'
			+ aos[rnd].text
			+'	<p><a class="navbutton" href="'+aos[rnd].url+'">Learn more</a></p>'
			+'</div>'
		);
	}
}
$(function(){
$( ".tabs" ).on( "tabsbeforeactivate", function(event, ui) {
	getaos();
});

});


