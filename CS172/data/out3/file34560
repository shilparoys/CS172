      <script type="text/javascript">
    var mainSlider = new hImageSwapper();

    mainSlider.wrapper         = 'mainSlider';  //name of div with class of imageswapper
    mainSlider.animationType   = 'slide'; //slide or fade
    mainSlider.continious      = 1; //0 no, 1 yes  (only matters for sliding)
    mainSlider.animationSpeed  = 40; //slide: lower-slower, fade: lower-faster
    mainSlider.direction       = 1; //1 or -1
    mainSlider.unit            = 'whole';   // each or whole (only matters for sliding)
    mainSlider.xmlurl          = '/library/xml/usc-12-main-slider-index.xml' + '?'+ Math.round(Math.random()*100000000);
    mainSlider.wrapperWidth    = 920;
    mainSlider.wrapperHeight   = 130;
    mainSlider.perUnit         = 1; //1 to fit images in horizontally OR enter how many (if in a grid). Fading *must* be set to how many fit.
    mainSlider.timeDelay       = 6500; //set to zero to not autorotate, (Will stop if this is faster than it takes to complete the animation)
    mainSlider.animate         = 1; // 1 yes - 0 no
    mainSlider.showControls    = 1; // 1 yes - 0 no
    
    DomReady.ready(function(){
        loadImageSwapper(mainSlider);
    });
    </script> 
