     <script type="text/javascript" language="JavaScript">
<!-- 

//////////////// BEGIN BSI VARS ////////( School Specific )/////
var bsiRotationTime = 7000;//Set rotation time here, amount/1000 = seconds
var layerHeightsLength = 5;//Set number of minis here!
var miniCount = 5;//Set number of minis here!
var arrayLength = 5;//Set number of minis here!
