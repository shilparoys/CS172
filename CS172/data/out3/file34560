var gigyaArray = new Array();
var gigyaSecondaryArray = new Array();
var gigya_bar_conf;

var dom_is_ready = false;
DomReady.ready(function(){  
    dom_is_ready = true;    
    
    gigya_bar_conf = { APIKey: gigya_oas_key }
    
    gigyaArray[0] = new gigya.services.socialize.UserAction();
    gigyaArray[0].setLinkBack('http://www.usctrojans.com/sports/m-basebl/recaps/030115aaa.html');
    gigyaArray[1] = new gigya.services.socialize.UserAction();
    gigyaArray[1].setLinkBack('http://www.usctrojans.com/blog/');
    gigyaArray[2] = new gigya.services.socialize.UserAction();
    gigyaArray[2].setLinkBack('http://www.usctrojans.com/sports/m-baskbl/recaps/022815aaa.html');
    gigyaArray[3] = new gigya.services.socialize.UserAction();
    gigyaArray[3].setLinkBack('http://www.usctrojans.com/sports/w-swim/recaps/022815aab.html');
    gigyaArray[4] = new gigya.services.socialize.UserAction();
    gigyaArray[4].setLinkBack('http://www.usctrojans.com/sports/w-wpolo/recaps/022815aaa.html');
    
    bsi_gigya_bar_share(0, gigya_bar_conf, btns, providers, more_providers);
    bsi_gigya_bar_share(1, gigya_bar_conf, btns, providers, more_providers);
    bsi_gigya_bar_share(2, gigya_bar_conf, btns, providers, more_providers);
    bsi_gigya_bar_share(3, gigya_bar_conf, btns, providers, more_providers);
    bsi_gigya_bar_share(4, gigya_bar_conf, btns, providers, more_providers);
    
});



function bsi_gigya_bar_share(gigindex, config, share_btns, providers, more_providers, container, layout) {
    var params = { 
        userAction: gigyaArray[gigindex],
        shareButtons: share_btns,
        showCounts: 'right',
        containerID: 'gigya-bar-' + gigindex,
		layout: 'vertical',
        iconsOnly: true
    };
    gigya.services.socialize.showShareBarUI(config, params);
}



function showGigya(index) {
    document.getElementById("bsi-sharebar-wrapper").style.display="block";
	/*
	if (!index) index = 0;
	document.getElementById("gigya-bar").innerHTML = "";
	if (dom_is_ready) {
	    bsi_gigya_bar_share(parseInt(index), gigya_bar_conf, btns, providers, more_providers);
    } else {
        setTimeout(function(){showGigya(index)}, 100);
    }
    */
}
function hideGigya() {
    document.getElementById("bsi-sharebar-wrapper").style.display="none";
}

