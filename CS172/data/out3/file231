    <script type="text/javascript" language="javascript">

    function encode(input) {
        return $('<div />').text(input).html();
    }

    function decode(input) {
        return $('<div />').html(input).text();
    }

    $.ui.autocomplete.prototype._renderItem = function (ul, item) {
        // Escape any regexy type characters so they don't bugger up the other reg ex
        var keywords = this.term.replace(/([\^\$\(\)\[\]\{\}\*\.\+\?\|\\])/gi, "\\$1");
        // Join the terms with a pipe as an 'OR'
        keywords = keywords.split(' ').join('|');
        //$.ui.autocomplete.escapeRegex(keywords)
        item.label = item.label.replace(new RegExp("(?![^&;]+;)(?!<[^<>]*)(" + keywords + ")(?![^<>]*>)(?![^&;]+;)", "gi"), "<strong>$1</strong>");
        return $("<li></li>")
                    .data("item.autocomplete", item)
                    .append("<a>" + item.label + "</a>")
                    .appendTo(ul);
    };

    $.ui.autocomplete.prototype._resizeMenu = function () {
        var ul = this.menu.element;
        ul.outerWidth(this.element.outerWidth());
    }

    var intMaxLength = 140;

    $(function () {
        $('#ctl00_cphMain_ctl00_ctl01_btnSearch').click(function () {
            var vLen = $('#ctl00_cphMain_ctl00_ctl01_txtSearch').val().length;
            if (vLen > intMaxLength) {
                alert('Search character limit has been reached. Results cannot be displayed until you reduce your entry.');
                return false;
            }
            $('#ctl00_cphMain_ctl00_ctl01_txtSearch').val(encode($('#ctl00_cphMain_ctl00_ctl01_txtSearch').val()));
        });
    });
</script> 
    <style type="text/css">
    .ui-autocomplete
    {
        overflow-y: auto;
        text-align: left;
    }
</style> 
