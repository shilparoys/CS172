   		ttSlider.continious 		   = 1; //0 no, 1 yes  (only matters for sliding)
   		ttSlider.animationSpeed 	= 30; //slide: lower-slower, fade: lower-faster 
   		ttSlider.direction	 	   = 1; //1 or -1
   		ttSlider.unit			      = 'each'; 	// each or whole (only matters for sliding)
   		ttSlider.xmlurl		      = '/library/xml/geot-13-multiswapper.xml';
   		ttSlider.wrapperWidth		= 935;
   		ttSlider.wrapperHeight	   = 97;
   		ttSlider.perUnit		      = 1; //1 to fit images in horizontally OR enter how many (if in a grid). Fading *must* be set to how many fit.
   		ttSlider.timeDelay	 	   = 7000; //set to zero to not autorotate
   		ttSlider.animate 	   	   = 1; // 1 yes - 0 no
   		ttSlider.showControls 	   = 0; // 1 yes - 0 no
		
   	
   		loadImageSwapper(ttSlider)
   		
		
   	//-->
   	</script> 
     </div> 
     <div class="imgswap-button-left">
      <a href="javascript:;" id="ttSlider-left">&lt;</a>
     </div> 
     <div class="imgswap-button-right">
      <a href="javascript:;" id="ttSlider-right">&gt;</a>
     </div> 
     <script type="text/javascript">
	DomReady.ready(function(){  
       	ttSlider_lkr = document.getElementById("ttSlider-right");
       	ttSlider_lkl = document.getElementById("ttSlider-left");
       	if (ttSlider_lkr.addEventListener){
       		ttSlider_lkr.addEventListener("click", function(){ swapperMove(ttSlider, 1); return false }, false);
       		ttSlider_lkl.addEventListener("click", function(){ swapperMove(ttSlider, -1); return false }, false);
       	} else if (ttSlider_lkr.attachEvent){
       	  	ttSlider_lkr.attachEvent("onclick", function(){ swapperMove(ttSlider, 1); return false });
       		ttSlider_lkl.attachEvent("onclick", function(){ swapperMove(ttSlider, -1); return false });
       	}
    });
   </script> 
    </div> 
