      <script type="text/javascript">

var myCounter = null
var timerId = null

function loadCounter(){
    myCounter = [
            new Counter("my_counter_1",
    {
                digitsNumber : 2,
                direction : Counter.ScrollDirection.Downwards,
                characterSet : Counter.DefaultCharacterSets.numericUp,
                charsImageUrl : "http://grfx.cstv.com/schools/geot/graphics/small-digits.png",
                markerImageUrl : "http://grfx.cstv.com/schools/geot/graphics/marker_small.png",
                extraClassName: {
                    left: "ex1_left",
    
                    right: "ex1_right",
    
                    inner: "ex1_inner"
                }
            }),
            new Counter("my_counter_2",
    {
                digitsNumber : 2,
                direction : Counter.ScrollDirection.Downwards,
    
                characterSet : Counter.DefaultCharacterSets.numericUp,
                charsImageUrl : "http://grfx.cstv.com/schools/geot/graphics/small-digits.png",
                markerImageUrl : "http://grfx.cstv.com/schools/geot/graphics/marker_small.png",
                extraClassName: {
                    left: "ex1_left",
    
                    right: "ex1_right",
    
                    inner: "ex1_inner"
                }
            }),
            new Counter("my_counter_3",
    {
                digitsNumber : 2,
                direction : Counter.ScrollDirection.Downwards,
                characterSet : Counter.DefaultCharacterSets.numericUp,
                charsImageUrl : "http://grfx.cstv.com/schools/geot/graphics/small-digits.png",
                markerImageUrl : "http://grfx.cstv.com/schools/geot/graphics/marker_small.png",
                extraClassName: {
                    left: "ex1_left",
    
                    right: "ex1_right",
    
                    inner: "ex1_inner"
                }
            }),
            new Counter("my_counter_4",
    {
                digitsNumber : 2,
                direction : Counter.ScrollDirection.Downwards,
                characterSet : Counter.DefaultCharacterSets.numericUp,
                charsImageUrl : "http://grfx.cstv.com/schools/geot/graphics/small-digits.png",
                markerImageUrl : "http://grfx.cstv.com/schools/geot/graphics/marker_small.png",
                extraClassName: {
                    left: "ex1_left",
    
                    right: "ex1_right",
    
                    inner: "ex1_inner"
                }
            })
        ];
    
    timerId = window.setInterval(function(){
            var destTime = new Date(2015, 02, 12, 05, 00, 0);
            var now = new Date();
            var now_utc = new Date(now.getUTCFullYear(), now.getUTCMonth(), now.getUTCDate(),  now.getUTCHours(), now.getUTCMinutes(), now.getUTCSeconds());
            if (now_utc > destTime) {timerId = 0; return false};
            var diff = Math.floor((destTime - now_utc) / 1000);

            myCounter[0].setValue(Math.floor((diff/(60*60*24)) % 365), 800);
            myCounter[1].setValue(Math.floor((diff/(60*60)) % 24), 800);
            myCounter[2].setValue(Math.floor((diff/60) % 60), 800);
            myCounter[3].setValue((diff % 60), 800);
            if (diff === 0) {myCounter = null}; 
            
        },
    1000); 
}

loadCounter();
</script> 
      <!-- COUNTDOWN NOTES:
   - Month should be 1 less than normal, so January is 0 and December is 11. 
   - Hour is UTC time, so you need to add 4 hours to an Eastern time in the code (5 in the winter). For example, a Noon start would be 16. 
   - For graphic below, just change the schoolcode at the end of the file name, and it will call in next week's version.
   - PLEASE check this countdown live on the site after you update it to confirm that it's counting to the right time!
   --> 
