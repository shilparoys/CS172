    $(".breadcrumb-back").click(function(){
        window.history.back();
    });

    var cookie = $.cookie('cookieEnable');
    if(cookie  != undefined && cookie == 1){
        $(".header-cookies").remove();
    }else{
        $(".header-cookies-close").click(function(){
            $.cookie('cookieEnable', 1, {path: '/' });
        });
    }

    if(typeof(publiSkin) != "undefined") {
        $('body').addClass('skin');
        $("#Top").addClass('visible2');
