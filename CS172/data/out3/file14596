            },
            "defaultTemplate": 'ListViewEstudios'
        },
        "hashid": "554589059fa7c90ea04a14016e507843",
        "facets": {
            "attached": "left"
        }
    }
    


    function fixDiv() {
        var $cache = $('#doofinder');
        if ($(window).scrollTop() > 100)
            $cache.css({'position': 'fixed', 'top': '50px'});
        else
            $cache.css({'position': 'absolute', 'top': '287px'});
    }
    $(window).scroll(fixDiv);
    fixDiv();

    $('#buscador14').bind("keyup keypress", function(e) {
        var code = e.keyCode || e.which;
        if (code  == 13) {
            e.preventDefault();
            return false;
        }
    });


    $(".selectOptionSearch").change(function(){
        if($(this).val() == "studies"){

            doofinder.options.hashid = '554589059fa7c90ea04a14016e507843';
            doofinder.options.results.defaultTemplate = 'ListViewEstudios';

            $("#buscador14").attr("action",$("#urlBuscadorEstudios").val());

        }else if($(this).val() == "universities"){

            doofinder.options.hashid = '7f1d266fe4ab9d0d99f12aa376438738';
            doofinder.options.results.defaultTemplate = 'ListViewUniversidades';

            $("#buscador14").attr("action",$("#urlBuscadorUniversidades").val());

        }
        doofinder.reload();
    });

    }
</script>
