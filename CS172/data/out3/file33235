       <div id="widget-box"> 
        <script type="text/javascript">
	$(document).ready(function () {
		// jQuery formatCurrency plugin: https://code.google.com/p/jquery-formatcurrency/
		// Format while typing & warn on decimals entered, 2 decimal places
		$('input[name=amt]').blur(function() {
			$(this).formatCurrency({ colorize: true, negativeFormat: '%s%n', roundToDecimalPlace: 2 });
		})
		.keyup(function(e) {
			var e = window.event || e;
			var keyUnicode = e.charCode || e.keyCode;
			if (e !== undefined) {
				switch (keyUnicode) {
					case 16: break; // Shift
					case 17: break; // Ctrl
					case 18: break; // Alt
					case 27: this.value = ''; break; // Esc: clear entry
					case 35: break; // End
					case 36: break; // Home
					case 37: break; // cursor left
					case 38: break; // cursor up
					case 39: break; // cursor right
					case 40: break; // cursor down
					case 78: break; // N (Opera 9.63+ maps the "." from the number key section to the "N" key too!) (See: http://unixpapa.com/js/key.html search for ". Del")
					case 110: break; // . number block (Opera 9.63+ maps the "." from the number block to the "N" key (78) !!!)
					case 190: break; // .
					default: $(this).formatCurrency({ colorize: false, negativeFormat: '%s%n', roundToDecimalPlace: -1, eventOnDecimalsEntered: true });
				} // IF
				var num = $(this).asNumber();
				if (num >= 100000) {
					$(".overlimit").fadeIn();
					$(this).parent().addClass("err");
				} else {
					$(".overlimit").fadeOut();
					$(this).parent().removeClass("err");
				}
			} // KEYUP
		}) 
		.bind('decimalsEntered', function(e, cents) {
			if (String(cents).length > 2) {
				$(this).css("color", "#FF681D");
				//var errorMsg = 'Please correct your cents (.' + cents + ')';
				//$('<div></div>').html("<p>" + errorMsg + "</p>");
			} else {
				$(this).css("color", "black");
			}
		}); // end currency formatter


		/*
		div summary
		#rethink
			little carat to let people go back and make changes
		#widget_title
			intro thank you text
		#amount_choice
			amount input box
		*/
		
		var lastchecked="";
		var previous_step = "step1";
		// enable button by default
		$("input[name=step2]").removeClass("inactive").prop("disabled", false).addClass("active");
		
		$("input[type=button]").click(function(){
			step = $(this).attr("name");

			switch(step) {
				case "step1":
					//go back to the beginning
					$(".step2, .greatest_needs, .fund_browser").hide();
					$(".step1").show();
					break;
				case "step2":
					//go to step 2
					//previous_step = "step1";
					//$(".step1, .greatest_needs, .fund_browser").hide();
					//$(".step2").show();
										
					var amount = $("input[name=amt]").asNumber();
					if (amount < 10) {
						// check for lte IE9
						if(!Modernizr.input.placeholder){
							$("input[name=amt]").val(10);
						} else {
							$("input[name=amt]").val("").attr("placeholder", "minimum $10");
						}		
						//$(this).addClass("inactive").prop("disabled", true).removeClass("active");					
					} 
					//go to step 2
					previous_step = "step1";
					$(".step1, .greatest_needs, .fund_browser").hide();
					$(".step2").show();
					$("#focus1").focus();
					break;
				case "greatest_needs":
					//show the greatest needs
					previous_step = "step2";
					$(".step2, .fund_browser").hide();
					$(".greatest_needs").show();
					$("#focus2").focus();
					break;
				case "browse_a_list":
					//show the fund browser
					previous_step = "step2";
					$(".step2").hide();
					//$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
					$(".fund_browser").show();
					$("#focus3").focus();					
					
					break;
				case "school-college":
					//show the school and college first dropdown. Hide the campus programs dropdowns.
					
					$(".browserbutton").removeClass("active").addClass("inactive");
					$(this).addClass("active").removeClass("inactive");

					$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
					//$("select[name=browse-school-college-choices]").val("");
					// reset when tabs are clicked to force the user to choose a new fund
					$("select[name=browse-school-college-choices]+span").text("Choose a giving opportunity");

					$(".browserdropdowns").hide();
					$("#school-college-wrapper").show();
					break;
				case "campus-program":
					//show the campus program first dropdown. Hide the school and college dropdowns.

					$(".browserbutton").removeClass("active").addClass("inactive");
					$(this).addClass("active");

					$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
					$("select[name=browse-campus-program-choices]").val("");
					// reset when tabs are clicked to force the user to choose a new fund
					$("select[name=browse-campus-program-choices]+span").text("Choose a giving opportunity");

					$(".browserdropdowns").hide();
					$("#campus-program-wrapper").show().focus();
					break;
				case "search":
					//show the search drawer
					previous_step = "step1";
					break;
				case "done":
					//move on to the online giving page, passing fund and org
					
					if ( !$("#campaigns").prop("checked") ) {
						$("input[name=org]").val("");
					}
					
					//var amount = $("input[name=amt]").asNumber(); // send only a float with 2 decimal places using currencyFormatter function
					var amount = $("input[name=amt]").val();

					window.location.href = 'http://give.berkeley.edu/egiving/index.cfm?amt=' + amount + '&fund=' + $("input[name=fund]").val() + '&org=' + $("input[name=org]").val() ;
					break;
				default:
					
			} 

			
		});
		
		$("input[name=giftselection]").click(function(){
			
			$("input[name=fund]").val( $(this).val() );
			$("input[name=org]").val( $(this).next("label").text() );
			if ($("select[name=campaign-classyear] option:selected").val() != "" ) {
					$("input[name=done]").removeClass("inactive").prop("disabled", false).addClass("active");
				}else {
					$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
				}

		});

		$("select[name=browse-school-college-choices], select[name=browse-campus-program-choices]").change(function(){

			$("input[name=fund]").val(  $("option:selected", this).val() );
			$("input[name=done]").removeClass("inactive").prop("disabled", false).addClass("active");

		});



		function ajaxSearch(unit) {
	
			if (unit == "") {
				return;
			}
	
			return $.ajax({
				url: "http://give.berkeley.edu/ws/ajax_unitsearch.cfm?u=" + unit,
				dataType: "html",
				method: "get"
			});			
		}



		$(".firstdropdown select").on("change", function(){
			//The first dropdown choice has been picked.
			var pickedunit = $("option:selected", this).val();
			var select_element_chosen = $(this);
			$(select_element_chosen).parents(".browserdropdowns").find(".seconddropdown span.select").text("Choose a giving opportunity").removeClass("sel-inactive");

			ajaxSearch(pickedunit).done(function (results) {

				$(select_element_chosen).parents(".browserdropdowns").find(".seconddropdown select").html(results).removeAttr("disabled");
				
				//append a final option that allows a user to visit the unit page
				$(select_element_chosen).parents(".browserdropdowns").find(".seconddropdown select").append('<option class="more-unit-choices" value="' + pickedunit + '">more...</option>');
				//$(select_element_chosen).parents(".browserdropdowns").find(".seconddropdown").show();

			});

			$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
			
		});

		$(".seconddropdown select").on("change", function(){
			
			if ( !isNaN( $("option:selected", this).val() ) ) {
				$("input[name=done]").addClass("inactive").prop("disabled", true).removeClass("active");
				// as a convenience let's bring over the value to the unit page
				var val = $("input[name=amt]").asNumber();
				if (val < 10) {
					var amt = 10;
					$("input[name=amt]").val(amt).formatCurrency();
				}
				amt = $("input[name=amt]").val();
				window.location.href = 'http://give.berkeley.edu/browse/index.cfm?u=' +  $("option:selected", this).val() + '&amt=' + amt;
			}
		});
		
		
		$('input[name=amt]').keyup(function(e){
			//var amount = $(this).val();
			//var number_only = amount.replace(/[\$\,]/g, '');

			var number_only = $(this).asNumber();
			
			var charCode = e.charCode || e.keyCode;

			jl_delay(function(){

				
				if ( number_only > 0 ) {
					
					$("input[name=step2]").removeClass("inactive").prop("disabled", false).addClass("active");

					if(charCode == 13) {
					e.preventDefault();
						$("input[name=step2]").trigger("click");
						return;
					}

				} else {
					
					//$("input[name=step2]").removeClass("active").prop("disabled", true).addClass("inactive");
				}


			}, 100 );

		});


		$("#rethink").click(function(){
		
			//go back a step

			$("input[name=" + previous_step + "]").trigger("click");			
		});


		$(document).keypress(function(e){
			var charCode = e.charCode || e.keyCode;
			
			if(charCode == 13) {
				e.preventDefault();
				return;
			}
		});

		$('.wsearch').click(function() {
			$("html, body").animate({
					scrollTop: 0
			}, 600, function(){
				$("#search").focus();
			});

		});
		
				
	}); // end document ready?
	
	

	function ok() {
		if ($("input[name=fund]").val()) {
			return true;
		} else {
			return false;
		}
	}

	
	//use jl_delay to delay an arbitrary callback for ms time
	var jl_delay = (function(){
		var timer = 0;
		return function(callback, ms){
			clearTimeout (timer);
			timer = setTimeout(callback, ms);
		};
	})();

</script> 
