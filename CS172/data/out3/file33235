      <script>
$(document).ready(function() {

	current_page = "";
	current_page_numbers = "";
	current_totalresults = "";
	
	// FIX FOR IE9 PLACEHOLDER TEXT
	if(!Modernizr.input.placeholder){
		$("#search").val("Search");		
	} // IE9 placeholder 


	// Open the Search mega menu 	
	$("#search").focus(function() {
		//$(this).css("color", "#343b40");
		$(this).addClass("open");
		$(".icon-search").addClass("open");
		$(".results").slideDown();
		$(".nav-plate").slideUp();
		
		// FIX FOR IE9 PLACEHOLDER TEXT
		if(!Modernizr.input.placeholder){
			//console.log("IE9 focus");
			$("input#search").val("");
		} 
		$(".nav-button button").attr("aria-expanded", "false");
		
		
		
	});
	$("#search").submit(function(e){
		e.preventDefault();
	});
		
	//hide any search results
	$(".close-x button").click(function() {
		$(".icon-search").removeClass("open");
		$(".results").slideUp();
		$("#search").removeClass("open").val("");
		
		window.location.hash = ""; // CLEAR THE HASH
		
		if(!Modernizr.input.placeholder){
			//console.log("IE9 close");	
			$("#search").val("Search");
		} 
		//508		
		$(".nav-button button").attr("aria-expanded", "false");
	}).focus("#content");
	
	// this might be causing esc on mega widget to not work
	// CLEAR SEARCH ON ESCAPE
	/*
	$("body").keypress(function(e){
		var keyUnicode = e.charCode || e.keyCode;
		if(keyUnicode == 27) {
			////console.log("esc key pressed");
			$(".close-x button").trigger("click");		}
	});
	*/

	// on mobile the icon would block the search. This focuses search.
	$(".icon-search").click(function(){
		$("#search").focus();
	});
	
			
	$("input[name=search]").on('input', function() {
		// attempt at closing results and removing hash on backspacing to nothing in the search.
		var searchterm = $(this).val();

		jl_delay(function(){

			window.location.hash = searchterm;

		}, 1000 );

	});

	//monitor changes to the hash tag, When it changes, change the search results
	$(window).bind({
		
		hashchange: function(event) {

			//Change the search to match the hash
			hash = location.hash.replace( /^#/, '' );
			
			hash = decodeURIComponent( location.hash.replace( /^#/, '' ) );
			var googleTrack = window.location.pathname + "#" + hash;
			
			if ( hash == "" || hash == "search" || hash == "content" || hash == "navbox" ) {
				return;
			}

			$("#search").val(hash);
			
			if (hash != "") {

				$.ajax({
					url: "http://give.berkeley.edu/lib/inc/ajax_search_results.cfm"
					,dataType: "json"
					,method: "get"
					,data: "searchparam=" + encodeURIComponent(hash)
					,success: function(data) {
	
						displaysearchresults(data);
						
							ga('send', 'pageview', googleTrack);
						
					}
				});			

				$("#search").trigger("input").focus();
			}



		},
		load: function(event) {

			hash = decodeURIComponent( location.hash.replace( /^#/, '' ) );
						
			if ( hash == "" || hash == "search" ) {
				return;
			}

			$("#search").val(hash);

			if (hash != "") {

				$.ajax({
					url: "http://give.berkeley.edu/lib/inc/ajax_search_results.cfm"
					,dataType: "json"
					,method: "get"
					,data: "searchparam=" + encodeURIComponent(hash)
					,success: function(data) {
						displaysearchresults(data);
					}
				});			

				$("#search").trigger("input").focus();
			}


		}
	});


	function displaysearchresults(data) {
		
		current_totalresults = data.TOTALRESULTS;
		
		if (data.TOTALRESULTS < 5) {
			pagesize = data.TOTALRESULTS;
		}else {
			pagesize = 5;
		}
		
		if ( data.TOTALRESULTS == 0 ) {
			$(".showing-x").text("No results found.");
			$("#results-set").text("");
			$(".results-content").slideDown();
			return;
		}

		current_page_numbers = Math.floor( (data.TOTALRESULTS - 1) / pagesize);
		
		$(".showing-x").html('<h2><span id="current-pagination-view">Showing 1 <span class="sr-only">through</span> <span aria-hidden="true">&mdash;</span> ' + pagesize + '</span> of ' + data.TOTALRESULTS + ' Results</h2>');
		
		var results = "";
		var schools_and_colleges = ["30","115","26","54","27","28","91","55","93","32","94","95","50","96"];
		
		$.each(data.RECORDS, function(index, value){
			
			unitid = $.trim(value.UNITID);
			page = Math.floor(index / pagesize);
			
			if ( $.inArray( unitid, schools_and_colleges ) > -1) {
				additionalclass = "college";
			}else {
				additionalclass = "";
			}
			
			if ( value.FUNDCODE == "" ) {
				//This record is a unit. Display as such
				//results += '<li class="unit ' + additionalclass + ' page_' + page + '"><a href="http://give.berkeley.edu/browse?u=' + unitid + '">' + value.UNIT + '</a><br>' +
				//	'<a href="http://give.berkeley.edu/browse?u=' + unitid + '" class="sub link">See all giving opportunities</a></li>';
					
				results += '<li class="unit ' + additionalclass + ' page_' + page + '"><a href="http://give.berkeley.edu/browse?u=' + unitid + '">' + value.UNIT + ' <span class="sr-only">unit giving page</span></a></li>';	

			} else {
				//display a fund
				//funds directly under schools and colleges are displayed slightly differently.
				//some funds have their own fund pages. If so, provide a link
				fundpage = "";
				if (value.HASFUNDPAGE) {
					fundpage = '&nbsp;<a class="fund-details" href="http://give.berkeley.edu/fund?f=' + value.FUNDCODE + '">details <span class="sr-only">about this fund</span></a>';
				}
				results += '<li class="fund page_' + page + '"><a href="http://give.berkeley.edu/egiving/index.cfm?fund=' + value.FUNDCODE + '"><span class="sr-only">Suport</span> ' + value.TITLE + ' <span class="sr-only">now</span></a>' + fundpage + '<br>' + 
					'<a href="http://give.berkeley.edu/browse?u=' + unitid + '" class="sub unit ' + additionalclass + '">' + value.UNIT + ' <span class="sr-only">unit giving page</span></a></li>';
			}
		});
		
		$("#results-set").html("<ul>" + results + "</ul>");
		/* <dl>
		  <dt>Coffee</dt>
		  <dd>- black hot drink</dd>
		  <dt>Milk</dt>
		  <dd>- white cold drink</dd>
		</dl> 
		
		*/
		
		
		
		paginate_results("page_0");

		$(".results-content").slideDown();
		$("#search-help").slideUp();
		$("#results-set").slideDown();
	}

	
	function paginate_results(page) {
		//display a section of results
		$("#results-set li").hide();
		$("#results-set li").each( function(index, value){ 

			if ( $(this).hasClass(page) ) {
				$(value).show();
				current_page = page;
			}
			
		});
		
		
	}

	
	// Open the Navigation menu and hide any search results
	$(".nav-button button").click(function() {
		$("#search").removeClass("open");
		$(".icon-search").removeClass("open");
		$(".nav-plate").slideToggle("slow");
		$(".results").slideUp();

		if ($(this).attr("aria-expanded") == "false"){
			$(this).attr("aria-expanded", "true");
		} else {
			$(this).attr("aria-expanded", "false");
			$("#content").focus();
		}

	});


	$("button.next, button.prev").click(function() {
		var pagenumber = current_page.replace( /page_/, '' );

		if ( $(this).val() == "next" ) {
			//increment the page counter, checking to see if you're at the end
			if (pagenumber == current_page_numbers) {
				return;
			}else {
				current_page = "page_" + ++pagenumber;
				paginate_results(current_page);
			}
		}else {
			//decriment the page counter, checking to see if you're at the beginning
			if (pagenumber == 0) {
				return;
			}else {
				current_page = "page_" + --pagenumber;
				paginate_results(current_page);
			}
		}
		recordbegin = (5 * pagenumber) + 1;
		recordend = (5 * pagenumber) + 5;
		if (recordend > current_totalresults) {
			recordend = current_totalresults;
		}
		$(".showing-x #current-pagination-view").html("Showing " + recordbegin + " <span class=\"sr-only\">through</span> <span aria-hidden=\"true\">&mdash;</span> " + recordend);
		
	});
	// 508 	
	




});// end document ready
</script> 
