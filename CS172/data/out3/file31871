              <div id="MPSearchBlock" role="search"> 
               <script type="text/javascript">

    $(document).ready(function () {

        $('#SearchControl_SearchOptions').click(function (e) {
            e.stopPropagation();
        });

        $('#SearchControl_SearchWhole').click(function () {
            $('#SearchControl_hiddenSearchWhole').click();
        });

        if ('False' == 'False' || 'False' == 'True') {
            $('#SearchControl_SearchOptions').find('#SearchCriteria').hide();
        }

        if ('True' == 'True') {

            if ($('#SearchControl_DateRangeDDL').find(":selected").text() == 'on this day') {
                $('#SearchControl_SearchOptionsEndDate').hide();
            }
            else {
                $('#SearchControl_SearchOptionsEndDate').show();
            }

            $('#SearchControl_WholeSearchTerm').focus(function () {
                $('#SearchControl_SearchOptions').show();
            });

            $('#SearchControl_AdvancedSearchLink').click(function () {
                $('#SearchControl_SearchOptions').find('input[type=checkbox]:checked').removeAttr('checked');
            });

            $('#SearchControl_DateRangeDDL').change(function () {
                if ($('#SearchControl_DateRangeDDL').find(":selected").text() == 'on this day') {
                    $('#SearchControl_SearchOptionsEndDate').hide();
                    $('#SearchControl_SearchOptionsEndDate').val("");
                }
                else {
                    $('#SearchControl_SearchOptionsEndDate').show();
                }
            });
        }
    });

</script> 
