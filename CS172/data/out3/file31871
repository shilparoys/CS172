    function checkMaxLen(txt, maxLen) {
        try {
            if (txt.value.length > maxLen) {
                var cont = txt.value;
                txt.value = cont.substring(0, maxLen);
                return false;
            };
        }
        catch (e) { }
    }
</script> 
