			</script> 
     </div> 
     <script type="text/javascript">
	function checkAdVisibility(iframe,contDiv) {
		var newheight;
        var newwidth;
        
        if(document.getElementById){
        	
        	
            newheight = $('#' + iframe).contents().find("body").height();
            if (newheight> 250 && newheight <300) newheight=250;
            
         	// scrollWidth used because sometimes child element is wider than width/height of body
            newwidth = $('#' + iframe).contents().find("body")[0].scrollWidth;
         	//newwidth = $('#' + iframe).contents().find("body").width();
            if (iframe=='top-banner-iframe' &&  newwidth < 728) {
            	newwidth=728;
            }
            if (iframe=='top-banner-iframe') {
            	newheight=90;
            } 
            if (iframe=='ad_shoppinglinks4-iframe' &&  newwidth > 188) {
            	newwidth=188;
            	$('#' + iframe).contents().find("div").css({"background-color": "#FFF"});
            }
            if (iframe=='ad_right2-iframe') {
            	if ($('#' + iframe).contents().find("div.jag-300x600-expandable").length > 0) {
            		newwidth = 300;
            		newheight = 600;
            	}
            }
        }
        
        $('#' + iframe).height(newheight);
        $('#' + iframe).width(newwidth);
        $('#' + iframe).show();
        
        conDivHeight = $('#'+contDiv).height();
        
        if (newheight > 0 || newwidth > 0 || conDivHeight > 35){
        	$('#' + contDiv).show();
        }
        if (newheight <= 38 && conDivHeight < 35 ){
        	$('#' + contDiv).hide();
        }
	};
	
	function loadAds() {
		for (var i = 0, len = adFunctionArray.length; i < len; i++) {
            adFunctionArray[i]();
        }
	};
	</script> 
