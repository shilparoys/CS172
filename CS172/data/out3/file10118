  <script type="text/javascript" src="/files/scripts/homepage-scroller-touch.js?v=7"></script> 
  <script type="text/javascript" src="/files/scripts/jquery.backstretch.min.js"></script> 
  <script type="text/javascript">

$.fn.setAllToMaxHeight = function(){
return this.height( Math.max.apply(this, $.map( this , function(e){ return $(e).height() }) ) );}

$(document).ready(function() {
	$(window).load(function() {
	$('#homepage-bar-links > ul > li').setAllToMaxHeight()
	});

	$(window).resize(function() {
	$('#homepage-bar-links > ul > li').css('height','auto');
    $('#homepage-bar-links > ul > li').setAllToMaxHeight();
	});

	$(window).bind('scroll',function(e){parallaxScroll();});
 
   	function parallaxScroll(){
   		var scrolledY = $(window).scrollTop();
		$('.backstretch').css('transform','translate(0px, '+((scrolledY*0.6))+'px)');
   	}

});

$('#homepage-bar-links ul li').css('cursor','pointer');
$("#homepage-bar-links ul li").click(function(){
     window.location=$(this).find("a").attr("href"); 
     return false;
});

</script> 
  <!-- Debug: total time - 0.0016171932220459 -->
 </body>
</html>