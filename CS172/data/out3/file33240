  <script type="text/javascript"> 
  $('html').addClass('js');
  $(document).ready(function() { 
    $('#know_show').css('height', 440);
    $('#know_show').css('width', 570);
   
   });
</script> 
  <script type="text/javascript"> 
$( function() { 
       $.ajax({
               type:"GET", 
         cache: false,
               url:"/emergency/status.xml", 
               dataType:"xml", 
               success: function(xml) {
                   var campus_status = $(xml).find("item").attr("ut:status");
                   if (campus_status == "2" ) { 
                       var title = $(xml).find("item title").text();
                       var description = $(xml).find("item description").text();
                       // set the Alert Div title from the RSS <title> tag
                       $("#alert_title a").text( title ).css("color","#333333");
                       // fill in the alert body with the RSS <description>
                       $("div.padding span").html( description );
// make the link color match the other links
$("div.padding span a").css("color", "#000").css("font-weight","bold").css("font-size" , "1em");
                       // reveal the alert box
                       $("#campus_status").show();
$("div.lessPad").hide();
$("div.toggler").hide();
if ($("#its_outage").is(":visible")) {
$("div.item").hide();
$("div.toggler").hide();
}
                   } 
}
           });
});
$( function() { 
        $.ajax({
                type:"GET",
        cache: false,
          url:"/its/emergencyfeed/",
                dataType:"xml", 
                success: function(xml) {
                    its_status = $(xml).find("item").attr("ut:status");
                    if (its_status == "2" ) { 
                        var title = $(xml).find("item title").text();
                        var description = $(xml).find("item description").text();
                        // set the Alert Div title from the RSS <title> tag
                        $("#outage_title a").text( title ).css("color","#333333");
                         // fill in the alert body with the RSS <description>
                       $("div.padding span#outage_text").html( description );
// make the link color match the other links
$("div.padding span#outage_text a").css("color", "#000").css("font-weight","bold").css("font-size" , "1em");
                       // reveal the alert box
                       $("#its_outage").show();
$("div.lessPad").hide();
$("div.toggler").hide();
if ($("#campus_status").is(":visible")) {
$("div.item").hide();
$("div.toggler").hide();
}
                   } 
}
           });
});
</script> 
