   <script type="text/javascript">
(function($) {
	// IX - switch mini calendar to fixed position
	// when user scrolls to top of component
	var $window   = $(window),
		$aside    = $('.calLeftCol aside'),
		aside_top = $aside.offset().top,
		is_fixed  = false;
	
	$window.scroll(function() {
		var pos = aside_top  - $window.scrollTop();
		
		// aside's top margin is 25px
		if (pos < 25 && !is_fixed) {
			$aside.addClass('fixed');
			is_fixed = true;
		}
		if (pos > 25 && is_fixed) {
			$aside.removeClass('fixed');
			is_fixed = false;
		}
	});
	
	// IX - Actions to after each view renders (page loads)
	$('body').bind('calLoad.lwcal', function(e, controller) {
		// register urchin page view
		var page = window.location.pathname + window.location.hash;
		if (page) UH.push(['_trackPageview', page]);
		
		return true;
	})
}(livewhale.jQuery));
</script>   
