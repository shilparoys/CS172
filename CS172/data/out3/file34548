  <script type="text/javascript">
function trackHashChangeAnalytics() {
	var pageHash = window.location.hash.substr(1),
		pageHashArray = pageHash.split("/"),
		trackEventArray = [],
		trackPageViewArray = [],
		trackEventCategory = "unknown",
		trackEventAction = pageHash.toString(),
		trackEventLabel = document.title;
	var trackPageViewObject = new Object();	
	var trackEventObject = new Object();
	var searchTermIndex = pageHashArray.indexOf("search", 1) + 1;
	var isListPage = (pageHashArray.indexOf("view") === 0) ? true : false;
	var isEventPage = (pageHashArray.indexOf("event_id") === 0) ? true : false;
	var isSearchPage = (searchTermIndex > 0) ? true : false;
	var searchTerm = (isSearchPage) ? pageHashArray[searchTermIndex] : "";
	if (isListPage) {
		if (isSearchPage) {
			trackEventCategory = "search";
		} else {
			trackEventCategory = "listing";
		}
	}
	if (isEventPage) {
		trackEventCategory = "event";
	}
	if (isSearchPage) {
		trackEventLabel = searchTerm;
	}
	trackEventArray.push("_trackEvent");
	trackEventArray.push(trackEventCategory);
	trackEventArray.push(trackEventAction);
	trackEventArray.push(trackEventLabel);
	trackEventArray.push(null);
	trackEventArray.push(false);
	trackPageViewArray.push("_trackPageview");
	trackPageViewArray.push(trackEventAction);
	trackPageViewObject.page = trackEventAction;
	trackPageViewObject.title = trackEventLabel;
	trackEventObject.hitType = 'event';
	trackEventObject.eventCategory = trackEventCategory;
	trackEventObject.eventAction = trackEventAction;
	trackEventObject.eventLabel = trackEventLabel;

	// for Classic Google Analytics
	_gaq.push(trackEventArray);
	_gaq.push(trackPageViewArray);

	// for Universal Google Analytics
	__gaTracker('send', 'pageview', trackPageViewObject);
	__gaTracker('send', trackEventObject);
}
window.addEventListener("hashchange", trackHashChangeAnalytics, false);
</script> 
