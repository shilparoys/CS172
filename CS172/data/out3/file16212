           <script type="text/javascript">
			//<![CDATA[
				var screenshot_limits = new Object;
				screenshot_limits[1] = 14;
				screenshot_limits[2] = 14;
				screenshot_limits[3] = 14;
				screenshot_limits[6] = 2;
			
				var screenshot_current = 0;
				var screenshot_fade = 3000;
				var screenshot_speed = 3000;
				var screenshot_started = 0;
				var screenshot_interval = 0;
				
				function bwSwitchScreenshot() {
					if (screenshot_started == 0) {
						screenshot_started = 1;
						screenshot_speed += screenshot_fade;
						clearInterval(screenshot_interval);
						screenshot_interval = setInterval('bwSwitchScreenshot();', screenshot_speed);
					}
					$('#teaser_screenshot').hide();
					$('#teaser_screenshot').css('backgroundImage', 'url(http://www.beep.com/wl/stylesets/1/images/layout/2/homepage-' + screenshot_current + '.jpg)');
					$('#teaser_screenshot').show();
			
					$('#teaser_screenshot_next').hide();
					screenshot_current++; if (screenshot_current > screenshot_limits[2]) screenshot_current = 0;
					$('#teaser_screenshot_next').css('backgroundImage', 'url(http://www.beep.com/wl/stylesets/1/images/layout/2/homepage-' + screenshot_current + '.jpg)');
					$('#teaser_screenshot_next').fadeIn(screenshot_fade);
				}
				screenshot_interval = setInterval('bwSwitchScreenshot();', screenshot_speed);
			//]]>
			</script> 
