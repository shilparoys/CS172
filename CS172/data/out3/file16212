(function() {
	var h = document.createElement('script'); h.type = 'text/javascript';
	h.src = 'http://www.beepworld.de/had/7e5ab6e52fbf3ef8eb3016e9c7a2ddf4.js?cache='+parseInt(Math.random()*99999999);
