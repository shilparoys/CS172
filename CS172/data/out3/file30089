            <script type="text/javascript">
    (function($) {
        "use strict";
        if (document.URL.indexOf('premium') != -1) {
            $('.compass .premium').show();
        } else {
            $('.compass .basic').show();
        }
    }(window.jQuery));
</script> 
            <script type="text/javascript">
    (function($) {
        "use strict";
        $(window.document).ready(function() {
            // Best colleges hero search
            (function() {
                var $collegeSearch = $(".college-hero-search"),
                    $stateDropdown, $searchInput, baseUrl, cannedSearch;
                // window.location.origin does not exist in IE, this adds it
                if (!window.location.origin) {
                    window.location.origin = window.location.protocol + "//" + window.location.hostname + (window.location.port ? ':' + window.location.port: '');
                }
                // This should only run if the college hero search exists
                if ($collegeSearch.length > 0) {
                    $stateDropdown = $('select', $collegeSearch);
                    $searchInput = $('input[type=text]', $collegeSearch);
                    baseUrl = window.location.origin + '/best-colleges';
                    // Limit autocomplete suggestions to selected state.
                    $searchInput.usnAutocomplete({
                        autocompleteOptions: {
                            serviceUrl: function(el, name) {
                                var url = '/college-names',
                                    selected = $stateDropdown.val().toUpperCase();
                                url += '?' + 'location=' + selected;
                                return url;
                            },
                            paramName: 'name',
                            minChars: 3
                        }
                    });
                    // On submit, check if text input is empty.
                    // If so, don't submit the form, and redirect
                    // to the canned state search for the selected
                    // state or to the basic search if state also empty.
                    $collegeSearch.submit(function (event) {
                        var name = $searchInput.val(),
                            state = $stateDropdown.val();
                        if (name === '' && state === '') {
                            event.preventDefault();
                            window.location = baseUrl + '/search';
                        } else if (name === '' && state !== '') {
                            event.preventDefault();
                            cannedSearch = '/' + state;
                            window.location = baseUrl + cannedSearch;
                        } else if (name !== '' && state !== '') {
                            event.preventDefault();
                            cannedSearch = '/' + state;
                            window.location = baseUrl + '/search?name=' + name + '&' + 'location=' + state.toUpperCase();
                        }
                    });
                }
            }());
        });
    }(window.jQuery));
</script> 
