       <p>Thank you for your interest in the University of Wyoming Cowboy Parent Program. Please answer the following questions.</p> 
       <script type="text/javascript">//<![CDATA[
function validate() {
    var valid = true;
                                        var objparent_guardian_full_name_s_ = document.getElementById("parent_guardian_full_name_s__err");
                if (document.getElementById("parent_guardian_full_name_s_").type == "checkbox")
                {
                    var checkboxStatus = false;
                    var myCheckboxes = document.getElementsByName("parent_guardian_full_name_s_[]").length;
                    <!-- Start Todds code here -->
                    if (myCheckboxes > 1){
                        for (var i = 0; i < myCheckboxes; i++){
                            //alert( document.getElementsByName("parent_guardian_full_name_s_[]").item(i).checked);
                            if (document.getElementsByName("parent_guardian_full_name_s_[]").item(i).checked == true) {
                                checkboxStatus = true;
                            }
                        }
                        if (checkboxStatus == false){
                            objparent_guardian_full_name_s_.innerHTML= 'Parent\/Guardian Full Name(s) is required';
                            alert("Parent\/Guardian Full Name(s) is required");
                            return false;
                        }
                    }
                    else {
                        if (document.getElementById("parent_guardian_full_name_s_").checked == false) {
                            alert("Parent\/Guardian Full Name(s) is required");
                            return false;
                        }
                    }
                    <!--End Todds code here -->
                } else {
                    if (document.getElementById("parent_guardian_full_name_s_").value == '') {
                        objparent_guardian_full_name_s_.innerHTML= 'Parent\/Guardian Full Name(s) is required';
                        alert("Parent\/Guardian Full Name(s) is required");
                        return false;
                    }
                }
