                        for (var i = 0; i < myCheckboxes; i++){
                            //alert( document.getElementsByName("city[]").item(i).checked);
                            if (document.getElementsByName("city[]").item(i).checked == true) {
                                checkboxStatus = true;
                            }
                        }
                        if (checkboxStatus == false){
                            objcity.innerHTML= 'City is required';
                            alert("City is required");
                            return false;
                        }
                    }
                    else {
                        if (document.getElementById("city").checked == false) {
                            alert("City is required");
                            return false;
                        }
                    }
                    <!--End Todds code here -->
                } else {
                    if (document.getElementById("city").value == '') {
                        objcity.innerHTML= 'City is required';
                        alert("City is required");
                        return false;
                    }
