  <script type="text/javascript">
            var msg;
            var timeOut;
            function timedMsg(time, grace, txtMsg)
            {
                msg = txtMsg;
                timeOut = (time - grace) * 60000;
                setTimeout("alert(msg)", timeOut);
            }
          </script>
