     <script>
	var equalheight = function(selector) {
			var $items = $(selector),
			calc = function(){
	
				var currentTallest = 0,
					currentRowStart = 0,
					rowDivs = new Array(),
					$el,
					topPosition = 0;
	
				$items.each(function() {
					$el = $(this);
					$($el).height('auto');
					topPostion = $el.position().top;
	
					if(currentRowStart != topPostion) {
						for(currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
							rowDivs[currentDiv].height(currentTallest);
						}
						rowDivs.length = 0; /* empty the array */
						currentRowStart = topPostion;
						currentTallest = $el.height();
						rowDivs.push($el);
					} else {
						rowDivs.push($el);
						currentTallest = (currentTallest < $el.height()) ? ($el.height()) : (currentTallest);
					}
	
					for(currentDiv = 0 ; currentDiv < rowDivs.length ; currentDiv++) {
						rowDivs[currentDiv].height(currentTallest);
					}
				});
			};
	
		$(window).load(calc);
		$(window).resize(calc);
		calc();
	};

	var makeRequestURL = function makeRequestURL(page, template, elementsPerPage, sortBy) {
		var url = "http://" + window.location.host + window.location.pathname;
		var aditionalFiltering = '';
		url +=  "?p=" + page;

		if(template) {
			url += "&t=" + encodeURIComponent(template);
		}
		url += aditionalFiltering;
		if(elementsPerPage) {
			url = updateQueryStringParameter(url,'elementsPerPage',elementsPerPage);
		}
		if(sortBy) {
			url = updateQueryStringParameter(url,'sort',sortBy);
		}
		if('') {
			url += this.appendURL(url, "search=" + encodeURIComponent(''));
		}
		return url;
	};

	var updateQueryStringParameter = function(uri, key, value) {
		var re = new RegExp("([?|&])" + key + "=.*?(&|$)", "i");
		separator = uri.indexOf('?') !== -1 ? "&" : "?";
		if(uri.match(re)) {
			return uri.replace(re, '$1' + key + "=" + value + '$2');
		} else {
			return uri + separator + key + "=" + value;
		}
	};

	var initPagination = function(page, searchString, elementsPerPage, listSize, filter, offerType, aditionalFiltering, tags, template) {
		var filterStr = "";

		if(template) filterStr += "&t=" + template;
		if(searchString) filterStr+="&search=" + searchString;

		filterStr += aditionalFiltering;
		listSize = parseInt(listSize);
		if(listSize > elementsPerPage) {
			var $pagination = $("[data-pagination]");
			$pagination.pagination(listSize, {
				items_per_page: elementsPerPage,
				num_display_entries: 4,
				link_to: "http://" + window.location.host + window.location.pathname + "?p=__id__"+filterStr,
				current_page: page - 1,
				callback: function(p){}
			});

			$pagination.find("a").each(function(){
				$(this).attr('rel', 'nofollow');
			});
		}
	};

	var highlightsExpander = function() {
		var $highlights = $("[data-highlights]");
		$highlights.on("click", "[data-expand]", function(e) {
			var $el = $(this);
			var isMore = ($el.data("expand") === "more");
			$(e.delegateTarget).toggleClass("show", isMore);
		});
	};
$script.ready("jquery", function() {
	equalheight("[data-auto-height]");
});
$script.ready(NC.scriptsBundle.getKeysAsArray(), function(){
	$(function() {
		$("[data-items-per-page]").change(function() {
			window.location.href = makeRequestURL(1, '', $(this).val(), '');
		});

		$("[data-sort-offers]").on("change", function() {
			window.location.href = makeRequestURL(1, '', '11', $(this).val());
		});

		$("[data-layout-switcher-link]").on("click", function() {
			window.location.href = makeRequestURL('', $(this).data("readablename"), '11', '');
		});

		shareOnTwitter();
		shareOnFB();


		highlightsExpander();
		$(".rdOfferImg").on("click", function(){
			var url = $(this).parents(".rdOfferPhoto").data("url");
			return !url ? false : window.location.href = url;
		});
		$(".rdOfferGallery").owlCarousel({
            navigation: true,
            slideSpeed: 300,
            paginationSpeed: 400,
            singleItem: true
		});
	});
});
</script> 
