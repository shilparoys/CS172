   <script>$script.ready(NC.scriptsBundle.getKeysAsArray(),
function(){
 // merchant home page hack
 var ssGetParameterByName = function(name) {
  var match = RegExp("[?&]" + name + "=([^&]*)").exec(window.location.search);
  return match && decodeURIComponent(match[1].replace(/\+/g, " "));
 }
 if(ssGetParameterByName("showRedeemPopUp")) {
  if(smRedeemWidget) {
   if($(".sm-portal").length) {
    $(window).load(function() {
     smRedeemWidget.showRedeemPopUp();
    });
    $(document).ajaxSuccess(function(event, xhr, settings) {
     if(!~settings.url.indexOf("/getSimpleJspContent.action")) return true;
     $("#smRedeemCoupon").val(ssGetParameterByName("showRedeemPopUp"));
    });
   }
  }
 }
});</script> 
  </div> 
