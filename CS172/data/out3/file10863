  <script>
$(document).ready(function () {
    $('#fade').popup({
      transition: 'all 0.3s',
      scrolllock: true
    });
    
    $('#btnsubmit').click(function() {
        var state = $('#local_state').val();
        window.location='http://www.collegescholarships.com/local/'+state+'-scholarships#down';
        $('.fade_close').click();
        return false;
    });
});
</script> 
