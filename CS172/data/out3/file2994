    <script>
jQuery( document ).ready( function( $ ) {
	
	//Test for Cookie
	if($.cookie('fly') == null ) {
		$('.article-end').waypoint(function() {
	  		$('#text-4').animate({
	  			right: "0" 
	  			});
	  	});
		
	}
	

	//set cookie
	$('#newsletter-flyout').click(function() {
        $.cookie('fly', 'yes', { expires: 7000, path: "/", domain: 'wyofile.com' });
        $('#text-4').hide();
        $('#text-4').addClass("displaynone");
    });

    $('newsletter-flyout .button').click(function() {
    	$.cookie('fly', 'yes', { expires: 7000, path: "/", domain: 'wyofile.com'});
    	$('#text-4').hide();
    	$('#text-4').addClass("displaynone");
    });

});
</script> 
