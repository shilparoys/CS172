function change_table_border(id)
{
	if(document.getElementById("HTEvent"+id).className=='HTEvent_new')
	{
		document.getElementById("HTEvent"+id).className='HTEvent';
	}else{
		document.getElementById("HTEvent"+id).className='HTEvent_new';
		}
}
