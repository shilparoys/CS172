    // Can also be used with $(document).ready()
    $(window).load(function() {
        $('.flexslider').flexslider({
        	animation: "slide",
			pauseOnHover: true,
			directionNav: false,
			start: function(){
			    $('.flex-active-slide').click(function(){
                    if ($(this).parent().parent().parent().hasClass('hasmodal')) {
                        //Do nothing since it is a modal
                    } else {
                        window.location.href=$('.flex-active-slide').find('a').attr('href');
                    }
                
			    }).css({'cursor':'pointer'});
			},
			after: function(){
			    $('.flex-active-slide').click(function(){
                    if ($(this).parent().parent().parent().hasClass('hasmodal')) {
                        //Do nothing since it is a modal
                    } else {
                        window.location.href=$('.flex-active-slide').find('a').attr('href');
                    }

			    }).css({'cursor':'pointer'});
			},
		});	    
	});
    
</script> 
