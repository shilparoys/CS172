  <section class="body container"> 
   <h1>Databases for Newspapers</h1> 
   <div id="content"> 
    <!-- InstanceBeginEditable name="content" --> 
    <!-- Tabs Begin --> 
    <div id="tabs"> 
     <ul> 
      <li><a href="/libraries/databases/index.php">Browse/Search</a></li> 
      <li><a href="/libraries/databases/list_az.php?nav=0-9">Browse Titles A-Z</a></li> 
      <li><a href="/libraries/databases/list_category.php" class="active_tab">Browse by Category</a></li> 
     </ul> 
     <div class="pull-right" style="margin-top:-10px;" id="libchat_c1c0ce60c4c1214a7d419920cbf00586"></div>
     <script type="text/javascript" src="//v2.libanswers.com/load_chat.php?hash=c1c0ce60c4c1214a7d419920cbf00586"></script> 
    </div> 
    <!-- Tabs End --> 
    <!-- Database search Begin --> 
    <div id="searchDb"> 
