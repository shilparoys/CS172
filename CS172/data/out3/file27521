							jQuery("#contactUsMessage").keypress(function(e) 
							{
								if (jQuery.browser.msie) 
								{
									e = window.event;
								}
							
								var code = (jQuery.browser.msie) ? e.keyCode : e.which;
								var startPos = jQuery("#contactUsMessage").get(0).selectionStart;
 								var endPos = jQuery("#contactUsMessage").get(0).selectionEnd;
  								var selected = endPos - startPos;
								
								if ((code > 0) && (code != 8) && (selected == 0))
								{
                                	var length = jQuery("#contactUsMessage").val().toString().length;
                                
	                                if (length >= 5000) 
	                                {
										e.returnValue = false;
										if (e.preventDefault) 
										{
											e.preventDefault();
										}
	                               	}
	                            }	
