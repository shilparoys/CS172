  <script type="text/javascript">
				//<![CDATA[

					jQuery.noConflict();

					// Put all your code in your document ready area
					jQuery(document).ready(function($)
					{
						bindViewStateActionOnChange(jQuery("#contactUsSubject"), updateContactUsSubject);
						bindViewStateActionOnKeyup(jQuery("#contactUsSubject"), triggerContactUsSubjectChange);
						jQuery("#localeHidden").val(jQuery("#locale").val());
						
						/*Run the ClearForm() function when the clear button is clicked on*/
						jQuery("#clearBtn").click(function (event) {
	                		event.preventDefault();
							ClearForm();
							//console.log("Clicked on Clear Button");
	                	});
	                	
	                	/*Run the ClearForm() function when the enter key is pressed while focus is set on the clear button*/
	                	jQuery("#clearBtn").keypress(function(event){
							if(event.which == 13){
								event.preventDefault();
								ClearForm();
								//console.log("Enter key was pressed on Clear Button");
							}
						});

					}); // end document.ready

					var updateContactUsSubject = function()
					{
                        toggleContactusOperatingSystemBrowser();
					};
					var triggerContactUsSubjectChange = function() {
						jQuery("#contactUsSubject").change();
					};
					var toggleContactusOperatingSystemBrowser  = function()
					{
						if (isTechnicalProblem ())
						{
                            jQuery("#contactUsOperatingSystem_question").fafsa.show();
                            jQuery("#contactUsBrowser_question").fafsa.show();
                        }
                        else
                        {
                            jQuery("#contactUsOperatingSystem_question").fafsa.hide();
                            jQuery("#contactUsBrowser_question").fafsa.hide();
                        }

					};

	                var isTechnicalProblem = function()
	                {
	                    var cntctSubject = jQuery("#contactUsSubject option:selected").val();
	                    return (cntctSubject == "2");
	                };
	                
	                function ClearForm(){
	                  document.FAFSA.reset();

	                  jQuery("#contactUsFirstName").val('');
	                  jQuery("#contactUsLastName").val('');
					  jQuery("#contactUsEmail").val('');
					  jQuery("#contactUsSubject").val('');
					  jQuery("#contactUsOperatingSystem").val('');
					  jQuery("#contactUsBrowser").val('');
					  jQuery("#contactUsMessage").val('');
                      jQuery("#localeHidden").val(jQuery("#locale").val());

					  toggleContactusOperatingSystemBrowser();
					  //console.log("The Form was Cleared");
					}

			    //]]>
			    </script> 
