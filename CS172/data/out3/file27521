                //
                // The purpose of this is to "unhide" page contents which are place outside of the window
                // to give the hide/show logic time to run first. We may need to tweak this if the order
                // isn't right.
                //
                $("#formBody").removeAttr("style");
                jQuery().fafsa.navLinkBlur();
                
             	// Perform browser-specific fixes
                InitializeBrowserFixes($);
            }); // end jQuery(document).ready
            
            jQuery(window).load(function(){
					CheckBrowser('');
					InitForm('', '', '', '');
			});// end jQuery(window).load
        //]]>
        </script> 
