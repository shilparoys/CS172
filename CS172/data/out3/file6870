    <script type="text/javascript">
function SelectTab(tabindex,count) {
	for (var i = 1; i <= count; i++) {
		var tabitem = document.getElementById('tab-body-' + i);
		var tabheader = document.getElementById('tab-header-' + i);
		if (i == tabindex) {
            tabheader.className = "tab-header-selected";
			tabitem.style.display = 'block';
		}
		else {
            tabheader.className = "tab-header";
			tabitem.style.display = 'none';
		}
	}
	return 0;
}
function SelectCenterTab(tabindex) {
	for (var i = 1; i <= 2; i++) {
		var tabitem = document.getElementById('center-tab-body-' + i);
		var tabheader = document.getElementById('center-tab-header-' + i);
		if (i == tabindex) {
			tabitem.style.display = 'block';
            tabheader.className = "tab-header-selected";
		}
		else {
			tabitem.style.display = 'none';
            tabheader.className = "tab-header";
		}
	}
	return 0;
}
function SelectSpecialNewsTab(tabindex) {
	var low=1;
	var high=6;
	if (tabindex<=3)
	{
		low=1;
		high=3;
	}
	else
	{
		low=4;
		high=6;
	}
	for (var i = low; i <= high; i++) {
		var tabitem = document.getElementById('specialnews-tab-body-' + i);
		var tabheader = document.getElementById('specialnews-tab-header-' + i);
		if (i == tabindex) {
			tabitem.style.display = 'block';
			tabheader.style.background = '#e4e7e9';//'#a4cde7';
		}
		else {
			tabitem.style.display = 'none';
			tabheader.style.background = '#f3f3f3';
		}
	}
	return 0;
}
</script> 
    <script type="text/javascript">
var headline_count;
var headline_interval;
var old_headline = 0;
var current_headline = 0;

$(document).ready(function(){
  headline_count = $("div.headline").size();
  $("div.headline:eq("+current_headline+")").css('top','8px');

  headline_interval = setInterval(headline_rotate,4000); //time in milliseconds
  /*$('#scrollup').hover(function() {
    clearInterval(headline_interval);
  }, function() {
    headline_interval = setInterval(headline_rotate,4000); //time in milliseconds
    headline_rotate();
  });*/
});

function headline_rotate() {
  current_headline = (old_headline + 1) % headline_count; 
  $("div.headline:eq(" + old_headline + ")").animate({top: -30},"slow", function() {
    $(this).css('top','30px');
  });
  $("div.headline:eq(" + current_headline + ")").show().animate({top: 8},"slow");  
  old_headline = current_headline;
}
</script> 
