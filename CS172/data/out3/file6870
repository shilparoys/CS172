	$(document).ready(function () {
		$('#photos-left').galleryView({
			panel_width: 940,
			panel_height: 130,
			transition_speed: 1500,
			transition_interval: 5000,
			nav_theme: 'dark',
			pause_on_hover: true
		});
	});
</script>  
