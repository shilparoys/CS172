var variableslide=new Array()

//variableslide[x]=["path to image", "OPTIONAL link for image", "OPTIONAL text description (supports HTML tags)"]

variableslide[0]=['./bannerhome/8.jpg', './bannerhome/8.jpg', '']
variableslide[1]=['./bannerhome/7.jpg', './bannerhome/7.jpg', '']
variableslide[2]=['./bannerhome/6.jpg', './bannerhome/6.jpg', '']
