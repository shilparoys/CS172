         <span class="thumb-right form_item-medium t-small" style="padding: .65em 10px .65em 0;">in</span> 
         <div class="unwrap"> 
          <input name="bhs-school-name" id="bhs-school-name" type="text" class="form_item-medium clearfield autocomplete" value="school name" rel="school name"> 
         </div> 
        </form> 
        <script>
        (function($) {
            $(document).ready(function () {
                function autocomplete_school_name_widget() {
                    var state = $("#bhs-school-state option:selected").val(),
                        url = 'http://' + window.location.hostname + '/education/best-high-schools/' + state + '/name-autocomplete?timestamp=0&limit=10',
                        redirect_url = 'http://' + window.location.hostname + '/education/best-high-schools/search-bounce';

                    if($.fn.usnAutocomplete) {
                        $('#bhs-school-name').usnAutocomplete({
                            responseFormat: 'newlineList',
                            autocompleteOptions : {
                                serviceUrl: url,
                                paramName: 'q',
                                onSelect: function () {
                                    window.location.href = redirect_url + '?' + $("#search-widget").serialize();
                                }
                            }
                        });
                    }
                }
                var $sel = $("#bhs-school-state");

                if($('.state-name').length) {
                    $('#search-widget .state-select option:contains(' + $('.state-name').html() + ')').attr('selected','selected').change();
                }
                if($('meta[name="current_state_abbreviation"]').length) {
                    $('#search-widget .state-select option:contains(' + $('meta[name="current_state_abbreviation"]').attr('content') + ')').attr('selected','selected').change();
                }

                $sel.change(autocomplete_school_name_widget);
                autocomplete_school_name_widget();
            });
        })(jQuery);
    </script> 
