  <script type="text/javascript">
function recordOutboundLink(link, category, action) {
  try {
    var myTracker=_gat._getTrackerByName();
    myTracker._trackEvent(category, action);
    if(link.target == "_blank"){
      setTimeout(window.open(link.href, null), 100);
    }else{
      setTimeout('document.location = "' + link.href + '"', 100);
    }
  }catch(err){}
}

$(document).ready(function(){
  var a_tags = jQuery("a");
  var out_links = [];
  var my_url = "^https?://" + document.location.host;
  
  for(var i=0; i<a_tags.length; i++){
    try {
      if(!(a_tags[i].href).match(my_url)){
        out_links.push(a_tags[i]);
      }
    }catch (e) {
      continue;
    }
  }
  for(var i=0; i<out_links.length; i++){
      jQuery(out_links[i]).bind("click",
        function(){
          recordOutboundLink(this, 'Outbound Links', this.href.replace(/https?:\/\//, ""));
          return false;
        }
    );
  }
});
</script> 
  <script type="text/javascript">
$(document).ready(function(){
    var subUrl = "";
    if (subUrl != "") {
        $('#grandNavi ul li ul li a[href^="/home///"]').addClass('active');
        if ($('#submenu').length > 0) {
            $('#submenu a').removeClass('active');
            $('#submenu a[href^="' + window.location.pathname + '"]').addClass('active');
            $('#submenu .active').parents('ul').prev('a').addClass('active');
        }

    } else {
        subUrl = "";
        if (subUrl != "") {
            $('#grandNavi ul li ul li a[href^="/home///"]').addClass('active');
           
        } else {
            $('#grandNavi ul li ul li a[href="' + window.location.pathname + '"]').addClass('active');
        }
    }

});
</script> 
