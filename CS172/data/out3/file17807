  <script type="text/javascript">
	jQuery(function() {
		var intDelay = 1800000,
		strQuery = 'tc=ar',
  		strHref = location.href;

		function initRefresh() {
			setTimeout(function () {
				if (strHref.indexOf('?') == -1) {
					location.href = strHref + '?' + strQuery;
				} else {
					if (strHref.indexOf('tc=ar') != -1) {
						location.reload();
					} else {
						location.href = strHref + '&' + strQuery;
					}
				}
			}, intDelay);
		}
		initRefresh();
	});
	</script> 
