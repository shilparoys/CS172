           <script type="text/javascript">
        function isValidLoginEmailAddress(sender, args) {
            if (args.Value.length == 0) {
                sender.innerHTML = ">> Enter Email address";
                sender.errormessage = "Enter Email address";
                args.IsValid = false;
            }
            else if (args.Value.length < 6) {
                sender.innerHTML = ">> Email address must be at least 6 characters";
                sender.errormessage = "Email address must be at least 6 characters";
                args.IsValid = false;
            }
            else {
                var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                var address = document.getElementById("ctl00_cphTalent_txtEmailAddress").value;
                if (reg.test(address) == false) {
                    sender.innerHTML = ">> Email address is invalid";
                    sender.errormessage = "Email address is invalid";
                    args.IsValid = false;
                }
            }
        }


        function isValidPassword(sender, args) {
            sender.innerHTML = "";
            var pattern1 = /^[a-zA-Z0-9-_@!$*&+#.]+$/;
            var pattern2 = /^[\-|_]/;
            var pattern3 = /[\-|_]$/;

            if (args.Value.length == 0) {
                sender.innerHTML = ">> Enter password";
                sender.errormessage = "Enter password";
                args.IsValid = false;
            }
            else {
                if (args.Value.length >= 6) {
                    if (pattern1.test(args.Value) == true && pattern2.test(args.Value) == false && pattern3.test(args.Value) == false) {
                        args.IsValid = true;
                    } else {
                        sender.innerHTML = ">> Invalid characters in password";
                        sender.errormessage = "Invalid characters in password";
                        args.IsValid = false;
                    }
                }
                else {
                    var reenterPasswordValidator = document.getElementById('ctl00_cphTalent_cmpValPassword');
                    if (reenterPasswordValidator != null)
                        reenterPasswordValidator.innerHTML = "";
                    sender.innerHTML = ">> Password must be at least 6 characters";
                    sender.errormessage = "Password must be at least 6 characters";
                    args.IsValid = false;
                }
            }
            if (args.IsValid) {
                var username = document.getElementById("ctl00_cphTalent_txtEmailAddress").value;

                if (args.Value == username) {
                    sender.innerHTML = ">> Email address and Password cannot be the same";
                    sender.errormessage = "Email address and Password cannot be the same";
                    args.IsValid = false;
                }
            }

        }
        
        function EnableActivationFields()
        {
            document.getElementById("ctl00_cphTalent_lblCongratulations").innerHTML = "Activate your account";
            if (document.getElementById("ctl00_cphTalent_hdnNote") != null && document.getElementById("ctl00_cphTalent_hdnNote").value !="")
							document.getElementById("ctl00_cphTalent_lblEmailValidation").innerHTML = "To activate your account, please provide "+  document.getElementById("ctl00_cphTalent_hdnNote").value+" master password <br/><br/>Password:";
            else
							document.getElementById("ctl00_cphTalent_lblEmailValidation").innerHTML = "To activate your account, please provide Focus/Talent master password <br/><br/>Password:";
            document.getElementById("ctl00_cphTalent_btnOpenActivate").style.display = 'none';
            document.getElementById("ctl00_cphTalent_txtMasterPassword").style.display = '';       
            document.getElementById("ctl00_cphTalent_btnActivate").style.display = '';     
            return false;
        }
        
         function isValidFEIN(sender, args) {
            reEIN = /\d{2}-\d{7}/;
            FEIN = document.getElementById("ctl00_cphTalent_txtFEIN");
            var rdbUpdateFEIN = document.getElementById("ctl00_cphTalent_rdlUpdateFEIN");
						var inputArrayDetail = rdbUpdateFEIN.getElementsByTagName('INPUT');
            
            if (inputArrayDetail[0].checked && (args.Value.length == 0 || args.Value == "__-_______")) {
                sender.innerHTML = ">> Enter Federal Employer ID";
                sender.errormessage = "Enter Federal Employer ID";
                args.IsValid = false;
            }
            else if (inputArrayDetail[0].checked && reEIN.test(FEIN.value) == false) {
                sender.innerHTML = ">> Federal Employer ID is not valid";
                sender.errormessage = "Federal Employer ID is not valid";
                args.IsValid = false;
            }
        }
        function EnableFEIN()
        {
					var rdbUpdateFEIN = document.getElementById("ctl00_cphTalent_rdlUpdateFEIN");
					var inputArrayDetail = rdbUpdateFEIN.getElementsByTagName('INPUT');
					if (inputArrayDetail[0].checked)
					{
						document.getElementById("ctl00_cphTalent_txtFEIN").disabled=false ;
					}
					else
					{
						document.getElementById("ctl00_cphTalent_txtFEIN").disabled=true ;
					}
        }

	</script> 
