						   function isValidContactEmailAddress(sender, args) {
									if (args.Value.length == 0) {
											sender.innerHTML = ">> Enter Email address";
											sender.errormessage = "Enter Email address";
											args.IsValid = false;
									}
									else if (args.Value.length < 6) {
											sender.innerHTML = ">> Email address must be at least 6 characters";
											sender.errormessage = "Email address must be at least 6 characters";
											args.IsValid = false;
									}
									else {
											var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
											var address = document.getElementById("ctl00_txtYourEmailAddress");
											if (address !=null && reg.test(address.value) == false) {
													sender.innerHTML = ">> Email address is invalid";
													sender.errormessage = "Email address is invalid";
													args.IsValid = false;
											}
									}
							}
