  <div id="carousel-items-backup" style="display:none"></div> 
  <div id="carousel-controls-backup" style="display:none"></div> 
  <script>
	$('#carousel-items-backup').html($('.carousel-inner').html());
	$('#carousel-controls-backup').html($('.carousel-controls').html());
	var slider;
	$(function(){
		if ( $('.carousel .panel').length > 1 ) {
			slider = $('.carousel-inner .view-content').bxSlider({
				auto: true,
				autoControls: true,
				pause: 5000,
				autoControlsSelector: '.carousel-controls .pause-play',
				prevSelector: '.carousel-controls .prev', 
				nextSelector: '.carousel-controls .next',
				onNextSlide: function(currentSlideNumber, totalSlideQty){
					if ( currentSlideNumber == 0 ) slider.stopShow();
			}
			});
		}
		else {
			slider = $('.carousel-inner .view-content').bxSlider({
				auto: false,
				autoControls: true,
				pause: 5000,
				autoControlsSelector: '.carousel-controls .pause-play',
				prevSelector: '.carousel-controls .prev', 
				nextSelector: '.carousel-controls .next',
				onNextSlide: function(currentSlideNumber, totalSlideQty){
					if ( currentSlideNumber == 0 ) slider.stopShow();
			}
			});
			$('.carousel .carousel-controls .prev').hide();
			$('.carousel .carousel-controls .next').hide();
		}
	}); 
  </script> 
  <script>
	$(window).load(function() {
		
		if ( !isMobileDevice() || $('.hub-tiles').length <= 0) { //Don't use flexslider for programs-events on touch devices like iPhone, iPad
		// The slider being synced must be initialized first
		$('#slider-nav').flexslider({
			animation: "slide",
			controlNav: false,
			slideshow: false,
			itemWidth: 72,
			itemMargin: 20,
			animationLoop: false,
			asNavFor: '#slider'
		});

		$('#slider').flexslider({
			animation: "slide",
			controlNav: false,
			slideshowSpeed: 5000,
			pauseOnHover: true,
			animationLoop: true,
			sync: "#slider-nav",
		});

		var thumbCount = $('#slider-nav .slides li').size();
		if (thumbCount <= 7) {
			$("#slider-nav .flex-direction-nav").hide();
		} 

    $('#slider_fade').flexslider({
			animation: "fade",
			slideshowSpeed: 3000,
			animationSpeed: 300,
			controlNav: false,
			directionNav: false,
			animationLoop: true
		});
		
		}
		else { 
			if ( $("#slider").size()>0 ) {
				$("#slider").removeClass("flexslider");
				$("#slider .slides > li").hide();
				$("#slider .slides > li:first-child").show();
			}
		}

	});
</script> 
  <script>
var prev_width = $(window).width();

 window.onresize = function(event) {
		var w_width = $(window).width();
    if ( (  w_width>=651 && prev_width<=651 ) || (  w_width>=635 && prev_width<=635 ) ||  (  w_width<=651 && prev_width>=651 ) ||  (  w_width<=636 && prev_width>=636 ) ) {
			$('.utility-nav').removeAttr('style');
			prev_width = w_width;
		}
	
		if ( (  w_width>=380 && prev_width<=380 ) || (  w_width>=651 && prev_width<=651 )) {
			if ( slider ) {
				
				$('.carousel-inner').html($('#carousel-items-backup').html());
				$('.carousel-controls').html($('#carousel-controls-backup').html());

				if ( $('.carousel .panel').length > 1 ) {
					slider = $('.carousel-inner .view-content').bxSlider({
						auto: true,
						autoControls: true,
						pause: 5000,
						autoControlsSelector: '.carousel-controls .pause-play',
						prevSelector: '.carousel-controls .prev', 
						nextSelector: '.carousel-controls  .next',
						onNextSlide: function(currentSlideNumber, totalSlideQty){
							if ( currentSlideNumber == 0 ) slider.stopShow();
					}
					});
				}
				else {
					slider = $('.carousel-inner .view-content').bxSlider({
						auto: false,
						autoControls: true,
						pause: 5000,
						autoControlsSelector: '.carousel-controls .pause-play',
						prevSelector: '.carousel-controls .prev', 
						nextSelector: '.carousel-controls .next',
						onNextSlide: function(currentSlideNumber, totalSlideQty){
							if ( currentSlideNumber == 0 ) slider.stopShow();
					}
					});
					$('.carousel .carousel-controls .prev').hide();
					$('.carousel .carousel-controls .next').hide();
				}

			}
			prev_width = w_width;
		}
		
		if ( (  w_width>=851 && prev_width<=851 ) || (  w_width<=850 && prev_width>=850 ) ||
				(  w_width>=836 && prev_width<=836 ) || (  w_width<=835 && prev_width>=835 ) ) {
	
			if ( $('#f-twitter .f-tweets') ) {
				$('#f-twitter').html($('#f-twitter-items-backup').html());
				$('#f-twitter .f-tweets').bxSlider({
				displaySlideQty: 3,
    		prevSelector: '#f-twitter .f-twitter-controls', 
      	nextSelector: '#f-twitter .f-twitter-controls',
					moveSlideQty: 3
				});
			}
			
			if ( $('#f-facebook .f-facebooks') ) {
				$('#f-facebook').html($('#f-facebook-items-backup').html());
				$('#f-facebook .f-facebooks').bxSlider({
				displaySlideQty: 3,
				prevSelector: '#f-facebook .f-facebook-controls', 
					nextSelector: '#f-facebook .f-facebook-controls',
					moveSlideQty: 3
				});
			}
			
			prev_width = w_width;
		}
		//$('.engage h2').html(prev_width+'--'+w_width);
		//$('.logo').html(prev_width+'--'+w_width);
	}
</script> 
  <script type="text/javascript">
var _sf_async_config={};
/** CONFIGURATION START **/
_sf_async_config.uid = 2327;
_sf_async_config.domain = "alumni.harvard.edu"; 
_sf_async_config.sections = "page";		
/** CONFIGURATION END **/

(function(){
  function loadChartbeat() {
    window._sf_endpt=(new Date()).getTime();
    var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src',
       (("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
       "js/chartbeat.js");
    document.body.appendChild(e);
  }
  var oldonload = window.onload;
  window.onload = (typeof window.onload != 'function') ?
     loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();

</script> 
  <!-- Lib for GA Social Button Tracking --> 
  <script src="/sites/all/themes/ahedu2012/a/j/ga_social_tracking.js"></script> 
  <!-- Load Twitter JS-API asynchronously with GA tracking --> 
  <script>
(function(){
var twitterWidgets = document.createElement('script');
twitterWidgets.type = 'text/javascript';
twitterWidgets.async = true;
twitterWidgets.src = 'http://platform.twitter.com/widgets.js';
// Setup a callback to track once the script loads.
twitterWidgets.onload = _ga.trackTwitter;
document.getElementsByTagName('head')[0].appendChild(twitterWidgets);
})();
</script> 
  <!-- Facebook like with GA tracking --> 
  <div id="fb-root"></div> 
  <script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=276219599104954";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script> 
