          <link href="http://thingstodo.buffalonews.com/movie-slider/movie.css" rel="stylesheet" type="text/css"> 
          <link rel="stylesheet" type="text/css" href="http://thingstodo.buffalonews.com/movie-slider/skin.css"> 
          <!-- <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script> --> 
          <script src="http://code.jquery.com/ui/1.10.4/jquery-ui.js"></script> 
          <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css"> 
          <script type="text/javascript" src="http://thingstodo.buffalonews.com/movie-slider/jquery.jcarousel.min.js"></script> 
          <script type="text/javascript">
	jQuery(document).ready(function() {
		jQuery( "#movie-date" ).datepicker({
			autoOpen: true,
			dateFormat: "yy-mm-dd",
			defaultDate: "0",
			changeMonth: true,
			numberOfMonths: 1,
			minDate: 0,
			maxDate: '+2W',
			onClose: function( selectedDate ) {
				jQuery( ".to" ).datepicker( "option", "minDate", selectedDate );
			}
		});
		jQuery('#mycarousel').jcarousel({
			scroll:5
		});
		jQuery('.li-pagination').click(function(e){
			jQuery('#pagination a.active').removeClass("active");
			jQuery(this).addClass('active');
			var scrollAmount = jQuery(this).attr('id').split('-')[2];
			scrollAmount = (scrollAmount * 5) + 1;
			jQuery('#mycarousel').jcarousel('scroll', scrollAmount);
		})
		jQuery('.jcarousel-next').click(function(){;
			if(jQuery(this).attr('disabled') !='true'){
		    	var currentActive = jQuery("#pagination a.active").attr("id");
		    	var nextActive = jQuery('#'+currentActive+'-parent').next().attr('id');
		    	jQuery('#pagination a.active').removeClass("active");
		    	jQuery('#'+nextActive+' a').addClass("active");
		    }
		})
		jQuery('.jcarousel-prev').click(function(){
			if(jQuery(this).attr('disabled') !='true'){
		    	var currentActive = jQuery("#pagination a.active").attr("id");
		    	var nextActive = jQuery('#'+currentActive+'-parent').prev().attr('id');
		    	jQuery('#pagination a.active').removeClass("active");
		    	jQuery('#'+nextActive+' a').addClass("active");
		    }
		})
		jQuery("#movie-date").bind('change',function(){
			jQuery("#ui-datepicker-div").css("display","none");
		})
		jQuery("#movies-widget-form").submit(function() {
			jQuery("#ui-datepicker-div").css("display","none");
			var selectedTheaterID = jQuery('#theater-id').val();
			var selectedDate = jQuery('#date-id').val();
			if(selectedDate=="calendar"){
				selectedDate=jQuery('#movie-date').val();
				selectedDate = selectedDate.replace(/-/g,"");
			}
			var selectedMovie = jQuery('#movies-id').val();

			var movieTimesURL="/wp-content/themes/tbn-2013/display-times.php";
			jQuery('#movie-thumbs-slider').css('display','none');

			if(selectedMovie!="0" && selectedTheaterID=="0"){
				movieTimesURL += "?movie="+selectedMovie+"&date="+selectedDate+"&type=allTheaters";
			}
			else{
				if(selectedMovie=="0" && selectedTheaterID=="0"){
					movieTimesURL += "?full=yes&date="+selectedDate;
				}
				else{
					movieTimesURL += "?theater="+selectedTheaterID+"&date="+selectedDate+"&movie="+selectedMovie;
				}
			}
			jQuery('#movies-widget-container').load(movieTimesURL, function() {
				jQuery('#movies-widget-container').fadeIn(600);

				jQuery('.zebra').each(function(){
					jQuery(this).find('tr.exterior:even').css('background-color','#ffffff')
				});
			});
			//alert(jQuery('#movies-widget-container').height());
			return false;
		});
		jQuery('#date-id').bind('change', function () {
			if( jQuery('#date-id').val() == 'calendar') {
			    jQuery('#date-picker-holder').css("display", "block");
			    jQuery('#date-id').css("display", "none");
			}
			else{
			    jQuery('#date-picker-holder').css("display", "none");
			}
		});
	});
</script> 
