            <script type="text/javascript">
				var multislider;
				jQuery(document).ready(function($){
					updateSlider('http://buffalo.com/slide-xml/?post_type=bn_slideshow&orderby=rand');
				});
				function updateSlider(xmlSource) {

					if(typeof multislider != 'undefined') {
						multislider.destroy();
						$('#multiple-slider').remove();
						$('<div class="advanced-slider BNcontainer category-slider multi-slide-shortcode" id="multiple-slider"></div>').appendTo($('.multislider-container'));

					}

					multislider = jQuery('#ultimate-guide-slider').advancedSlider(
						{slideshow:false,
						width:430,
						height: 230,
						xmlSource:xmlSource,
						responsive: true,
						skin: 'multiple-posts',
						shadow: false,
						effectType: 'swipe',
						pauseSlideshowOnHover: true,
						swipeThreshold: 30,
						slideButtons: false,
						glow: false,
						keyboardNavigation: false,
						border: false,
						thumbnailBorder: false,
						thumbnailGlow: false,
						slideArrowsToggle: false,
						timerAnimation: false,
						thumbnailArrows: false,
						thumbnailType: 'scroller',
						thumbnailWidth: 20,
						thumbnailHeight: 20,
						thumbnailButtons: false,
						thumbnailSwipe: true,
						thumbnailScrollerResponsive: true,
						minimumVisibleThumbnails: 1,
						maximumVisibleThumbnails: 20,
						slideArrowsToggle: false,
						allowScaleUp: true,
						slideshowLoop: true,
						timerAnimation: false,
						timerAnimationControls: false,
						preloadNearbyImages: true,
						disableRightClick:true,
						alignType:'centerCenter',
						slideshowControls: false,
						slideshowControlsToggle: false,
						swipeMouseDrag:false,
						swipeGrabCursor:false
						}
					);
				}
			</script> 
