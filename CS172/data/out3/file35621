        <script type="text/javascript">
var links = new Array();
var pics = new Array();
var index = 0;
var intervalIndex = 0;


links[0] = "http://alumniblog.ucr.edu/do-you-know-a-ucr-alumnus-or-alumna-that-deserves-recognition/";
pics[0] = "http://www.alumni.ucr.edu/image/banners/2015/alumniawards.jpg";
links[1] = "http://www.alumni.ucr.edu/chancellor-receptions";
pics[1] = "http://www.alumni.ucr.edu/image/banners/2015/receptions.jpg";
links[2] = "http://alumni.ucr.edu/travel";
pics[2] = "http://www.alumni.ucr.edu/image/banners/2014/03/travel.jpg";

 


$(document).ready(function(){

$("a#link").attr("href",links[index]);
$("img#pic").attr("src",pics[index]);
$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});

$("a#link0").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 0;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link1").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 1;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link2").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 2;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link3").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 3;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link3").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 3;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link4").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 4;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

$("a#link3").click(function(){
	$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});						
	index = 4;
	$("a#link").attr("href",links[index]);
	$("img#pic").attr("src",pics[index]);
	$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	clearInterval(intervalIndex);	
});

});

function slideFade(){
	// animation to hide pic
	$("img#pic").animate({opacity:0.0},500, function(){
													 
		$("a#link"+index).css({"text-decoration":"none","color":"#2d6cc0"});
		
		if(index == (links.length - 1)){
			index = 0;
		}
		else{
			index = index + 1;
		}
		// switch pic
		$("a#link").attr("href",links[index]);
		$("img#pic").attr("src",pics[index]);	
		
		$("img#pic").animate({opacity:1.0},200);
		$("a#link"+index).css({"text-decoration":"underline","color":"#F1AB00"});
	});	
	
}


$(function() {
    intervalIndex = setInterval( "slideFade()", 6000 );
});
</script> 
