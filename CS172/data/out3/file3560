     <script type="text/javascript">
	
//window.setInterval(both(),10000);	
//myVar = window.setInterval(function(){greeting(); setTimeout(showAlert2(),5000);},10000);	

timmerCount = 1

function startTimer(){
	myVar1 = window.setInterval(function(){doTicketCount();},7000);	
	myVar = window.setInterval(function(){showAlertForMax();},3000);	
}


	

function doTicketCount(){
	loadFragmentInToElement('https://secure.westmont.edu/forms/giving/president_breakfast14/ticket_count', 'numTickets');
	loadTextInToElement(timmerCount,"timmerCount");
	timmerCount++;
}

	
function showAlertForMax(){
	numTicketsNow = document.getElementById('numTickets');
	if((Number(numTicketsNow.innerHTML))>=229){
		alert("Sorry, since you started the form all tickets have been sold.");
		//alert("Sorry, since you started the form all tickets have been sold. "+Number(numTicketsNow.innerHTML));
		myStopFunction();
		}
//	if((Number(numTicketsNow.innerHTML))<100){
		//alert("Welcome, but max NOT reached: "+Number(numTicketsNow.innerHTML));
//		}
	
	
	//alert("Welcome, hmmm...: "+Number(numTicketsNow.innerHTML));
	
}
	
function myStopFunction(){
	clearTimeout(myVar);
	clearTimeout(myVar1);
}		
</script> 
     <script type="text/javascript">
		window.onunload = function(){
