     <script type="text/javascript">
	function calculateTotalCost(){
		var totalCharge = 'Select a ticket number above';
		var ticketNumberOne = document.form1.ticket_type[0];
		var ticketNumberTwo = document.form1.ticket_type[1];
		var ticketNumberThree = document.form1.ticket_type[2];
		var ticketNumberFour = document.form1.ticket_type[3];
		
		
		if (ticketNumberOne.checked) totalCharge = "$125";
		if (ticketNumberTwo.checked) totalCharge = "$250";
		if (ticketNumberThree.checked) totalCharge = "$375";		
		if (ticketNumberFour.checked) totalCharge = "$500";
				
		loadTextInToElement(totalCharge, "total_charge");
	}

	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}

	</script> 
     <script type="text/javascript">

var xmlhttp=false;
/*@cc_on @*/
/*@if (@_jscript_version >= 5)
// JScript gives us Conditional compilation, we can cope with old IE versions.
// and security blocked creation of the objects.
 try {
 xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
 } catch (e) {
 try {
 xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
 } catch (E) {
 xmlhttp = false;
 }
 }
@end @*/
if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
 xmlhttp = new XMLHttpRequest();
}


function loadFragmentInToElement(fragment_url, element_id) {
 	var element = document.getElementById(element_id);
 	element.innerHTML = '<em>Loading ...</em>';
 	xmlhttp.open("POST", fragment_url);
 	xmlhttp.onreadystatechange = 
 		function() {
 			if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
 				element.innerHTML = xmlhttp.responseText;
		 		}
 		}
 	xmlhttp.send(null);
}


	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}
</script> 
     <script type="text/javascript">
	function makeSubmitEnabled(){
		document.getElementById('submit_form').disabled='';
	}
</script> 
