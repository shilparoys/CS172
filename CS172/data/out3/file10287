  <script type="text/javascript">

function fadeout(o) {
	  var f = document.getElementById('fade');
	  if (o>1) {
		//var lcon = document.getElementById('login').innerHTML
		return;
		}
	  //f.style.-moz-opacity=o;
	  f.style.opacity=o;
	  f.style.filter='alpha(opacity='+o*100+')';
          fs = "fadeout("+parseFloat(o+0.1)+")";setTimeout(fs,200);
}
	
function setLoginString() {
		//alert("loaded");
		var fb = document.getElementById('fade');
		if (fb) fadeout(0);
		var userfield=document.getElementById('user');
		if (userfield) userfield.focus();


		var utiglogin=document.getElementById('utiglogin');
		var login=readCookie('UTIG_LOGIN');
		var name=readCookie('UTIG_FULLNAME');

		//alert(name);
		if (!utiglogin) return null;
		if (!name || name == '') {
			utiglogin.innerHTML = "You are not logged in. (<a href='/scripts/nislogin.php'>Login</a>)";
			}
		else {
			//utiglogin.innerHTML = "You are logged in as<br>"+name.replace(/\+/g,' ')+". (<a href='/scripts/nislogout.php'>Logout</a>)<br><a style='font-weight:bold;color:#3E5878' href='/internal/'>UTIG INTRANET</a>";
			utiglogin.innerHTML = "You are logged in as<br>"+name.replace(/\+/g,' ')+". (<a href='/scripts/nislogout.php'>Logout</a>)<br><br><a style='font-weight:bold;color:#3E5878' href='/research/proposals/'>UTIG PROPOSALS</a>";
			}
}

function moveLightBox() {
	   var lb = document.getElementById('light');
	   if (lb) {
		if (document.layers) {
			lb.style.top=window.pageYOffset+0.15*document.body.clientHeight;
			lb.style.left=window.pageXOffset+0.15*document.body.clientWidtH;
		}
		else {
			lb.style.top=document.body.scrollTop+0.15*document.body.clientHeight;
			lb.style.left=document.body.scrollLeft+0.15*document.body.clientWidth;
		}
	   }

}
	
function readCookie(name) {
		var cookiename = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(cookiename) == 0) return c.substring(cookiename.length,c.length);
			}
		return null;
}

function doOnLoads() {
	if(typeof setLoginString == 'function') { setLoginString(); }
	if(typeof initialize == 'function') { initialize(); }
	if(typeof geinit == 'function') { geinit(); }
}	
	window.onload = doOnLoads;
	window.onscroll = moveLightBox;
</script> 
