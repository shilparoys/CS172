   <script type="text/javascript">
//Moved from assets/js/script.js so that php variables can be used
//$(function () {
$(window).load(function(){
	$("#loading").hide("slow");
	//On load functions
	if($('#class-search').length) {
		checkSummerSession();
		loadSubjects($("#term").val());
		//loadCourseNumbers($("#terms").val(),$("#subjects").val());

		$('#term').change(function () {
			checkSummerSession();
			loadSubjects($(this).val());
		});
		$('#subject').change(function () {
			loadCourseNumbers($("#term").val(),$("#subject").val());
		});
	}
});

function checkSummerSession() {
	var sem = $('#term').val().substring(3);
	if (sem == 5) {
		$('#session_container').show('slow');
	} else {
		$('#session_container').hide('slow');
	}
}

function loadSubjects(term) {
	$('#subject').empty();
	$('#subject').append(new Option('-- Processing --', ''));
	var jqxhr = $.getJSON('subjects.load.php?term='+term, function(data) {
        $('#subject').empty();
        $('#subject').append(new Option('Select a subject...', ''));
        $.each(data, function() {
            //$('#subject').append(new Option(this.descr, this.value));
	    $('#subject').append("<option value='"+this.value +"'>"+this.descr+"</option>");
        });
    }).always(function() {
        //console.log(term + "**" + $("#subjects").val());
    	//loadCourseNumbers(term,$("#subjects").val());
    	$('#subject option[value=]').attr('selected', 'selected');
    	loadCourseNumbers($("#term").val(),$("#subject").val());
	}).fail(function( jqxhr, textStatus, error ) {
	    var err = textStatus + ", " + error;
	    console.log( "Request Failed: " + err );
		$('#subject').empty();
		$('#subject').append(new Option('-- An error occurred --', ''));
	});
}

function loadCourseNumbers(term,subject) {
	if (term != '' && subject == '') {
		$('#catalog_nbr').empty();
		$('#catalog_nbr').append(new Option('Select a course number...', ''));
	} else if (term != '' && subject != '') {
		$('#catalog_nbr').empty();
		$('#catalog_nbr').append(new Option('-- Processing --', ''));
		var jqxhr = $.getJSON('subjects.load.php?term='+term+'&subject='+subject, function(data) {
			$('#catalog_nbr').empty();
			$('#catalog_nbr').append(new Option('Select a course number...', ''));
	        $.each(data, function(key, val) {
	            //$('#catalog_nbr').append(new Option(val, val));
		    $('#catalog_nbr').append("<option value='"+val+"'>"+val+"</option>");
	        });
	    }).always(function() {
        	//console.log(term + "**" + $("#subjects").val());
    		//loadCourseNumbers(term,$("#subjects").val());
    		$('#catalog_nbr option[value=]').attr('selected', 'selected');
		});
	}
}
</script> 
