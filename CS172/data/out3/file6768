												<a class="pp_next" href="#">next</a> \
												<a class="pp_previous" href="#">previous</a> \
											</div> \
											<div id="pp_full_res"></div> \
											<div class="pp_details"> \
												<div class="pp_nav"> \
													<a href="#" class="pp_arrow_previous">Previous</a> \
													<p class="currentTextHolder">0/0</p> \
													<a href="#" class="pp_arrow_next">Next</a> \
												</div> \
												<p class="pp_description" align="center"></p> \
												<a class="pp_close" href="#">Close</a> \
											</div> \
										</div> \
									</div> \
								</div> \
								</div> \
							</div> \
							<div class="pp_bottom"> \
								<div class="pp_left"></div> \
								<div class="pp_middle"></div> \
								<div class="pp_right"></div> \
							</div> \
						</div> \
						<div class="pp_overlay"></div>'
				});
                        } else {
				$("a[rel^='prettyPhoto']").each(function(){ $(this).attr('href', $(this).data('mp4file')); });
                        }
   	
  
          // featured content slider
// The slider being synced must be initialized first
function sliderResize() {
	var $docWidth = $(document).width();
	if ($docWidth > 959) {//desktop
		$('.carousel').flexslider({
			animation: "slide",
			controlNav: false,
			directionNav: false,
			animationLoop: false,
			itemWidth: 120,
			asNavFor: '.carousel-slider'
		});

		$('.carousel-slider').flexslider({
			animation: "slide",
			controlNav: false,
                  	animationLoop: true,
                        slideshow: true,
                        slideshowSpeed: 6000,
                        animationSpeed: 600,
			sync: ".carousel"
		});
	}

	if ($docWidth > 767 && $(document).width() < 959) {//tablet
		$('.carousel').flexslider({
			animation: "slide",
			controlNav: false,
			directionNav: false,
			animationLoop: false,
			itemWidth: 94,
			asNavFor: '.carousel-slider'
		});

		$('.carousel-slider').flexslider({
			animation: "slide",
			controlNav: false,
                  	animationLoop: true,
                        slideshow: true,
                        slideshowSpeed: 6000,
                        animationSpeed: 600,
			sync: ".carousel"
		});
	}
}

sliderResize();

$(window).on("resize", sliderResize);

  
          $('iframe[name="google_conversion_frame"]').hide();
	});
