      <div id="profilesCol" class="singleBox"> 
       <div class="parsys singleBox03">
        <div class="multipleprofiles section"> 
         <script type="text/javascript">
	$(document).ready(function() {

			//choose random profile type to display on page load
			$("#ba").removeClass("active");
			$("#baProfile").removeClass("display");						
			var myArray = ['ba', 'ma', 'faculty', 'alumni'];
			var rn = Math.floor(Math.random()*4);
			$("#" + myArray[rn]).addClass("active");
			$("#" + myArray[rn] + "Profile").addClass("display");
			
							   
		$("#profilesNav li").click(function () {
			// Get id of clicked item
			var currentId = $(this).attr("id");
			
			// Remove and Set active class and profiles
			$("#profilesNav li").each(function () {
				if ($(this).attr("id") != currentId) {
					// Remove active class
					$(this).removeClass("active");
					// Hide current profile
					var profileId = "#" + $(this).attr("id") + "Profile";
					$(profileId).removeClass("display");
				} else {
					// Add active class
					$(this).addClass("active");
					// Display active profile
					var profileId = "#" + $(this).attr("id") + "Profile";					
					$(profileId).addClass("display");
				}
			});
		});		
	});	
</script> 
         <div id="profilesHomepage"> 
