  <script type="text/javascript">
        function Search() {
            window.location = "http://www.ucr.edu/search.php?q=" + document.getElementById("search-textbox").value; return false;
        }

                function OnFocus()
                { 
                        document.getElementById("search-textbox").className = "normal";   
                }

                function OnBlur()
                {
                        var textValue = document.getElementById("search-textbox").value;
                        if (textValue.length == 0)
                        {
                                document.getElementById("search-textbox").className = "watermark";
                        }
                        else
                        {
                                document.getElementById("search-textbox").className = "normal";
                        }
                }
      </script>
