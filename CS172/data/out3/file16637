       <!-- **************************** START PAGE TITLE ************************************ --> 
       <h1>National Veterans TEE Tournament</h1> 
       <!-- **************************** END PAGE TITLE ************************************** --> 
      </div> 
      <!--End main content wrapper --> 
      <div id="tier2gallery">
       <!-- begin apple images gallery--> 
       <div id="gallery">
        <!-- begin apple images gallery--> 
        <img src="http://www.va.gov/opa/speceven/tee/slider/images/image-slider-1.jpg" width="740" height="370" alt="Image of Veterans at the Tee games"> 
        <!--
<div id="sliderFrame">
        <div id="slider">
            
