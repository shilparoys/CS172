  <script type="text/javascript">

jQuery(document).ready(function(){
	jQuery(".NewsTabs").tabs({
		show : function(event, ui){
			var $ths = jQuery(this);
			//alert('show'+ui.oldPanel.id);
			jQuery(' > div',$ths).css({'display':''});
			jQuery('#'+ui.panel.id,$ths).css({'display':'block'});
		}
	});

	$.ajax({
		type:'POST',
		url:"htmlfiles/middle/mediacenter_news_latesthome.html",
		beforeSend:function(){
			//$(".latestnews").html('');
		},
		success:function(d){
			$(".latestnews").html(d);
			jQuery(".newsitems").fancybox({
				'overlayShow'	: false,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
		}
	});

	});

jQuery(document).ready(function(){
	jQuery(".Ticker").jCarouselLite({ 	btnPrev: ".Prev", 	btnNext: ".Next", 	auto:3000,speed:8000,
 	visible:	1});
	});




jQuery(document).ready(function(){

jQuery("div.gmail img,#gmailPopup").hover(
		function(){
			jQuery("#gmailPopup").show();
			},
		function(){
				jQuery("#gmailPopup").hide();
			}
		);

jQuery("div.moodle img,#moodlePopup").hover(
		function(){
			jQuery("#moodlePopup").show();
			},
		function(){
				jQuery("#moodlePopup").hide();
			}
		);

	jQuery(".text-input").focus(function(){

		id=jQuery(this).attr('id');
		jQuery('#mssgDiv').empty();
		jQuery("#"+id).val("");
		jQuery("#"+id).addClass("effect");
	});

	jQuery(".text-input").click(function(){
	id=jQuery(this).attr('id');
	jQuery('#mssgDiv').empty();
	jQuery("#"+id).val("");
		jQuery("#"+id).addClass("effect");
	});
	
	jQuery("#reset").click(function(){
		jQuery("#txtuserName").val("Username or email");
		jQuery("#txtpassword").val("password");
		jQuery('#mssgDiv').empty();
	});
	jQuery.fn.login = function()
	{
		var id = jQuery("#txtuserName").val();
		var pass = jQuery("#txtpassword").val();
			data = {userid:id,password:pass}
			jQuery("#signin").ajaxStart(function(){
				jQuery("#processing").html("<img src='images/loader.gif' />");
			});
			jQuery.ajax({
				type:"POST",
				data:data,
				url:"logincheck.php",
				success:function(d){
					jQuery("#processing").empty();
					if(jQuery.trim(d)=="success")
						window.location.href = 'index1.php';
					else
						jQuery("#mssgDiv").html(d);
				}
			});
	}
	jQuery(window).bind("keydown",function (e) {
		if (e.keyCode == 13)
			jQuery.fn.login();
		});
	jQuery('#signin').bind("click",function (e){
		jQuery.fn.login();
	});
});
</script> 
