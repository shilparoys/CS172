  <script>
		$(function(){
			// Set starting slide to 1
			var startSlide = 1;
			// Get slide number if it exists
			if (window.location.hash) {
				startSlide = window.location.hash.replace('#','');
			}
			// Initialize Slides
			$('#slides').slides({
				preload: true,
				preloadImage: 'img/loading.gif',
				generatePagination: true,
				play: 5000,
				pause: 2500,
				hoverPause: true,
				// Get the starting slide
				start: startSlide,
				animationComplete: function(current){
					// Set the slide number as a hash
					//window.location.hash = '#' + current;
				}
			});
		});
	</script> 
  <script type="text/javascript">
$(document).ready(function(){
	
  


/*accordion*/
	//$(".accordion h3:first").addClass("active");
	$(".accordion .whyaud").hide();


	$(".accordion h3").click(function(){
		
		$(this).next(".whyaud").slideToggle("slow")
		.siblings(".whyaud:visible").slideUp("slow");
		
		classname=($(this).attr("class"));
		arr=classname.split(" ");
		if(arr[0]!="")
		classname=arr[0];
		
		if(document.getElementById("span_"+classname))
		{
		if(arr[1]=="active")
		{
		document.getElementById("span_"+classname).style.backgroundPosition="right -51px";
		}
		else	
		{
		document.getElementById("span_"+classname).style.backgroundPosition="right 5px";
		}
		}
		
		$(this).toggleClass("active");
		//background-position: right 5px;
		$(this).siblings("h3").removeClass("active");
	});




	

	jQuery(".newsitems").fancybox({
				'overlayShow'	: false,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});


$(".mantolytebox").fancybox({
				'overlayShow'	: false,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic',
				'type'			:	'inline',
				'onComplete'	:function(){
					
			
						$(".imglytebox").fancybox({
						'overlayShow'	: false,
						'transitionIn'	: 'elastic',
						'transitionOut'	: 'elastic',
						'onComplete'	:function(){
							$("#fancybox-content").prepend('<a href="#mantofestival" class="mantolytebox backtoindex"><span>Back To Index</span></a>');
							$(".mantolytebox").fancybox({
				'overlayShow'	: false,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic',
				'type'			:	'inline'
							});
							
							}
			});
				}

			});

jQuery(".eventlytebox").fancybox({
		'overlayShow'	: false,
		'transitionIn'	: 'elastic',
		'transitionOut'	: 'elastic',
		'type'			: 'iframe',
		'height'		:'450',
		'width'		:'600'
	});

});
			
