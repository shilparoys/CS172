        <script language="javascript">
					function toggle() {
						var ele = document.getElementById("dept-toggle");
						var text = document.getElementById("show-dept");
						if(ele.style.display == "block") {
								jQuery('#dept-toggle, #get-toggle, #connect-toggle').hide();
						}
						else {

								jQuery('#dept-toggle').show();
								jQuery('#get-toggle, #connect-toggle').hide();
						}
					}
					function toggleget() {
						var ele = document.getElementById("get-toggle");
						var text = document.getElementById("show-get");
						if(ele.style.display == "block") {
								jQuery('#dept-toggle, #get-toggle, #connect-toggle').hide();
						} else {
								jQuery('#get-toggle').show();
								jQuery('#dept-toggle, #connect-toggle').hide();
						}
					}
					function toggleconnect() {
						var ele = document.getElementById("connect-toggle");
						var text = document.getElementById("show-connect");
						if(ele.style.display == "block") {
								jQuery('#dept-toggle, #get-toggle, #connect-toggle').hide();
						}
						else {

								jQuery('#connect-toggle').show();
								jQuery('#dept-toggle, #get-toggle').hide();
						}
					}
					</script> 
