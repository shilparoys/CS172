        <script type="text/javascript">
function submitLogin(eventName, eventIndex, validate, async) {
  if (eventName == undefined) eventName = '';
  if (eventIndex == undefined) eventIndex = '';
  if (validate == undefined) validate = false;
  if (async == undefined) async = false;
  if (eventName == 'Login_Submit') validate = true;
  if (validate) {
    if (isEmpty(document.Login.elements['Login.20Name'].value)) {
      alert('Login Name is required.');
      setFocus('Login', 'Login.20Name');
      return false;
    }
  }
  document.Login.elements['_EventName'].value = eventName;
  document.Login.elements['_EventIndex'].value = eventIndex;
  document.Login.elements['_PageState'].value = pageState;
  document.Login.elements['_LastFocus'].value = Login_Focus;
  if (window.pageYOffset) {
    document.Login.elements['_ScrollPosition'].value = window.pageXOffset + ',' + window.pageYOffset;
  }
  else if (document.documentElement && document.documentElement.scrollTop) {
    document.Login.elements['_ScrollPosition'].value = document.documentElement.scrollLeft + ',' + document.documentElement.scrollTop;
  }
  else if (document.body) {
    document.Login.elements['_ScrollPosition'].value = document.body.scrollLeft + ',' + document.body.scrollTop;
  }
  if (eventName != 'Login_Submit') document.Login.submit();
  else return true;
}
</script> 
