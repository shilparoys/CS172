  <script type="text/javascript">


	/*  Parallax Scrolling Effect
	    ----------------------------------------------------------------------------------- */

	$(function() {
		// Sadly, not every browser can participate
		// iOS - freezes rendering while scrolling, so we can't really do the effect.
		// IE - sucks
		var iOS = navigator.userAgent.match(/(iphone|ipod|ipad|MSIE 8.0|MSIE 7.0)/i);

		if (!iOS) {
			var $imageBlock		= $('#brand-blocks'), // The element we want to parallax
				speed			= 15, // Arbitrary number for determining parallax speed. Lower is faster
				$window			= $(window),

				// We can't use background-position-y in stupid browsers, so we have to split the background position
				// string and assume the second value is the y position.
				yOriginPercent	= parseInt($imageBlock.css('background-position').split(' ')[1]);

			// Parallax Animation ______________________________________________________________
			// Finally the good stuff. Adjust the background position at a different rate than the scroll speed
			$window.scroll(function() {
				var yPos = yOriginPercent + ( $window.scrollTop() / speed );
				$imageBlock.css({
					backgroundPosition: 'center '+ yPos + '%'
				});
			});
		}
	});
</script> 
