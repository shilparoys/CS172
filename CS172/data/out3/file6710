  <script type="text/javascript">
    function initMenu(){
        $('.elist-men ul ul').hide();
        $('.elist-men ul li a').click(
            function()
            {
                var checkElement = $(this).next();
                if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
                    checkElement.slideToggle();
                    return false;
                }
                if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
                    $('.elist-men ul ul:visible').slideUp();
                    checkElement.slideDown();
                    return false;
                }
            }
        );
    }
    $(document).ready(function() { $('#eu-mobile-menu').click(function(){ $('.elist-quick').slideUp(); $('.elist-men').slideToggle(); return false; }); initMenu(); });

    function initQuick(){
        $('.elist-quick ul ul').hide();
        $('.elist-quick ul li a').click(
            function()
            {
                var checkElement = $(this).next();
                if((checkElement.is('ul')) && (checkElement.is(':visible'))) {
                    checkElement.slideToggle();
                    return false;
                }
                if((checkElement.is('ul')) && (!checkElement.is(':visible'))) {
                    $('.elist-quick ul ul:visible').slideUp();
                    checkElement.slideDown();
                    return false;
                }
            }
        );
    }
    $(document).ready(function() { $('#eu-mobile-quicklinks').click(function(){ $('.elist-men').slideUp(); $('.elist-quick').slideToggle(); return false; }); initQuick(); });
</script> 
  <!-- B.H.Note: TEST TO REMOVE PAGE ELEMENTS WITH JQUERY --> 
  <script type="text/javascript">
$(document).ready(function(){
	var $window = $(window);
    /* WEB BASED */
    var $headCont = $('#hc-web');
	var $menuNewsContainer = $('.eumn-container');
    var $eventContainer = $('.events-container');
    var $bottomMenu = $('.bottom-menu');
    /* MOBILE BASED */
    var $mheadCont = $('#hc-mobile');
    var $newsMobile = $('.newsm');
    var $eventMobile = $('.eventsm');

	function checkWidth(){
		var windowsize = $window.width();
		var x;
		if(windowsize >= 979){
            $headCont.show();
            $menuNewsContainer.show();
            $eventContainer.show();
            $bottomMenu.show();
            $mheadCont.hide();
            $newsMobile.hide();
            $eventMobile.hide();
		}else{
            $headCont.hide();
            $menuNewsContainer.hide();
            $eventContainer.hide();
            $bottomMenu.hide();
            $mheadCont.show();
            $newsMobile.show();
            $eventMobile.show();
		}
	}
	// execute on load
	checkWidth();
	$(window).resize(checkWidth);
});
</script> 
  <!-- EVENTS CAROUSEL JQUERY FUNCTION --> 
