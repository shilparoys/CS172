			var searchtext = $("#searchtextfield").val();
			if (searchtext.length == 0){
				window.open('http://www.jstor.org/','_blank');
			}else{
				window.open("http://www.jstor.org/action/doBasicSearch?Query="+searchtext+"&acc=on&wc=on&fc=off", '_blank');
			}
		});});
