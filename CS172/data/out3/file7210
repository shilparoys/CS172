                     <input class="inputbox" type="text" name="real" value="enter your name here" onfocus="if(this.value=='enter your name here') this.value='';">
                     <br> 
                     <input class="inputbox" type="text" name="email" value="enter your email address" onfocus="if(this.value=='enter your email address') this.value='';">
                     <br> 
                     <input class="button sub" type="button" style="font-size:9px" onclick="return mlm_m_doSub()" value="">
                     <input style="font-size:9px" type="button" class="button unsub" onclick="return mlm_m_doUnsub()" value=""> 
