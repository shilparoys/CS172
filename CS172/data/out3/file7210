                    <h3>WUA Update</h3> 
                    <script language="javascript">
function mlm_m_doSub()
{
	document.emform.which.value = 'sub';
	mlm_m_check();
}

function mlm_m_doUnsub()
{
	document.emform.which.value = 'unsub';
	mlm_m_check();
}

function mlm_m_check()
{
	var email = document.emform.email.value;
	if (!((email.indexOf(".") > 2) && (email.indexOf("@") > 0)))
	{
		alert('Email address does not appear to be valid');
	}
	else
	{
		document.emform.submit();
	}
}
</script> 
                    <form action="index.php?option=com_ml_goodie&amp;task=sub" method="post" name="emform" id="emform"> 
