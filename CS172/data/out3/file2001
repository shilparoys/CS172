													function checkJavascript()
													{
													    
													    var d=new Date(), f = document.loginForm1;
														var n = f.nickname.value;
														if (n.length>0) {
															f.nickname.value = n = n.replace(/(^\s+)|(\s+$)/g, '');
														}
														
														f.js.value='1';
														
							
														var bGood =((n.length>0) && (f.password.value.length>0));
							
														if (bGood) {
															var obj = document.getElementById("loginID");
															if (obj)
																obj.style.display = 'none';
							}
							
														return bGood;
													}

