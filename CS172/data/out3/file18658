       <script>(function(){var $jsi={'swlb_lag':'',};var swbcnt=0,swlcnt=0,lastLBIndex=0,$=jQuery;$jsi.swlb_lag=$jsi.swlb_lag||9;function createSWB(){var id,name,ad,card=$('.cm-storycard').slice(-4,-3);if(card.data('ad-candidate')){return;}else{card.data('ad-candidate',true);}
id=cmg.harmony.slot('SWB').divId+'-'+swbcnt,name='SWB-card-'+swbcnt,ad=$('<div>',{id:id,'class':'ad-card-content'}).appendTo(card);cmg.harmony.defineSlot({name:name,sizes:cmg.harmony.slot('SWB').sizes,adunit:cmg.harmony.slot('SWB').adunit,breakpoint:'sw-ads',id:id,targeting:{ad_slot:'SWB0'+(((swbcnt-1)%3)+2)},callback:function(event){if(!event.isEmpty){card.addClass('cm-storycard-ad');card.contents().not('.ad-card-content').remove();ad.show();}}});swbcnt+=1;cmg.harmony.show.slot(name);}
