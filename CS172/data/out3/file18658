       <script>(function(){var $jsi={};var story_rail_manager=function($){var $=$||cmg.query,$most_read=$('.cm-most-read'),$most_read_height=$most_read.height(),$outbrain=$('.cm-outbrain-container'),$outbrain_height=$outbrain.height(),$story_content=$('.cm-story-content'),$story_content_height=$story_content.height(),$story_rail=$('.cm-story-rail'),$story_rail_height=$story_rail.height();if($outbrain.length&&$outbrain_height==0){$outbrain_height=800;$story_rail_height+=$outbrain_height;}
