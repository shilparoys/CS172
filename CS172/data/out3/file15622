function goToEvent(eventId,eventDate)
{
	var base_url=document.getElementById("base_url").value;
	//alert(base_url+"index.php?option=com_jtagcalendar&eventId="+eventId);
	window.location = base_url+"index.php?option=com_jtagcalendar&date="+eventDate+"&eventId="+eventId+"&Itemid=153&viewMode=daily";
}
 
