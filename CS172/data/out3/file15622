$s('#my_calendar').datepicker({
		beforeShowDay: function(date) {
			var event = events[date];
			
			if (event) {
				return [true, '', event.text, event.multiEvent, event.eventcount, event.eventId];
			}
			else {
				return [false, 'noevent', '','','',''];
			}
		},
	
	});
});

//$s("#my_calendar").datepicker("setDate", "10/05/2012");
</script> 
