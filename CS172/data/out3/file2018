  <script type="text/javascript">
function callOnPageLoad()
{
var url = window.location.href;
if(url == "https://support.uwyo.edu/")
{
document.getElementById('formContainer').style.display = 'inherit';
}
else{document.getElementById('formContainer').style.display = 'none';}
}
</script> 
