  <script>
(function($){

    // Wait for document ready
    $(document).ready(function(){


        hasMoreCheck();


        // Handle post click
        $('.post-more-toggle').on('click', function(event){
            event.preventDefault();

            
            $(this).closest('.post').toggleClass('post-expanded');

            
          $('.newslettercontainer').isotope( 'layout');
        });
    });

    $(window).on('resize', function(){
        hasMoreCheck();
    });

    function hasMoreCheck(){
        $('.post-content').each(function(){
            if($(this).height() < this.scrollHeight){
                $(this).closest('.post').addClass('post-has-more');
            };
        });

    };

})(jQuery);
</script> 
  <script>
$('.post-more-toggle').on('click', function(event){

    $(document).mouseup(function(e)
    {
        var subject = $(".post-expanded"); 

        if(e.target.class != subject.attr('class')&& !subject.has(e.target).length)
        {
           event.preventDefault();

            $('.post-expanded').find('.post-more-toggle').toggleClass('checked');
            $('.post-expanded').closest('.post').toggleClass('post-expanded');

            

            
          $('.newslettercontainer').isotope( 'layout');
        }
    });
});
</script> 
  <script>
 $('.zipmirror').on('change ', function() {
    $('#zip').val($(this).val());
    $('.zipform').val($(this).val());
    $('[name=zip]').change();

});
$('.emailmirror').on('blur ', function() {
    $('#email').val($(this).val());
    $('.emailform').val($(this).val());
    $('[name=email]').change();

    });
$('.zipmirrormo').on('change ', function() {
    $('#zip').val($(this).val());
    $('[name=zip]').change();

});
$('.emailmirrormo').on('blur ', function() {
    $('#email').val($(this).val());
    $('[name=email]').change();
    });


    </script> 
  <script>
$(document).ready(function (e) {


    
    $(document).on('click', '#btnyay', function () {
    if($('#theforminputs').hasClass('has-success')){
  var subbutnclick = $(this).attr('subbutnclick');
    
    omnijax.event(subbutnclick);
        $.growl("Subscription Saved! <br/> Feel free to keep browsing!", {
    animate: {
        enter: 'animated flipInX',
        exit: 'animated flipOutX',
         delay: 4000,
    }
        });
        $('.header-3-sub').addClass('animated zoomOut');
$("input[id=slid]").val($(this).attr('customfieldname="custom_CUSTOMFIELDS" data-slid-val'));
$("input[id=customfield]").attr('name',($(this).attr('customfieldname')));

$('.post-content').attr('class', 'subscribedalready');
$('.fui-checkbox-unchecked').addClass('iconsactive');
$('.checkbox').removeClass('post-more-toggle');
$('.ppl').addClass('submittime');
$('.accordionnotsub').addClass('hiddenstate');
$('.accordionhassub').removeClass('hiddenstate');
$('.newslettercontainer').isotope( 'layout');
$("input[id=cmd]").val('subscribe');
// $('.ppl').attr('type', 'submit');
 $('.ppl').attr('submittime', 'two'); 
 $(".bv-hidden-submit").trigger("click");
}else
    {$(".bv-hidden-submit").trigger("click")}
});



$(document).on('click', '.tilebtn', function () {
  if($('#theforminputs').hasClass('has-success')){
     var subbutnclick = $(this).attr('subbutnclick');
    
    omnijax.event(subbutnclick);
        $.growl("Subscription Saved! <br/> Feel free to keep browsing!", {
    animate: {
        enter: 'animated flipInX',
        exit: 'animated flipOutX',
         delay: 4000,
    },
        });
        $('.header-3-sub').addClass('animated zoomOut');
$("input[id=slid]").val($(this).attr('data-slid-val'));
$("input[id=customfield]").attr('name',($(this).attr('customfieldname')));
$('.post').removeClass('post-expanded');
$('.post-content').attr('class', 'subscribedalready');
$('.fui-checkbox-unchecked').addClass('iconsactive');
$('.checkbox').removeClass('post-more-toggle');
$('.ppl').addClass('submittime');


$('.newslettercontainer').isotope( 'layout');
$("input[id=cmd]").val('subscribe');
// $('.ppl').attr('type', 'submit');
$('.ppl').attr('submittime', 'two');
 $(this).parentsUntil('.bottomrw').children('.col-xs-offset-2').attr('submittime', 'one');
 $(".bv-hidden-submit").trigger("click");
}else{$(".bv-hidden-submit").trigger("click")}

});

// $(document).on('click', '.subyay', function () {
//   if($('#theforminputs').hasClass('has-success')){
//         $.growl("Subscription Saved! <br/> Feel free to keep browsing!", {
//     animate: {
