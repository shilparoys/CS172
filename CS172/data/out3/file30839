  <script>
 $('a.newsampm').on('click', function(e) {
    var src = $(this).attr('data-src');
    var height = $(this).attr('data-height') || 300;
    var width = $(this).attr('data-width') || 670;
    
    $("#newsampm iframe").attr({'src':src,
                        'height': height,
                        'width': width});
});
$(document).on('click', '.newsampmmodalbtn', function () {
 if($('#theforminputs').hasClass('has-success')){
    $('.header-3-sub').addClass('animated zoomOut');
$('.modal').modal('hide');

$(".tilebtn.newsampmcontainerbtn").trigger("click"); 
$("#newsampmcontainer .checkbox").addClass('checked'); 
$('#newsampmcontainer  .ppl').attr('submittime', 'one');
$("#newsampmmodal").addClass('hidestate');
$(".newsampmsubscribeagain").removeClass('hiddenstate');
}else{$(".bv-hidden-submit").trigger("click")}

});
$('#newsampmsecond').parent().click(function() { 
     $(".newsampmsubscribeagain").children('.checkbox ').toggleClass('checked'); 
 });

$('.newsampmsubscribeagain').click(function() { 
    $("#newsampmsecond").trigger("click");
    return false;
});
$(document).on('click', '.newsampmcontainerbtn', function () { 
$(".newsampmsubscribeagain").addClass('hidestate'); 

});
    
   

</script> 
