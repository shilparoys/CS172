$(document).on('click', '.submittime', function () {
    var subbutnclick = $(this).attr('subbutnclick');
    omnijax.event(subbutnclick);
  

        $.growl("Subscription Saved! <br/> Feel free to keep browsing!", {
            animate: {
                enter: 'animated flipInX',
                exit: 'animated flipOutX',
                delay: 4000,
            }
        });
        $('.post').removeClass('post-expanded');
        $("input[id=cmd]").val('subscribe');
        $("input[id=customfield]").val('nope');
        $("input[id=slid]").val($(this).children('.checkbox').attr('data-slid-val'));
        if($(this).attr('submittime')==='two'){
             $(this).attr('submittime','one');
            $("input[id=cmd]").val('subscribe');
            $("input[id=customfield]").attr('name',($(this).children('.checkbox').attr('customfieldname')));
             $("input[id=customfield]").val('subscription');
            $("input[id=slid]").val($(this).children('.checkbox').attr('data-slid-val'));
            $("#masterform").submit();

        }
        else{
             $(this).attr('submittime','two');
            $("input[id=cmd]").val('unsubscribe');
            $("input[id=customfield]").attr('name',($(this).children('.checkbox').attr('customfieldname')));
            $("input[id=customfield]").val('nope');
            $(this).children('.checkbox').removeClass('checked');
            $("input[id=slid]").val($(this).children('.checkbox').attr('data-slid-val'));
            $("#masterform").submit();
        }
        

  });


 });
 </script> 
  <script>


function onFormSuccess(e) {
 $('#errors').html('');
};

$('#masterform').ready(function() {
    $('#masterform').bootstrapValidator({
            excluded: 'disabled',
            live: 'enabled',
            feedbackIcons: {
            valid: 'glyphicon glyphicon-ok',
            invalid: 'glyphicon glyphicon-remove',
            validating: 'glyphicon glyphicon-refresh'
        },
submitButtons: 'button[type="submit"]',
 fields: {
            zip: {
               onSuccess: function(e, data) {
                        $('.btnyay').addClass('clickmezip');
                        $('.tilebtn').addClass('clickmezip');

                        
                    
                      $('.tilebtn').prop('disabled', false);
                      $(".zipmirror").css({"color":"#2ecc71", "border-color":"#2ecc71", "border-size":"2px"});
                       $(".zipmirrormo").css({"color":"#2ecc71", "border-color":"#2ecc71", "border-size":"2px"});
                       $(".zipmirror").parent('div').addClass('successfulyay');
                       $(".zipmirrormo").parent('div').addClass('successfulyay');
                       $(".zipmirror").parent('div').removeClass('rurow');
                       $(".zipmirrormo").parent('div').removeClass('rurow');
                       $('#errors').find('li[data-field="' + data.field + '"]').remove();
                    },
                     onError: function(e, data) {
                        
                        $('.tilebtn').prop('disabled', true);
                       $(".zipmirror").css({"color":"#e74c3c", "border-color":"#e74c3c", "border-size":"2px"});
                       $(".zipmirror").parent('div').addClass('rurow');
                       $(".zipmirrormo").parent('div').addClass('rurow');
                        $(".zipmirror").parent('div').removeClass('successfulyay');
                       $(".zipmirrormo").parent('div').removeClass('successfulyay');
                        $(".zipmirrormo").css({"color":"#e74c3c", "border-color":"#e74c3c", "border-size":"2px"});
                          var messages = data.bv.getMessages(data.element);

            // Remove the field messages if they're already available
            $('#errors').find('li[data-field="' + data.field + '"]').remove();

            // Loop over the messages
            for (var i in messages) {
                // Create new 'li' element to show the message
                $('<li/>')
                    .attr('data-field', data.field)
                    .wrapInner(
                        $('<a/>')
                            .attr('href', 'javascript: void(0);')
                            .html(messages[i])
                            .on('click', function(e) {
                                // Focus on the invalid field
                                data.element.focus();
                            })
                    )
                    .appendTo('#errors');

                    }
                     data.element
                .data('bv.messages')
                .find('.help-block[data-bv-for="' + data.field + '"]')
                .hide();
                },


                trigger: 'change',
                threshold:4,
                selector:'#zip',
                validators: {
                        regexp: {
                        regexp: /^\d{5}$/,
                        message: 'The US zipcode must contain 5 digits'
                    }
                }
            },
            

            email: {
                 onSuccess: function(e, data) {
                    $('.btnyay').addClass('clickmeemail');
                    $('.tilebtn').addClass('clickmeemail');
                        
                         $('.tilebtn').prop('disabled', false);
                         $(".emailmirror").css({"color":"#2ecc71", "border-color":"#2ecc71", "border-size":"2px"});
                          $(".emailmirrormo").css({"color":"#2ecc71", "border-color":"#2ecc71", "border-size":"2px"});
                          $(".emailmirror").parent('div').addClass('successfulyay');
                          $(".emailmirrormo").parent('div').addClass('successfulyay');
                           $(".emailmirror").parent('div').removeClass('rurow');
                         $(".emailmirrormo").parent('div').removeClass('rurow');
                          $('#errors').find('li[data-field="' + data.field + '"]').remove();


                    },
                    onError: function(e, data) {
                        
                         $(".emailmirror").parent('div').addClass('rurow');
                         $(".emailmirrormo").parent('div').addClass('rurow');
                          $(".emailmirror").parent('div').removeClass('successfulyay');
                          $(".emailmirrormo").parent('div').removeClass('successfulyay');
                        $('.tilebtn').prop('disabled', true);
                      $(".emailmirror").css({"color":"#e74c3c", "border-color":"#e74c3c", "border-size":"2px"});
                        $(".emailmirrormo").css({"color":"#e74c3c", "border-color":"#e74c3c", "border-size":"2px"});
                        var messages = data.bv.getMessages(data.element);

            // Remove the field messages if they're already available
            $('#errors').find('li[data-field="' + data.field + '"]').remove();

            // Loop over the messages
            for (var i in messages) {
                // Create new 'li' element to show the message
                $('<li/>')
                    .attr('data-field', data.field)
                    .wrapInner(
                        $('<a/>')
                            .attr('href', 'javascript: void(0);')
                            .html(messages[i])
                            .on('click', function(e) {
                                // Focus on the invalid field
                                data.element.focus();
                            })
                    )
                    .appendTo('#errors');

                    }
                     data.element
                .data('bv.messages')
                .find('.help-block[data-bv-for="' + data.field + '"]')
                .hide();
                    },

                    onStatus: function(e, data) {
                        // Do something ...
                    },

                trigger: 'change',

                selector:'#email',
                validators: {
                    
                    remote: {
                        message: 'Whoops, that email doesn&#39;t look right',
                        url: 'startup/common-files/js/thefile2.php?email=' + $('#email').val(),
                        type: 'get',
                       

                    }
                }
            }
        }



            })
.on('success.form.bv', function(e) {
    if ($('input#hnyptinput').val().length != 0) {
            return false;
        } else{

            e.preventDefault();
            var $form = $(e.target);
            var bv = $form.data('bootstrapValidator');
            $.post($form.attr('action'), $form.serialize(), function(result) {
            }, 'json');

            $('#zipsecond').val($('#zip').val());
$('#emailsecond').val($('#email').val());
            e.preventDefault();
            var $formtwo = $('#secondform');
            var bv = $formtwo.data('#secondform');
            $.post($formtwo.attr('action'), $formtwo.serialize(), function(result) {
            }, 'json');

            

            $('#zipthird').val($('#zip').val());
            $('#emailthird').val($('#email').val());
            $('.subscribeinmodal.accordion').addClass('hiddenstate');
            $('.accordionhassub').removeClass('hiddenstate');
            
            var $formthree = $('#offersform');

    
            var bv = $formthree.data('#offersform');

            
            $.post($formthree.attr('action'), $formthree.serialize(), function(result) {
                
            }, 'json');
            $('#realdealsslid').attr('value','null');
            $('#realdealscmd').attr('value','null');
            $('#secondformcmd').attr('value','subscribe');
}
$("#finishbtn").removeClass('hiddenfinish');
 });

 });


</script> 
