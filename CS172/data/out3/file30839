  <script>
 $('a.politicsnews').on('click', function(e) {
    var src = $(this).attr('data-src');
    var height = $(this).attr('data-height') || 300;
    var width = $(this).attr('data-width') || 670;
    
    $("#politicsnews iframe").attr({'src':src,
                        'height': height,
                        'width': width});
});
$(document).on('click', '.politicsnewsmodalbtn', function () {
 if($('#theforminputs').hasClass('has-success')){
    $('.header-3-sub').addClass('animated zoomOut');
$('.modal').modal('hide');

$(".tilebtn.politicsnewscontainerbtn").trigger("click"); 
$("#politicsnewscontainer .checkbox").addClass('checked'); 
$('#politicsnewscontainer  .ppl').attr('submittime', 'one');
$("#politicsnewsmodal").addClass('hidestate');
$(".politicsnewssubscribeagain").removeClass('hiddenstate');
}else{$(".bv-hidden-submit").trigger("click")}

});
$('#politicsnewssecond').parent().click(function() { 
     $(".politicsnewssubscribeagain").children('.checkbox ').toggleClass('checked'); 
 });

$('.politicsnewssubscribeagain').click(function() { 
    $("#politicsnewssecond").trigger("click");
    return false;
});
$(document).on('click', '.politicsnewscontainerbtn', function () { 
$(".politicsnewssubscribeagain").addClass('hidestate'); 

});
    
   

</script> 
