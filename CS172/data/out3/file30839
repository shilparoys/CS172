  <script>

$(document).on('click', '.checkboxmodal', function () {

    
    omnijax.event('nwsltr_opt-out_offersandinfo');
  
    if($(this).hasClass('checked')){
       
     $("input[id=customoptout]").attr('value', 'no');
    $("input[id=customoptouttwo]").attr('value', 'no');
     $('.sendmetoo').attr('id', 'offersnogo');
    

    }else{
         $("input[id=customoptout]").attr('value', 'yes');
    $("input[id=customoptouttwo]").attr('value', 'yes');
   $('.sendmetoo').attr('id', 'offersform');
   
    }
   });


</script> 
