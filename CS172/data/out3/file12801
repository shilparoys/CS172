     <!--
    	Custom Codes
    	Date: 12/26/2012
    	Author: Khon
    	Description: Show the crumb text only for TDW pages using Wide No Left Nav page layout. In addition remove
    	the '(None)' item from the Release dropdown list.
    --> 
     <script type="text/javascript">
    	var url = window.location.href.toLowerCase();
    	var searchText = "/pages/nces%20naep/tdw/";
    	if (url.indexOf(searchText) == -1)
    	{
    		// Display mode
			$('#ctl00_PlaceHolderMain_DisplayModePanel1_CrumbTrailDisplay_upMain').hide();
			
			// Edit mode
			$('#ctl00_PlaceHolderMain_EditModePanel2_CrumbTrailEdit_upMain').hide();
			
			$('tr.dummy-TDWRelease').hide();
    	}
    	else
    	{
    		// Make Release as required field. The first list item contains the text '(None)' which
    		// should not be available for the user to select from. We need to remove it.

    		var release = $("select[title='Release'] option[value='0']");
    		if (release.length > 0)
			{
				//12/20/2012 - Khon
				//Remove the '(None)' from the dropdown list
				release.remove();	 
	    	}
	    	
	    	// ***** 8/22/2014 - Mi-kyung *******************************************************************************************//
			// ***** BEGIN: add "Return to TDW Home Page" default link to TDW pages. ************************************************//
			var isEditMode = jQuery("#ctl00_PlaceHolderMain_EditModePanel").length > 0;
		    var emptyLink = jQuery("#ctl00_PlaceHolderMain_EditModePanel_BacktoMainPageLink_ctl00_RichLinkField_EmptyPanel");

		    // When 'Back To Main Page URL and Text' has value,
		    // the inline style "display: none;" is presented in the HTML.
			var hasLink = (emptyLink.css("display") == "none");
					    
		    // Check if the page is in edit mode and the back to main URL has no link value.
		    if (isEditMode && !hasLink)
		    {
		    	var goToTDWHomeText = "Return to TDW Home Page";
		    	var tdwHomeUrl = "/nationsreportcard/tdw"
		    	var linkWrapper = jQuery("#ctl00_PlaceHolderMain_EditModePanel_BacktoMainPageLink_ctl00_RichLinkField_AssetSelectedPanel");
		    	// Hide the link EmptyPanel.
		    	emptyLink.css("display", "none");
		    	// Show the link AssetSelectedPanel.
		    	linkWrapper.css("display", "inline");
		    	// Add "Return to TDW Home Page" default link.
		    	linkWrapper
		    			.find("a.ms-rtestate-write")
			    		.attr({
			    			title: goToTDWHomeText,
			    			href: tdwHomeUrl,
			    		})
			    		.text(goToTDWHomeText);
		    }
		    // ***** END: add "Return to TDW Home Page" default link to the TDW pages. *********************************************** //
	    }    	
    </script> 
     <!--
    	End of Custom Codes    	
    --> 
