     <script type="text/javascript">

	$(document).ready(
		function() {
			var hasNoRightCol = (document.getElementById("RightCol") == null) ||
				("" + $("#RightCol").css("display")) == "none";
			var tdwBanner1 = document.getElementById("TDWBanner1");
			
			var linkpath = $.trim(window.location.pathname.toLowerCase());			
			
			if (linkpath == "/nationsreportcard/")
				$("#socialMediaShareBlock").remove();
			else if (hasNoRightCol && tdwBanner1 == null && linkpath.indexOf("/nationsreportcard/default") < 0)
			{
				$("#socialMediaShareBlock")
				.remove()
				.prependTo("#contentWrapper")
				.css("margin-top", "0");
			}
			else
				$("#socialMediaShareBlock").remove();
		}
	);

	function facebookHandler() {
		try {
			//var popup = window.open('http://www.facebook.com/sharer.php?u=http%3A%2F%2Fnces%2Eed%2Egov%2Fnationsreportcard%2Fabout%2F%3Fsmref%3Dfb','sharer','toolbar=0,status=0,resizable=1,width=626,height=436');
		    //popup.focus();
		    var popup = window.open('http://www.facebook.com/sharer.php?u=http%3a%2f%2fnces.ed.gov%2fnationsreportcard%2fdefault.aspx%3fsmref%3dfb', 'sharer', 'toolbar=0,status=0,resizable=1,width=626,height=436');
		}
		catch(e) { return true; } /* use the href */
		return false; // ignore the href
	}

	function twitterHandler() {
		try {
		    //var popup = window.open('http://twitter.com/intent/tweet?url=http%3A%2F%2Fnces%2Eed%2Egov%2Fnationsreportcard%2Fabout%2F%3Fsmref%3Dtw&amp;related=NAEP_NCES&amp;text=' + encodeURIComponent(document.title),'sharer','toolbar=0,status=0,resizable=1,width=626,height=436');
		    var popup = window.open('http://twitter.com/intent/tweet?url=http%3a%2f%2fnces.ed.gov%2fnationsreportcard%2fdefault.aspx%3fsmref%3dtw&related=NAEP_NCES&text=' + encodeURIComponent(document.title), 'sharer', 'toolbar=0,status=0,resizable=1,width=626,height=436');
			//popup.focus();
		}
		catch(e) { return true; } /* use the href */
		return false; // ignore the href
	}

	function emailHandler() {
		try {
		    //var popup = window.open('http://nces.ed.gov/nationsreportcard/emailshare.asp?dest=http%3A%2F%2Fnces%2Eed%2Egov%2Fnationsreportcard%2Fabout%2F&amp;pageTitle=' + encodeURIComponent(document.title),'sharer','toolbar=0,status=0,resizable=1,width=626,height=320');
		    var popup = window.open('/nationsreportcard/emailshare.aspx?dest=http%3a%2f%2fnces.ed.gov%2fnationsreportcard%2fdefault.aspx&pageTitle=' + encodeURIComponent(document.title), 'sharer', 'toolbar=0,status=0,resizable=1,width=626,height=320');
			//popup.focus();
		}
		catch(e) { return true; } /* use the href */
		return false; // ignore the href
	}
