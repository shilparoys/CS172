                 <script>
										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData44120212').appendTo('div#metaInfoData44120212');
											$('#shareData44120212').appendTo('#moreInfoData44120212');
											$(".shareData44120212").click(function(event){
												if ($('#moreInfoData44120212').is(':visible') && $('#shareData44120212').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData44120212").hide();
												    $("#shareData44120212").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData44120212").show();
												    $("#shareData44120212").show();
												    $("#uploadData44120212").hide();
												}
											});
										   $('#closeUData44120212').click(function(event){				   
												$("#moreInfoData44120212").hide();				    
										   });
										   $('#closeSData44120212').click(function(event){				   
												$("#moreInfoData44120212").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData44120212').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData44120212').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData44120212').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
									</script> 
