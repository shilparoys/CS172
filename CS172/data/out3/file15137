                 <script>
										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData74120212').appendTo('div#metaInfoData74120212');
											$('#shareData74120212').appendTo('#moreInfoData74120212');
											$(".shareData74120212").click(function(event){
												if ($('#moreInfoData74120212').is(':visible') && $('#shareData74120212').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData74120212").hide();
												    $("#shareData74120212").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData74120212").show();
												    $("#shareData74120212").show();
												    $("#uploadData74120212").hide();
												}
											});
										   $('#closeUData74120212').click(function(event){				   
												$("#moreInfoData74120212").hide();				    
										   });
										   $('#closeSData74120212').click(function(event){				   
												$("#moreInfoData74120212").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData74120212').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData74120212').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData74120212').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
									</script> 
