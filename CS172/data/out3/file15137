                 <script>
										$(document).ready(function(){
											/*Funciones para mover el contenido al div del webcontent para IMG*/
											$('div#contenidoMetaInfoData14120212').appendTo('div#metaInfoData14120212');
											$('#shareData14120212').appendTo('#moreInfoData14120212');
											$(".shareData14120212").click(function(event){
												if ($('#moreInfoData14120212').is(':visible') && $('#shareData14120212').is(':visible')) 
												{
													event.preventDefault();
												    $("#moreInfoData14120212").hide();
												    $("#shareData14120212").hide();
												} else {
													event.preventDefault();
												    $("#moreInfoData14120212").show();
												    $("#shareData14120212").show();
												    $("#uploadData14120212").hide();
												}
											});
										   $('#closeUData14120212').click(function(event){				   
												$("#moreInfoData14120212").hide();				    
										   });
										   $('#closeSData14120212').click(function(event){				   
												$("#moreInfoData14120212").hide();				    
										   });
										   var clip = null;
										   clip = new ZeroClipboard.Client();
										   
										   $('#copyData14120212').mouseover(function(event){				 
											   clip.setHandCursor(true);
											   clip.addEventListener('load', function (client, text) {});
											   clip.addEventListener('mouseOver', function (client) {
												   clip.setText( $('#tocopyData14120212').val());
												});	
									           clip.glue(this);		           		      
									           clip.addEventListener('complete', function(client, text) {
									        	   nuevoClip();
									        	   $('#tocopyData14120212').select();
									           });
											});			   
										   function nuevoClip(){	
											  /*alert("copiado");*/
											   if(clip != null){					  
											   		clip.destroy();
											   		clip = new ZeroClipboard.Client();
											   }		    
										   };  
										
										});
									</script> 
