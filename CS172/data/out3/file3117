  <script>
                    springSpace.Common = springSpace.Common || { };
                        
                    springSpace.Common.constant = {
                            PROCESSING: {
                                ACTION_DISPLAY_POLL: 159
                            }
                    };
                    
                    springSpace.publicObj.loadGuideCounts=function(config) {
                        xhr = jQuery.ajax({
                            url: "index_process.php?action=" + config.action + "&site_id=" + config.site_id + "&group_id=" + config.group_id + "&key=" + key,
                            type: "GET",
                            dataType: "json",
                            success: function(response, textStatus, jqXHR) {
                                if ( response.errCode == 200 ) {
                                    var count_type = "";
										switch (config.action) {
											case 176:
												count_type = "guidetype";
												break;
											case 177:
												count_type = "group";
												break;
											case 178:
												count_type = "owner";
												break;
										}
										
                                    jQuery.each(response.data, function(idx, elt) {
											jQuery("#s-lg-index-" + count_type + "-" + elt.id).show();
											jQuery("#s-lg-index-" + count_type + "-count-" + elt.id).html(elt.count);
                                    });
                                } else {
                                    // Show error message.
                                    
                                }
                            },
                            error: function (jqXHR, textStatus, errorThrown) {
                                // error
                                alert("Oops, sorry! Something unexpected happened: " + errorThrown + " \n\nThat might not mean much to you, but it probably does to the Springy Techs...you can let them know at support@springshare.com.");
                            }
                        });
                    }
                                            
                    springSpace.homepage = springSpace.homepage || {
                        default_list_order: 0,
                        current_list: 172,
                        current_btn_id: "s-lg-index-subject-btn",
                        current_num_cols: 3,
                        current_type_label: ""
                    };
                    
                    jQuery(function() {
						// Clear out the search term from previous search/back button.
						jQuery("#s-lg-guide-search").val("");
						
						// Load guide list.
						springSpace.publicObj.loadHomepageList({
                            action: 172,
                            group_id: "0",
                            order: springSpace.homepage.default_list_order,
                            num_cols: 3,
                            elt_id: "s-lg-index-list",
                            button_id: "s-lg-index-subject-btn",
                            type_label: "Subjects",
							display_sort: true
                        });

                        // Select default dropdown option.
                        jQuery("#s-lg-guide-order > option[value=" + springSpace.homepage.default_list_order + "]").prop("selected",true);
                        
                        // Load guide type counts.
                        springSpace.publicObj.loadGuideCounts({
                            action: 176,
                            site_id: 235,
                            group_id: 0
                        });
                        
                        // Load group guides counts.
                        springSpace.publicObj.loadGuideCounts({
                            action: 177,
                            site_id: 235
                        });
						
						   // Load owner group counts.
                        springSpace.publicObj.loadGuideCounts({
                            action: 178,
                            site_id: 235,
							   group_id: 0
                        }	);
						
                    });springTrack.init({_st_site_id: '235'});
                        jQuery(function() {springTrack.trackPage({_st_type_id: '11'});
                        });
                </script> 
