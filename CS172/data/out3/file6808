               <script type="text/javascript">

jQuery(document).ready(function(){
  jQuery.ajax({
    dataType: 'jsonp',
    url: 'https://shop.haaga-helia.com/epages/Kaupat.sf/fi_FI/?ObjectPath=/Shops/Haaga/Categories/&ViewAction=JSONGetVilkasProducts',
    success: function(data){
       jQuery.each(data.Products, function (index, value) {

            var elementID = this.Alias;
            jQuery("#slider-landing-page-product-carousel").append( '<li id="' + elementID + '"></li>' );

            if (this.Image) {
              jQuery("#"+elementID).append('<div class="image"><img src="' + this.Image + '" alt="" width="100"></div>');
            }
if (this.Prices) {
          jQuery("#"+elementID).append('<div class="item-description"><b style="font-weight:bold; font-size:13px;">'+ this.NameOrAlias.fi + '</b><p style="margin-top:6px; font-size:13px;">' + this.Prices.EUR + ' &euro; </p><a href="' + this.WebUrlSSL + '">Buy</a></div>');
}else{
jQuery("#"+elementID).append('<div class="item-description"><b style="font-weight:bold; font-size:13px;">'+ this.NameOrAlias.fi + '</b><br/><br/><a href="' + this.WebUrlSSL + '">Buy</a></div>');
}

       });
    }
  });
});

(function ($) {
  Drupal.behaviors.flexisel_carousel_products = {
    attach: function (context, settings) {
      $(window).load(function() {
jQuery("#block-haaga-helia-landing-pages-landing-page-product-carousel").css("display","block");
        $("#slider-landing-page-product-carousel").flexisel({
          visibleItems: 1,
          enableResponsiveBreakpoints: true,
          responsiveBreakpoints: {
            portrait: {
              changePoint:480,
              visibleItems: 1
            },
            landscape: {
              changePoint:640,
              visibleItems: 1
            },
            tablet: {
              changePoint:768,
              visibleItems: 1
            }
          }
        });
      });
    }
  };
})(jQuery);
</script> 
