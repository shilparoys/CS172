}


$pathsvg = $("#pathsvg");

$pathsvg.waypoint(function(event, direction) {
	$pathsvg.waypoint('remove');


	var t;
	function easeInCubic(x, t, b, c, d) {
		return c*(t/=d)*t*t + b;
	}

	d = new Date()
	starttime = d.valueOf();
	duration = 750;
	$c = $('#circleclip');
	
	t = window.setInterval(function(){
		n = new Date();
		//null, current_time, start_value, end_value, total_time)
		$c.attr('r',easeInCubic(null, (n.valueOf() - starttime), 0, 700, duration));
		if ((n.valueOf() - starttime) >= duration){
			window.clearTimeout(t);
		}					
	}, 8);	
},{
		offset: '30%'
	}
);

})
