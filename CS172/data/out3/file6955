function GLb01094_hideCode() {

	GL_getObjCss("Menub010941").visibility = "hidden";
	GL_getObjCss("Menub010942").visibility = "hidden";
	GL_getObjCss("Menub010943").visibility = "hidden";
	GL_getObjCss("Menub010944").visibility = "hidden";
