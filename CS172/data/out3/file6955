	switch(mitm_id)	{
case "Menu8f53a90":changeBGcolor('audMenu231414','#FFFFFF','#A39161');; break;
case "Menu8f53a91":changeBGcolor('audMenu23261','#FFFFFF','#A39161');; break;
case "Menu8f53a92":changeBGcolor('audMenu227739','#FFFFFF','#A39161');; break;
case "Menu8f53a93":changeBGcolor('audMenu222212','#FFFFFF','#A39161');; break;
case "Menu8f53a94":changeBGcolor('audMenu24104','#FFFFFF','#A39161');; break;
case "Menu8f53a95":changeBGcolor('audMenu2677','#FFFFFF','#A39161');; break;
case "Menu8f53a96":changeBGcolor('audMenu2676','#FFFFFF','#A39161');; break;
case "Menu8f53a97":changeBGcolor('audMenu220099','#FFFFFF','#A39161');; break;
case "Menu8f53a98":changeBGcolor('audMenu24040','#FFFFFF','#A39161');; break;
	}
	GLV_currentROitem["8f53a9"]=mitm_id;
}
function GL8f53a9_out(mitm_id)	{
	if (!mitm_id)	mitm_id=GLV_currentROitem["8f53a9"];
	switch(mitm_id)	{
case "Menu8f53a90":changeBGcolor('audMenu231414','#807F83','#FFFFFF');; break;
case "Menu8f53a91":changeBGcolor('audMenu23261','#807F83','#FFFFFF');; break;
case "Menu8f53a92":changeBGcolor('audMenu227739','#807F83','#FFFFFF');; break;
case "Menu8f53a93":changeBGcolor('audMenu222212','#807F83','#FFFFFF');; break;
case "Menu8f53a94":changeBGcolor('audMenu24104','#807F83','#FFFFFF');; break;
case "Menu8f53a95":changeBGcolor('audMenu2677','#807F83','#FFFFFF');; break;
case "Menu8f53a96":changeBGcolor('audMenu2676','#807F83','#FFFFFF');; break;
case "Menu8f53a97":changeBGcolor('audMenu220099','#807F83','#FFFFFF');; break;
case "Menu8f53a98":changeBGcolor('audMenu24040','#807F83','#FFFFFF');; break;
	}
}

function GL8f53a9_getMouse(e) {
	if (GLV_menuOn["8f53a9"]!=null && !GLV_dontFollowMouse["8f53a9"]){

	}
	GL_mouseMoveEvaluate("8f53a9");
}
function GL8f53a9_hideCode() {

GL8f53a9_out("");
}
function GL8f53a9_doTop(WMid,id) {
var parentObject = GL_getObj(GLV_menuXY[WMid][id][2]);
		var TI_width = parentObject.width;
		var TI_height = parentObject.height;
		var anchorObj = GL_getObj(GLV_menuXY[WMid][id][3]);
		var TI_x = anchorObj.x;
		var TI_y = anchorObj.y;
			
GLV_menuOn["8f53a9"].left = (GLV_curLayerX["8ae408"]+TI_x+163)+"px";
GLV_menuOn["8f53a9"].top =  (GLV_curLayerY["8ae408"]+TI_y+parseInt(document.getElementById(GLV_menuXY[WMid][id][2]).parentNode.parentNode.style.marginTop.replace('px',''))+-6)+"px";






	if(document.getElementById(id).firstChild){
							var scrolledVal = 0;
							if(typeof(window.pageYOffset)=='number'){scrolledVal=window.pageYOffset;}else{scrolledVal=document.body.scrollTop;}
							var menuFreeSpace = document.body.parentNode.clientHeight+scrolledVal-parseInt(GLV_menuOn["8f53a9"].top.replace('px',''));
							if(menuFreeSpace < document.getElementById(id).firstChild.firstChild.offsetHeight){								
								document.getElementById(id).firstChild.style.height = menuFreeSpace+'px';
							}
							tMenuLay_PrepareHideScroll(document.getElementById(id).firstChild);
						}
}
function GL8f53a9_restoreMenu() {

}
function GL8f53a9_resetSubMenus() {
	var resetSubMenu=1;

	return resetSubMenu;
}

