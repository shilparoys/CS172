GLV_menuXY["674070"]["Menu6740703"] = new Array(0,0,"itemID287d927776","anchorID287d927776");
GLV_menuXY["674070"]["Menu6740704"] = new Array(0,0,"itemID3432d592b0","anchorID3432d592b0");

// Alternative rollover/out functions for use with GMENU_LAYER
function GL674070_over(mitm_id)	{
	GL674070_out("");	// removes any old roll over state of an item. Needed for alwaysKeep and Opera browsers.
	switch(mitm_id)	{
case "Menu6740700":changeBGcolor('aboutMenu1557','transparent','#FFFFFF',1);; break;
case "Menu6740701":changeBGcolor('aboutMenu1556','transparent','#FFFFFF',1);; break;
case "Menu6740702":changeBGcolor('aboutMenu11549','transparent','#FFFFFF',1);; break;
case "Menu6740703":changeBGcolor('aboutMenu1555','transparent','#FFFFFF',1);; break;
case "Menu6740704":changeBGcolor('aboutMenu16419','transparent','#FFFFFF',1);; break;
	}
	GLV_currentROitem["674070"]=mitm_id;
}
function GL674070_out(mitm_id)	{
	if (!mitm_id)	mitm_id=GLV_currentROitem["674070"];
	switch(mitm_id)	{
case "Menu6740700":changeBGcolor('aboutMenu1557','transparent','#FFFFFF',0);; break;
case "Menu6740701":changeBGcolor('aboutMenu1556','transparent','#FFFFFF',0);; break;
case "Menu6740702":changeBGcolor('aboutMenu11549','transparent','#FFFFFF',0);; break;
case "Menu6740703":changeBGcolor('aboutMenu1555','transparent','#FFFFFF',0);; break;
case "Menu6740704":changeBGcolor('aboutMenu16419','transparent','#FFFFFF',0);; break;
	}
}

function GL674070_getMouse(e) {
	if (GLV_menuOn["674070"]!=null && !GLV_dontFollowMouse["674070"]){

	}
	GL_mouseMoveEvaluate("674070");
}
function GL674070_hideCode() {

	GL_getObjCss("Menu6740700").visibility = "hidden";
	GL_getObjCss("Menu6740701").visibility = "hidden";
	GL_getObjCss("Menu6740702").visibility = "hidden";
	GL_getObjCss("Menu6740703").visibility = "hidden";
	GL_getObjCss("Menu6740704").visibility = "hidden";
GL674070_out("");
GL_hideAll("2351cf");
GL_hideAll("600e69");
GL_hideAll("0c9ada");
GL_hideAll("ec3ab6");
GL_hideAll("d2375e");
}
function GL674070_doTop(WMid,id) {
var parentObject = GL_getObj(GLV_menuXY[WMid][id][2]);
		var TI_width = parentObject.width;
		var TI_height = parentObject.height;
		var anchorObj = GL_getObj(GLV_menuXY[WMid][id][3]);
		var TI_x = anchorObj.x;
		var TI_y = anchorObj.y;
			
GLV_menuOn["674070"].left = (TI_x+175)+"px";
GLV_menuOn["674070"].top =  (TI_y+60-GL_getObj(id).height)+"px";






	if(document.getElementById(id).firstChild){
							var scrolledVal = 0;
							if(typeof(window.pageYOffset)=='number'){scrolledVal=window.pageYOffset;}else{scrolledVal=document.body.scrollTop;}
							var menuFreeSpace = document.body.parentNode.clientHeight+scrolledVal-parseInt(GLV_menuOn["674070"].top.replace('px',''));
							if(menuFreeSpace < document.getElementById(id).firstChild.firstChild.offsetHeight){								
								document.getElementById(id).firstChild.style.height = menuFreeSpace+'px';
							}
							tMenuLay_PrepareHideScroll(document.getElementById(id).firstChild);
						}
}
function GL674070_restoreMenu() {

GL_restoreMenu("2351cf");
GL_restoreMenu("600e69");
GL_restoreMenu("0c9ada");
GL_restoreMenu("ec3ab6");
GL_restoreMenu("d2375e");
}
function GL674070_resetSubMenus() {
	var resetSubMenu=1;

if (!GLV_doReset["2351cf"] && GLV_currentLayer["2351cf"]!=null)	resetSubMenu=0;
if (!GLV_doReset["600e69"] && GLV_currentLayer["600e69"]!=null)	resetSubMenu=0;
if (!GLV_doReset["0c9ada"] && GLV_currentLayer["0c9ada"]!=null)	resetSubMenu=0;
if (!GLV_doReset["ec3ab6"] && GLV_currentLayer["ec3ab6"]!=null)	resetSubMenu=0;
if (!GLV_doReset["d2375e"] && GLV_currentLayer["d2375e"]!=null)	resetSubMenu=0;
	return resetSubMenu;
}

GLV_timeout_pointers[GLV_timeout_count] = "674070";
GLV_timeout_count++;

