// Alternative rollover/out functions for use with GMENU_LAYER
function GL600e69_over(mitm_id)	{
	GL600e69_out("");	// removes any old roll over state of an item. Needed for alwaysKeep and Opera browsers.
	switch(mitm_id)	{
case "Menu600e690":changeBGcolor('audMenu227981','#FFFFFF','#A39161');; break;
case "Menu600e691":changeBGcolor('audMenu225123','#FFFFFF','#A39161');; break;
case "Menu600e692":changeBGcolor('audMenu227493','#FFFFFF','#A39161');; break;
case "Menu600e693":changeBGcolor('audMenu220142','#FFFFFF','#A39161');; break;
case "Menu600e694":changeBGcolor('audMenu2706','#FFFFFF','#A39161');; break;
case "Menu600e695":changeBGcolor('audMenu223016','#FFFFFF','#A39161');; break;
case "Menu600e696":changeBGcolor('audMenu223092','#FFFFFF','#A39161');; break;
case "Menu600e697":changeBGcolor('audMenu2707','#FFFFFF','#A39161');; break;
case "Menu600e698":changeBGcolor('audMenu2709','#FFFFFF','#A39161');; break;
case "Menu600e699":changeBGcolor('audMenu2711','#FFFFFF','#A39161');; break;
case "Menu600e6910":changeBGcolor('audMenu2713','#FFFFFF','#A39161');; break;
case "Menu600e6911":changeBGcolor('audMenu2714','#FFFFFF','#A39161');; break;
	}
	GLV_currentROitem["600e69"]=mitm_id;
}
function GL600e69_out(mitm_id)	{
	if (!mitm_id)	mitm_id=GLV_currentROitem["600e69"];
	switch(mitm_id)	{
case "Menu600e690":changeBGcolor('audMenu227981','#807F83','#FFFFFF');; break;
case "Menu600e691":changeBGcolor('audMenu225123','#807F83','#FFFFFF');; break;
case "Menu600e692":changeBGcolor('audMenu227493','#807F83','#FFFFFF');; break;
case "Menu600e693":changeBGcolor('audMenu220142','#807F83','#FFFFFF');; break;
case "Menu600e694":changeBGcolor('audMenu2706','#807F83','#FFFFFF');; break;
case "Menu600e695":changeBGcolor('audMenu223016','#807F83','#FFFFFF');; break;
case "Menu600e696":changeBGcolor('audMenu223092','#807F83','#FFFFFF');; break;
case "Menu600e697":changeBGcolor('audMenu2707','#807F83','#FFFFFF');; break;
case "Menu600e698":changeBGcolor('audMenu2709','#807F83','#FFFFFF');; break;
case "Menu600e699":changeBGcolor('audMenu2711','#807F83','#FFFFFF');; break;
case "Menu600e6910":changeBGcolor('audMenu2713','#807F83','#FFFFFF');; break;
case "Menu600e6911":changeBGcolor('audMenu2714','#807F83','#FFFFFF');; break;
	}
}

function GL600e69_getMouse(e) {
	if (GLV_menuOn["600e69"]!=null && !GLV_dontFollowMouse["600e69"]){

	}
	GL_mouseMoveEvaluate("600e69");
}
function GL600e69_hideCode() {

	GL_getObjCss("Menu600e692").visibility = "hidden";
	GL_getObjCss("Menu600e693").visibility = "hidden";
	GL_getObjCss("Menu600e694").visibility = "hidden";
	GL_getObjCss("Menu600e697").visibility = "hidden";
	GL_getObjCss("Menu600e698").visibility = "hidden";
	GL_getObjCss("Menu600e699").visibility = "hidden";
GL600e69_out("");
}
function GL600e69_doTop(WMid,id) {
var parentObject = GL_getObj(GLV_menuXY[WMid][id][2]);
		var TI_width = parentObject.width;
		var TI_height = parentObject.height;
		var anchorObj = GL_getObj(GLV_menuXY[WMid][id][3]);
		var TI_x = anchorObj.x;
		var TI_y = anchorObj.y;
			
GLV_menuOn["600e69"].left = (GLV_curLayerX["674070"]+TI_x+163)+"px";
GLV_menuOn["600e69"].top =  (GLV_curLayerY["674070"]+TI_y+parseInt(document.getElementById(GLV_menuXY[WMid][id][2]).parentNode.parentNode.style.marginTop.replace('px',''))+-6)+"px";






	if(document.getElementById(id).firstChild){
							var scrolledVal = 0;
							if(typeof(window.pageYOffset)=='number'){scrolledVal=window.pageYOffset;}else{scrolledVal=document.body.scrollTop;}
							var menuFreeSpace = document.body.parentNode.clientHeight+scrolledVal-parseInt(GLV_menuOn["600e69"].top.replace('px',''));
							if(menuFreeSpace < document.getElementById(id).firstChild.firstChild.offsetHeight){								
								document.getElementById(id).firstChild.style.height = menuFreeSpace+'px';
							}
							tMenuLay_PrepareHideScroll(document.getElementById(id).firstChild);
						}
}
function GL600e69_restoreMenu() {

}
function GL600e69_resetSubMenus() {
	var resetSubMenu=1;

	return resetSubMenu;
}

