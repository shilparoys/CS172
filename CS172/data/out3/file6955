function changeBGcolor(id,color,linkColor,changeU) {	
				if (document.getElementById && document.getElementById(id))	{
					document.getElementById(id).style.background = color;
					if(linkColor){
						if(document.getElementById(id).getElementsByTagName("A").length > 0)
							document.getElementById(id).getElementsByTagName("A")[0].style.color = linkColor;
					}
					if(typeof(changeU) == 'number'){
						if(document.getElementById(id).getElementsByTagName("A").length > 0){
							if(changeU==1){						
								document.getElementById(id).getElementsByTagName("A")[0].style.textDecoration = 'underline';
							}else{
								document.getElementById(id).getElementsByTagName("A")[0].style.textDecoration = 'none';	
							}
						}
					}
					return true;
				} else if (document.layers && document.layers[id]) {
					document.layers[id].bgColor = color;
					return true;
				}
			}
		


