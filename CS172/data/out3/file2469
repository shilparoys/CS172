        currTypeImage = document.getElementById(TypeImage + i);
        currTypeSubDiv = document.getElementById(TypeSubDiv1 + i);
        if ( i == 1 ) {
            currTypeLink = TypeLink1;
        }else if ( i == 2 ) {
            currTypeLink = TypeLink2;
