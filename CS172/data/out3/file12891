  <link rel="stylesheet" id="shortcodesStyleSheets-css" href="http://www.mycompasstest.com/wp-content/themes/TheCorporation/epanel/shortcodes/shortcodes.css?ver=3.9.3" type="text/css" media="all"> 
  <script type="text/javascript" src="http://www.mycompasstest.com/wp-includes/js/jquery/jquery.js?ver=1.11.0"></script> 
  <script type="text/javascript" src="http://www.mycompasstest.com/wp-includes/js/jquery/jquery-migrate.min.js?ver=1.2.1"></script> 
