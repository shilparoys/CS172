          <script type="text/javascript">
	var keyValues = {};
	var currentKey;
	var searchOptions;
	function initSearch() {
		keyValues.key = document.getElementsByName("key")[0].getAttribute("value");
		keyValues.key2 = document.getElementsByName("key2")[0].getAttribute("value");
		searchOptions = document.getElementsByName("search-switch");
		setSearchKey(keyValues['key']);
		searchOptions[0].onclick = function() {setSearchKey(keyValues.key)};
		searchOptions[1].onclick = function() {setSearchKey(keyValues.key2)};
		setDefaultRadio();
	}
	function setSearchKey(key) {
		currentKey = key;
		keyInput = document.getElementsByName("cx")[0];
		keyInput.setAttribute("value", currentKey);
	}
	function setDefaultRadio() {
		var cx = '';
		if(cx == document.getElementsByName("key")[0].getAttribute("value")) {
			searchOptions[0].checked = true;
		}
		if(cx == document.getElementsByName("key2")[0].getAttribute("value")) {
			searchOptions[1].checked = true;
		}
	}
</script> 
