  <script type="text/javascript">
$(document).ready(function() {
  $('.fancybox-partners-coin').fancybox({
    openEffect  : 'none',
    closeEffect : 'none',
    
    beforeShow: function () {
            if (this.title) {
                // New line
       this.title = "<h3 style='padding-bottom:20px;'>"+ this.title +"</h3> <h6 style='padding-bottom:20px;'>" + $(this.element).data("caption") + "</h6>";            
      }
        },
    afterShow: function() {
      this.title = $(this.element).data("caption");
        },
    helpers : {
      title : {type: 'inside', position: 'bottom'},
      media : {}
    }
  });
});
</script> 
