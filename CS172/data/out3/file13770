   <div id="newsletter-signup" class="newsletter"> 
    <div class="close-x">
     <a id="close-x" class="close-subscribe-dialog"></a>
    </div> 
    <p id="message-wrapper" class="is-hidden"></p> 
    <h2>Move over wise old grandpas—we’re sending timeless advice to your inbox.</h2> 
    <p>Our monthly newsletter is chock full of soul-feeding inspiration, guiding words to live by, and actionable steps to get you on your path.</p> 
    <form id="frm-newsletter-popup" action="" method="get"> 
     <input name="txtSubscribeEmail" id="txtSubscribeEmail" class="email" type="text" placeholder="E-mail" data-required="true"> 
     <input id="btn-newsletter-submit" type="submit" value="Sign Up">&nbsp;&nbsp;&nbsp;
     <a class="close-subscribe-dialog">No thanks</a> 
    </form> 
    <div id="thankyou-message" class="is-hidden"> 
     <p><a class="close-subscribe-dialog">Close</a></p> 
    </div> 
   </div> 
   <script>

		function openPopup()
		{
			$('#newsletter-signup').dialog('open');
		}

		$(function() {
			 $('#newsletter-signup').dialog({
			 	autoOpen: false,
	            draggable: false,
	            modal: true,
	            position: "center",
	            resizable: false,
	            width: 500
	        });
			
			var isLoggedIn = false;
			var cookieSet = $.cookie('preventPopup');
		    if(!cookieSet && !isLoggedIn)
		    {
				setTimeout(openPopup, 2000);
				$.cookie('preventPopup', 1, {expires: 30, path: '/'});
		    }

			$('.close-subscribe-dialog').on('click', function() {
				$('#txtSubscribeEmail').val('');
				$('#newsletter-signup').dialog('close');
				$('#thankyou-message').addClass('is-hidden').html('');
				$('#message-wrapper').addClass('is-hidden').removeClass('is-error').html('');
			});

			$('#btn-newsletter-submit').on('click', function(e) {
				e.preventDefault();
				var $frmNewsletter = $('#frm-newsletter-popup');
				var subscribeEmail = $('#txtSubscribeEmail').val();
				if (subscribeEmail !== '' && subscribeEmail.match(/^.+@.{2,}\..{2,}$/)) 
				{
					$.ajax({
						type: 'POST',
						url: '/default/ajaxNewsletterSubscribe',
						data: {subscribe_email: subscribeEmail},
						cache: false,
						contentType: 'application/x-www-form-urlencoded',
						success: function(data) {
							var result = JSON.parse(data);
							if(result.email !== undefined)
							{
								$('#message-wrapper').html('').addClass('is-hidden')
								$frmNewsletter.addClass('is-hidden');
								$('#thankyou-message').removeClass('is-hidden').prepend('<p class="is-success">Thank you for signing up! Check your inbox to confirm your subscription.</p>');
							}
							else
							{
								$('#thankyou-message').addClass('is-hidden').html('');
								$('#message-wrapper').removeClass('is-hidden').addClass('is-error').html(result.error);
							}	
						}
					});
				}
				else
				{
					$('#thankyou-message').addClass('is-hidden').html('');
					$('#message-wrapper').removeClass('is-hidden').addClass('is-error').html('Please enter a valid email');
				}
			});
		});
		
	</script> 
