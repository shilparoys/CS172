      <script>
(function($) {
$(function() {
var
clientWidth = document.body.clientWidth,
container = $('.rich-v2'),
checkLayout = function() {
  containerWidth = container.width();

  if (containerWidth >= 690 && containerWidth <= 900) {
    if (!container.hasClass('medium')) {
      container.removeClass('small').addClass('medium'); 
	}
  } else if (containerWidth < 690) {
    if (!container.hasClass('small')) {
      container.removeClass('medium').addClass('small');
	}
  } else {
    container.removeClass('small medium');
  }
};

var iScrollers = [];

var refreshScrollers = function() {
  for (var i = 0; i < iScrollers.length; i++) {
    if (typeof iScrollers[i] != "undefined") {
      iScrollers[i].refresh();
    }
  }
};
  
checkLayout();

$('.rich-v2 .related-links').each(function(i) {
  iScrollers[i] = new IScroll($(this).get(0), { scrollX: true, scrollY: false, mouseWheel: true, click: true });
});

$(window).on("resize", function() {
  if (this.resizeTO) clearTimeout(this.resizeTO);
  this.resizeTO = setTimeout(function() {
    $(this).trigger("resizeEnd");
  }, 200);
});

$(window).on('resizeEnd', function() {
  checkLayout();
  refreshScrollers();
});

$(window).on('load', function() {
  refreshScrollers();
});


});
})(jQuery);
</script> 
