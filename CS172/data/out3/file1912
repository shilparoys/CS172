   <script language="javascript" type="text/javascript">
function fixPositioning()
{
 $('div[id *= "_panelPopup"]').appendTo('body');
 $('div[id *= "_designPaneCloak"]').css({"top": "0px","left": "0px"});
 $('.DesignPane').css("position", "relative");
}
