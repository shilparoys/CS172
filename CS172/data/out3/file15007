    <script type="text/javascript" charset="utf-8">

		$(document).ready(function(){
			$('#carouselV2Unav > div').click(function(){
				 if($(this).find('div.hasVideo').length != 0){
					/*alert("abre video");*/
					$(this).find('div.hasVideo').show();
					$(this).find('div.hasVideo').appendTo('div#wrapper');								
					
				 }
				else{
					if(typeof $(this).attr('href') != 'undefined')
						location.href=$(this).attr('href');
				}	 
			});
			$('.lightBox img.closeVideo').click(function() {
				if( $(this).parent().css('display')!= 'none'){
					$(this).parent().hide();
					$(this).parent().appendTo('div.'+$(this).parent().attr('id'));
					
					/*$('#carouselActividad2n1 menu').show();*/
				}
			});
		});
	</script> 
