       <h4 class="h-slack">College Search</h4> 
       <form class="form-aurStandard college-rightrail-search" action="/best-colleges/search" method="get"> 
        <fieldset class="form_joined"> 
         <input type="text" name="name" placeholder="College name" value="" class="clearfield form_item" style="width:145px; height:33px;" autocomplete="off"> 
         <select name="location" class="chosen form_item" style="width: 105px;"> <option value="" selected>All States</option> <option value="ak">AK</option> <option value="al">AL</option> <option value="ar">AR</option> <option value="az">AZ</option> <option value="ca">CA</option> <option value="co">CO</option> <option value="ct">CT</option> <option value="dc">DC</option> <option value="de">DE</option> <option value="fl">FL</option> <option value="ga">GA</option> <option value="hi">HI</option> <option value="ia">IA</option> <option value="id">ID</option> <option value="il">IL</option> <option value="in">IN</option> <option value="ks">KS</option> <option value="ky">KY</option> <option value="la">LA</option> <option value="ma">MA</option> <option value="md">MD</option> <option value="me">ME</option> <option value="mi">MI</option> <option value="mn">MN</option> <option value="mo">MO</option> <option value="ms">MS</option> <option value="mt">MT</option> <option value="nc">NC</option> <option value="nd">ND</option> <option value="ne">NE</option> <option value="nh">NH</option> <option value="nj">NJ</option> <option value="nm">NM</option> <option value="nv">NV</option> <option value="ny">NY</option> <option value="oh">OH</option> <option value="ok">OK</option> <option value="or">OR</option> <option value="pa">PA</option> <option value="ri">RI</option> <option value="sc">SC</option> <option value="sd">SD</option> <option value="tn">TN</option> <option value="tx">TX</option> <option value="ut">UT</option> <option value="va">VA</option> <option value="vt">VT</option> <option value="wa">WA</option> <option value="wi">WI</option> <option value="wv">WV</option> <option value="wy">WY</option> </select> 
         <input type="submit" value="Go" class="form_item-medium button" style="position:relative; top: -7px;"> 
        </fieldset> 
       </form> 
       <script type="text/javascript">
    (function($) {
      "use strict";
      $(window.document).ready(function() {
        // Inline header searches
        (function() {
          var $searchContainer = $(".college-rightrail-search");
          if ($searchContainer.length > 0) {
            // There's something to do!
            $searchContainer.find('.autocompleteEnabled').each(function(idx, elem) {
              var $elem = $(elem),
                endpoint = $elem.data('usn-autocomplete-url');
              $elem.autocomplete(endpoint);
            });
          }
        }());
        // Colleges special search
        (function() {
          var $collegeSearch = $(".college-rightrail-search"),
            $stateDropdown,
            $searchInput,
            baseUrl,
            cannedSearch;
          // This should only run if a college header search exists
          if ($collegeSearch.length > 0) {
            $stateDropdown = $('select', $collegeSearch);
            $searchInput = $('input[type=text]', $collegeSearch);
            baseUrl = window.location.origin + '/best-colleges';
            // Limit autocomplete suggestions to selected state.
            $searchInput.usnAutocomplete({
              autocompleteOptions: {
                serviceUrl: function(el, name) {
                  var url = '/college-names',
                    selected = $stateDropdown.val().toUpperCase();
                  url += '?' + 'location=' + selected;
                  return url;
                },
                paramName: 'name',
                minChars: 3
              }
            });
            // On submit, check if text input is empty.
            // If so, don't submit the form, and redirect
            // to the canned state search for the selected
            // state or to the basic search if state also empty.
            $collegeSearch.submit(function(event) {
              var name = $searchInput.val(),
                state = $stateDropdown.val();
              if (name === '' && state === '') {
                event.preventDefault();
                window.location = baseUrl + '/search';
              } else if (name === '' && state !== '') {
                event.preventDefault();
                cannedSearch = '/' + state;
                window.location = baseUrl + cannedSearch;
              } else if (name !== '' && state !== '') {
                event.preventDefault();
                cannedSearch = '/' + state;
                window.location = baseUrl + '/search?name=' + name + '&' + 'location=' + state.toUpperCase();
              }
            });
          }
        }());
      });
    }(window.jQuery));
  </script> 
      </div> 
     </div> 
