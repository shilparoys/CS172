  <script>
window.onload = function() {
  var redirectUrl = 'https:\x2F\x2Fwww.google.com\x2Faccounts\x2FAccountChooser?hd=citadel.edu\x26continue=https%3A%2F%2Fmail.google.com%2Fmail%2F\x26service=mail\x26ss=1\x26ltmpl=default';
  var domain = 'citadel.edu';
  var hash = window.location.hash;
  if (hash) {
  var match = hash.match(/[#&]Email=([^&]+)/);
  if (match) {
  redirectUrl += "&Email=" + match[1] + "@" + domain;
  }
  }
  window.location.replace(redirectUrl);
};
</script>   
