      <p class="center"> <a title="Plazos de solicitud" id="get-data" class="center" href="javascript: getformdata();"> <img src="images/btn_viewdeadlines_wh.gif" title="Plazos de solicitud" alt="Plazos de solicitud" onmouseover="imgIn(this, 'off');" onmouseout="imgOut(this, 'off');"></a> </p> 
      <script>

	pageGlobals = {};

	pageGlobals.asterisk = 'Es posible que se necesite otro formulario. Consulte con la oficina de asistencia econ&#243;mica de la instituci&#243;n educativa o la agencia estatal de educaci&#243;n superior.';
	pageGlobals.caret = 'Es posible que se necesite otro formulario.';
	pageGlobals.plus = 'Conviene conservar constancia de la presentaci&#243;n de la solicitud, ya sea imprimiendo la p&#225;gina de confirmaci&#243;n (FAFSA electr&#243;nica) u obteniendo el comprobante de env&#237;o por correo (FAFSA impresa).';
	pageGlobals.pound = 'Para consideraci&#243;n prioritaria, presente la solicitud antes de la fecha indicada.';
	pageGlobals.atsign = ' antes de la medianoche (hora del centro).';
    
    jQuery('#get-data').click(function ()
    {
    	var state = jQuery('#state-selected').val();
    	var year  = jQuery('#school-year-selected').val();

	    var foundError = false;
		//  reset alerts first.

      	jQuery('div#alerts-schoolyearselect').hide();
      	jQuery('div#alerts-stateselect').hide();
		jQuery('div#college-info').hide();
      	jQuery('div#errorbox').hide();

      	// clear view

		jQuery('div#deadline-data').hide();

		// make sure no states are hanging around..

		jQuery('.state-list').hide();

		if (state == '')
		{
      		jQuery('div#alerts-stateselect').show();
      		jQuery('div#errorbox').show();
      		

			
			foundError = true;
		}

		

      	if (year == '')
		{
      		jQuery('div#alerts-schoolyearselect').show();
      		jQuery('div#errorbox').show();

			foundError = true;
		}
		
		if (foundError == true)
		{
			return;
		}

		jQuery('div#school-year-2015').hide();
		jQuery('div#school-year-2016').hide();
		jQuery('div#deadline-data').show();

    	jQuery('div#school-year-' + year).show()

     	jQuery('div#federal-info').show();
     	jQuery('#gov-deadline-' + jQuery('div#selected-year').text()).hide()
     	jQuery('#gov-deadline-' + year).show()

      	jQuery('div#state-info').show();
      	
      	jQuery('#select-' + state).html(jQuery('#select-' + state).html().replace(/\*/g, pageGlobals.asterisk));
      	jQuery('#select-' + state).html(jQuery('#select-' + state).html().replace(/\^/g, pageGlobals.caret));
      	jQuery('#select-' + state).html(jQuery('#select-' + state).html().replace(/\+/g, pageGlobals.plus));
      	jQuery('#select-' + state).html(jQuery('#select-' + state).html().replace(/\#/g, pageGlobals.pound));
      	jQuery('#select-' + state).html(jQuery('#select-' + state).html().replace(/@/g, pageGlobals.atsign));
      		
      	jQuery('#select-' + state + '-2015').html(jQuery('#select-' + state + '-2015').html().replace(/\*/g, pageGlobals.asterisk));
      	jQuery('#select-' + state + '-2015').html(jQuery('#select-' + state + '-2015').html().replace(/\^/g, pageGlobals.caret));
      	jQuery('#select-' + state + '-2015').html(jQuery('#select-' + state + '-2015').html().replace(/\+/g, pageGlobals.plus));
      	jQuery('#select-' + state + '-2015').html(jQuery('#select-' + state + '-2015').html().replace(/\#/g, pageGlobals.pound));
      	jQuery('#select-' + state + '-2015').html(jQuery('#select-' + state + '-2015').html().replace(/@/g, pageGlobals.atsign));

		if (year == '2016')
		{
			//jQuery('div#select-' + jQuery('div#selected-state').text()).hide();
			jQuery('div#select-' + state).show();
		}

		if (year == '2015')
		{
		    //jQuery('div#select-' + jQuery('div#selected-state').text()).hide();
			jQuery('div#select-' + state + '-2015').show();
		}

		jQuery('#right_sub_section').stickyfloat({ duration: 400 });


		jQuery('div#college-info').show();
      	jQuery('div#selected-state').text(state);
      	jQuery('div#selected-year').text(year);

    });
</script> 
