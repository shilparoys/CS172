     <script type="text/javascript">  
        // perform JavaScript after the document is scriptable.
        jQuery(document).ready(function() {
            // setup ul.tabs to work as tabs for each div directly under div.panes
        
        
        jQuery("ul.pewschedule_tab").tabs("div.panes > div", {
            
                
                
                
                
            
        })
        
            jQuery(".corner").cornerz({radius:10});
            jQuery(".morelink, .lesslink").click(function(){
                var extraText = jQuery(this).siblings(".extra_text");
                jQuery(extraText).toggleClass("nodisplay");
                jQuery(this).toggleClass("nodisplay");
                jQuery(this).siblings(".morelink, .lesslink").toggleClass("nodisplay");
            });
            var currentTabsApi = jQuery("ul.pewschedule_tab").data("tabs");
            
            jQuery("#top_link_oa").click(function(){
                jQuery.scrollTo(jQuery("#schedules_anchor"));
                currentTabsApi.click(0);
                return false;
            });
            jQuery("#top_link_edge").click(function(){
                jQuery.scrollTo(jQuery("#schedules_anchor"));
                currentTabsApi.click(1);
                return false;
            });
            jQuery("#top_link_premier").click(function(){
                jQuery.scrollTo(jQuery("#schedules_anchor"));
                currentTabsApi.click(2);
                return false;
            });

        });
    </script> 
