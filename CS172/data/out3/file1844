            var guide_config = {
                pid : 0,
                sid : 0
            }
            
            // For backwards compatibility.
            var content_config = {};
            content_config.la_iid = 636;
            
            function nav(elt, action) {
                var options = elt.options;
                var selected_text = encodeURIComponent(elt.options[elt.options.selectedIndex].text);

                document.location.href = '?action=' + action + '&list_id=' + elt.value + '&list_name=' + selected_text;
            }
            
            if ( document.location.search.length == 0 ) {
                // SpringTracking
                SpringTrack.init({
                            '_st_iid' : '1923'
                        });
                
                SpringTrack.trackPage({ 'st_type' : TRACKING_TYPE_HOME_PAGE_MOBILE });
            }
        </script> 
