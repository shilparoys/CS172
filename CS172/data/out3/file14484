  <script type="text/javascript">
	/*Para los paneles desplegables*/
	jQuery(document).ready(function(){
		jQuery('.enlaceN1').click(function(){
			var miID = $(this).id;
			var miContenedor =  'cont' + miID.substring(3);
			jQuery('.visor').hide();	
			jQuery('.visorN1').hide();	
			jQuery('.visorN2').hide();	
			jQuery('[id='+miContenedor+']').show();
		});							
		jQuery('.enlaceN2').click(function(){
			var miID = $(this).id;
			var miContenedor =  'cont' + miID.substring(3);
			jQuery('.visor').hide();
			jQuery('.visorN2').hide();
			jQuery('#temas').show();
			jQuery('[id='+miContenedor+']').show();
		});							
		jQuery('.enlace').click(function(){
			//var miID = $(this).id;
			var miID = $(this).attr('id');
			var miContenedor =  'cont' + miID.substring(3);
			var miClase = miID.substring(3);
			jQuery('.visor').hide();
			jQuery('[id='+miContenedor+']').toggle();
			jQuery('#temas').show();
			jQuery('.'+miClase).show();
		});
	});
</script> 
  <script type="text/javascript">
	$(document).ready(function(){
		$("nav#primary-nav ul.menunavbar").each(function () {
			var primaryNav = $(this);
			var parents = primaryNav.children("li");
			primaryNav.hover(function(){
				parents.children("nav#primary-nav ul.dropdown").stop(true, false).animate({height:"250px"});
			}, function(){
				parents.children("nav#primary-nav ul.dropdown").stop(true, false).animate({height:"0"});
			});
		});
		/* Secondary Dropdown */
		$('.dropdown-toggle').dropdown();
    });  
</script> 
