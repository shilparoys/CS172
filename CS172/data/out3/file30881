     <script type="text/javascript">
/* <![CDATA[ */
    jQuery(function() {
        setSelectYear('customfield_year_of_birth');

        jQuery('input#newsletter_opt_out')
            .change( function(e){

                if ( jQuery('input#newsletter_opt_out:checked').val()) {
                    jQuery('input#customfield_newsletter').val(0);
                }
                else {
                    jQuery('input#customfield_newsletter').val(1);
                }
            });
        jQuery('input#daily_headlines_opt_out')
            .change( function(e){

                if ( jQuery('input#daily_headlines_opt_out:checked').val()) {
                    jQuery('input#customfield_daily_headlines').val(0);
                }
                else {
                    jQuery('input#customfield_daily_headlines').val(1);
                }
            });
    });
