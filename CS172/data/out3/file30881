             <div class="hint">
              Your "username*" is a unique identifier of your account. It is displayed in your profile URL and as such, not private. It is also displayed when you post in forums. Please note: no special characters - numbers and letters only. 
              <script type="text/javascript">
    /* <![CDATA[ */
    function check_username () {
        // FIXME
        // use jquery here to get around IE6/IE7/Opera bug
        // this is a dependency I dislike, but time is the issue here.
        var el = jQuery('#username').get(0);
        clean_value(el);
        var u = el.value;
        if (u=='') {
            alert('Please enter a username first.');
            return;
        }
        var m = document.getElementById('username_msg');
        m.innerHTML = '';
        var path = "https://signup.advance.net/community/";                    
        path = path.replace(/^([^\.]+)[^\/]+(.*)$/g,"$1."+advDomain+"$2");
        var url = path + '?__mode=check_username&username=' + u;

        var xh = mtGetXmlHttp();
        if (!xh) return false;

        xh.open('GET', url, true);
        xh.onreadystatechange = function() {
            if ( xh.readyState == 4 ) {
                if ( xh.status && ( xh.status != 200 ) ) {
                    // error - ignore
                } else {
                    // eval(xh.responseText); // jsonp
                    if (xh.responseText == '0') {
                        m.innerHTML = 'This username is available.';
                    } else {
                        m.innerHTML = 'This username is taken. Please try another.';
                    }
                }
            }
        };
        xh.setRequestHeader( 'Content-Type', 'application/x-www-form-urlencoded' );
        xh.send(null);
    }
    function clean_value(el) {
        el.value = el.value.replace(/^\s+|\s+$/g,'');
    }
    /* ]]> */
    </script> 
              <a href="javascript:void()" onclick="check_username(); return false;">Check username availability</a>.
