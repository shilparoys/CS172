            <div class="CloseupRichPinContent Module" id="CloseupRichPinContent-37"> 
             <div class="richPinInformation" itemscope itemtype="http://schema.org/Recipe"> 
              <h2 class="richPinName" itemprop="name"> <a href="http://domesticsuperhero.com/2013/10/23/copycat-outback-bloom-sauce/" rel="nofollow" class="richPinNameLink" itemprop="url"> Copycat Outback Bloom Sauce </a> </h2> 
              <div class="richPinTitleWrapper">
                Vegetarian • Gluten free 
              </div> 
              <div class="richPinSourceWrapper"> 
               <a href="http://domesticsuperhero.com/2013/10/23/copycat-outback-bloom-sauce/" rel="nofollow" class="richPinSourceLink"> <img src="https://s-media-cache-ak0.pinimg.com/favicons/ffd724656d19449af3b6f7197a37f48c997aca8d6eb954de3d9c4074.ico?346f6ee69c1f2078312ef1d1ab22142f" class="richPinSourceFavicon"> domesticsuperhero.com </a> 
              </div> 
              <div class="richPinMeta "> 
               <div class="richPinTableColumn richPinTableColumnLeft"> 
                <h3 class="richPinRecipeIngredientHeading">Produce</h3> 
                <table class="richPinIngredientTable"> 
                 <tbody>
                  <tr> 
                   <td class="richPinRecipeIngredientAmount"> 1/8 tsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Oregano, dried </td> 
                  </tr> 
                 </tbody>
                </table> 
                <h3 class="richPinRecipeIngredientHeading">Condiments</h3> 
                <table class="richPinIngredientTable"> 
                 <tbody>
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 2 tbsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Horseradish, creamy </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1 tbsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Ketchup </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1/2 cup </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Mayonnaise </td> 
                  </tr> 
                 </tbody>
                </table> 
               </div> 
               <div class="richPinTableColumn richPinTableColumnRight"> 
                <h3 class="richPinRecipeIngredientHeading">Baking &amp; Spices</h3> 
                <table class="richPinIngredientTable"> 
                 <tbody>
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1 pinch </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Black pepper, ground </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1/3 tsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Cayenne pepper </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1/3 tsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Paprika </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1/4 tsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Salt </td> 
                  </tr> 
                 </tbody>
                </table> 
               </div> 
