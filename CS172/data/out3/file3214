jQuery(document).ready(function() {
	jQuery(".button").button();
	jQuery("#query").focus();

    var localeCode = 'en-US';

    jQuery(".calendarInput").datepicker({
		showOn: "button",
		showAnim: "slideDown",
		autoSize: true,
		buttonImage: "http://static1.worldcat.org/wcpa/rel20150127/images/icon_calendar.png",
		buttonImageOnly: true,
		buttonImageText: "Show calendar",
		buttonText: "Show calendar",
		prevText: "&laquo; Previous month",
		nextText: "Next Month &raquo;",
		appendText: "mm/dd/yyyy"
    });

    jQuery(".calendarInput" ).datepicker( jQuery.datepicker.regional[localeCode]);

    if (localeCode == 'ko') {
       jQuery( ".calendarInput" ).datepicker( "option", "dateFormat", "yy.mm.dd" );
    }else if (localeCode == 'pt') {
       jQuery( ".calendarInput" ).datepicker( "option", "dateFormat", "dd-mm-yy" );
    }else if (localeCode == 'zh-TW') {
       jQuery( ".calendarInput" ).datepicker( "option", "dateFormat", "mm/dd/yy" );
    }else if (localeCode == 'zh-CN') {
       jQuery( ".calendarInput" ).datepicker( "option", "dateFormat", "mm/dd/yy" );
    }




});

function createCourseLink(obj) {
    LibServices(obj,'wctlink_CourseReserves_createcourseclick',';wctlink_CourseReserves_createcourseclick');
	location.href='/wcpa/courseReserves?action=courseReserveManagerCreateCourse';
}


function onStatViewCourse(me) {
   LibServices(me,'wctlink_CirculationServices_CourseReservesViewCourse',';wctlink_CirculationServices_CourseReservesViewCourse');
   return;
}


function onStatExternalLink(me) {
   LibServices(me,'wctlink_CirculationServices_CourseReservesExternalLink',';wctlink_CirculationServices_CourseReservesExternalLink');
   return;
}



