     <div class="formpage"> 
      <h1>Search for courses or reserve items</h1> 
      <div class="content-fullwidth-wrap grey"> 
       <div class="content-fullwidth"> 
        <script type="text/javascript" language="JavaScript">

var defaultTextCourses = "Enter * to see all courses";
var defaultTextItems = "Enter * to see all items";
var ghostTestThere = false;


jQuery(document).ready(function() {
	jQuery("input:submit.button").button();
    ghostText(defaultTextCourses);
    setSearchMatching();

    // Apply onfocus logic
    jQuery("#query").keydown(function(event) {
    // If the current value is our default value

    var thequery = jQuery("#query").val();

    if ((thequery == defaultTextCourses) || (thequery == defaultTextItems)) {
       // clear it and set the text color to black
       this.value = "";
       jQuery("#query").css("color", "#000");
       jQuery("#query").css("font-style", "normal");
       }
    });


    jQuery("#query").blur(function() {
    });

    jQuery('#query').focus(function() {
        var thequery = jQuery("#query").val();
        if (ghostTestThere == true) {
            jQuery('#query').setCursorPosition(0);
        } else {
            var len = thequery.length;
            if (len > 0) {
               jQuery('#query').setCursorPosition(len);
            }
        }

    });


    jQuery("#query").change(function() {
        if (ghostTestThere == true) {
            jQuery("#query").css("color", "#000");
            jQuery("#query").css("font-style", "normal");
        }
        ghostTestThere = false;

    });


    jQuery('#query').setCursorPosition(0);

    var include  = "";
    var searchin = "";
    if (include == 'yes') {
       jQuery("#includeInactive").attr('checked', true);
    }
    else {
       jQuery("#includeInactive").attr('checked', false);
    }

    if (searchin == 'Items') {
       jQuery("#searchInItemsRadio").attr('checked', true);
       jQuery("#searchInCoursesRadio").attr('checked', false);
       enableInclude('no');
    }
    else {

       jQuery("#searchInItemsRadio").attr('checked', false);
       jQuery("#searchInCoursesRadio").attr('checked', true);
       enableInclude('yes');
    }

    jQuery('#query').focus();
});



function setSearchMatching() {

    var matchType = "";
    if (matchType.length > 0) {
        if (matchType == "AnyWords"){
           jQuery("#searchAnyWords").attr('checked', true);
           jQuery("#searchAllWords").attr('checked', false);
        }

        if (matchType == "AllWords"){
           jQuery("#searchAnyWords").attr('checked', false);
           jQuery("#searchAllWords").attr('checked', true);
        }
    }
}


function onSubmit()
{
    var sortElement = jQuery("#sortValue :selected").val();
    jQuery("#searchSortState").val(sortElement);
    //Saw this during maintenance, doesn't look like its called, but definitely do not want an alert out to a user
    //alert("sortElement=" + sortElement);
    return true;
}


function onClickSubmit(){
    var thequery = jQuery("#query").val();
    // If the current value is our default value
    if ((thequery == defaultTextCourses) || (thequery == defaultTextItems)) {
       // clear it
       jQuery("#query").val("*");
     }
    return true;
}


function enableInclude(enable)
{
    if (enable == 'yes') {
       jQuery("#includeInactiveSection").removeClass("disabled");
       jQuery("#includeInactive").attr('disabled', false);
       jQuery("#searchmodifiers").css('display', "block");


       var thequery = jQuery("#query").val().trim();
       if ((thequery == defaultTextItems) || (thequery == "") || (thequery == "*")){
           ghostText(defaultTextCourses);
       }
    }
    else {
       jQuery("#includeInactiveSection").addClass("disabled");
       jQuery("#includeInactive").attr('checked', false);
       jQuery("#includeInactive").attr('disabled', true);
       jQuery("#searchmodifiers").css('display', "none");

       var thequery = jQuery("#query").val().trim();
       if ((thequery == defaultTextCourses) || (thequery == "") || (thequery == "*")){
           ghostText(defaultTextItems);
       }
    }

    return false;
}


function ghostText(text) {
   jQuery("#query").val(text);
   jQuery("#query").css("color", "#aaa");
   jQuery("#query").css("font-style", "italic");
   ghostTestThere = true;
}


new function($) {
  $.fn.setCursorPosition = function(pos) {
    if ($(this).get(0).setSelectionRange) {
      $(this).get(0).setSelectionRange(pos, pos);
    } else if ($(this).get(0).createTextRange) {
      var range = $(this).get(0).createTextRange();
      range.collapse(true);
      range.moveEnd('character', pos);
      range.moveStart('character', pos);
      range.select();
    }
  }
}(jQuery);


</script> 
