  <script>
jQuery(function(){

if(!ismobile()){
var  controller = $.superscrollorama ();

//the people parallax
controller.addTween(
	'#society',
	(new TimelineLite())
		.append([
			TweenMax.fromTo($('#people1'), 1,
				{css:{top: 100}, immediateRender:true},
				{css:{top: -100}}),
			TweenMax.fromTo($('#peopel2'), 1,
				{css:{top: 200}, immediateRender:true},
				{css:{top: -200}}),
			TweenMax.fromTo($('#peopel3'), 1,
				{css:{top: 300}, immediateRender:true},
				{css:{top: -300}})

		]),
	700,
	200
);
controller.addTween('#chop', TweenMax.from( $('#chop'), .2, {css:{left:'-100%'}}), 150, -200);
controller.addTween('#jcaption',  TweenMax.from($('#jcaption'), .5, {css:{opacity:0}}));
}

$('.person').click(function(){
	$('.popup').hide();
	$("#"+$(this).attr('rel')).show();
	return false;
});
$('.popup').click(function(){
	$('.popup').hide();
});

$('#youlearnfor').stickem();

})
</script> 
