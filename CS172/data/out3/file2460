     <script type="text/javascript">
var homeskateurl = 'index.html?content_type=plugin&block_id=2114&date=';



// Load xmlhttprequest object
if (document.all) {
	this.homeskatexml = new ActiveXObject("Microsoft.XMLHTTP");
}
if (!this.homeskatexml && typeof XMLHttpRequest != 'undefined') {
	try {
		this.homeskatexml = new XMLHttpRequest();
	} catch (e) {
		this.homeskatexml = false;
	}
}


function homeskatecal_load(date) {
	this.homeskatexml.open("GET", homeskateurl + date, true);
	this.homeskatexml.onreadystatechange = function() {
		if (homeskatexml.readyState == 4) {
			calElem = document.getElementById('homeskatecal');
			calElem.innerHTML = homeskatexml.responseText;
			$('#skatecell .item').first().addClass('active');
		}
	};
	this.homeskatexml.send(null);
	
}

$(function() {
	$('#skatecell .item').first().addClass('active');
	var startval = $('#skatecell .active').attr('id').match(/\d+/);
	$("td.skateday[data-val='" + startval +"']").addClass('currday');
	$('#skatecell').carousel({interval: false});
	$(document).on('mouseleave', '#skatecell', function(){
		$(this).carousel('pause');
	});
	$('#skatecell').on('slid.bs.carousel', function () {
		$('.skateday').removeClass('currday');
		var slideval = $('#skatecell .active').attr('id').match(/\d+/);
		$("td.skateday[data-val='" + slideval +"']").addClass('currday');
	});
	$(document).on("click","td.skateday",function(g){
		$('.skateday').removeClass('currday');
		$(this).addClass('currday');
		var curval = $(this).data('val');
		var tarval = '#homeskate_' + curval;
		$('#skatecell .item').removeClass('active');
		$(tarval).addClass('active');
	});
});

</script>
