        <!-- FILE: /main/production/doc/data/repository.uwyo.edu/assets/ir_cover_text.inc --> 
        <h2>Browse Research and Scholarship <span class="site-follow">
          <!-- FILE: /main/production/doc/data/assets/site/nw_follow_ctx.inc --> <a rel="nofollow" href="http://network.bepress.com/api/follow/subscribe?institution=NmM3YzcyY2Y0Y2Q4ZWMyOA%3D%3D&amp;format=html" data-follow-set="institution:NmM3YzcyY2Y0Y2Q4ZWMyOA==" title="Follow Wyoming Scholars Repository" class="btn followable">Follow</a> 
          <!-- FILE: /main/production/doc/data/repository.uwyo.edu/assets/ir_cover_text.inc (cont) --></span></h2> 
        <div id="cover-browse"> 
         <h3><a href="http://repository.uwyo.edu/communities.html">Research unit, center, or department</a></h3> 
         <h3><a href="http://repository.uwyo.edu/sw_gallery.html">SelectedWorks Gallery</a></h3> 
         <h3><a href="http://repository.uwyo.edu/student_scholars">Student Scholarship</a></h3> 
        </div> 
        <p>The repository is a service of University of Wyoming Libraries. Research, scholarly, and creative output included here has been selected and deposited by the faculty, researchers and students of University of Wyoming. WySR, Wyoming Scholars Repository, is part of the larger Open Access movement to transform scholarly communications.</p>
        <!-- FILE: /main/production/doc/data/assets/site/ir_index.inc (cont) --> 
        <!-- FILE: /main/production/doc/data/assets/site/ir_disc_sunburst.inc -->
        <!--[if lt IE 9]><style>#dcn-home .box.sunburst {display:none;width:0;height:0;}</style><![endif]--> 
        <div class="clear"></div> 
        <div class="box sunburst"> 
         <div id="chart"></div> 
         <p class="view-sunburst"><a href="http://repository.uwyo.edu/sunburst.html">View Larger <i class="icon-arrow-right"></i></a></p> 
        </div> 
        <script src="/assets/d3/d3.v2.js"></script> 
        <script type="text/javascript" src="/assets/scripts/sunburst.pack.js"></script> 
        <script type="text/javascript">

	var sunburst = new DisciplineSunburst(
			DisciplineSunburst.nextId(),
			{
				data_url: "/assets/disciplines/repository.uwyo.edu/disciplines.json"
				
				, canvas: {
					w: 200
					, h: 200
				}
				
				, graphClickFn: function( d ) {
					document.location = "http://repository.uwyo.edu/sunburst.html";
				}

				// In the network, the pathFromURLComponents function will build
				// the discipline path that we can use as the document.location
				, destinationURLFn: DisciplineSunburst.pathFromURLComponents
				
				, hoverPanel: null
				, infoPanel: null
				
				, centerPanel: {
					styles: {
						'font-size': '.9em'
						, 'font-family': "'Helvetica Neue', Helvetica, 'Arial Narrow', Arial, sans-serif"
						, 'font-weight': "bold"
					}
					, img: {
						href: "/assets/images/DC_Logo_small.svg"
						, w: 75
						, h: 48
						, opacity: 0.2
					}
					, lineHeight: 1.2
					, runSpotlight: false
					// , img: {} no image for the small graph
					// This will always return the label of the root discipline
					, labelFn: function( inDatum, rootDatum ) {
						var textLines = [];
						
						if ( inDatum.key === -1 ) {
							if ( inDatum.total_disciplines && inDatum.total_disciplines > 0 ) {
								textLines.push( "Explore works in " + DisciplineSunburst.formatInt( inDatum.total_disciplines ) + " disciplines" );
							}
						} else {
							textLines.push( inDatum.label )

							textLines.push( "Explore " + DisciplineSunburst.formatInt( inDatum.total_works ) + " works" );
							
							if ( inDatum.total_disciplines && inDatum.total_disciplines > 0 ) {
								textLines.push( "in " + DisciplineSunburst.formatInt( inDatum.total_disciplines ) + " disciplines" );
							}
						}
					
						return textLines;
					}
				}

			}
		);
	sunburst.init();
</script> 
