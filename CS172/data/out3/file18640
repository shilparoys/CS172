  <script type="text/javascript">
/* <![CDATA[ */

if(typeof(vo)=="undefined"){
    if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"initializing vo"});}
    voCatcher = false;
    vo = new Object({
        embed:[],
        embedPost:[],
        standalone:[]
    });
    
    function tncms_video_ad(adId, videoUrl, videoClickUri, properties){
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"catching video ad: " + adId});}
        
        videoUrlPath = window.location.protocol+"//"+window.location.host

        if(properties.embed==1){
            if(properties.rolltype=="post"){
            vo.embedPost.push({
                adId:adId,
                videoUrl:videoUrlPath+videoUrl,
                videoClickUri:videoClickUri,
                width:properties.width,
                height:properties.height,
                volume:properties.volume,
                autoplay:properties.autoplay,
                rolltype:properties.rolltype,
                playlistItem:{


    url:window.location.protocol+"//"+window.location.host+videoUrl,
    autoPlay:false,
    autoBuffering:false,
    onStart: function(clip){
        jQuery("#"+this.id()).addClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'play', "adID: "+vo.embedPost[0].adId+" url: "+clip.url]);
        }
    
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Start Event: "+clip.url});}
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Playing: "+clip.url});}
                
    },
    
    onResume: function(clip) {  
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'resume', "adID: "+vo.embedPost[0].adId+" url: "+clip.url]);
            }
    },

    onPause: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'pause', "adID: "+vo.embedPost[0].adId+" url: "+clip.url]);
        }

        if(typeof(vo.embedPost[0].videoClickUri)!='undefined'){
            window.open(decodeURIComponent(vo.embedPost[0].videoClickUri));
        }
    },

    onStop: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'stop', "adID: "+vo.embedPost[0].adId+" url: "+clip.url]);
            }
    },

    onFinish: function(clip) {
        jQuery("#"+this.id()).removeClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'finish', "adID: "+vo.embedPost[0].adId+" url: "+clip.url]);
        }
                
        vo.embedPost[0].watched = true;
        vo.embedPost[0].lock = false;

            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"finished playing ad video"});}
        
        var next = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").first();
        if(automaticSwapFromAd && next.find(".video-type").text()!="video"){
            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"attempting to swap to active video in to "+this.id()});}
            swapSubstituteHTML("#"+this.id(),jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing a.load-video").first());

        }

        automaticSwapFromAd = false;
        vo.embedPost.shift();
        
    }


},
                properties:properties
            });
            }else{
            vo.embed.push({
                adId:adId,
                videoUrl:videoUrlPath+videoUrl,
                videoClickUri:videoClickUri,
                width:properties.width,
                height:properties.height,
                volume:properties.volume,
                autoplay:properties.autoplay,
                rolltype:properties.rolltype,
                playlistItem:{


    url:window.location.protocol+"//"+window.location.host+videoUrl,
    autoPlay:false,
    autoBuffering:false,
    onStart: function(clip){
        jQuery("#"+this.id()).addClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'play', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }
    
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Start Event: "+clip.url});}
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Playing: "+clip.url});}
                
    },
    
    onResume: function(clip) {  
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'resume', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
            }
    },

    onPause: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'pause', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }

        if(typeof(vo.embed[0].videoClickUri)!='undefined'){
            window.open(decodeURIComponent(vo.embed[0].videoClickUri));
        }
    },

    onStop: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'stop', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
            }
    },

    onFinish: function(clip) {
        jQuery("#"+this.id()).removeClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'finish', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }
                
        vo.embed[0].watched = true;
        vo.embed[0].lock = false;

            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"finished playing ad video"});}
        
        var next = jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing").first();
        if(automaticSwapFromAd && next.find(".video-type").text()!="video"){
            if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"attempting to swap to active video in to "+this.id()});}
            swapSubstituteHTML("#"+this.id(),jQuery("#"+this.id()).parents(".blox-multimedia").find(".blox-multimedia-text .item.playing a.load-video").first());

        }

        automaticSwapFromAd = false;
        vo.embed.shift();
        
    }


},
                properties:properties
            });
            }
        }else{
            vo.standalone.push({
                adId:adId,
                videoUrl:videoUrlPath+videoUrl,
                videoClickUri:videoClickUri,
                width:properties.width,
                height:properties.height,
                volume:properties.volume,
                autoplay:properties.autoplay,
                rolltype:properties.rolltype,
                playlistItem:{


    url:window.location.protocol+"//"+window.location.host+videoUrl,
    autoPlay:false,
    autoBuffering:false,
    onStart: function(clip){
        jQuery("#"+this.id()).addClass("advertisement");
        
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'play', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }
    
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Start Event: "+clip.url});}
        if(typeof(ninjaLog)!="undefined"){ninjaLog.Update({text:"Video Ad - Playing: "+clip.url});}
                
    },
    
    onResume: function(clip) {  
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'resume', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
            }
    },

    onPause: function(clip) {
        if(typeof(_gaq)!="undefined"){
            _gaq.push(['_trackEvent', 'videos', 'pause', "adID: "+vo.embed[0].adId+" url: "+clip.url]);
        }

        if(typeof(vo.embed[0].videoClickUri)!='undefined'){
            window.open(decodeURIComponent(vo.embed[0].videoClickUri));
