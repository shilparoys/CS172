    	figure.inline-child.image img {
    		display: block;
    		margin: 0 auto;
    		width: 263px; }
    	figure.inline-child.image img:hover { cursor: pointer }
    	/* inline image expanded */
    	figure.inline-child.image.expanded .image-box { z-index: 2 }
    	figure.inline-child.image.expanded .image-box figcaption {
    		display: block;
