/* Javascript specific to only this page (not site) */
jQuery(document).ready(function($){
	

	$('#sitenav').before('<div id="ritnav-menu-icon">Menu</div>');
	$('#sitenav').before('<div id="ritsearch-menu-icon">Search</div><div id="ritsearch-spacer"></div>');
	
	$("#ritnav-menu-icon").on("click", function(){
		$("#sitenav").slideToggle("fast");
		$(this).toggleClass("active");
	});

	$("#ritsearch-menu-icon").on("click", function(){
		$("#ritsearch-spacer").slideToggle("fast");
		$("#metaritsearch").slideToggle("fast");
		$(this).toggleClass("active");
	});
	
});
</script> 
