            $.fn.Shuffle = function (childEl) {
                this.each(function () {
                    var $this = $(this), els = $this.children(childEl);
                    els.sort(function () { return (Math.round(Math.random()) - 0.5); });
                    $this.empty();
                    for (var i = 0; i < els.length; i++) $this.append(els[i]);
                });
            };
        })(jQuery);
