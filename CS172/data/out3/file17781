  <script type="text/javascript">
  jQuery(window).load(function(){
  	jQuery("img.lazy").lazyload({
    		effect: "fadeIn",
    		failure_limit : 8, // fixes load bug for imgs higher in DOM but visually lower than others
    		threshold : 100 // loads imgs that are 100px below viewport
  }).removeClass("lazy");
  });
  jQuery(document).ajaxStop(function(){
  	jQuery("img.lazy").lazyload({
  		effect: "fadeIn",
    		failure_limit : 8, // fixes load bug for imgs higher in DOM but visually lower than others
    		threshold : 100 // loads imgs that are 100px below viewport
  	}).removeClass("lazy");
  });
  function truncateString (string, limit, breakChar, rightPad) {
  	if (string.length <= limit+20) return string;
  	var substr = string.substr(0, limit);
    if ((breakPoint = substr.lastIndexOf(breakChar)) >= 0) {
    	if (breakPoint < string.length -1) {
      	  return string.substr(0, breakPoint) + rightPad;
        }
    }
  }
  function slideshowReadMoreFormat(){
  	$('.slideshowExcerpt').each(function (i, div) {
  		var text = $(div).text();
  		$(div).attr('data-custom',text);
  		text = truncateString(text,160," ","<a href='javascript:void(0)' class='readMoreExcerpt'> ...Read More</a>");
  		$(div).html(text);
	});
  }
  function resetSlideshow(control){
  	var originalHeight = $(".slideshowExcerpt").height();
	control.parent().html("<span class='full-slide-text'>"+control.parent().attr('data-custom')+"</span>");
	var newHeight = $('.full-slide-text').height() - originalHeight;
	$('#MainSlider .slider-main').height(450+newHeight);
	$('#MainSlider .slide-wrapper').height(450+newHeight);
  }
  function resetSliderHeight(){
  	$('.full-slide-text').each(function (i, div) {
  		text = truncateString($(div).text(),160," ","<a href='javascript:void(0)' class='readMoreExcerpt' id='read-more-"+i+"' onclick='resetSlideshow($(this))'> ...Read More</a>");
  		$(this).parent().html(text);
  	});
	$('#MainSlider .slider-main').height(450);
	$('#MainSlider .slide-wrapper').height(450);
  }
  $(document).ready(function () {
	slideshowReadMoreFormat();
	$(".readMoreExcerpt").click(function(){
		var originalHeight = $(".slideshowExcerpt").height();
		$(this).parent().html("<span class='full-slide-text'>"+$(this).parent().attr('data-custom')+"</span>");
		var newHeight = $('.full-slide-text').height() - originalHeight;
		$('#MainSlider .slider-main').height(450+newHeight);
		$('#MainSlider .slide-wrapper').height(450+newHeight);
	});
	$(".captionContainer").css('height','auto');
  });
</script> 
