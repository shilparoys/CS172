    var disqus_shortname = 'buffalonews'; // required: replace example with your forum shortname

    function cust_load_disqus_count(){
        var s = document.createElement('script'); s.async = true;
        s.type = 'text/javascript';
        s.src = '//' + disqus_shortname + '.disqus.com/count.js';
        (document.getElementsByTagName('HEAD')[0] || document.getElementsByTagName('BODY')[0]).appendChild(s);
    }

    $('.more_post a').on("click", function(){
        //cust_load_disqus_count();
        setTimeout(cust_load_disqus_count,2000);
    });

    $(document).ready (function () {
        cust_load_disqus_count();
    });

