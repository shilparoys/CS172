  <title>Університет банківської справи - ГОЛОВНА</title>
  <link rel="icon" href="/favicon.ico" type="image/x-icon">
  <link rel="stylesheet" type="text/css" href="style.css"> 
  <script>
	build_img_opas=0
	build_img=1;
	build_img_change_opas=1;
	build_time=40;
	function change_img_build()
	{
		document.getElementById('img_build').src='http://www.ubs.gov.ua/images/75/build_'+build_img+'.jpg';
	    document.getElementById('img_build').style.opacity=(build_img_opas/10);
	    document.getElementById('img_build').style.filter = 'alpha(opacity=' + build_img_opas*10 + ')';
		build_img_opas+=(build_img_change_opas/2);
		if (build_img_opas>=10)
		{
			build_img_change_opas=-1;
		};
		if (build_img_opas<=-1)
		{
			build_img_change_opas=1;
			build_img++;
			if (build_img>=5)
			build_img=1;
		};
		if (build_img_opas!=-1 && build_img_opas!=10)
		build_time=30;
		else
		build_time=800;
		setTimeout("change_img_build();",build_time);
	};
    </script> 
 </head> 
