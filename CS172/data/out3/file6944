    <script type="text/javascript">
  (function ($) {
    $(function () {
      $('.uk-nav-offcanvas li.tm-parent').on('click', function (e) {
        var bar       = $(this).find('.uk-offcanvas-bar:first'),
            rootUL    = $(this).closest('ul'),
            rootLI    = $(this);

        rootUL.children('li').not(this).hide();
        bar.addClass('uk-offcanvas-bar-show').width();
      });

      $('a.tm-offcanvas-back').on('click', function (e) {
        e.preventDefault();
        var offcnvs = $(this).parent(),
            rootUL  = $(this).closest('ul');
        setTimeout(function(){
          rootUL.children('li').show();
          offcnvs.removeClass("uk-offcanvas-bar-show");
        }, 50);
      });

    });
  })(jQuery);
</script> 
