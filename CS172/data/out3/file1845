  <script id="/sites/all/modules/custom/angularmods/app/shared/local_suite/templates/wxcard.summary.html" type="text/ng-template"><section class="wxcard wxcard-summary" data-ng-class="{'is12Hour' : is12Hour, 'is24Hour' : !is12Hour}">
    <div class="heading" role="heading" aria-level="3">
        <h2 data-ng-if="!isFullDay" data-ng-bind="data.title"></h2>
        <h2 data-ng-if="isFullDay" data-ng-bind="data.title"></h2>
        <span data-ng-if="isFullDay || !isFullDay && !is12Hour" class="sub-heading">
            <span data-ng-if="data.holidayName" data-ng-bind-template="{{data.holidayName}}"></span>
            <span data-ng-if="!data.holidayName" data-dsxdate data-datetime="data.getForecastDate()" data-format="'MMM d'" data-placeholder="''"></span>
        </span>
    </div>
    <div data-wxicon data-sky-code="daypart().getSkyCode()"></div>
    <p class="hi-temp" aria-label="{{'local-suite.TEMP' | translate}}">
        <span ng-if="isFullDay || is12Hour" data-wx-temperature="daypart().getTemp()" data-show-temp-unit="wxcardShowTempUnit"></span>
        <span ng-if="!isFullDay && !is12Hour">--</span>
    </p>
    <p data-ng-if="isFullDay || !isFullDay && !is12Hour" class="lo-temp" aria-label="{{'local-suite.LOWTEMP' | translate}}" data-wx-temperature="data.night.getTemp()"  data-show-temp-unit="wxcardShowTempUnit && !isFullDay && !is12Hour"></p>
    <h3 class="weather-phrase" aria-label="{{'local-suite.CONDDESC' | translate}}">
        <span data-wx-alert-badge="daypart().getSkyXCode()"></span>
        <span data-ng-bind-template="{{daypart().getSensibleWx() | safeDisplay}}"></span>
    </h3>
    <p>
        <span class="precip" aria-label="{{'local-suite.PRECIPCHANCE' | translate}}"
              data-wx-precip="daypart().getChanceOfPrecip()"
