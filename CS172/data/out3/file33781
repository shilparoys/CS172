      <script type="text/javascript">

$(document).ready(function() {
    $.ajax({
        type: 'get',
        url: '/participating-school/counties.xml',
        dataType: 'xml',
        success: function(xml) {
            parseXML(xml);
            initCounties();

            $('#find-schools').click(function() {
                $('#school-finder-form').dialog('open');
            });
        },
        error: function(xhr, status, error) {
            console.debug(error);
        }      
    });
});
</script> 
