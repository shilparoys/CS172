  <script type="text/javascript">
var imgs=new Array("Basorelief, Facultatea de Litere.jpg","Cicerone.jpg","Palatul Facultatii de Drept.jpg",
"Piata Universitatii, 1930.jpg","Sala Profesorilor, BCU.jpg","Facultatea de Istorie, 1921.jpg","Sala de lectura, Facultatea de Litere.jpg");
var imgs_nm=new Array("Bas-relief, Faculty of Letters.jpg","Cicero","School of Law Palace",
"University Square, 1930.jpg","Teachers Hall, Central University Library","Faculty of History, 1921","Reading Room, Faculty of Letters");
var index_img=gen_rnd();

function gen_rnd(){
	var i=imgs.length;
	while(i>=imgs.length){i=Math.floor(Math.random() * imgs.length);}
	return i;
}
function ch_pict(i){var img=document.getElementById("imgub");
	img.src="/templates/ub_home002/img/"+imgs[i];
	var im_name=imgs_nm[i].split('.jpg')[0];
	document.getElementById('img_name').innerHTML=im_name;
	wimg=document.getElementById('img'+index_img);wimg.src="/templates/main/img/mic_wh7x7.gif";
	wimg=document.getElementById('img'+i);wimg.src="/templates/main/img/mic_wh7x7r.gif";
	index_img=i;
}
function flipDiv(what){
  var div=document.getElementById(what);
  if(div.ff) div.ff=!div.ff;else div.ff=true
  if(div.ff){div.style.display='block';}else div.style.display='none';
}
</script> 
