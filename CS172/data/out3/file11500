$("#showgrid").click(function(){
  $("#showgrid").addClass('active');
  $("#showtable").removeClass('active');
  $("#facultygrid").addClass("active");
  $("#facultytable").removeClass("active");
});

$("#showtable").click(function(){
  $("#showgrid").removeClass('active');
  $("#showtable").addClass('active');
  $("#facultygrid").removeClass("active");
  $("#facultytable").addClass("active");
});
</script>
