        function BrowserCheck()
        {
          var N= navigator.appName, ua= navigator.userAgent.toLowerCase(), tem;
          var M= ua.match(/(opera|chrome|safari|firefox|msie|trident|palemoon)\/?\s*(\.?\d+(\.\d+)*)/i);
          if(M && (tem= ua.match(/version\/([\.\d]+)/i))!= null) {M[2]=tem[1];}
          M= M? [M[1], M[2]]: [N, navigator.appVersion,'-?'];
          return M;
        }

        var browserInfo = {
           browser:'',
           version:''
        };

        var regexp = /^\d+/;
        browserInfo.browser = BrowserCheck()[0];
        browserInfo.version = BrowserCheck()[1];


        if((browserInfo.browser.toLowerCase() == "chrome" && browserInfo.version.toString().split(".")[0] < 38) || (browserInfo.browser.toLowerCase() == "firefox" && browserInfo.version.toString().split(".")[0] < 24) || (browserInfo.browser.toLowerCase() == "opera" && browserInfo.version.toString().split(".")[0] < 25)  || (browserInfo.browser.toLowerCase() == "trident" && browserInfo.version.toString().split(".")[0] < 7) || (browserInfo.browser.toLowerCase() == "msie") || ((browserInfo.version.toString().split(".")[0]+"."+browserInfo.version.toString().split(".")[1]) < 6.2)){

          document.getElementById('browserAlertBar').style.display = 'block';

        }

