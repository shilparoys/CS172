   <script type="text/javascript">
    function changeSearchConference() {
      var conference = $("#ddlSearchConference").val();
      var school = $("#ddlSearchSchool").val();

      if(conference !== '') {
          require(["backbone/collections/SchoolsCollection"], function(SchoolsCollection) {
              var schoolsCollection = new SchoolsCollection();
              schoolsCollection.fetch({
                  dataType: 'json',
                  data: {clientID: conference},
                  error: function(jqXHR, textStatus, errorThrown){
                      console.log("SchoolsCollection fetch error: " + JSON.stringify(textStatus));
                  },
                  success: function(schools){
                      $("#ddlSearchSchool").find("option:not(:first)").remove();
                      _.each(schoolsCollection.models, function(school) {
                          var id = school.get('id');
                          var name = school.get('name');
                          $("<option value='" + id + "'>" + name + "</option>").appendTo($("#ddlSearchSchool"));
                      });
                  }
              });
          });
      } else {
          var defaultSchools = fixStringMap('{50592=NAIA produced events, 88143=Arizona Christian, 20153=Biola, 95=Concordia (Calif.), 103899=Hope International, 118286=San Diego Christian, 65971=The Master\'s College, 88513=Vanguard, 161=Westmont, 171709=William Jessup}');
          $("#ddlSearchSchool").find("option:not(:first)").remove();
          _.each(defaultSchools, function(name, key) {
              $("<option value='" + key + "'>" + name + "</option>").appendTo($("#ddlSearchSchool"));
          });
      }
  }
  </script> 
