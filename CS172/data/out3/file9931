$("table#list tbody tr#ACCT110 td").click(function(){
var ct = $('table#list tbody tr td.detail').size() + 1;
if (ct == 1){
$("#ACCT110").after("<tr><td class='detail' colspan='5'>row contentrow contentrow contentrow contentrow contentrow content</td></tr>");
}
else{
$("table#list tbody tr td.detail").remove();
}
});
