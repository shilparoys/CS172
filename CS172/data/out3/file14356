            <div class="cover"> 
             <div class="cover-image-container"> 
              <div class="cover-outer-align"> 
               <div class="cover-inner-align"> 
                <img alt="Mathematics" class="cover-image" data-cover-large="https://lh4.ggpht.com/nWk4_yM-Na2ZeqsYHjlRPHtS0D_iwwxppLusgNd5_RO1fqHcxroIUnxLFL_IxSVC8cVo=w340" data-cover-small="https://lh4.ggpht.com/nWk4_yM-Na2ZeqsYHjlRPHtS0D_iwwxppLusgNd5_RO1fqHcxroIUnxLFL_IxSVC8cVo=w170" src="https://lh4.ggpht.com/nWk4_yM-Na2ZeqsYHjlRPHtS0D_iwwxppLusgNd5_RO1fqHcxroIUnxLFL_IxSVC8cVo=w170" aria-hidden="true"> 
