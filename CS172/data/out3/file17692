    window.getScript("http://zor.livefyre.com/wjs/v3.0/javascripts/livefyre.js").then(function() {
        _M.ready(function(core) {
            var lf_debug =  false; 
            window.livefyreManager = new LivefyreManager(core, lf_debug);
        });
    });
}
