    {
    	itm.setAttribute('text', 'My Account');
    	itm.setAttribute('description', 'Contact Information, Profile and Other Settings');
    	itm.setAttribute('onmenuclick', 'STSNavigate2(event,\'\u002fmyaccount\u002f\')'); 
