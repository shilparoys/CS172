      <script language="JavaScript">
function Mycommand( theForm )
{
	theForm.SQLCommand.value = "select name, label, received, status, modified, media, seedsize, reqfile, req_type from requests order by received";
	theForm.submit()
	return true;
}

function Mycommand2( theForm )
{
	theForm.SQLCommand.value = "select name, label, received, status, modified, media from archreq where received > sysdate-3 order by name;"

	theForm.submit()
	return true;
}	
  </script> 
