var onloadFuncList = new Array();

function addOnloadFunction(f){
	onloadFuncList[onloadFuncList.length] = f;
}//addOnloadFunction

function runOnloadFunctions(){
	for(i=0; i<onloadFuncList.length; i++){
		eval(onloadFuncList[i]);
	}
	
}//runOnloadFunctions

</script> 
