          <!-- INCLUDE LOG IN SIDEBAR --> <script src="/js/jquery-1.11.1.min.js"></script> <script language="javascript">
function parseField(response,field){
	return response.getElementsByTagName(field)[0]==null?"":response.getElementsByTagName(field)[0].firstChild==null?"":response.getElementsByTagName(field)[0].firstChild.data;
}//parseField

function submitLogin(){
	var username = document.getElementById("username").value;
	var password = document.getElementById("password").value;
	
	// use AJAX to determine need for assessments, baseline, login questions
	$.post( "/loginXML.jsp",
	{
		username:username,
		password:password
	}, 
	function( data ) {
		processLogin(data);
	});
	function processLogin(data){
	
			var xmlobj = data.getElementsByTagName("login");
			var destination = parseField(xmlobj[0],"destination");
			var sid = parseField(xmlobj[0],"sid");
			if(destination=="popup"){
				launchPopupDocument("loginPopup.jsp?username=" + username,275,350,0,false);
			}else if(destination=="failure"){
				alert("Sorry, either your username or password is incorrect.  Please check the values and log in again, or use the \"Sign up now!\" button if you are a new member.");
			}else if(destination=="withdraw"){
				alert("Sorry, you have been withdraw from the program. If this is a mistake, please contact the Oregon Research Institute at mylastdip@ori.org");
			}else if(destination=="chewfree"){
				location.href = "https://www.chewfree.com/chewfreelogin.cfm?subjID=" + sid;
			}else{
				location.href = "" + destination;
			}
		
	};
}//submitLogin


function checkKeyEnter(e){
	if(window.event){
		if(window.event.keyCode == 13){
			submitLogin();
		}
	}else if(e.which){
		if(e.which == 13){
			submitLogin();
		}
	}
}
</script> 
