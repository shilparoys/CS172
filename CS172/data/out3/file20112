            <div> 
            </div> 
            <h5>Alternate Business Names</h5> 
            <span> Phi Theta Kappa</span> 
           </div> 
           <div id="business-additional-info-map"> 
            <script type="text/javascript">
            var map, markerLatLng, panorama;
            var addy = '1625 Eastover Dr, Jackson MS 39211';
            
                var geocoder = new google.maps.Geocoder();
                geocoder.geocode({ 'address': addy }, function (results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {
                        markerLatLng = results[0].geometry.location;
                        initializeMap();
                    }
                });
            

            function initializeMap() {
                var mapOptions;
                
                    mapOptions = { zoom: 14, center: markerLatLng, mapTypeId: google.maps.MapTypeId.ROADMAP, streetViewControl: false };
                
                map = new google.maps.Map(document.getElementById('map-canvas2'), mapOptions);
                var marker = new google.maps.Marker({
                    map: map, position: markerLatLng, title: addy
                });
                
                google.maps.event.addDomListener(window, 'load', initializeMap);
            }

            function processSVData(data, status) {
                if (status != google.maps.StreetViewStatus.OK) {
                    $('.map-controls span.divider').hide();
                    $('.map-controls a.street').hide();
                }
            }

            function showPanorama() {
                var panoramaOptions = {
                    position: markerLatLng,
                    pov: { heading: 34, pitch: 10 }
                };
                panorama = new google.maps.StreetViewPanorama(document.getElementById("map-canvas2"), panoramaOptions);
                map.setStreetView(panorama);
                $('.map-controls .street').addClass('back')
                $('.map-controls .street').text('Back to Map');
            }

            function resizeMap(height, width) {
                $('#map-canvas2').animate({
                    height: height,
                    width: width
                }, 1000, function () {
                    google.maps.event.trigger(map, "resize");
                    map.panTo(markerLatLng)
                });
            }

            $(document).ready(function () {
                $('.map-controls a.street').click(function (e) {
                    e.preventDefault();
                    if ($(this).hasClass('back')) {
                        initializeMap();
                        $(this).removeClass('back')
                        $(this).text('Street View');
                    } else {
                        showPanorama();
                        $(this).addClass('back')
                        $('.map-controls .street').text('Back to Map');
                    }
                });

                $('.map-controls a.enlarge2').click(function (e) {
                    e.preventDefault();
                    if ($(this).hasClass('large')) {
                        //Shrink Map
                        $(this).removeClass('large');
                        $(this).text('Enlarge');
                        resizeMap('232', '232');
                    } else {
                        $(this).addClass('large');
                        $(this).text('Shrink');
                        resizeMap('434', '626');
                    }
                });
            })
        </script> 
            <div class="expanding-holder"> 
             <div id="map-canvas2" style="height: 232px; width: 232px;"></div> 
             <p class="map-controls"> <a href="/mississippi/business-reviews/fraternal-organizations/phi-theta-kappa-honor-society-in-jackson-ms-140010259/directions">Directions</a> | <a href="#" class="enlarge2">Enlarge</a> </p> 
            </div> 
