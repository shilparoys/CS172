   <script>
	var timeout    = 5000;
	var closetimer = 0;
	var dropMenu = 0;
	
	function dropMenuOpen(e) {  
		dropMenuCancelTimer(); 
		dropMenuClose();
		
		dropMenu = $(this).find('ul').css("visibility","visible"); 
	}
	
	
	function dropMenuClose(e)
	{
		dropMenuCloseWithKey();
	}
	
	function dropMenuTime()
	{
		closetimer = window.setTimeout(dropMenuClose, timeout);
	}
	
	function dropMenuCancelTimer()
	{  if(closetimer)
	   {  window.clearTimeout(closetimer);
	      closetimer = null;}}
	
	$(document).ready(function(){
		$('#drop_menu > li').bind('mouseover', dropMenuOpen);
		$('#drop_menu > li').bind('mouseout', dropMenuClose);
				
	});
	
	document.onclick = dropMenuClose;
	
	
	function dropMenuOpenWithKey(event, theID){	
		
			dropMenuCancelTimer(); 
			dropMenuClose();
			var thisID='ID_'+theID;
			dropMenuCloseWithKey();
			document.getElementById(thisID).style.visibility='visible';	
			dropMenu = $(this).find('ul').css("visibility","visible");

			$("#"+thisID).find("a").css("height","auto");
	}
	
	function dropMenuCloseWithKey(){
		document.getElementById("ID_under").style.visibility='hidden';
		document.getElementById("ID_before").style.visibility='hidden';
		document.getElementById("ID_follow").style.visibility='hidden';
	}
</script> 
