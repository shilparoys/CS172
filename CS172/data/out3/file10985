  <script type="text/javascript">
	var url = location.href;
	var jsrc="";
	if(url.substring(0,15) == "https://student" || url.substring(0,15) == "https://www.stu"){
		jsrc="js/webtrends_studentloans.js";
		
	} else{ jsrc="js/webtrends_studentloans_test.js";}
	
	function loadjsfile(filename){
	  var fileref = document.createElement("script");
	  fileref.setAttribute("type","text/javascript");
	  fileref.setAttribute("src", filename);
	  document.getElementsByTagName("head")[0].appendChild(fileref);
	}
	
	loadjsfile(jsrc); //dynamically load and add this .js file  
	
	function notifyError(element,after){
		var notifier = $(".notify_error"); 
		var notifierExists = notifier.length;
		
	 	if(!notifierExists){  		
	 		if(shouldNotify()){  		
	 			var errorDiv = buildErrorDiv();
	 	  		if(!after){
	 				var html = element.html();
	 				element.prepend(errorDiv)[0].focus();
	 	  		}else{
	  			element.append(errorDiv)[0].focus();
	 	  		}
	 		}
	 	 }else if(notifierExists && !shouldNotify()){
	 		notifier.remove();
	 	 }
	 }
	 
	 function shouldNotify(){	   
	  return $(".tooltip:visible").length || $("span[id$='.errors']").length;
	 }
	 
	 function notificationShowing(){
	  return $(".notify_error").length > 0;	
	 }
	 
	 function buildErrorDiv(){
	  return  $("<div class='notify_error'>").html("There are one or more errors on this page");
	 }
	
	 function yesButtonText() {
	  return "Yes";
	 }
	
	 function noButtonText() {
	  return "No";
	 }
	 
	 function deletSchoolMessageText() {
	 	  return "Are you sure you want to delete <school_name>?";
	 }
	 
	 function getLoanAmountReqText() {
	 	  return "Loan Amount Requested:";
	 }
	 
	 function getTotalLoanAmountReqText() {
	 	  return "Total Loan Amount Requested:";
	 }
	
	 function getModifyLoanAmountText() {
	 	  return "I would like to specify a loan amount.";
	 }
	
	 function getModifyLoanAmountTextTotal() {
	 	  return "I would like to specify a new total loan amount.";
	 }
	
	 function getPleaseSelectText() {
	  return "-- Select --";
	 }
	 
	 function getSelectStudentText() {
	 	  return "-- Select Student --";
	 }
	 
	 function getEmailAltText() {
	 	  return 'Because you receive correspondence electronically, an email address is required.  If you prefer to receive correspondence through the U.S. Postal Service, return to "My Preferences" at https://studentloans.gov/myDirectLoan/preferences.action to change your option.';
	 }
	 
	 function getHeaderCollapsedAltText() {
	 	  return 'Collapse Section Button. Press enter twice to open this section.';
	 }
	 
	 function getHeaderExpandedAltText() {
	 	  return 'Expand Section Button. Press enter twice to access the content in this section.';
	 }
	 
	 function getCheckMarkAltText() {
	 	  return 'Checkmark';
	 }
	 
	 function getConsolidatedWarning() {
	 	  return 'You are choosing to remove this loan from the &#39;Loans You Want to Consolidate&#39; section.  Are you sure you want to do this?';
	 }
	 
	 function getNotConsolidatedWarning() {
	 	  return '<p>You are choosing to add this loan to &#39;Loans You Want to Consolidate&#39; section.</p>You may <b>not</b> consolidate loans that are<br/><ul><li>Already held by the U.S. Department of Education; </li><li>In default; </li><li>Loans you are seeking to have discharged in bankruptcy through an adversary proceeding; or</li><li>Loans in an &#39;in-school&#39; status.</li></ul>Are you sure you want to do this?';
	 }
	 
	 function getConsolidate() {
	 	  return 'Consolidate This Loan';
	 }	 
	 
	 function getDoNotConsolidate() {
	 	  return 'Do Not Consolidate This Loan';
	 }
	 
	 function getUpdateAltText() {
		 return 'Click here to update';
	 }
	 
	 function getToggleTableViewAll() {
		 return '<b>Displaying <span>' + arguments[0] + '</span> of  ' + arguments[1] + ' loans.</b> Click <a href="#">here</a> to view all of your student loans.';
	 }
	 
	 function getToggleTableViewFewer() {
		 return '<b>Displaying <span>' + arguments[0] + '</span> of  ' + arguments[1] + ' loans.</b> Click <a href="#">here</a> to view fewer of your student loans.';
	 }
	 
	 function getExpandIconAltText() {
		 return 'Expand Icon';
	 }
	 
	 function getCollapseIconAltText() {
		 return 'Collapse Icon';
	 }
	 
	 //this function selects the proper spring message to return for error icon alt text
	 //should only be called when the error icon alt text contains an <a href="...">
	 //NOTE: additional cases will need to be added to the switch statement in the future
	 function getErrorAltText(pathName) {
	 	  pathName = pathName.substring(pathName.lastIndexOf('.') + 1);
	 	  
	 	  switch(pathName) {
	 	  	case 'emailAddress':
	 	  		return getEmailAltText();
	 	  		break;
	 		default:
	 			alert('get____AltText() function not implemented');
	 	  		return;
	 	  }
	 }
