(function($) {
  $(function() {

    $('#jump-to-global-nav').click(function(e) {
      e.preventDefault();
      $(this).toggleClass('active');
      $('#global-nav').toggleClass('active');
    });

    $('#global-nav .close').click(function(e) {
      $('#jump-to-global-nav').removeClass('active');
      $('#global-nav').removeClass('active');
      return false;
    });

        $('.has-submenu > a', '#global-nav').on('click', function() {
      $(this).parent().toggleClass('active');
      return false;
    });
  
    $(document).on('click', function(e) {
      $('.has-submenu.active').removeClass('active');
    });

  });
})(jQuery);
</script> 
