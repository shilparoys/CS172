         <a id="main-content"></a> 
         <h1 class="title" id="page-title">Speaker's Bureau</h1> 
         <div class="tabs"> 
         </div> 
         <div class="region region-content"> 
          <div id="block-system-main" class="block block-system"> 
           <div class="content"> 
            <div id="node-234236" class="node node-page clearfix" about="/about/staff/speakers-bureau" typeof="foaf:Document"> 
             <span property="dc:title" content="Speaker's Bureau" class="rdf-meta element-hidden"></span>
             <span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span> 
             <div class="content"> 
              <div class="field field-name-body field-type-text-with-summary field-label-hidden">
               <div class="field-items">
                <div class="field-item even" property="content:encoded"> 
                 <style type="text/css">
    div.dir_box {
        height: 300px;
    }

    div.dir_box_content img {
        width: 135px;
        height: 185px;
    }
</style> 
                 <div class="dir_box"> 
                  <div class="dir_box_content">
                   <img alt="Dawneen Banks" src="/sites/default/files/images/honors/banks_dawneen2013.jpg"> 
                   <ul> 
                    <li><span class="dir_name">Dawneen Banks</span></li> 
                    <li><span class="dir_title">Associate Director of Regional &amp; Chapter Development Div. IV</span></li> 
                    <li><a href="#sb-bio2" class="cbox-bio internal">Read Dawneen's Biography</a></li> 
                   </ul> 
                  </div> 
                 </div> 
                 <div class="dir_box"> 
                  <div class="dir_box_content">
                   <img alt="Jennifer Blalock" src="/sites/default/files/images/honors/blalock_jennifer_2014.jpg"> 
                   <ul> 
                    <li><span class="dir_name">Jennifer Blalock</span></li> 
                    <li><span class="dir_title">Chief Student Support Officer</span></li> 
