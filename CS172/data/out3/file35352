    function displayHeader1() {
        var h1 = document.getElementById("entertainment-header");
        var h2 = document.getElementById("golfsports-header");
        var h3 = document.getElementById("artgalleries-header");
        h1.style.display = "block";
        h2.style.display = "none";
        h3.style.display = "none";
    };
    function displayHeader2() {
        var h1 = document.getElementById("entertainment-header");
        var h2 = document.getElementById("golfsports-header");
        var h3 = document.getElementById("artgalleries-header");
        h1.style.display = "none";
        h2.style.display = "block";
        h3.style.display = "none";
    };
    function displayHeader3() {
        var h1 = document.getElementById("entertainment-header");
        var h2 = document.getElementById("golfsports-header");
        var h3 = document.getElementById("artgalleries-header");
        h1.style.display = "none";
        h2.style.display = "none";
        h3.style.display = "block";
    };
