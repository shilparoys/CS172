  <script type="text/javascript">      
$(function(){
	t('ca');
	$('.slideshow-fotos').cycle({
		fx:  'fade',
		speed: 2000,
		timeout: 10000,
		random: true
	});
	$('.slideshow-alumni').cycle({
		fx:  'fade',
		speed: 1000,
		timeoutFn: calculateTimeout,
		random: false
	});

function calculateTimeout(currElement, nextElement, opts, isForward) { 
    var index = opts.currSlide; 
    return index == 0 ? 0:8000; 
} 
    

	$('#mes-info .form').load('./content/mesinfo.html #mesInfoContent', function(){
		$(this).find('textarea').val('');
	});
	$('#mes-info').dialog({
		modal: true,
		width: 900,
                height: 540,
		autoOpen: false,
		resizable: false,
		title: 'Sol·licita més informació',
		buttons: [
			{text: 'Envia', click: function(){
				$.post('./content/envia.php', $('#mes-info form').serialize(), function(data){
					var jsonData = $.parseJSON(data);
					var classMessage;
					if(jsonData.success) {
						classMessage = "alert-success";
						$('.ui-dialog-buttonpane').hide();
						$("#mes-info").dialog('close');
					}
					else {
						classMessage = "alert-error";
					}
					$('#mes-info .result').html('<div class="alert '+classMessage+'">' + jsonData.message + '</div>');
				});
			}},
			{
				text: 'Cancel·la', click: function(){
					$(this).dialog('close');
				}
			}
		],
		open: function() {
			$('.ui-dialog').css("z-index", "6000");
			$('.ui-widget-overlay').css("z-index", "5900");
			$('.ui-dialog-titlebar').hide();
		}
	});
	$('.mes-info').click(function(){
		$('#mes-info').dialog('open'); 
	});
});
</script> 
