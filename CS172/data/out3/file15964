$(document).ready(function(){

//console.log($('div:last').attr('id'));
// This fixed the responsive bug when EE Debugger is visible

if($('div:last').attr('id') == undefined)
	{
		$('div:last').parent().css('margin','0px auto');
		$('div:last').parent().css('width','80%');
		//$('div:last').parent().css('max-width','360px');
		$('div:last').parent().css('overflow','hidden');
		$('div:last').parent().css('word-wrap','break-word');
	}

});

</script> 
  <!-- <script type="text/javascript" src="/inc/v2/js/jquery.min.1.11.0.js"></script> --> 
  <script type="text/javascript" src="/inc/v2/js/jquery.tablesorter.js"></script> 
  <script type="text/javascript" src="/inc/v2/js/jquery.tablesorter.pager.js"></script> 
  <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js"></script> 
  <script type="text/javascript" src="http://ajax.microsoft.com/ajax/jquery.validate/1.7/jquery.validate.min.js"></script> 
  <script type="text/javascript">

$(document).ready(function() {

	//new Scroller(".m-vert", Scroller.vertical);

	$("#table1")
		.tablesorter({widthFixed: true, widgets: ['zebra'] }) //alternate colors for each row
		.tablesorterPager({positionFixed: false, size: 25, container: $("#pager1")});

	$("#table2")
		.tablesorter({widthFixed: true, widgets: ['zebra'] }) //alternate colors for each row
		.tablesorterPager({positionFixed: false, size: 25, container: $("#pager2")});


	$("#form_srch_name").validate({
		rules: {
			srch_name: "required",
		email: {
				required: true,
				email: true
			}
		}
	});
	$("#form_srch_dept").validate({
		rules: {
			srch_dept: "required",
		email: {
				required: true,
				email: true
			}
		}
	});
	$("#form_srch_camp").validate({
		rules: {
			srch_camp: "required",
		email: {
				required: true,
				email: true
			}
		}
	});




	var delay = (function(){
	  var timer = 0;
	  return function(callback, ms){
	    clearTimeout (timer);
	    timer = setTimeout(callback, ms);
	  };
	})();


	/*
	$('#srch_name').keyup(function() {
	    delay(function(){
	      alert('Time elapsed!');
	    }, 1000 );
	});
	*/


	// Each time search_dpt txt box onkeyup. Run this function.
	$('#srch_name').keyup(function(){
	//assign search_dpt value to variable value
	var value = $(this).val();

	// We only want to start searching once the value of search_dpt contains at least 3 characters.
	// So as long as value.length <= 2 dont do anything.
	if(value.length > 2)
		{
			delay(function(){
				// Incase the quesry results runs a bit too long, simply display Searching... while the browser waits for data retreival
				$('#searchresults').html('&nbsp; &nbsp; Searching...');
				
				// Call template display_department_ajax and pass the value searched for query
				$.get('/directory/directory_ajax', {val: value}, function(data){
					// Fill in div searchresults with the returned $display_string from the directory_ajax template.
					$('#searchresults').html(data);
				}) ;
		    }, 500 );			
		}
	});
	
});

</script> 
