           <script type="text/javascript">
            $(function() {
                try {
    	            var act = new gigya.services.socialize.UserAction();
    	            act.setTitle("Help us continue the NU legacy");
    	            act.setLinkBack("http://www.give2gether.com/projects/niagara-university-7/?lang=en&vv=<!-- VISITOR TOKEN PLACEHOLDER -->");
    	            
    	                act.setDescription("Make a difference today, I did!");
    	            
    	            
    	                act.addMediaItem({type: 'image', src: 'http://www.give2gether.com/static/uploads/2013/12/04/50573394126230_983212030_n.jpg', href: 'http://www.give2gether.com/projects/niagara-university-7/?lang=en&vv=<!-- VISITOR TOKEN PLACEHOLDER -->' });
    	            
    	            
    	            function gigya_share(provider, utm_source) {
    	                var id = 'gigya_share_' + provider.toLowerCase();
    	                jQuery('<div/>', {id: id}).appendTo('#gigya_share');
    	                var act2 = act.clone();
    	                act2.setLinkBack(act.linkBack + '&utm_source=' + utm_source);
    	                if (provider == 'Twitter') {
    	                    act2.setTitle("Make a difference today, I did!");
    	                }
    	                gigya.services.socialize.showShareBarUI({ 
    	                    containerID: id,
    	                    shareButtons: provider,
    	                    showCounts: 'none',
    	                    shortURLs: 'always',
    	                    cid: 'niagara-university-7',
    	                    userAction: act2,
    	                    onSendDone: function(e) {
    	                        $.post("/projects/niagara-university-7/on_project_shared_event/", {
    	                            provider: e.providers, 
    	                            context: 'main_page'}
    	                        );
    	                    }
    	                });
    	            }
    	            gigya_share('Facebook', 'FBS');
    	            gigya_share('Twitter', 'TWS');
    	            gigya_share('Googleplus', 'GOO');
    	            gigya_share('LinkedIn', 'LIS');
    	            gigya_share('Pinterest', 'PTS');
    	            setTimeout(function() {
    	            	$('#share_popup_toggle').css('visibility', 'visible');
    	            }, 1000);
                }
                catch (e) {
                }
            });
        </script> 
