      function init_activist() {
          
              window.location.href = '/fundraisers/create_page/niagara-university-7/';
          
          return false;
      }
      $(function() {
    	  $('.champ_button').each(function() {
    		  if (!$(this).attr('onclick')) {
    			  $(this).click(init_activist).attr('target', null);
    		  }
    	  });
      });
      
      
          $('a.thickbox').colorbox({iframe: true, innerWidth: '650px', height: '500px', opacity: 0.5});
          function tb_remove() {
        	  // for backwards compatibility with thickbox 
        	  $.colorbox.close();
          }
      
          
      
          _gaq.push(['_setCustomVar',3,'donation source','campaign page',2]);
      
  </script> 
   </div> 
