  <script>
		function cerrarPopup(pid)
		   {
			  document.getElementById(pid).style.visibility = 'hidden';      
		   }
		   
		$(function() {
		
			$("#slideshow > div:gt(0)").hide();
	
			setInterval(function() { 
			  $('#slideshow > div:first')
			    .fadeOut(1000)
			    .next()
			    .fadeIn(3000)
			    .end()
			    .appendTo('#slideshow');
			},  3000);
			
		});	
	</script> 
