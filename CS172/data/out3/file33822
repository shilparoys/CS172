  <script>
 $(document).ready(function() {
 
 var start=new Date(2014, 10, 28, 16, 25, 0);
var end=new Date(2014, 10, 28, 19, 25, 0);
var today = new Date();

if (today < end && today > start)	
  {
	//alert("IN BETWEEN DATES");
	$( ".carousel-inner .active" ).removeClass( "active" )
	$( ".carousel-linked-nav .active" ).removeClass( "active" )
	var $newdiv1 = $( "<div class='item active'/></div>" );
	$($newdiv1).append("<a href='http://www.watchriverside.com'><img class='bimg' src='/images/slider/2014/folswitchslide.jpg'></a>");
	$( ".carousel-inner" ).append( $newdiv1 );
	$(".carousel-linked-nav ul").append("<li class='active'><a href='#8'>8</a></li>")
	
  }
else
  {
	//alert("AFTER DATE SPAN");
  }

});
</script> 
  <script>
	  // invoke the carousel
$('#slider').carousel({
  interval: 8000
});

/* SLIDE ON CLICK */ 

$('.carousel-linked-nav > ul > li > a').click(function() {

    // grab href, remove pound sign, convert to number
    var item = Number($(this).attr('href').substring(1));

    // slide to number -1 (account for zero indexing)
    $('#slider').carousel(item - 1);

    // remove current active class
    $('.carousel-linked-nav .active').removeClass('active');

    // add active class to just clicked on item
    $(this).parent().addClass('active');

    // don't follow the link
    return false;
});

/* AUTOPLAY NAV HIGHLIGHT */

// bind 'slid' function
$('#slider').bind('slid', function() {

    // remove active class
    $('.carousel-linked-nav .active').removeClass('active');

    // get index of currently active item
    var idx = $('#slider .item.active').index();

    // select currently active item and add active class
    $('.carousel-linked-nav li:eq(' + idx + ')').addClass('active');

});
</script> 
  <script>
      !function ($) {
