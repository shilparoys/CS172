  <script type="text/javascript">
	$(window).load(function(){
		if($(window).width() >= 320 && $(window).width() <= 639){
			calendarListView();
		}
		if(!isMSIE){ imgResize(); }
		
		//AJAX HIDDEN SITE SHORTCUTS
		if ($(".sp").length) {
			$(".sp.column.one .hidden-shortcuts").load("/dedoe #sw-content-container11", function(){
				$(".sp.column.one .hidden-shortcuts li").each(function(){
					if ($("ul", this).length) { 
						$(this).prepend("<div class='bullet expandable'></div>");
					} else {
						$(this).prepend("<div class='bullet'></div>");
					}
				});
			});
		}
		//AJAX QUICK RESOURCES
		if ($(".sp").length) {
			$(".sp #slide-container").load("/dedoe #sw-content-container3", function(){
				$("#slide-container-outer").show();
			
				$("#slide-header-outer .slide-header").click(function() {
					$("#slide-container").toggle();
					
					$("html, body").animate({
						scrollTop: $("#slide-container-outer").offset().top
					}, 500);
				});
				
				$("#slide-container .ui-widget.app").each(function() {
					$("#slide-container .ui-widget.app:last",this).addClass("last-app");
				});
			});
		}	
		
		if($(".hp #slide-container #sw-content-container3 .ui-widget.app .ui-widget-detail").length > 0) {
			$("#slide-container-outer").show();
			
			$("#slide-header-outer .slide-header").click(function() {
				$("#slide-container").toggle();
				
				$("html, body").animate({
					scrollTop: $("#slide-container-outer").offset().top
				}, 500);
			});
			
			$("#slide-container .ui-widget.app").each(function() {
				$("#slide-container .ui-widget.app:last",this).addClass("last-app");
			});
		}
		
		//DDOE STATE HEADER
		ddoeStateHeader();
		
		//DOC active tab color
		var activeText = $("li.sw-channel-item.active > a > span").text();
		if(activeText == "Reports & Resources") {
			$("#gb-tabs").removeClass("dedoe");
			$("#gb-tabs .tab.six").css("background" , '#377BAB');
		}
		if(activeText == "State Board") {
			$("#gb-tabs").removeClass("dedoe");
			$("#gb-tabs .tab.seven").css("background" , '#377BAB');
		}

	});
	
	$(function(){
		// SET IE CLASSES TO BODY
		if(isMSIE){ $("body").addClass("msie"); }
		if(isMSIE7){ $("body").addClass("msie7"); }
		if(isMSIE8){ $("body").addClass("msie8"); }
		if(isMSIE9){ $("body").addClass("msie9"); }
		if(isMSIE10){ $("body").addClass("msie10"); }
	
		$(window).bind("resize", function(){ pgScreenSize(); imgResize(); });
		
		//GLOBAL ICONS
		$("#gb-global-icons.hide480, #gb-global-icons.show480").creativeIcons({
			'folderURL'	: '//extend.schoolwires.com/creative/scripts/creative/tools/creative-icons/font-files/', // HOLDS ALL OF THE FONT AND CSS FILES
			'iconNum'	: ' 4', // NUMBER OF ICONS
			'iconColor'	: '#BAA932', // ICON COLOR
