  <script type="text/javascript">
	
	function SWEndPreviewMode() { 
		var data = "{}";
		var success = "window.location='';";
		var failure = "CallControllerFailure(result[0].errormessage);";
		CallController("http://www.doe.k12.de.us/site/SiteController.aspx/EndPreviewMode", data, success, failure);
	}
	
	function SWEndEmulationMode() {
		SetUnescapedCookie('UserID', GetCookie('SourceEmulationUserID'));
		DeleteCookie('SourceEmulationUserID');
		window.location = 'http://www.doe.k12.de.us/ums/Users/Users.aspx';
	}

	function SWEndPreviewConfigMode() {
	    var data = "{}";
	    var success = "window.location='';";
	    var failure = "CallControllerFailure(result[0].errormessage);";
	    CallController("http://www.doe.k12.de.us/site/SiteController.aspx/EndPreviewConfigMode", data, success, failure);
	}
</script> 
