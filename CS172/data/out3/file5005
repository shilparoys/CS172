    function openDemo(url, width, height) {
        if (!width || width < 1)
            width = screen.width;
        if (!height || height < 1)
            height = screen.height;

        window.open(url, "ILPlayer", "width=" + width + ",height=" + height + ",left = 0,top = 0,fullscreen=yes, navigationbar=no,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no");
        return false;
    }

