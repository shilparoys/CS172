            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSessionText2'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_hidSession2'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSession2'),
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_trSession2'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_tdSession2'), sc2, scc2, s2, t2, r2);
            
         SetElements($('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_btnSession3'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSessionText3'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_hidSession3'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSession3'),
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_trSession3'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_tdSession3'), sc3, scc3, s3, t3, r3);
            
         SetElements($('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_btnSession4'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSessionText4'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_hidSession4'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_lblSession4'),
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_trSession4'), 
            $('#ctl00_cphMain_ucProductDetails1_ucSessionSelect_tdSession4'), sc4, scc4, s4, t4, r4);        
        return false;
   }
    function SetElements(btn, lbl, hid, disp, tr, td, sc, scc, s, t, readonly){
        if (CheckExists(sc)) {
            btn.val(s);
            lbl.html(s);
            btn.onclick = TriggerGoogleAnalytics(scc);
            hid.val(sc);
            SetSelect(td, btn, lbl, readonly);
            disp.html(t);
            tr.show(); 
        }
        else {
            tr.hide();
        }
    }
    function SetSelect(td, btn, lbl, readonly){
        td.removeClass();
        td.addClass('date');
        if(readonly){
            td.addClass('selected');
            btn.hide();
            lbl.show();
        }
        else{
            btn.show();
            lbl.hide();
        }
    }
    function CheckExists(s1) {
        return (typeof (s1) != "undefined" && s1 != '');
    }
    function TriggerGoogleAnalytics(code) {
        var val = '_gaq.push(["_trackEvent", "Select Session","SessionClick","$$$"]);';
        eval(val.replace("$$$", code));
        return true;
    }
</script> 
