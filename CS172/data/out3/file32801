         <script id="breadcrumbTemplate" type="text/x-handlebars-template">
   <div class="search-breadcrumbs {{#ifNotEmpty this.peers.[0].[0]}}{{#unless ../peers.[0].[24]}}has-siblings{{/unless}}{{/ifNotEmpty}}">
       <ul class="col-sm-12">
           <li class="home">
               <a href="/content/shc/en.html"><span class="icon"></span>Home</a>
           </li>
           {{#foreach this.trail.[0]}}
               <li>
                   <a href="{{this.path}}">{{this.title}}</a>
                   {{#if ../peers.[0]}}
                       {{#unless ../../peers.[0].[24]}}
                           {{#if $last}}
                               <div class="child-pages hidden-sm row">
                                   <ul class="col-md-4">
                                       {{#peerList ../../../../peers.[0]}}
                                           <li{{#ifCond this.current true}} class="selected"{{/ifCond}}>
                                               <a href="{{this.path}}">{{this.title}}</a>
                                           </li>
                                       {{/peerList}}
                                   </ul>
                               </div>
                               <div class="child-pages visible-sm row">
                                  <ul class="col-sm-6">
                                      {{#peerListTwoColumn ../../../../peers.[0]}}
                                          <li{{#ifCond this.current true}} class="selected"{{/ifCond}}>
                                              <a href="{{this.path}}">{{this.title}}</a>
                                          </li>
                                      {{/peerListTwoColumn}}
                                  </ul>
                              </div>
                           {{/if}}
                       {{/unless}}
                   {{/if}}
               </li>
           {{/foreach}}
       </ul>
   </div>
</script> 
