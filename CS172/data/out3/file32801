                <a href="#" class="active sort-option" data-sort-value="all" data-sort-name="ALL CONDITIONS AND TREATMENTS">ALL CONDITIONS AND TREATMENTS</a>
            </li>
        {{#ifEqual this.facets.type.[1].value "treatment"}}
            <li>
                <a href="#" class="sort-option" data-sort-value="treatment" data-sort-name="ONLY Treatments">ONLY Treatments</a>
            </li>
        {{/ifEqual}}
        {{#ifEqual this.facets.type.[0].value "condition"}}
            <li>
                <a href="#" class="sort-option" data-sort-value="condition" data-sort-name="ONLY Conditions">ONLY Conditions</a>
            </li>
        {{/ifEqual}}
        </ul>
    </div>
</script> 
