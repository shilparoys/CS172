  <!--[if lte IE 9]>
    <script>
        // if presentation mode in IE9 or less, redirect to presentation mode's path
        function checkForRequestedPresentation(url) {
            var urlSplit =  url.split('.html');            
            if (urlSplit.length <= 1 || urlSplit[1].length <= 0) {
                return;
            }
            urlSplit = urlSplit[1].split('/presentation-mode');
            if (urlSplit.length <= 1 || urlSplit[1].length <= 0) {
                return;
            }            
            var path = urlSplit[1] + '.html';
            window.location.replace = path;
        }
        var presentationRawUrl = window.location.href;
        checkForRequestedPresentation(presentationRawUrl);
    </script>
<![endif]--> 
