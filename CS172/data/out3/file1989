function fcnTczcCitadelInVideo_loadXmlDoc(id0, u0, funcnameOnLoad0) { 
  var xmlhttp; 
  if (window.XMLHttpRequest) {  
    xmlhttp=new XMLHttpRequest(); /* ie7+, ff, chr, opera, safari */  
  } else {  
    xmlhttp=new ActiveXObject("Microsoft.XMLHTTP"); /* ie5, ie6 */ 
  } 
  xmlhttp.onreadystatechange=function() { 
    if (xmlhttp.readyState==4 && xmlhttp.status==200) { 
      document.getElementById(id0).innerHTML=xmlhttp.responseText; 
      if (funcnameOnLoad0.length >= 1) { setTimeout(funcnameOnLoad0, 100); } 
    } 
  } 
  xmlhttp.open("GET", u0, true); 
  xmlhttp.send(); 
} 
