                        document.write('<ins class="adsbygoogle" style="display:inline-block;width:320px;height:50px" data-ad-client="ca-pub-8194720559236679" data-ad-slot="3155950892"></ins>');
                        (adsbygoogle = window.adsbygoogle || []).push({});
                    }
                </script> 
        </div> 
        <!-- end A --> 
       </div> 
      </div> 
