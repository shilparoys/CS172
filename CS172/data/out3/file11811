  <script>
$(function(){

	$("#field_nameofyourcurrentschool").autocomplete({
			minLength: 3
			,appendTo: "#frm_field_1037_container"
			,source: function( request, response ) {
				$.ajax({
					url: "https://apply.drew.edu/apply/ceebLookupapi.php",
					dataType: "jsonp",
					data: {
						term: request.term
					},
					success: function( data ) {
						response(data);
					}
				});
			}
		});

});
</script>   
