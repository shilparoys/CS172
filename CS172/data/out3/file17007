jQuery(document).ready(function($) {
  
  	$(".allCourses").click(function(){
	  
	  if($("#all-programs").attr('class').indexOf("right") >= 0){
  		$("#all-product-links").slideDown();
	  	$("#all-programs").attr('class', 'collapse-arrow point-down');
	  } else {
	  	$("#all-product-links").slideUp();
	  	$("#all-programs").attr('class', 'collapse-arrow point-right');
	  }
	  //alert("I am an alert box!");
	});
  
  	if (window.location.href.substring(window.location.href.length - 21) == '?subscribe=newsletter') {
	  //alert("Benchmark");
	  $('#lightbox').show();
		$('#lightbox').css("display", "block");
	}
  
    $('.lightbox_trigger').click(function(e) {
        //prevent default action (hyperlink)
        e.preventDefault();
        
        $('#lightbox').show();
		$('#lightbox').css("display", "block");
      
    });

    $('#close').click(function(e) {
		$('#lightbox').hide();
		$('#lightbox').css("display", "none");
    });
  
  	$('#lightbox').click(function(e) {
	  	if (e.target == this) { 
			$('#lightbox').hide();
			$('#lightbox').css("display", "none");
		}
    });

    $('.more-text').click(function(e) {
        //prevent default action (hyperlink)
        e.preventDefault();
		$('.more-text div').css("visibility", "visible");
		$('.more-text div').css("display", "inherit");
		$('a.display-more-link').css("display", "none");
    });
});
    

</script> 
  <!-- End: Benchmark Email Signup Form Code --> 
