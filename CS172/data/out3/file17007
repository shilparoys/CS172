  <script type="text/javascript">
    jQuery('document').ready(function() {
		$( "#accordion" ).accordion({
			active: false,
			collapsible: true,
		  	icon: icons
	  	});
	  
	  var icons = {
		header: url('images/upRedArrow.jpg'),
		activeHeader: "ui-icon-circle-arrow-s"
	  };
	});
</script> 
  <script type="text/javascript">
    jQuery('document').ready(function() {
	  $offSet = jQuery('.programs').offset();
	  
	  function rightSide($element){
		$offSet = $element.offset();
		$rightSide = $element.width();	
		$leftSide = parseInt($element.css('left'));
		if(!$leftSide){
		  $leftSide = 0;
		}
		$rightSide = $offSet.left + $rightSide - $leftSide;
	  	return $rightSide;
	  }
	  $wrapper = rightSide(jQuery('#wrapper'));
	  $programs = rightSide(jQuery('.programs .sub_menu_div table'));
	  $support = rightSide(jQuery('.support .sub_menu_div table'));
	  $programs += 9999;
	  $support += 9999;
	  
	  if($programs - $wrapper > 0){
		$leftWidth = -($programs - $wrapper);
		$('.programs table').css({"left":$leftWidth});
	  }

	  if($support - $wrapper > 0){
		$leftWidth = -($support - $wrapper);
		$('.support table').css({"left":$leftWidth});
	  }	  
	});
</script> 
  <!-- Place this tag after the last +1 button tag. --> 
