    <div class="container clearfix"> 
     <div class="grid_6 push_3"> 
      <div class="cormain"> 
       <div></div> 
       <div class="block">
        <!-- Start of Brightcove Player --> 
        <div style="display:none">
          &nbsp;
        </div> 
        <!--
