  <!-- initialize slideshow --> 
  <script type="text/javascript">
$(document).ready(function() {
  $('#imageList').bxSlider({
    'mode': 'fade', 	//type of transition
    'speed:': 1000 ,		//amount of time for transition
    'controls' : true,		//show controls
    'randomStart' : true,	//display in random order
    'pause' : 12000,		//time bewteen slides
    'auto' : true,		//start automatically
    'pager' : false,		//shows page numbers
				'prevText' : 'Previous item', //Text for the Previous link
				'nextText' : 'Next item', //Text for the Next link
    'easing' : 'easeOutCubic'	//type of transition easing
  });
  $('#imageList li').fadeIn();
});
</script> 
