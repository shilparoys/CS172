      <script src="/info/klass.min.js"></script> 
      <script src="/info/code.photoswipe.jquery-3.0.5.min.js"></script> 
      <script src="/info/masonry.pkgd.min.js"></script> 
      <script src="/info/imagesloaded.pkgd.min.js"></script> 
      <script>
(function(window, $, PhotoSwipe){			
$(function() {
	
var container = $('#photo-gallery1').addClass('loading');
var photoSwipeInstance;
var pageIndex = 0;

var attachScrollEvent = function() {
	$(window).on('scroll.gallery', function() {
		if (!container.hasClass('loading')) {
			if ($(window).scrollTop() + $(window).height() >= (container.height() + container.offset().top) - 200) {
				pageIndex++;
    			fetchMore(pageIndex);
    		}
    	}
	});	
};
		
var initPhotoSwipe = function() {
	var options = {
		captionAndToolbarShowEmptyCaptions: false,
		captionAndToolbarAutoHideDelay: 0,
		zIndex:9999
	}
			
	if (typeof photoSwipeInstance != "undefined" && photoSwipeInstance != null) {
		PhotoSwipe.unsetActivateInstance(photoSwipeInstance);
		PhotoSwipe.detatch(photoSwipeInstance);
		attachScrollEvent();
	}

	photoSwipeInstance = PhotoSwipe.attach($('a.photo-box', container), options);
};

var initMasonry = true;
var doMasonry = function() {
	var items = $('.item', container).not('.ready');
	var pos = 0;
	
	items.each(function(i) {
		$(this).data({'loaded': false, 'displayed': false});
		
    	$(this).imagesLoaded().progress(function(instance, image) {
		
						if (!image.isLoaded) {
				$(image.img).parent().parent().remove();
				items = $('.item', container).not('.ready');
				return;
			}
			
    		$(image.img).parent().parent().data('loaded', true);
    			
    		var el = items.eq(pos);
    		while (el && el.data('loaded')) {
    			if (!el.data('displayed')) {
        			el.addClass('ready');
        			if (initMasonry) {
        				initMasonry = false;
        				container.masonry({
        					itemSelector: '.item.ready',
    						transitionDuration: 0
        				});
        			} else {
        				container.masonry('appended', el, true);
        			}
    				el.addClass('masonry-brick').data('displayed', true);
    					
    				    				if (pos >= (items.length - 1)) {
    					container.removeClass('loading');
    				}
    			}
    			pos++;
    			el = items.eq(pos);
    		}
    	});
	});
};

var fetchMore = function(i) {

	container.addClass('loading');
	var url = "/sports/xc/2013-14/photos/0005/index?tmpl=photo-gallery-template&id=photo-gallery1&init=false&ajax=true&pageIndex=" + i;
	
	jQuery.get(url, function(response) {
					
		response = $('<div>').html(response);
		var newElems = response.find('.item');
		
		if (newElems.length) {
			container.append(newElems);
		
			initPhotoSwipe();
			doMasonry();
			
		} else {
			container.removeClass('loading');
			$(window).off('scroll.gallery');
		}
	});
};
		
var initGallery = function() {

	initPhotoSwipe();
	doMasonry();
	
	attachScrollEvent();
};
		
if (typeof Modernizr == "undefined") {
	$('html').removeClass('no-js').addClass('js no-touch');
}
		
		
if (container.css('position') == 'relative') {
	initGallery();
} else {
	container.hide();
	$('<link>').appendTo($('head')).attr({type : 'text/css', rel : 'stylesheet'}).attr('href', '/info/photo-gallery.css');
	var handle = window.setInterval(function() {
		if (container.css('position') == 'relative') {
			container.show();
			initGallery();
			window.clearInterval(handle);
		}
	}, 30);
}
	
});
}(window, window.jQuery, window.Code.PhotoSwipe));
</script> 
