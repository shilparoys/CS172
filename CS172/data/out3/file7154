  <script type="text/javascript" charset="utf-8">
			function menugetir(id){

					$("#aid").val(id);
					
					$.ajax({
						url: "anamenu_create.php",	
						type: "POST",
						data:$("#frm").serialize(),
						cache: false,
						success: function (response) {		
								$("#menu"+id+"_icerik").html(response);	
						},
						error: function(x,e){

 						}

					});
			}
 	
</script> 
