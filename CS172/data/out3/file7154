function openIt() {
		$("#mysplash").colorbox({rel:'mysplash', transition:"fade"});
		$("#mysplash").click();
		
		setTimeout(function(){
		    $("#mysplash").colorbox.close();
		}, 18000);
    }
	
function closeIt() {
        $("#mysplash").colorbox.close();
    }
