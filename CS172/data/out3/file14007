$(function () {
    var tabContainers = $('div.publication-box > div');
    
    $('div.tabs a').click(function () {
        tabContainers.hide().filter(this.hash).show();
        
        $('div.tabs a').removeClass('selected');
        $(this).addClass('selected');
        
        return false;
    }).filter(':first').click();
});
