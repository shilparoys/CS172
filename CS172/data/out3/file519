   <script>
	function invalidLogin()
	{
	    $('#loginWrap').effect("shake","slow");
	    $('#invalidLogin').show();
	
	}
	function addWheel()
	{
	  $('#button').remove();
	  var opts = {
	  lines:17,
	  length:2,
	  width:11,
	  radius:48,
	  corners:1,
	  rotate:26,
	  trail:91,
	  speed: 1.3,
	  color:'#77BDB7',
	  direction:1,
	  shadow:true,
	  hwaccel:true
	  };
	  var target = document.getElementById('wheel');
	  var spinner = new Spinner(opts).spin(target);
	  return true;
	}
	$(document).ready(function() {
		var def = $("#text").html();
		$('#more_show').click(function() {
			$('#text').html($('#more').html());
			$('#more_hide').on("click", function() {
				$('#text').html(def);
			});
		});
		$('#username').focus();
	})
	$(window).load(function(){
			})
	</script> 
