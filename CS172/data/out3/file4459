     <script type="text/javascript">
	function calculateCost(){
	var total=0.0;
	var sum=0.0;
	var cost_per_player=100;
	
	var sponsorNumberOne = document.form1.sponsor[0];
	var sponsorNumberTwo = document.form1.sponsor[1];
	var sponsorNumberThree = document.form1.sponsor[2];
	var sponsorNumberFour = document.form1.sponsor[3];	
	
	var giftDonation = document.form1.gift_donation.value*1;
	
	var playerWantsToGolf = document.form1.playerWantsToGolf;
	var playerTwo = document.form1.player_2_last_name;
	var playerThree = document.form1.player_3_last_name;
	var playerFour = document.form1.player_4_last_name;
	
	if (sponsorNumberOne.checked)
	total = 5000;
	if (sponsorNumberTwo.checked)
	total = 2500;
	if (sponsorNumberThree.checked)
	total = 250;
	//if (sponsorNumberFour.checked)
	//total = 200;
	
	//Tee sponsor is only one person
	//Single Player Entry is only one person
	
	
	if (!sponsorNumberOne.checked && !sponsorNumberTwo.checked){ 
		if (playerWantsToGolf.checked) total += cost_per_player;
		if (playerTwo.value!="") total += cost_per_player;
		if (playerThree.value!="") total += cost_per_player;
		if (playerFour.value!="") total += cost_per_player;
	}
	
	
	
	total += giftDonation;
	
	total = looksLikeMoney(total);
	
	loadTextInToElement(total, "total_cost");
	}

	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}
	function looksLikeMoney(total) {
		var total = (parseInt((total+.001)*100))/100;
		var niceTotal = "$"+total;
		if(niceTotal.substring(niceTotal.length-2,niceTotal.length-1)=="."){
			niceTotal=niceTotal+"0";
		}
 		return niceTotal;
	}

	</script> 
     <script type="text/javascript">

	function makeSubmitEnabled(){
		document.getElementById('submit_form').disabled='';
	}

</script> 
