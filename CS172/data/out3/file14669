      <td class="pageContents" width="100%"> <p class="locationBar"> </p> <script language="JavaScript" type="text/javascript">
    function conMayusculas(field) {
				
				//document.write(field.value.toUpperCase());
				field.value = field.value.replace("á","?");
				field.value = field.value.replace("é","?");
				field.value = field.value.replace("í","?");
				field.value = field.value.replace("ó","?");
				field.value = field.value.replace("ú","?");	

				field.value = field.value.replace("Á","?");
				field.value = field.value.replace("É","?");
				field.value = field.value.replace("Í","?");
				field.value = field.value.replace("Ó","?");
				field.value = field.value.replace("Ú","?");

