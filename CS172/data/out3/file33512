  <script type="text/javascript">
 $(document).ready(function(){
   $("a#quick_login").click(function(event){
	 $("div#quick_login_menu").toggle();
	 $("#alerttab img").toggle();
	 event.preventDefault();
   });
   
	
	$("#its_search_text").focus(function(){
		if (($(this).val())=="Search ITS Site") $(this).val('');
	});
	$("#its_search_text").blur(function(){
		if (($(this).val())=="") $(this).val('Search ITS Site');
	});

	// Set value of input#help_search to "Search Help & Support..." on load
	var current_value = $("input#help_search").val();
	if (current_value == "")
	{
		$("input#help_search").val("Search Help & Support...");
	}

	// Clear the contents of input#help_search on-click if the value is set to "Search Help & Support..."
	$("input#help_search").click(function () { 
		$("input#help_search").val("");
	});

	// Set value of input#help_search to "Search Help & Support..." when focus is lost and no value has been entered
	$("input#help_search").blur(function () {
		if ($("input#help_search").val() == "")
		{
			$("input#help_search").val("Search Help & Support...");
		}
	});

	/* Anythingslider */
	
	ie7 = 0;

 if (($.browser.msie) && (parseInt($.browser.version,10)<8)) {
 	ie7 = 1;
 }

 themeset = 'cs-portfolio-mod';
 if (ie7 == 1) themeset = 'cs-portfolio-ie7';

 $('#slider1').bind('initialized',function(e, slider){
 	//alert(slider.pages);
	var page_count = slider.pages;
	//$('#slider1').anythingSlider(1);
	page_count = page_count -4;
	left = 144;
	left = left-(page_count*7.8);
	left = Math.floor(left);
	left = left+"px";
	right = 196;
	right = right-(page_count*8);
	right = right+"px";
	$("div.anythingSlider-cs-portfolio-mod .back").css("left",left);
	$("div.anythingSlider-cs-portfolio-mod .forward").css("right",right);
//	$('#slider_inner_container').delay(800).css("display","block");
 });
 $('#slider_inner_container').fadeIn(50);
 $('#slider1')
  .anythingSlider({
   width          : 411,
   height         : 195,
   startPanel     : 1,
   toggleControls : true,
   startStopped   : false,
   resizeContents : true,
   toggleControls : false,
   theme          : themeset,
   autoPlay       : true,
   autoPlayLocked : true,
   delay		  : 6000,
   animationTime  : 300,
   infiniteSlides : true
  });
  
  /* End anything slider */
	
 });

</script> 
