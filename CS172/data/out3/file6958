   <script>
    function onUpdate(event, ui) {
        $(".column").each(function () {
            var ids = [];
            $(this).find(".container").each(function (i, e)
            { ids.push($(e).find(".hiddenID").val()); });   // collect all IDs in array
            $(this).find(".hiddenOrder").val(ids.join(","));   //set value of hidden field
        });
    }
</script> 
