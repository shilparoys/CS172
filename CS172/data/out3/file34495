  <script type="text/javascript">
$(document).ready(function(){

   // profile
  $('#profile').hide();
  $('a#profile-show').click(function() {
    $('#profile').slideToggle('slow');
    return false;
  });
  $('a#profile-hide').click(function() {
    $('#profile').slideUp('fast');
    return false;
  });
  
  })
  
  
