     <script>
var filecount=7;
var bannernum=1;
rotatetime=5000;
fadetime=200;
$(document).ready(function() {
	window.setTimeout(swap,rotatetime);
});

function swap(){
	var randomnumber=Math.floor(Math.random()*(filecount)) + 1;
	while(randomnumber == bannernum){
		randomnumber=Math.floor(Math.random()*(filecount+1)) + 1;
	}
	bannernum = randomnumber;
	$('#banner').fadeOut(fadetime,function(){
		$("#banner").attr("src","images/Home" + bannernum + ".png").
		fadeIn(fadetime,function(){
			window.setTimeout(swap,rotatetime);
		});
	});
		//, window.setTimeout(swap,rotatetime));
}
</script> 
