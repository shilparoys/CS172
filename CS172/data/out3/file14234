  <script type="text/javascript">var UserController=function(){var USER_STATUS_LOGGED_IN='L';var USER_STATUS_NOT_LOGGED='N';var USER_UNDER_13='Y';var USER_OVER_13='N';var COOKIE_NAME_SESS='sess';var COOKIE_NAME_FAV='fav';var COOKIE_NAME_SUPERSESS='ss';var PREMIUM_STATUS_VALUE=9;var FAV_COOKIE_PREMIUM_STATUS_KEY=0;var FAV_COOKIE_FAV_COUNT_KEY=1;var FAV_COOKIE_USER_UNDER_13_KEY=2;var isLoggedIn=null;var isPremium=null;var isSuperSess=null;var favNumber=null;var isUnder13=null;var displayAds=null;function checkIsUserUnder13(){if(isLoggedIn!==true){return false;}
