        <script src="https://www.trumba.com/scripts/spuds.js" type="text/javascript"></script> 
        <script type="text/javascript">
				$Trumba.addSpud (
				{ webName: "uwyos-calendar",
				  spudType: "upcoming",
				  //spudType: "crawler",
				  HeaderColor: "#f7d96b",
