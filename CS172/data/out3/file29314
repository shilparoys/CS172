		CNBC_Utils.execute('CommentsCount', function(){
			CNBC_Settings.commentsCountParam = {
                provider: {
                    lib:         'CNBC_Disqus',
                    apiEndpoint: 'http://commentsapi.cnbc.com',
                    authTimeout: 1
                },
                disqus: {
                    shortname:      'cnbc'
                },
                label: {showZero: 1, position: 'after', templates: {zero: 'Ghost town thread', singular: ' $COUNT$ comment ', plural: ' $COUNT$ comments '}, templatesArticle: {zero: '<div class=\"story_commentsCount zero\"><span id=\"comments-text\">COMMENTS</span><a id=\"discussion\">Start the Discussion</a></div>', singular: '<div class=\"story_commentsCount\"><div id=\"total-comments\"><span>$COUNT$</span></div><span id=\"comments-text\">COMMENT</span><a id=\"discussion\">Join the Discussion</a></div>', plural: '<div class=\"story_commentsCount\"><div id=\"total-comments\"><span>$COUNT$</span></div><span id=\"comments-text\">COMMENTS</span><a id=\"discussion\">Join the Discussion</a></div>'}},
                anchorAttrName: 'data-disqus-identifier'
			};
			
				var commentsObj = new CNBC_Comments();
				commentsObj.counts(CNBC_Settings.commentsCountParam);
			
		});
	
