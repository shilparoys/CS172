      <noscript> 
       <a href="http://ad.doubleclick.net/N2620/jump/'+mps.dartsitename+'/technology_socialmedia_;site=cnbc;sect=technology;sub=socialmedia;cont=cnbcnewsstory;franid=122;franid=internet;franid=internetretail;franid=media;franid=retailsales;franid=socialmedia;franid=twitterinc;pageid=102464172;!c=technology;!c=socialmedia;dcopt=ist;pos=1;tile=1;sz=728x90,970x66,970x250,970x90;ifr=n;mtfIFPath=/AdSales/;ord=123456a?"> <img src="http://ad.doubleclick.net/N2620/ad/'+mps.dartsitename+'/technology_socialmedia_;site=cnbc;sect=technology;sub=socialmedia;cont=cnbcnewsstory;franid=122;franid=internet;franid=internetretail;franid=media;franid=retailsales;franid=socialmedia;franid=twitterinc;pageid=102464172;!c=technology;!c=socialmedia;dcopt=ist;pos=1;tile=1;sz=728x90,970x66,970x250,970x90;ifr=n;mtfIFPath=/AdSales/;ord=123456a?" border="0" alt="Click Here!"></a> 
