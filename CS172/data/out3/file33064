    <!-- jumbotron menu end ---> 
    <script type="text/javascript">
//<![CDATA[


var menuCloseTimer; //tabCloser countdown
var menuOpenerTimer; //tabCloser countdown

( function($) {
$(document).ready(function() {

    $.easing.PopupMenuEasing = function(x, t, b, c, d) {
        return c * ((t = t / d - 1) * t * t + 1) + b;
    }
    

    var menuCloseTimeout = 400; // time in ms before menu disappears on mouseout
    var menuOpenTimeout = 100; // time in ms before menu appears on mouseenter

    //move popup menu html into position
    var popupMenu = $(".popupMenu").detach();
    popupMenu.appendTo("#dropDownMenu");
							   
	// on entering a tabOpener stop the countdown to close the menu and show the selected tab
	$(".tabOpener").mouseenter(function(event) {
        var selectedTab = $(this); 
		event.preventDefault();
        cancelCloseTab();

        cancelOpenTab();
        initOpenTab(menuOpenTimeout, selectedTab);

        logPopupMenuEvent("MOUSEENTER OPENER;");
		//showTab(selectedTab);
	});

	$(".tabOpener").mouseleave(function(event) {
        cancelOpenTab();
	});
	
	
    // on leaving a tabCloser restart the countdown to close the menu
	$(".tabCloser").mouseleave(function(event) {
		event.preventDefault();
        cancelCloseTab();
        initCloseTab(menuCloseTimeout);
        logPopupMenuEvent("MOUSELEAVE CLOSER;");
	});

    // on entering a tabCloser stop the countdown to close the menu
	$(".tabCloser").mouseenter(function(event) {
        //stop the countdown to close the menu
        event.preventDefault();
		cancelCloseTab();
        logPopupMenuEvent("MOUSEENTER CLOSER;");
	});
						   
	//alert("hooked-up");					   
						   
});
} ) ( jQuery );




	
showTab = function (el) {
( function($) {

	// read desired tab from elements 'tab' attribute
    var tabTitle = el.attr("data-tab");
    var objPopupMenu = $(".popupMenu");

    logPopupMenuEvent("SHOWTAB '" + tabTitle + "';");
    
    // find previously selected tab opener and deselect
    var previousTab = $(".tabOpener", ".selected");
	$(".tabOpener").removeClass("selected");
	
    // set selected tab opener as selected
	el.addClass("selected");

    //find desired popup menu panel
    selectedTabPanel = objPopupMenu.find("." + tabTitle);    

    // hide all popup menu panels
    objPopupMenu.find(".category").hide();
    
	
	if (selectedTabPanel.length) {
		//popup panel exists
        selectedTabPanel.hide();

        if (previousTab.length) {
            // a tab was previously selected
            selectedTabPanel.show();
            objPopupMenu.slideDown(500,'PopupMenuEasing');
            } else {
            // no tab was previously selected
            selectedTabPanel.show();
            objPopupMenu.slideDown(500,'PopupMenuEasing');    
            }
    

		} else {
            // no selected menu
		    objPopupMenu.slideUp(500,'PopupMenuEasing');
		}
		} ) ( jQuery );
	};	


initCloseTab = function (delay) {
    logPopupMenuEvent("InitCloseTab;");
    menuCloseTimer = setTimeout(closeTab, delay);
    ;}

closeTab = function () {
( function($) {
    logPopupMenuEvent("CloseTab;");
    $(".tabOpener").removeClass("selected");
    $(".popupMenu").slideUp(500,'PopupMenuEasing');
			} ) ( jQuery );
    ;}

cancelCloseTab = function () {
    logPopupMenuEvent("CancelCloseTab;");
    clearTimeout(menuCloseTimer);
    ;}



initOpenTab = function (delay, selectedTab) {
    logPopupMenuEvent("InitCloseTab;");
    menuOpenerTimer = setTimeout(function() {openTab(selectedTab);}, delay);
    //document.title =  ("initOpenTab");
    ;}

openTab = function (selectedTab) {
    logPopupMenuEvent("OpenTab;");
    showTab(selectedTab);
    //document.title = ("OpenTab");
    ;}

cancelOpenTab = function () {
    logPopupMenuEvent("cancelOpenTab;");
    clearTimeout(menuOpenerTimer);
    //document.title =  ("cancelOpenTab");
    ;}


logPopupMenuEvent = function(msg) {
    //$("#lblDebug").append(msg + '<br/>');
}


//]]>
</script>  
