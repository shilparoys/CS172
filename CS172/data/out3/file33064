<![endif]--> 
  <script type="text/javascript">
    (function ($) {

        // private variables
        var pluginName = "requestInformationManager";
        
        // private instance constructor
        function requestInformationManager(element, customOptions) {
            
            // plug-in common fields
            var domElement = element;      
            var $domElement = $(element);
            var pluginId = "plugin_" + pluginName;
            var options = $.extend({}, $.fn.requestInformationManager.defaultOptions, customOptions);
            
            // private fields
            var isActive = false;
            var instanceId = options.instanceId;
            var requestForm = element;
          
            
            function init() {
                // Add any initialization logic here...

                // bind event handlers
                console.log(instanceId + ": init binding event handlers.");

                // wire-up elements that invoke validation on blur
                $(options.fieldIdentifiers.causesValidation, requestForm)
                    .on("blur.requestInformationManager", function() {
                        console.log(instanceId + ": onValidationRequired event; isActive: " + isActive);
                        if (isActive)
                            validateFormFields();
                    });

                // wire-up elements that invoke submission on click
                $(options.fieldIdentifiers.causesSubmission, requestForm)
                    .on("click.requestInformationManager", function() {
                        console.log(instanceId + ": onSubmissionRequested event.");
                        sendMail();
                    });

                //override tooltip handling
                $.validity.outputs.tooltip.start = function() {
                    onTooltipStart();
                };

                $.validity.outputs.tooltip.raise = function($obj, msg) {
                    onTooltipRaise($obj, msg);
                };


                hook('onInit');
            }
            
            function option(key, val) {
                if (val) {
                    options[key] = val;
                } else {
                    return options[key];
                }
            }
            
            function hook(hookName) {
                if (options[hookName] !== undefined) {
                    // Call the user defined function.
                    // Scope is set to the jQuery element we are operating on.
                    options[hookName].call(domElement);
                }
            }

            function destroy() {
                // Iterate over each matching element.
                $domElement.each(function() {
                    var domElement = this;
                    var $domElement = $(this);

                    // Add code to restore the element to its original state...

                    hook('onDestroy');
                    // Remove plug-in instance from the element.
                    $domElement.removeData(pluginId);
                });
            }
            
            /*===================================================
            /*  Public Methods
            /*===================================================*/
            
            function sendMail() {
                console.log(instanceId + "::sendMail");
                isActive = true;
                
                // check for spam
                if (spamCheck()) {
                    showStatusMessage(true, 'Sorry, we are unable to process your request at this time');
                    console.log(options.instanceId + ": sendMail failed spam check -- abort");
                    isActive = false;
                    return this; 
                }
                console.log(options.instanceId + ": sendMail passed spam check");

                // validate input
                var validationResult = validateFormFields();
                if (!validationResult.valid) {
                    console.log(instanceId + ": sendMail failed form validation -- abort");
                    return this;
                }
                console.log(instanceId + ": sendMail passed form validation");

                // create message
                var message = generateMessage();

                // post message
                var result = postMessage(message);


                console.log(instanceId + ": sendMail complete");


                return this;
            }


            function validateFormFields() { 
                console.log(instanceId + "::validateFormFields");
                
                var fieldIdentifiers = options.fieldIdentifiers;
                var messages = options.validityMessageTemplates;

                $.validity.start();

                $(fieldIdentifiers.senderName, requestForm)
                    .require(messages.missingName);

                console.log(instanceId + "validated for pattern " + fieldIdentifiers.senderName);

                $(fieldIdentifiers.senderEmail, requestForm)
                    .require(messages.missingEmail)
                    .match('email', messages.invalidEmail);
                
                console.log(instanceId + "validated for pattern " + fieldIdentifiers.senderEmail);

                $(fieldIdentifiers.messageBody, requestForm)
                    .require(messages.missingQuestionTest);
                
                console.log(instanceId + "validated for pattern " + fieldIdentifiers.messageBody);

                var result = $.validity.end();

                console.log(instanceId + ": validateFormFields complete");
                return result;
            }
            
            /*===================================================
            /*  Private Methods
            /*===================================================*/
            
          
            
            function spamCheck() {
                if ($(options.fieldIdentifiers.recipientEmail, requestForm).val() != "") {
                    // a spam agent has put data in the form -- abort processing: log error
                    showStatusMessage(true, options.statusMessageTemplates.failedSpamCheck);
                    return true;
                }
                return false;
            }
            
            function generateMessage() {
                var formValues = {
                    name: $('input[name="Name"]', requestForm).val(),
                    email: $('input[name="EmailAddress"]', requestForm).val(),
                    topic: $('input[name="rbSubject"]:checked', requestForm).val(),
                    comment: $('textarea[name="Comment"]', requestForm).val()
                };

                var messageElements = {
                    sender: formValues.email,
                    subject: options.requestTemplates.subject
                        .replace(/\{topic\}/g, formValues.topic),
                    body: options.requestTemplates.body
                        .replace(/\{senderName\}/g, formValues.name)
                        .replace(/\{senderEmail\}/g, formValues.email)
                        .replace(/\{messageText\}/g, formValues.comment)
                };

                console.log(instanceId + ": message generated");
                return messageElements;
            }
            

            // (chainable)
            function postMessage(message) { 
                var messageHandlerUrl = options.messageHandlerUrl;

                var postData = "{\"sender\":" + JSON.stringify(message.sender)
                    + ", \"subject\":" + JSON.stringify(message.subject)
                    + ", \"body\":" + JSON.stringify(message.body) + "}";

                showStatusMessage(true, options.statusMessageTemplates.sending, "/images/styleElements/mail_sent.png");

                $.ajax({
                    type: 'POST',
                    url: messageHandlerUrl,
                    data: postData,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    cache: false,
                    success: onSuccess,
                    error: onError
                });

                console.log(options.instanceId + ": message posted (async)");

                return this;
            }

            // (chainable)
            function showStatusMessage(visible, text, icon) {

                var status = $('.oce-requestInfo-status', requestForm);

                if (!visible) {
                    status.fadeOut();
                }

                if (typeof text !== "undefined") {
                    var htmlMessageTemplate = "<div>{text}</div>";
                    var htmlMessage = htmlMessageTemplate.replace(/\{icon\}/, icon).replace(/\{text\}/, text);

                    if (typeof icon !== "undefined") {
                        // /images/styleElements/mail_sent.png
                        htmlMessageTemplate = "<img src=\"{icon}\" style=\"float:left;\" /><div style=\"margin-left:40px;\">{text}</div>";
                        htmlMessage = htmlMessageTemplate.replace(/\{icon\}/, icon).replace(/\{text\}/, text);
                    }

                    status.html(htmlMessage);
                }

                if (visible) {
                    status.fadeIn();
                }

                console.log(instanceId + ": status message set");
                return this;

            }
            
            // success handler for async ajax call
            // chainable
            function onSuccess(response) {
                console.log(instanceId + ": onSuccess");

                // process response
                var data = (typeof response.d) == 'string' ? JSON.parse('(' + response.d + ')') : response.d;
                var timestamp = new Date(+data.Timestamp.replace(/\/Date\((-?\d+)\)\//gi, "$1"));
                var timestring = timestamp.format("h:MM tt mm/dd/yyyy");
                var safeRecipient = escapeHtml(data.Recipient);
                var safeTimestring = escapeHtml(timestring);
                var safeReplyTo = escapeHtml(data.ReplyTo);

                if (data.Success) {
                    //show status (success)
                    var successMessage = options.statusMessageTemplates.success
                        .replace(/\{recipient\}/, safeRecipient)
                        .replace(/\{datetime\}/, safeTimestring)
                        .replace(/\{replyto\}/, safeReplyTo);
                    showStatusMessage(true, successMessage, "/images/styleElements/mail_sent.png");

                    // clear text area
                    $('textarea[name="Comment"]', requestInformationManager.requestForm).val("");

                } else {

                    // show status (server error sending mail)
                    var errorMessage = options.statusMessageTemplates.postofficeError
                        .replace(/\{recipient\}/, safeRecipient)
                        .replace(/\{datetime\}/, safeTimestring);
                    showStatusMessage(true, errorMessage, "/images/styleElements/mail_sent.png");
                }

                return this;
            }

            // error handler for async ajax call
            // chainable
            function onError(data) {
                console.log(instanceId + ": onError");

                var msgError = options.statusMessageTemplates.error;
                showStatusMessage(true, msgError + ':' + data);

                return this;
            }

            // event handler for tooltip
            // chainable
            function onTooltipStart() {
                // clear tooltips
                $(".oce-tooltip", requestForm).remove();

                return this;
            }
            
            // chainable
            function onTooltipRaise($obj, msg) {
                // create message html
                var tooltipHtmlTemplate = "<div class=\"oce-tooltip\">{tooltipText}<div class=\"oce-tooltip-outer\"><div class=\"oce-tooltip-inner\"></div></div></div>";
                var tooltipHtml = tooltipHtmlTemplate.replace(/\{tooltipText\}/, msg);

                $(tooltipHtml)                                      // create tooltip node
                    .click(function () { $obj.focus(); })           // tooltip onClick focuses on associated form element
                    .hide()                                         // hide tooltip
                    .prependTo($obj.parent())                       // insert tooltip node into document
                    .fadeIn();                                      // reveal tooltip

                return this;
            }
            
            // static string function
            function escapeHtml(text) {
                var entityMap = {
                    "&": "&amp;",
                    "<": "&lt;",
                    ">": "&gt;",
                    '"': '&quot;',
                    "'": '&#39;',
                    "/": '&#x2F;'
                };

                return String(text).replace(/[&<>"'\/]/g, function (s) {
                    return entityMap[s];
                });
            }

            init();
            
            // expose public methods
            return {
                option: option,
                destroy: destroy,
                sendMail: sendMail,
                validateFormFields: validateFormFields
            };
        }
        
        /***********************************************************************
        *   jQuery Plug-in: requestInformationManager
        *
        *   "$.fn.[class_name]" is shortcut for "jQuery.protoype.[class_name]"
        ************************************************************************/
        $.fn.requestInformationManager = function (args) {

            var pluginId = "plugin_" + pluginName;

            if (typeof arguments[0] === 'string') {

                // this is a method call
                var methodName = arguments[0];
                var methodArgs = Array.prototype.slice.call(arguments, 1);
                var returnValue;

                this.each(function() {
                    // very plug-in exists for element and supports requested method
                    if ($.data(this, pluginId) && typeof $.data(this, pluginId)[methodName] === 'function') {
                        console.log("requestInformationManager plug-in: executing method [" + methodName + ']');
                        returnValue = $.data(this, pluginId)[methodName].apply(this, methodArgs);
                    } else {
                        throw new Error('Method [' + methodName + '] does not exist in jQuery.' + pluginName);
                    }
                });

                if (returnValue !== 'undefined') {
                    return returnValue;
                } else {
                    return this;
                }
            
            } else if (typeof args === "object" || !args) {

                // this is an instantiation request 
                return this.each(function() {
                    if (!$.data(this, pluginId)) {
                        console.log("requestInformationManager plug-in: instantiating instance");
                        $.data(this, pluginId, new requestInformationManager(this, args));
                    } else {
                        console.log("requestInformationManager plug-in: plug-in already present at target");
                        //the plug-in already attached to object;
                    }
                });
                
            } else {
                // unrecognized argument
                throw new Error('Unexpected argument type for requestInformationManager.  Expected object, null, or string.');
            }
        };

        /***********************************************************************
        *   jQuery Plug-in Defaults: requestInformationManager
        *
        *   
        ************************************************************************/
        $.fn.requestInformationManager.defaultOptions = {

            // event handlers
            onInit: function () {},
            onDestroy: function () {},

            instanceId: "RIM",
            messageHandlerUrl: "/utilities/sendMail.aspx/SendMessage",
            tooltipTemplate: "<div class=\"oce-tooltip\">{tooltipText}<div class=\"oce-tooltip-outer\"><div class=\"oce-tooltip-inner\"></div></div></div>",
            
            validityMessageTemplates: {
                missingName: "Please provide your name.",
                missingEmail: "Please provide an email address.",
                invalidEmail: "Please provide a valid email address.",
                missingQuestionTest: "How may we help you?"
            },
            
            statusMessageTemplates: {
                sending: "Sending your message",
                success: "Your message was sent to {recipient} at {datetime}.  We'll get back to you at '{replyto}' as soon as we can.",
                error: "An error occurred sending your message.",
                postofficeError: "An error occurred sending your message to {recipient} at {datetime}.  We'll check into it.",
                failedSpamCheck: "Sorry, we are unable to process your request at this time."
            },
            
            fieldIdentifiers: {
                senderName: ".oce-requestInfo-name",
                senderEmail: ".oce-requestInfo-email",
                recipientEmail: ".oce-requestInfo-recipient",
                messageBody: ".oce-requestInfo-message",
                subject: "input[name=\"rbSubject\"]:checked",
                causesValidation: ".oce-requestInfo-validateOnBlur",
                causesSubmission: "button"
            },
            
            requestTemplates: {
                subject: "[oce.illinois.edu] Request for {topic} information",
                body: "FROM: \t{senderName}\r\n"
                        + "EMAIL: \t{senderEmail}\r\n"
                        + "QUESTION/COMMENT:\r\n{messageText}\r\n"
            }
        };
    })(jQuery);
</script> 
  <script type="text/javascript">
    $(document).ready(function () {
        // prepare request form
        var rimOptions = {instanceId: "[RIM 001]"};
        var rim = $(".requestInfo").requestInformationManager(rimOptions);
    });
</script> 
