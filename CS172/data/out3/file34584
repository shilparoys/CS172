      <script type="text/javascript">
	jQuery(function($) {
		$(".next_feature").click(function() {
			var $c=$(".feature:visible");	
			$c.hide(); 										
			var $n=$c.next();							
			if($n.length<1)
				$n=$(".first");
			$n.show();										
			return false;
		});
	});
	
	var $s = new Array('.first','.second','.third','.fourth');
	var $a = new Array('first','second','third','fourth');
	
	function next_feature() {
		for($i=0;$i<4;$i++) {
			$f=$($s[$i]);
			$f.hide();
			$f.removeAttr('class');
			$f.attr('class','feature');
			$n=$f;
			for($j=0;$j<4;$j++) {
				$n=$n.next();
				if($n.length==0)
					$n=$(".feature").first();
			}
			$n.show();
			$n.attr('class',$a[$i]+' feature');
		}
		return false;
	}

	function prev_feature() {
		for($i=3;$i>-1;$i--) {
			$f=$($s[$i]);
			$f.hide();
			$f.removeAttr('class');
			$f.attr('class','feature');
			$n=$f;
			for($j=3;$j>-1;$j--) {
				$n=$n.prev();
				if($n.length==0)
					$n=$(".feature").last();
			}
			$n.show();
			$n.attr('class',$a[$i]+' feature');
		}
		return false;
	}

</script> 
