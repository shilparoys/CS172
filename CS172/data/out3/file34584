  <script type="text/javascript">


  j('#showMenu').click(function() {
			j('li#menu').addClass('selected');
  });
  j('#purpleOverlay').click(function() {
			j('li#menu').removeClass('selected');
  }); 
  j('#closeMenu').click(function() {
			j('li#menu').removeClass('selected');
  });
  j('#showNav3').click(function() {
			j('div#third_level_nav_menu').addClass('selected');
  });
  j('#closeNav3').click(function() {
			j('div#third_level_nav_menu').removeClass('selected');
  });

	j('#form_saddr').click(function() {
		navigator.geolocation.getCurrentPosition(
          function(pos) {
            j('input#saddr').val(pos.coords.latitude+','+pos.coords.longitude)
          }
        )
	});
	
	j('#get_directions').click(function() {
		j('#form_saddr').click();
	});
	
</script> 
