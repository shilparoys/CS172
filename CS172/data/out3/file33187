		var vp_height = jQuery(window).height();
		var vp_width = jQuery(window).width();
		
		jQuery('.hsplash').height(vp_height - offset);
		
		var img_height = jQuery('.hsplash img.main').attr('height'); 
		var img_width = jQuery('.hsplash img.main').attr('width');
		var img_ratio = img_width / img_height;
	
		if (vp_height > img_height) {
			img_width = vp_height * img_ratio; 
			jQuery('.hsplash img.main').css('width', img_width);
		}
		
		css_width = jQuery('.hsplash img.main').width();
		
		if (vp_width > css_width) {
			img_width = vp_width;
			jQuery('.hsplash img.main').css('width', img_width);
		}
		
		jQuery('.hsplash img.main')
