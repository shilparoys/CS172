           <script type="text/template" id="plotgroup-header-daily-template">
	<div class="cond-highlow">
		<span class="cond-high"><%= condition.high %>&deg;</span>
		<span class="split">|</span>
		<span class="cond-low"><%= condition.low %>&deg;</span>
	</div>
	<div class="cond-icon-auto">
		<img src="//icons.wxug.com/i/c/v1/<%= condition.icon %>.svg" alt="<%= condition.icon %>" />
	</div>
	<div class="cond-text"><%= condition.condition %></div>
	<div class="cond-qpf">
		<span class="icon-qpf <% if (condition.precip_type === 'snow') { %> snow<% } if (condition.liquid_precip>0) { %> wet<% } %>"></span>
		<% if (condition.precip_type === 'snow') { %>
		<%
				var snowRange = condition.snow_range;
                var showUnits = true;

                if (snowRange == null) {
                     if (opts.source === 'bestfct') {
                        snowRange = opts.units === 'metric' ? '< 2' : '< 1';
                     } else {
                        snowRange = '--';
                        showUnits = false;
                     }
                }
            %>
		<%= snowRange %> <% if (showUnits) { %><%= labels.snow_depth.units %><% } %>
		<% } else { %>
		<% var liquidPrecip = condition.liquid_precip || 0; %>
		<%= liquidPrecip %> <%= labels.rainfall.units %>
		<% } %>
	</div>
</script> 
           <script type="text/template" id="plotgroup-header-hourly-template">
