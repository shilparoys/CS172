        <script type="text/template" id="today_summary_template">
	<% var today = weather.forecast.days[0];
		var current_observation = weather.current_observation;
		var station = current_observation.station;
		var yesterday_datestr = wui.date.strftime('%Y%m%d', yesterday_date, true);
		var valueOrDash = function(value) {
			return ($.isNumeric(value) ? value : '--');
		}
	%>
	<div class="small-6 columns">
		<span class="today" title="Today's weather summary for <%= station.name %>">Today</span>
		<div>High <strong class="high"><%= valueOrDash(today.summary.high) %></strong> <span class="split">|</span> Low <strong class="low"><%= valueOrDash(today.summary.low) %></strong> <%= labels.temperature.units %></div>
		<div><span class="pop<% if (today.summary.precip_type === 'snow') {%> snow<% } if(today.summary.pop >= 30) {%> wet<%}%>" title="chance of precipitation"></span><strong><%= valueOrDash(today.summary.pop) %></strong><%= labels.pop.units %> Chance of Precip.</div>
	</div>
	<div class="small-6 columns">
		<a href="/personal-weather-station/dashboard?ID=<%= station.id %>#history/s<%= yesterday_datestr %>/e<%= yesterday_datestr %>/mdaily" title="Yesterday's weather summary for <%= station.name %>">Yesterday</a>
		<div>High <strong class="high"><%= valueOrDash(current_observation.yesterday_max_temperature) %></strong> <span class="split">|</span> Low <strong class="low"><%= valueOrDash(current_observation.yesterday_min_temperature) %></strong> <%= labels.temperature.units %></div>
		<div>
		Precip. <strong><%= valueOrDash(current_observation.yesterday_precip_total) %></strong> <%= labels.rainfall.units %>
		<% if (current_observation.yesterday_precip_total>0 && current_observation.station.country==='US') { %>(<a href="/weather-radar/united-states/wy/laramie/cys/history/?date=" class="radar-loop" title="Laramie Radar Loop">Radar Loop</a>)<% } %>
		</div>
	</div>
</script> 
