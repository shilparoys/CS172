         <h3>History</h3> 
         <table id="almanac-history"> 
          <!-- insert almanac history template --> 
         </table> 
         <div class="footer"> 
          <a class="left" href="/history/airport/KLAR/2015/3/1/MonthlyCalendar.html?req_city=Laramie&amp;req_state=WY&amp;reqdb.zip=82051&amp;reqdb.magic=1&amp;reqdb.wmo=99999#calendar"><i class="fi-calendar"></i> March Calendar View</a> 
         </div> 
