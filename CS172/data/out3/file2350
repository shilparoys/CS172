		<% _.each(weather.forecast.days, function(day) { %>
		<% if (day.summary) { %>
		<%
						var snowRange = day.summary.snow_range;
						var showUnits = true;
						var isSnow = day.summary.precip_type === 'snow';

						if (snowRange == null) {
							 if (weather.forecast.source === 'nws') {
							 	snowRange = '--';
							 	showUnits = false;
							 } else {
							 	snowRange = weather.response.units === 'metric' ? '< 2' : '< 1';
							 }
						}

						var precipAmount = isSnow ? snowRange : (day.summary.liquid_precip || 0);
						var precipUnits = isSnow ? labels.snow_depth.units : labels.rainfall.units;
					%>
		<li id="fctDay-<%= day.summary.date.year %><%= wui.date.pad(day.summary.date.month) %><%= wui.date.pad(day.summary.date.day) %>">
