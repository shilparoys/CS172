           <script type="text/template" id="plotgroup-legend-template">
	<ul class="inline-list right">
	<% _.each(variables, function(variable) { %>
		<% if (variable.show !== false && variable.legend !== false && variable.enabled !== false) { %>
		<li>
		<span class="legend-swatch" style="background-color: <%= variable.legendFillColor || variable.color || variable.options.color %>; border-color: <%= variable.legendBorderColor || variable.color || variable.options.color %>;"></span>
		<%
					var units = '';
					var legendUnits = variable.legendUnits;
					if (legendUnits === false) {
						// no units shown in the legend label
					} else if (_.isString(legendUnits)) {
						units = $.trim(legendUnits);
					} else {
						units = $.trim(variable.units);
					}
				%>
		<span class="legend-label">
		<%= variable.options.label %>
		<% if (units) { %> (<%= units %>)<% } %>
		</span>
		</li>
		<% } %>
	<% }); %>
	</ul>
</script> 
           <script type="text/template" id="plotgroup-variables-menu-template">
	<ul class="no-bullet">
	<% var isNWS = (forecastSource == 'nws'); %>
	<% _.each(plots, function(plot) { %>
		<% _.each(plot.variables, function(variable) { %>
		<% if (variable.checkbox !== false) { %>
		<% var disabled = (isNWS && variable.missingFromNWS); %>
		<li>
		<input type="checkbox" id="cp_var_<%= variable.id %>" data-variable-id="<%= variable.id %>" <% if (variable.show !== false && !disabled) { %>checked="checked"<% } %> <% if (disabled) { %>disabled="disabled"<% } %> />
		<label for="cp_var_<%= variable.id %>" <% if (disabled) { %>class="disabled"<% } %>>
		<%= variable.options.label %>
		<% if (disabled) { %>*<% } %>
		</label>
		</li>
		<% } %>
		<% }); %>
	<% }); %>
	</ul>
	<% if (isNWS) { %><p>* Not available with NWS</p><% } %>
</script> 
