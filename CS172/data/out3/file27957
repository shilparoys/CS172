  <script type="text/javascript">
        NYTD.jQuery(window).load(function() {
        var request_uri = location.hash.replace("#", "");
        if(request_uri != ""){
            var uriArray = request_uri.split("/");
                if(uriArray[0] == "columnists"){
                    NYTD.jQuery('.' + uriArray[1]).click();
                } else if (uriArray[0].indexOf('Global') != "-1") {
                    NYTD.jQuery('.' + uriArray[0]).click();
                } 
        }
        });
    </script> 
