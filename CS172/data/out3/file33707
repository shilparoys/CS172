  <script type="text/javascript">
 function showArrow(li)
 {
	li.setAttribute("class", "lihover");
	li.setAttribute("className", "lihover");
 }
 
 function hideArrow(li)
 {
 	li.removeAttribute("class");
 	li.removeAttribute("className");
 }
 
 function randomImg()
 {
 	var div = document.getElementById("image");
	var img = document.createElement("img");
	var src = "https://www.wisconsin.edu/wp-content/themes/_tk-uwsa-theme/frontpage/retro-images/home/banner/";
	var files = new Array();
	
	files[0] = "discovery.jpg";
	files[1] = "explore.jpg";
	files[2] = "innovate.jpg";
	files[3] = "learn2.jpg";
	files[4] = "learn3.jpg";
	files[5] = "success.jpg";
	files[6] = "community.jpg";
	files[7] = "discovery2.jpg";
	files[8] = "inspire.jpg";
	files[9] = "service5.jpg";
	files[10] = "learn4.jpg";
	files[11] = "teaching1.jpg";
	files[12] = "teaching2.jpg";
	files[13] = "education.jpg";
	files[14] = "service4.jpg";
	
	
	
	var numfiles = files.length;
	var d = new Date();
	var secs = d.getSeconds();
	
	src += files[(secs%numfiles)];
	
	img.setAttribute("src", src);
	img.setAttribute("alt", "Banner image");
	div.appendChild(img);
 }
</script> 
