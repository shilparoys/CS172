  <script type="text/javascript">
	jQuery(document).ready(function($){
		
		$("#nationality option[value='']").attr("text","Nationality");
		$("#field_4").val("Query");

		$('#IWForm .submitIWForm').click(function (e) {

			var firstName = $("#firstName");
            var lastName = $("#lastName");
			var search_str = /^[\w\-\.]+@[\w\-\.]+(\.\w+)+$/;
            var email_val = $("#email").val();
			var mobile = $("#mobile");
            var optiontext = $("#nationality").find("option:selected").text();
			var field_4 = $("#field_4");
            
			if(firstName.val()==""){
				alert("firstName is null");
				$('#firstName').focus();
				return false;
			}

			if(firstName.val()=="First Name"){
				alert("firstName is null");
				$('#firstName').focus();
				return false;
			}

			if(lastName.val()==""){
				alert("lastName is null");
				$('#lastName').focus();
				return false;
			}

			if(firstName.val()=="Last Name"){
				alert("Last Name is null");
				$('#firstName').focus();
				return false;
			}

			if(!search_str.test(email_val)){       
            alert("please input right email !");
            $('#email').focus();
            return false;
            }

			if(isNaN(mobile.val())){
                alert("Phone number must be Numbers");
				$('#mobile').focus();
				return false;
            }

			if(optiontext == "Nationality"){
				alert("please select your Nationality");
				return false;
			}



			if(field_4.val()==""){
				alert("Query is null");
				$('#field_4').focus();
				return false;
			}

			if(field_4.val()=="Query"){
                alert("Please fill the query");
				$('#field_4').focus();
				return false;
			}

			$('.action_UI action_button submitIWForm').hover(function(){
                $(this).css("background-color","#003281");
	        },function(){
	            $(this).css("background-color","#e2e2e2");
	        }
	        );
            
			if ($(this).hasClass('sending')) { } else {
				var IWFormLink;
				var openLink,
			IW_queries = [],
			joiner = '&';
				IWFormLink = $(this).attr('data-form-link');
				var IW_form = $("#submit").parents("#IWForm");
				$(IW_form).find("input[type=text],input[type=hidden],textarea,select").each(
			function () {
				var queryName = $(this).attr('data-query'),
					queryVal = $(this).val();
				IW_queries.push(encodeURIComponent(queryName) + '=' + encodeURIComponent(queryVal));
			});
				openLink = IWFormLink + '?' + IW_queries.join(joiner);

				// $('#IWForm input[type=button].submitIWForm').addClass('sending').val('Sending...');
				$('body').append('<iframe id="a"  src="' + openLink + '" width=500px height=500px style="display:none"></iframe>');

				setTimeout(function () {
					$('#IWForm input[type=button].submitIWForm').val('Send');
					//alert(openLink);
					alert('Thank you. Your query has been received. We will follow up with you within 2 working days.');
					'';
					$("#firstName").attr("value","");
                    $("#lastName").attr("value","");
                    $("#email").attr("value","");
					$("#mobile").attr("value","");
					$("#field_4").attr("value","");
					$("#nationality").val(0);

				}, 1000)
			}
			
		});
	});
	
</script> 
