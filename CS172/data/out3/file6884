        <script type="text/javascript" charset="utf-8">
								
					function infoChanger(theCount, theDuration){
								
						$('#overlay_box #nav_thumbnails a').removeClass('current')
						$('#overlay_box #nav_thumbnails a.feature_thumb_' + theCount).addClass('current');
						
												
						$('#overlay_box #info_box').removeClass('current');
						$('.feature_info_' + theCount).addClass('current');
						
										
 					} 						
 					
 					function imageChanger(theCount, theDuration){
						if($.browser.msie && parseInt($.browser.version) <= 7)
						{$('#IE7_image_box').cycle(theCount-1);}
		 				else{$('#image_box').kenBurns.gotoImage(theCount-1);}
 					}
 					
 					
 					
				</script> 
