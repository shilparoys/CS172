  <script type="text/javascript">

/*Example message arrays for the two demo scrollers*/


		
		var pausecontent=new Array()
		pausecontent[0]='<a href="https://twitter.com/rucareers" target="_blank"><span class="reg_10">Follow us on twitter @ RU Careers</span></a>'
		pausecontent[1]='<a href="https://www.facebook.com/RutgersCareerServices" target="_blank"><span class="reg_10">Like us on facebook @ RutgersCareerServices</span></a>'





