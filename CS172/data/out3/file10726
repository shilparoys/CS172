     <script language="javascript" type="text/javascript">
			function findPosX(obj)
			{
				var curleft = 0;
				if (obj.offsetParent)
				{
					while (obj.offsetParent)
					{
						curleft += obj.offsetLeft
						obj = obj.offsetParent;
					}
				}
				else if (obj.x)
					curleft += obj.x;
				return curleft;
			}

			function findPosY(obj)
			{
				var curtop = 0;
				if (obj.offsetParent)
				{
					while (obj.offsetParent)
					{
						curtop += obj.offsetTop
						obj = obj.offsetParent;
					}
				}
				else if (obj.y)
					curtop += obj.y;
				return curtop;
			}
			
			function show(left, top)
			{
				var menu = document.getElementById('Nav2_switch_dropdown');
				var left = findPosX(document.getElementById('mainButton'));
				var top =  findPosY(document.getElementById('mainButton')) + document.getElementById('mainButton').offsetHeight;

//				var top =  findPosY(document.getElementById('mainButton'));
				
				menu.style.left = left;
				menu.style.top = top;
				menu.style.display='block';
			}
			
			function hide()
			{
				document.getElementById('Nav2_switch_dropdown').style.display='none';
			}
        </script> 
