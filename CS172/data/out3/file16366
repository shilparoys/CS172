   <script>

	function randomIntFromInterval(min,max) {
	    return Math.floor(Math.random()*(max-min+1)+min);
	}
	
	function adify(id) {
		var str = "<div class=\"forumTile tile\"><div id=\'" + id + "\' style=\'width:300px; height:250px;\'>\r\n<script type=\'text\/javascript\'>\r\ngoogletag.cmd.push(function() { googletag.display(\'" + id + "\'); });\r\n<\/script>\r\n<\/div><\/div>";
		return $.parseHTML(str, document, true)[0];
	}
	
    jQuery(document).ready(function() {
    
    	var forumTiles = $('#forum-tiles');
    
        var $container = forumTiles.packery({
            gutter:0,
            columnWidth:310,
            isFitWidth: true,
            itemSelector: ".forumTile"
        });
        
        var packery = $container.data('packery');
        
        $(".tile-2h").each(function (index) { //Prevent 2h tiles
        	$(this).removeClass("tile-2h");
		});
		
		// Add a single ad unit in the stream of tiles
			
			var adPlaced = false;
  
    		forumTiles.css('visibility', 'hidden');
    	
			$container.packery( 'on', 'layoutComplete', function (packery, items) {
				
				if (adPlaced) {
					forumTiles.css('visibility', 'visible');
					packery.options.transitionDuration = "0.4s";
					return;
				} 
				
				// This code will not work at all w/ 2 high tiles...
				// Just trying to place a tile in a visible area of the screen, since we only display 2 rows of tiles!
				// This also won't work if they re-size their window after the tile is shown, but whatever right?
				
				var tiles = $(".forum-tiles .forumTile");
				
				// If you have 2 rows and only 3 tiles, it will look like [ {0,1}, {2,2} ]
				var rows = [ {leftIndex: 0, rightIndex: 0} ];
				var curRow = rows[0];
				
				var previousRowTop = null;
				
				for (i = 0; i < tiles.length; i++) {
					var tile = $(tiles[i]);
					var top = tile.position().top;
					
					if (previousRowTop == null) {
					
						previousRowTop = top;
						
					} else if (top > previousRowTop) { // new row
						
						previousRowTop = top;
						
						curRow = {leftIndex: i, rightIndex: i};
						rows.push( curRow );						
					
					} else {
						
						curRow.rightIndex = i;
					}
				}
				
				var startIndex = 0;
				var endIndex = 0;
				
				// Only 2 rows of tiles are visible
				for (i = 0; i < Math.min(2, rows.length); i++) {
					endIndex = rows[i].rightIndex;	
				}
				
				var numTilesVisible = (endIndex - startIndex) + 1;
				
				var index = -1;
				
				if (numTilesVisible == 2) {
					// Don't put ad as first tile if you can only see two (like on phone)
					index = 1;
				} else if (numTilesVisible > 2) {
					// Ignore first tile for now
					index = randomIntFromInterval(1, endIndex);
				}
								
				var ad = adify("div-gpt-ad-1382036790176-1");
				
				if (index != -1) {
					$(tiles[index]).before(ad);
				} else {
					forumTiles.append(ad);
				}
				
				
				
				// Relayout and show screen
				adPlaced = true;
				packery.options.transitionDuration = "0s";
				$container.packery('reloadItems');
				$container.packery();
			});

			$container.packery();
		
					
        $('.overflow').overflow();
	});
    //Re-overflow tiles when screen is resized
    $( window ).resize(function() {
        
        $('.overflow').overflow();
    });

    function preventBubbling() {
		$(".tile a").click(function (e) { //Prevent click bubbling in tiles
			e.stopPropagation();
		});
	}
	preventBubbling();
</script> 
