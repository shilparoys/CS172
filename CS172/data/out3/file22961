  <script type="text/javascript">
		// Bootstrap script
		(function() {
			// Setup main dispute form view
			var mainView = new DisputeForm.MainView({
				el: AC.Element.select('#card-view')
			});

			// Setup translatable main view
			var mainTranslatableView = new Translator.View('main', 'en'),
					select = $('language-chooser');

			// Set select value to current language
			select.value = Translator.language.get();

			// Set language on language chooser change
			select.observe('change', function() {
				Translator.language.set(select.value);
			});

			// Setup translations for AC.OverlayPanel
			Event.onDOMReady(function() {
				var el = AC.OverlayPanel.overlay.sectionWithId('internal_without_browser').content
				var view = new Translator.View(el, 'en');
			});

		})();
	</script> 
