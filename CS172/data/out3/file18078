        <script type="text/javascript">
		var data, hideSponsoredText, main, preselectDegreeLevel, selectStudyAreas, showSponsoredText;

		selectStudyAreas = function($, generalStudyAreaId, emphasisId) {
		  $("#study_area").val(generalStudyAreaId).change();
		  if (emphasisId != null) {
			return $("#category-emphases").val(emphasisId);
		  }
		};

		preselectDegreeLevel = function($) {
		  var pathname;
		  pathname = location.pathname;
		  if (pathname.match(/associates-degree/)) {
			return $("#degree_level_id").val(5);
		  } else if (pathname.match(/bachelors-degree/)) {
			return $("#degree_level_id").val(6);
		  } else if (pathname.match(/masters-degree/)) {
			return $("#degree_level_id").val(7);
		  } else if (pathname.match(/doctorates-degree/)) {
			return $("#degree_level_id").val(8);
		  } else if (pathname.match(/mba-programs/)) {
			$("#degree_level_id").val(7);
			return selectStudyAreas($, 3);
		  } else if (pathname.match(/accounting-programs/)) {
			return selectStudyAreas($, 3, 4);
		  } else if (pathname.match(/art-programs/)) {
			return selectStudyAreas($, 18);
		  } else if (pathname.match(/business-management-programs/)) {
			return selectStudyAreas($, 3, 5);
		  } else if (pathname.match(/communications-programs/)) {
			return selectStudyAreas($, 3, 47);
		  } else if (pathname.match(/computer-science-programs/)) {
			return selectStudyAreas($, 14, 16);
		  } else if (pathname.match(/criminal-justice-programs/)) {
			return selectStudyAreas($, 1);
		  } else if (pathname.match(/education-and-teaching-programs/)) {
			return selectStudyAreas($, 33);
		  } else if (pathname.match(/engineering-programs/)) {
			return selectStudyAreas($, 229);
		  } else if (pathname.match(/english-and-humanities-programs/)) {
			return selectStudyAreas($, 40, 192);
		  } else if (pathname.match(/finance-programs/)) {
			return selectStudyAreas($, 3, 4);
		  } else if (pathname.match(/marketing-programs/)) {
			return selectStudyAreas($, 3, 8);
		  } else if (pathname.match(/nursing-programs/)) {
			return selectStudyAreas($, 23, 24);
		  } else if (pathname.match(/political-science-programs/)) {
			return selectStudyAreas($, 40, 196);
		  } else if (pathname.match(/psychology-programs/)) {
			return selectStudyAreas($, 27, 45);
		  }
		};

		showSponsoredText = function($) {
		  return $(".sponsored-text").show();
		};

		hideSponsoredText = function($) {
		  return $(".sponsored-text").hide();
		};

		data = {
		  emphases_by_study_area: {
			1: "<option value=\"530\">Corrections</option>\n<option value=\"204\">Court Reporting</option>\n<option value=\"90\">Criminal Investigation</option>\n<option value=\"88\">Criminal Justice Administration</option>\n<option value=\"256\">Emergency and Fire Management</option>\n<option value=\"228\">Fire Protection</option>\n<option value=\"2\">Forensics</option>\n<option value=\"12\">Homeland Security / Public Safety</option>\n<option value=\"200\">Law Degree</option>\n<option value=\"11\">Law Enforcement / Security</option>\n<option value=\"30\">Paralegal / Legal Studies</option>\n<option value=\"58\">Public Administration</option>",
			3: "<option value=\"4\">Accounting / Finance</option>\n<option value=\"84\">Administrative Professional</option>\n<option value=\"5\">Business Administration / Management</option>\n<option value=\"72\">Business Information Systems</option>\n<option value=\"47\">Communications / Public Relations</option>\n<option value=\"85\">Economics</option>\n<option value=\"65\">Entrepreneurship</option>\n<option value=\"63\">Hospitality / Tourism</option>\n<option value=\"9\">Human Resources</option>\n<option value=\"50\">International Business</option>\n<option value=\"8\">Marketing</option>\n<option value=\"225\">MBA</option>\n<option value=\"282\">Office Management</option>\n<option value=\"68\">Organizational Management</option>\n<option value=\"61\">Project Management</option>\n<option value=\"288\">Retail and Sales</option>\n<option value=\"528\">Supply Chain Management</option>",
			14: "<option value=\"16\">Computer Science / Programming</option>\n<option value=\"252\">Data Management</option>\n<option value=\"216\">Database Administration</option>\n<option value=\"15\">Information Systems</option>\n<option value=\"276\">Library Science</option>\n<option value=\"54\">Networking / Security</option>\n<option value=\"106\">Technical Support</option>\n<option value=\"105\">Web Development / Internet</option>",
			18: "<option value=\"22\">Animation / Game Design</option>\n<option value=\"226\">Architecture / Urban Planning</option>\n<option value=\"244\">Art and Illustration</option>\n<option value=\"227\">Audio / Visual Production</option>\n<option value=\"254\">Digital Design</option>\n<option value=\"116\">Drafting</option>\n<option value=\"118\">Fashion Design / Merchandising</option>\n<option value=\"198\">Fine Arts</option>\n<option value=\"101\">Graphic / Visual Arts</option>\n<option value=\"21\">Interior Design</option>\n<option value=\"120\">Performing Arts</option>\n<option value=\"119\">Photography / Film Studies</option>\n<option value=\"284\">Radio and Television</option>\n<option value=\"296\">Theater</option>\n<option value=\"20\">Web Design / Multimedia</option>",
			23: "<option value=\"234\">Alternative Therapy</option>\n<option value=\"188\">Athletic Training</option>\n<option value=\"127\">Dental</option>\n<option value=\"235\">Diet / Nutrition</option>\n<option value=\"137\">Emergency Medicine</option>\n<option value=\"122\">Health Information Systems</option>\n<option value=\"262\">Health Science</option>\n<option value=\"264\">Health Services</option>\n<option value=\"25\">Healthcare Administration / Management</option>\n<option value=\"139\">Massage Therapy</option>\n<option value=\"28\">Medical Assisting</option>\n<option value=\"125\">Medical Coding &amp; Billing</option>\n<option value=\"201\">Medical Technologist</option>\n<option value=\"532\">Medical Technology</option>\n<option value=\"212\">Medicine</option>\n<option value=\"24\">Nursing</option>\n<option value=\"280\">Nutrition and Wellness</option>\n<option value=\"128\">Pharmacy</option>\n<option value=\"130\">Physical Therapy / Respiratory Therapy</option>\n<option value=\"26\">Public Health</option>\n<option value=\"132\">Veterinary Studies</option>",
			27: "<option value=\"526\">Addiction Studies</option>\n<option value=\"248\">Child and Family Studies</option>\n<option value=\"150\">Child Development</option>\n<option value=\"49\">Counseling</option>\n<option value=\"45\">Psychology</option>\n<option value=\"292\">Social Science</option>\n<option value=\"154\">Social Work</option>",
			33: "<option value=\"240\">Adult Education</option>\n<option value=\"38\">Continuing Education</option>\n<option value=\"57\">Curriculum and Instruction</option>\n<option value=\"162\">Early Childhood Education</option>\n<option value=\"37\">Educational Leadership</option>\n<option value=\"55\">Elementary Education</option>\n<option value=\"166\">English as a Second Language</option>\n<option value=\"266\">Higher Education</option>\n<option value=\"272\">Instructional Technology</option>\n<option value=\"169\">Secondary Education</option>\n<option value=\"164\">Special Education</option>\n<option value=\"35\">Teacher Education</option>\n<option value=\"294\">Technology in Education</option>",
			40: "<option value=\"192\">English</option>\n<option value=\"190\">Foreign Language</option>\n<option value=\"260\">Geography</option>\n<option value=\"193\">History</option>\n<option value=\"205\">Humanities</option>\n<option value=\"274\">Liberal Arts</option>\n<option value=\"187\">Philosophy and Religious Studies</option>\n<option value=\"196\">Political and Social Sciences</option>\n<option value=\"286\">Religious Studies</option>",
			175: "<option value=\"42\">Automotive Technology / Mechanic</option>\n<option value=\"176\">Aviation Technology / Pilot</option>\n<option value=\"246\">Career Training</option>\n<option value=\"237\">CDL / Heavy Equipment Operation / Repair</option>\n<option value=\"250\">Conservation</option>\n<option value=\"181\">Construction Management</option>\n<option value=\"182\">Construction Trades</option>\n<option value=\"178\">Cosmetology</option>\n<option value=\"236\">Craftsman Trades</option>\n<option value=\"43\">Culinary Arts</option>\n<option value=\"199\">Electronic / Computer Technology</option>\n<option value=\"258\">Esthetics</option>\n<option value=\"268\">High School Diploma</option>\n<option value=\"189\">Life Skills / Hobbies</option>\n<option value=\"238\">Marine Technology / Captain</option>\n<option value=\"278\">Mechanics</option>\n<option value=\"232\">Military / ROTC</option>\n<option value=\"239\">Other Trades</option>\n<option value=\"44\">Real Estate</option>\n<option value=\"233\">Secondary Diplomas &amp; Certificates</option>\n<option value=\"183\">Welding / Fabrication</option>",
			229: "<option value=\"242\">Aeronautics</option>\n<option value=\"230\">Agriculture</option>\n<option value=\"191\">Biology</option>\n<option value=\"177\">Engineering</option>\n<option value=\"224\">Environment / Natural Resources</option>\n<option value=\"270\">Industrial Technology</option>\n<option value=\"194\">Mathematics</option>\n<option value=\"231\">Physical Sciences</option>"
		  }
		};

		main = function() {
		  return jQuery(document).ready(function($) {
			$("#study_area").change(function(event) {
			  var blank, disabled, study_area_id;
			  study_area_id = $(event.target).val();
			  disabled = !!study_area_id ? false : "disabled";
			  $("#category-emphases").prop("disabled", disabled);
			  blank = "<option value=''>Select a Subject</option>";
			  return $("#category-emphases").html(blank + data.emphases_by_study_area[study_area_id]);
			});
			preselectDegreeLevel($);
			return $(".sponsored-with-text").hover((function() {
			  return showSponsoredText($);
			}), (function() {
			  return hideSponsoredText($);
			}));
		  });
		};
		main();
	  </script> 
       </div> 
