    	var schoolhtml = '<div id="Search-schools-Button" class="Search-schools-Button"><a onClick="'+click_event+'" href="javascript:void(0);"><span>Search schools</span><i><img src="/wp-content/themes/affordable/images/search-school-button.png"></i></a></div>';
    	jQuery( "#most-popular-pages" ).append( schoolhtml );
		
    }
    jQuery(".Search-schools-Button a").click(function(e) { 
		if (jQuery('body').hasClass('active')) { 
			resetFun();
		    jQuery('html,body').animate({
	        	scrollTop: 65},
	        'slow');
	        return false;
		} else {
			 jQuery('html,body').animate({
	        	scrollTop: 150},
	        'slow');
	        return false;
		}
	});
