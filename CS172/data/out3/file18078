$(document).ready(function(){
	
	$('.sponsored-with-text').mouseover(function(){
        $('.sponsored-with-text .sponsored-text').css({'display': 'block'});
    });
			
    $('.sponsored-with-text').mouseleave(function(){
        $('.sponsored-with-text .sponsored-text').css({'display': 'none'});
    });
    
    $('#cmn-edudirect-submit_button').click(function() {
        $('#cmn_qdf_form').submit();
        return false;
    });
    
    $('#queen-edudirect-submit_button').click(function(event) {
        var degreeval = $('#queenschoolwithtool .number.degree select option:selected').text();
     	var categoryval = $('#queenschoolwithtool .number.category select option:selected').text();
     	if (degreeval == "Select a Degree Level" || categoryval == "Select a Category" ) {
     		event.preventDefault();
     		return false;
     	} else {
          	$('#queen_qdf_form').submit();
            return false;
     	}
    });
    
    $('#cmn-edudirect-submit_button-green').click(function() {
        $('#cmn_qdf_form_green').submit();
        return false;
    });
    
    $('#queen-edudirect-submit_button-green').click(function(event) {
        var degreeval = $('#queenschoolwithtool-green .number.degree select option:selected').text();
     	var categoryval = $('#queenschoolwithtool-green .number.category select option:selected').text();
     	if (degreeval == "Select a Degree Level" || categoryval == "Select a Category" ) {
     		event.preventDefault();
     		return false;
     	} else {
          	$('#queen_qdf_form_green').submit();
            return false;
     	}
    });
    
    $('#oneonone-edudirect-submit_button').click(function() {
        $('#oneonone_qdf_form').submit();
        return false;
    });
	
