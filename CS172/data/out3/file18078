var state_template = 'TemplateForDegreePrograms.php';
jQuery("document").ready(function(){
	
	var click_event = "_gaq.push(['_trackEvent', 'Search-schools-left-rail', 'Search-schools-left-now', 'Search-schools-left-rail-click'])";
    if (search_box == 'yes' && left_navigation != 'no' ) {
    	var schoolhtml = '<div id="Search-schools-Button" class="Search-schools-Button"><a onClick="'+click_event+'" href="javascript:void(0);"><span>Search schools</span><i><img src="/wp-content/themes/affordable/images/search-school-button.png"></i></a></div>';
