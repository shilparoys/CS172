	$(window).resize(function() {
		
        var wi = $(window).width();
        if(wi >= 800 ) {
		    $(window).scroll(function(){
			    	var body = document.body,
			    	html = document.documentElement;
					var height = Math.max( body.scrollHeight, body.offsetHeight, 
			                       html.clientHeight, html.scrollHeight, html.offsetHeight );
			        var footerheight = $(".footerpart").height();
			        var leftnavheight = $(".left-part .left-content").height();
			        var finalheight = height - (footerheight + leftnavheight + 20);
				        
				    if ($(window).scrollTop() > toppos && $(window).scrollTop()  < finalheight){
				       	$(".left-part .left-content").css("position", "fixed");
				       	$(".left-part .left-content").css("top", "70px");
				     } else {
				        $(".left-part .left-content").css("position", "relative");
				        $(".left-part .left-content").css("top", "0px");
				     }
				    
				});
        	}
	});
	
});
