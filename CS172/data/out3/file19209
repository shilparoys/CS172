  <div id="sms-codes-dialog" class="modal-container"> 
   <div class="close-modal-background-target"></div> 
   <div class="modal modal-medium draggable"> 
    <div class="modal-content"> 
     <button type="button" class="modal-btn modal-close js-close"> <span class="Icon Icon--close Icon--medium"> <span class="visuallyhidden">Close</span> </span> </button> 
