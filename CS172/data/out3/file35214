</script> <script>
  function itemSelectHandler(sType, aArgs) {
        var aData = aArgs[2];
        var id = aData[1];
        location.replace('/findaprovider/provider/view/'+id);
  }
              function init_fullname_ac() {

                  var dataSource = new YAHOO.widget.DS_XHR('/findaprovider/provider/fullNameAutoComplete', ['result','name','id']);
                  dataSource.scriptQueryAppend = "";
                  dataSource.connMethodPost=true;

              var fullnameAc = new YAHOO.widget.AutoComplete('fullname', 'cgui_d917cdd8575b6238827b3742e76d62a5', dataSource);
              fullnameAc.prehighlightClassName = 'yui-ac-prehighlight';
              fullnameAc.minQueryLength = 3;
              fullnameAc.textboxFocusEvent.subscribe(function() {
                  var sInputValue = YAHOO.util.Dom.get('fullname').value;
                  if (sInputValue.length === 0) {
                      var oSelf = this;
                      setTimeout(function() {
                          oSelf.sendQuery(sInputValue);
                      }, 0);
                  }
              });
              fullnameAc.minQueryLength = 2;
              fullnameAc.useShadow = false;
              fullnameAc.queryDelay = 0.2;
              fullnameAc.queryMatchContains = false;
              fullnameAc.itemSelectEvent.subscribe(itemSelectHandler);
          }
      YAHOO.util.Event.onDOMReady(init_fullname_ac);
      queryForm.q.focus();
</script> </td> 
