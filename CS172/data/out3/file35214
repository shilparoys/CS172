       <script type="text/javascript">
									/* <![CDATA[ */
									function submitSearch() {
										var query = jQuery('#hmcq').val();
										var searchType = jQuery('#search').val();
										if (searchType == 'people') {
											var str = query.replace(/ /, '*');
											searchUrl = 'http://www.psu.edu/cgi-bin/ldap/ldap_query.cgi?cn=*' + escape(str) + '*';
										}
										else {
											searchUrl = 'http://search.pennstatehershey.org/search?site=' + searchType + '&client=psh&output=xml_no_dtd&proxystylesheet=psh&proxyreload=1&q=' + escape(query);
										}
										parent.location.href = searchUrl;
									}
									function initSearchBox(selector){
										var $input = jQuery(selector);
										var label_text = 'Search for';
										$input.val(label_text);
										$input.focus(function() {
											if (this.value == label_text) {
												jQuery(this).removeClass('placeholder').val('');
											};
										}).blur(function() {
											if (this.value == '') {
												jQuery(this).addClass('placeholder').val(label_text);
											};
										});
									}
								   jQuery.extend({
										getUrlVars: function(){
											var vars = [], hash;
											var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
											for(var i = 0; i < hashes.length; i++)
											{
												hash = hashes[i].split('=');
												vars.push(hash[0]);
												vars[hash[0]] = hash[1];
											}
											return vars;
										},
										getUrlVar: function(name){
											return jQuery.getUrlVars()[name];
										}
									});
									jQuery(document).ready(function(){
										jQuery("#search > option").each(function() {
											if (this.value == jQuery.getUrlVar('collection')) {
												 this.selected = true
											}
										});
										initSearchBox('#psh-search-form .text');
									});
									/* ]]> */
								</script>
