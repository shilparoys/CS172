document.getElementById('mydesc').innerHTML=imgArray[slide_num][4];
document.getElementById('mystorylink').href=imgArray[slide_num][2];
document.getElementById('IVHmoreNum').innerHTML=myFeature;
}

function changeFocus() {
  setTimeout("document.getElementById('IVHnew').focus()",250);
}

function slideshowUp() {
  num++;
  myFeature=num+1;
  if (myFeature == 6) { myFeature = 1 }
  num = num % imgArray.length;
  slideshow(num);
}

function slideshowBack() {
  num--;
  myFeature=num+1;
  if (myFeature == 0) { myFeature = 5 }
  if (num < 0) { num = imgArray.length-1; }
  num = num % imgArray.length;
  slideshow(num);
}


if (browserName == "safari") {
document.getElementById("IVHnew").innerHTML="<a id=\"mylink\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\"><img src=\"" + xmlDoc.getElementsByTagName("imagelink")[0].childNodes[0].nodeValue + "\" id=\"mypic\" alt=\"" + xmlDoc.getElementsByTagName("imagealt")[0].childNodes[0].nodeValue + "\" border=\"0\" height=\"130\" width=\"180\" class=\"storyimg\"><\/a><h2><a id=\"mytitle\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\">" + xmlDoc.getElementsByTagName("titleshort")[0].childNodes[0].nodeValue + "<\/a><\/h2><p style=\"margin-top: 0px; padding-top: 0px; \"><span id=\"mydesc\">" + xmlDoc.getElementsByTagName("intro")[0].childNodes[0].nodeValue + "<\/span>&nbsp;<a id=\"mystorylink\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\"><em>Read the full story&hellip;<\/em><\/a><\/p>";

document.getElementById("IVHmore").innerHTML= "<div id=\"IVHcontrols\"><span id=\"IVHmoreNum\">" + myFeature + "<\/span> of 5 &nbsp;<a href=\"#\" onClick=\"slideshowBack(); changeFocus(); return false; \" title=\"Previous Inside Veterans Health story\"><img src=\"/health/images/larrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"> Previous<\/a> &#124; <a href=\"#\" onClick=\"slideshowUp(); changeFocus(); return false; \" title=\"Next Inside Veterans Health story\"> Next <img src=\"/health/images/rarrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"><\/a><\/div> <span id=\"morestories\"><a href=\"/health/InsideVHA.asp\" title=\"More Stories (accessible story list)\" id=\"morestories\">More Stories <img src=\"/health/images/rarrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"><\/a><\/span>";
}
else {
document.getElementById("IVHnew").innerHTML="<a id=\"mylink\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\"><img src=\"" + xmlDoc.getElementsByTagName("ivh:imagelink")[0].childNodes[0].nodeValue + "\" id=\"mypic\" alt=\"" + xmlDoc.getElementsByTagName("ivh:imagealt")[0].childNodes[0].nodeValue + "\" border=\"0\" height=\"130\" width=\"180\" class=\"storyimg\"><\/a><h2><a id=\"mytitle\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\">" + xmlDoc.getElementsByTagName("ivh:titleshort")[0].childNodes[0].nodeValue + "<\/a><\/h2><p style=\"margin-top: 0px; padding-top: 0px; \"><span id=\"mydesc\">" + xmlDoc.getElementsByTagName("ivh:intro")[0].childNodes[0].nodeValue + "<\/span>&nbsp;<a id=\"mystorylink\" href=\"" + xmlDoc.getElementsByTagName("item")[0].getElementsByTagName("link")[0].childNodes[0].nodeValue + "\" title=\"full story\"><em>Read the full story&hellip;<\/em><\/a><\/p>";

document.getElementById("IVHmore").innerHTML=  "<div id=\"IVHcontrols\"><span id=\"IVHmoreNum\">" + myFeature + "<\/span> of 5 &nbsp;<a id=\"IVHprevious\" href=\"#\" onClick=\"slideshowBack(); changeFocus(); return false; \" title=\"Previous Inside Veterans Health story\"><img src=\"/health/images/larrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"> Previous<\/a> &#124; <a id=\"IVHnext\" href=\"#\" onClick=\"slideshowUp(); changeFocus(); return false; \" title=\"Next Inside Veterans Health story\"> Next <img src=\"/health/images/rarrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"><\/a><\/div> <span id=\"morestories\"><a href=\"/health/InsideVHA.asp\" title=\"More Stories (accessible story list)\" id=\"morestories\">More Stories <img src=\"/health/images/rarrow.gif\" alt=\"\" width=\"8px\" height=\"10px\"><\/a><\/span>";
}


