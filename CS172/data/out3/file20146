      <script type="text/javascript">
      $(document).ready(function () {
        var ipGeoCoderService = new dhi.jobseeker.ipGeoCoderService(jQuery, undefined);
        console.log('init search bar');
        dhi.jobseeker.searchBar.init(ipGeoCoderService);

        //This wires up the placeholder plugin if the current browser is detected to not support html5 placeholder
        if (!Modernizr.input.placeholder) {
          $('#quick-find-jobs input[placeholder]').placeholder();
        }

        //This assigns the clearable behavior to any input with the correct data-clearable attribute
        if (Modernizr.input.placeholder) {
          //This polyfill doesn't play nicely with the placeholder polyfill - so do not use it unless native placeholder is supported
          $("#quick-find-jobs input[data-clearable=true]").each(function () {
            $(this).clearable();
          });
        }
      });
    </script> 
