  <script type="text/javascript">

        aa.handleHideAdPostMessages = function(event) {
            var msg = event.data;
            if (event.origin !== 'http://media2.legacy.com') return;
            var adId;
            if (/hideAdById\((.+)\)/i.test(msg)) { // kviframeid in adcall, banner -> parent.postMessage('hideAdById(_ADDCP(iframeid)_)', '*');
                adId = RegExp.$1;
            }
            else if (/hideAdByAlias\((.+)\)/i.test(msg)) { // banner -> parent.postMessage('hideAdByAlias(_ADALIAS_)', '*');
                var adAlias = RegExp.$1;
                for (var i in this.iframes) {
                    var iframe = document.getElementById(this.iframes[i]);
                    if (iframe.src.indexOf(adAlias) > 0) {
                        adId = iframe.id;
                        break;
                    }
                }
            }
            if (adId) {
                aa.hideAd(adId);
            }
        };

        // Listen to message from child window
        var eventMethod = window.addEventListener ? 'addEventListener' : 'attachEvent';
        var eventer = window[eventMethod];
        var messageEvent = eventMethod == 'attachEvent' ? 'onmessage' : 'message';
        eventer(messageEvent, function(e) { aa.handleHideAdPostMessages(e) }, false);
</script> 
