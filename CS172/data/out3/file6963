var loadedobjects=""
var rootdomain="http://"+window.location.hostname
 
function ajaxpage(url, containerid){
  if(document.getElementById(containerid)!=null){
  document.getElementById(containerid).innerHTML=''
  var page_request = false
  if (window.XMLHttpRequest) // if Mozilla, Safari etc
  page_request = new XMLHttpRequest()
  else if (window.ActiveXObject){ // if IE
    try {
      page_request = new ActiveXObject("Msxml2.XMLHTTP")
    }
    catch (e){
      try{
        page_request = new ActiveXObject("Microsoft.XMLHTTP")
      }
    catch (e){}
    }
  }
  else
  return false
  page_request.onreadystatechange=function(){
    loadpage(page_request, containerid)
  }
    page_request.open('GET', url, true)
    page_request.send(null)
    }
}

function loadpage(page_request, containerid){
  if (page_request.readyState == 4 && (page_request.status==200 || window.location.href.indexOf("http")==-1))
  document.getElementById(containerid).innerHTML=page_request.responseText
}
 
function loadobjs(){
  if (!document.getElementById)
  return
  for (i=0; i<arguments.length; i++){
    var file=arguments[i]
    var fileref=""
    if (loadedobjects.indexOf(file)==-1){ //Check to see if this object has not already been added to page before proceeding
      if (file.indexOf(".js")!=-1){ //If object is a js file
        fileref=document.createElement('script')
        fileref.setAttribute("type","text/javascript");
        fileref.setAttribute("src", file);
      }
      else if (file.indexOf(".css")!=-1){ //If object is a css file
        fileref=document.createElement("link")
        fileref.setAttribute("rel", "stylesheet");
        fileref.setAttribute("type", "text/css");
        fileref.setAttribute("href", file);
      }
    }
    if (fileref!=""){
      document.getElementsByTagName("head").item(0).appendChild(fileref)
      loadedobjects+=file+" " //Remember this object as being already added to page
    }
  }
}
 
//reset menu items

function resetItems(){
if(document.getElementById('uu_left_menu_content')!=null){
var menuLength=document.getElementById('uu_left_menu_content').childNodes.length;
for(var i=0;i<menuLength;i++){
  var menuItem=document.getElementById('uu_left_menu_content').childNodes[i];
  menuItem.style.borderRight = '1px solid #CCCCCC';
  menuItem.style.backgroundColor = '#FFFFFF';
  menuItem.getElementsByTagName('a')[0].className='no';
}
document.getElementById('uu_left_menu_content').childNodes[menuLength-1].style.borderBottom ='1px solid #CCCCCC';
}
}

//change the selected menu-item style 
 
function setActiveMenu(menuid){
var activeMenu = document.getElementById('li'+menuid);
if(activeMenu!=null){
activeMenu.style.borderRight= '0 none';
activeMenu.style.borderBottom= '0 none';
activeMenu.style.backgroundColor = '#E8F4FE';
activeMenu.getElementsByTagName('a')[0].className='act';
}
} 
 
