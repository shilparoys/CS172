     <script>
	/**
	 * Adjust margin top on top posts/blog landing
	 */
	jQuery(document).ready(function ($) {
		var $categoryFeatures = $('#category-features'),
			$categoryFeatureList = $('#category-feature-list'),
			$blogLandingSidebar = $('#blog-landing-sidebar'),
			$topBlog = $('#top-blog'),
			minHeight = 247,
			categoryFeaturesHeight = $categoryFeatures.height(),
			categoryFeatureListPaddingTop = parseInt($categoryFeatureList.css('padding-top'), 10),
			categoryFeatureListPaddingBottom = parseInt($categoryFeatureList.css('padding-bottom'), 10),
			heightDifference
		;

		if (categoryFeaturesHeight <= minHeight) {return;}

		heightDifference = (categoryFeaturesHeight - minHeight);

		function increaseHeightOnEl ($el) {
			$el.css('margin-top', heightDifference + 'px');
		}

		increaseHeightOnEl($topBlog);
		increaseHeightOnEl($blogLandingSidebar);

		$categoryFeatureList.height(categoryFeaturesHeight - (categoryFeatureListPaddingTop + categoryFeatureListPaddingBottom));
	});
</script> 
    </div>
