   <script type="text/javascript" charset="utf-8">

(function () {
var enableFarmTimingSampling = true,
    enablePageTimingSampling = false,
    enableLightHouseTimingSampling = true,
    enableYCPIFarmUrls = false,
    enableYCSFarmUrls = false,
    enableYCSCTFarmUrls = true,
    enableAkamaiFarmUrls = false,
    enableAkamaiFreeflowFarmUrls = false,
	enableCacheWarming = true,
	enableHTTPS = true;


function lighthouseBeacon() {
	var bucket, scheme;

	if (window.LH && window.LH.isInitialized && window.load_photo_only) {
		LH.tag('pageType', {val: 'photostream'});

		if (enableCacheWarming) {
			bucket = 'CacheWarm';
		} else if (enableYCPIFarmUrls) {
			bucket = 'YCPI';
		} else if (enableYCSFarmUrls) {
			bucket = 'YCS';
		} else if (enableYCSCTFarmUrls) {
			bucket = 'YCS';
		} else if (enableAkamaiFarmUrls) {
			bucket = 'akamai';
		} else if (enableAkamaiFreeflowFarmUrls) {
			bucket = 'akamai';
		} else {
			bucket = 'origin';
		}

		if (enableHTTPS) {
			scheme = 'https';
		} else {
			scheme = 'http';
		}

		// CacheWarm bucket has no scheme appended, takes days to change the names
		if(bucket != 'CacheWarm') {
			LH.tag('b', {val: bucket + '_' + scheme});
		} else {
			LH.tag('b', {val: bucket});
		}

		// If the user is logged in, tag with "l"
		if (F.config && F.config.flickr.user.user_ok) {
			LH.tag('l');
		}

		// beacon user timing metrics, then clear them so they don't get sent again in the nav timing beacon.
		LH.beacon({clearMarks: true, clearMeasures: true});
	} else {
		setTimeout(function () { lighthouseBeacon(); }, 500);
	}
}


	
	
	
	(function() {
		var img = new Image();
		
		LH.mark('time_to_load_photo_only_start');
		img.onload = function() {
			LH.mark('time_to_load_photo_only_end');
			window.load_photo_only = true;
			LH.measure('time_to_load_photo_only', {startMark: 'time_to_load_photo_only_start', endMark: 'time_to_load_photo_only_end'});
		}
		
		img.src = "https://c1.staticflickr.com/9/8305/7980469030_0c636aa154.jpg";
		
	})();
	

	

var toLoad = window.F.toLoad;
var len = toLoad.length;
var first = true;
var img, toDo = len;
for (var i = 0; i < len; i++) {
	img = new Image();
	
	img.onload = function() {
		if (enableFarmTimingSampling || enablePageTimingSampling) {

			if(first) {
				if (enablePageTimingSampling) {
					page_timing.time_to_first_photo = new Date().getTime();
				} else if (enableFarmTimingSampling) {
					farm_timing.time_to_first_photo = new Date().getTime();
				}
			}

			toDo--;

			if(toDo == 0) {
				if (enablePageTimingSampling) {
					page_timing.photos_above_fold = new Date().getTime();
				} else if (enableFarmTimingSampling) {
					farm_timing.time_to_first_photo = new Date().getTime();
				}
			}

		} 

		if (enableLightHouseTimingSampling && window.LH && first) {
			first = false; // looks like lighthouseBeacon can fail, which then makes the first flag never get set to false properly.

			LH.mark('mark_time_to_first_photo');

			lighthouseBeacon();
		}

		if (first) {
			first = false;
		}
	}

	img.src = toLoad[i];
}
})();

</script> 
