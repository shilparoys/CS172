      <script type="text/javascript">
var teach = new Array();

teach[0] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[1] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[2] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[3] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[4] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[5] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');
teach[6] = new Array('Compass Mis-placed','Recent HS graduate','Transfer from another college','Did not meet pre-req grade on prior course');

function getteach() {
	var classbox = document.getElementById("course");
	var cl = classbox.options[classbox.selectedIndex].value;
	var teachbox = document.getElementById("teachers");
	if (cl > -1) {
		var list = teach[cl];
		teachbox.options.length = 0;
		for(i=0;i<list.length;i++)
		{
			teachbox.options[i] = new Option(list[i],list[i]);
		}
	}
}

</script> 
