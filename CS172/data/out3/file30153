      $(document).ready(function () {
          $(".fancybox").fancybox({
              afterLoad: function() {
                  var data = $(this.element).data();
                  // add topic gallery link to title
                  if (data.linkurl) {
                      this.title = '<a href="' + data.linkurl + '">' + data.linktext + '</a><br/>' + this.title;
                  }
                  // make good and sure the user sees the ad
                  if ($('#leaderboardA').length > 0) {
                      window.scrollTo(0, $('.header').height());
                      $('.fancybox-overlay');
                  }
                  // trigger ad updating
                  $('body').trigger('slideAdvanced');
              },
              helpers : {
                  title: {
                      type: 'inside'
                  }
              }
          });
      });
