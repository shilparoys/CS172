  <script>
    var isQuickLinkOpened = false;
    $('#top-quick-link').click(function () {
        $('#topbar .quicklinks').slideToggle('slow', function () {
            if (!isQuickLinkOpened) {
                $('#top-quick-link').css('backgroundPosition', '0 -25px');
                isQuickLinkOpened = true;
            } else {
                $('#top-quick-link').css('backgroundPosition', '0 0');
                isQuickLinkOpened = false;
            }
        });
    });
</script> 
