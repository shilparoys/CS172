  <script language="JavaScript">
function fucValidate(){
  //case 00045103
  var invalidname=false;
  var filteredValues = "}{|^%#&";
    //var filteredValues = "}{|+^%#";	
  if(document.frmEmail.EmailAdd.value==""){
    alert("Please enter email address!");
    document.frmEmail.EmailAdd.focus();
    return false;
  }
    else    
    {
        var strEmail = document.frmEmail.EmailAdd.value;
        if (strEmail.indexOf("\"") > -1)
        {
          alert('Email cannot contains double quote (\").\n');
          document.frmEmail.EmailAdd.focus();
        return false;
        }
        
        //case 00045103
        if (document.frmEmail.EmailAdd.value.length!=0){
      for (i = 0; i < document.frmEmail.EmailAdd.value.length; i++) {  // Search through string and append to unfiltered values to returnString.
        var c = document.frmEmail.EmailAdd.value.charAt(i);
        if (filteredValues.indexOf(c) != -1) invalidname=true;
      }
      
      if (invalidname==true){
        alert('Email is invalid (no special character is allowed).\n');
        document.frmEmail.EmailAdd.focus();
        return false;
      }
    }
        
    }       
  
  return true;
}
    </script> 
