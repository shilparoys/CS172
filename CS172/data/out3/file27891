    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/22001031/posts/default/8986345836013476530";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/22001031/8986345836013476530/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 8986345836013476530
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/22001031/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/22001031/comments/default";
          
          holder.currentBlogUrl = "http://notofgeneralinterest.blogspot.com/";
          holder.currentBlogId = "22001031";
        
        return holder;
      });
    }
  </script> 
