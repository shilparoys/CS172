          <script type="text/javascript">
jQuery(document).ready(function() {

	jQuery( ".from" ).datepicker({
		defaultDate: "0",
		changeMonth: true,
		numberOfMonths: 2,
		minDate: 0,
		maxDate: '+2M',
		onClose: function( selectedDate ) {
			jQuery( ".to" ).datepicker( "option", "minDate", selectedDate );
		}
	});
	jQuery( ".to" ).datepicker({
		defaultDate: "0",
		changeMonth: true,
		numberOfMonths: 2,
		maxDate: '+2M',
		onClose: function( selectedDate ) {
			jQuery( ".from" ).datepicker( "option", "maxDate", selectedDate );
		}
	});

	//start out with stories checked
	jQuery(":radio[name$='search_for'][value='0']").attr('checked', 'checked');
	jQuery(".search-container").hide();
	jQuery("#the-finder1-0").show();

	jQuery(".go-box").change(function(){
		jQuery(".go-box").val(jQuery(this).val());
	});

	//show form fields based on radio button in search for area
	jQuery("input[name$='search_for']").change(function() {
		var fieldNumber = jQuery(this).val();
		jQuery(".search-container").hide();
		jQuery("#the-finder1-" + fieldNumber).show();
	});

	//show form fields based on radio button
	jQuery("input[name$='looking_for']").change(function() {
		var fieldNumber = jQuery(this).val();
		jQuery(".select-container").hide();
		jQuery("#more-fields-" + fieldNumber).show();
	});

	if(window.location.href.indexOf("food") > -1) {
		jQuery(":radio[name$='looking_for'][value='2']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-2").show();
	} else if (window.location.href.indexOf("music") > -1) {
		jQuery(":radio[name$='looking_for'][value='1']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-1").show();
	} else if (window.location.href.indexOf("movies") > -1) {
		jQuery(":radio[name$='looking_for'][value='6']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-6").show();
	} else if (window.location.href.indexOf("theater") > -1) {
		jQuery(":radio[name$='looking_for'][value='5']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-5").show();
	} else if (window.location.href.indexOf("art") > -1) {
		jQuery(":radio[name$='looking_for'][value='3']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-3").show();
	} else {
		val = jQuery(":radio[name$='looking_for']:checked").val();
		if (!val) {
			val = 0;
		}
		jQuery(":radio[name$='looking_for'][value='"+val+"']").attr('checked', 'checked');
		jQuery(".select-container").hide();
		jQuery("#more-fields-"+val).show();
	}

	//DATE OPTIONS
	// if = calendar show date picker
	jQuery('.select-field-date').bind('change', function () {
		jQuery('#date-picker-container').css("display", "none");
		//alert(jQuery(this).closest('form').find('.select-field-date').val()); //.attr('id'));
		//jQuery(this).parents('form:first #date-picker-container')
		//#date-picker-container
		if( jQuery(this).closest('form').find('.select-field-date').val() == 'range') {
		    jQuery(this).closest('form').find('#date-picker-container').css("display", "block");
		}
		else{
		    jQuery(this).closest('form').find('#date-picker-container').css("display", "none");
		}
	});
	// if radio element checked that doesn't need a date, hide datepicker
	jQuery("input[name$='looking_for']").click(function() {
	   if(jQuery('.hide-date-picker').is(':checked')) {
		jQuery('#date-picker-container').css("display", "none");
	   }
	   else if(jQuery('.hide-date-picker').not(':checked') && jQuery('.select-field-date').val() == 'calendar'){
		jQuery('#date-picker-container').css("display", "block");
	   }
	});

	//custom handling for movie and stage theater forms
   $("#finder_5, #finder_6").submit(function(e){
      e.preventDefault();
      var idVar = $(this).attr('id');
      var venueId = $("#"+idVar+" .theater-venue").val();
      var date1Var = $("#"+idVar+" input[name='date1']").val();
      var date2Var = $("#"+idVar+" input[name='date2']").val();
      var dateVar = "&date="+ $("#"+idVar+" .select-field-date").val();
      function convertDate(date){
        var dateConvert = Date.parse(date);
        return dateConvert.toString('yyyyMMdd');
      }
      switch(idVar) {
          case "finder_5":
              // stage theater
              if(date1Var !== "" && date2Var !== "" ){
                var dateVar = "&date=range&date1="+convertDate(date1Var)+"&date2="+convertDate(date2Var);
              }
              var urlBase = "http://thingstodo.buffalonews.com/";
              var url2 = "";
              if(venueId == ""){
                //http://thingstodo.buffalonews.com/venue-search.php?business-type=movie-theater
                url2 += 'venue-search.php?business-type=stage-theater'
              }else {
                //http://thingstodo.buffalonews.com/event-search.php?venue_RecordID=MAN-9705667&date=20140731
                //http://thingstodo.buffalonews.com/event-search.php?venue_RecordID=MAN-9705667&date=range&date1=20140724&date2=20140901
                url2 += 'event-search.php?'
                url2 += 'venue_RecordID='+venueId+dateVar;
              }
              urlBase += url2;
              break;
          case "finder_6":
              // movie theater
              var urlBase = "";
              var url2 = "";
              var movieName = $("#"+idVar+" .movie-title").val();
              console.log(movieName);
              if(venueId == ""){
                //http://buffalo.com/movies
                if(movieName !=0){
                	urlBase = "http://thingstodo.buffalonews.com/movie.php?id="+movieName;
                }
                else{
                	urlBase += 'http://buffalo.com/movies';
                }
              }else {
                //http://thingstodo.buffalonews.com/venue/cns-3524/filter?date=20140731
                var venueId = 'CNS-'+venueId;
                urlBase += 'http://thingstodo.buffalonews.com/';
                if(movieName ==0){
                	url2 += 'venue/'+venueId+'/filter?date='+dateVar+'#movie-times';
                }
                else{
                	url2 += 'venue/'+venueId+'/filter?date='+dateVar+'&movie='+movieName+'#movie-times';
                }
                urlBase += url2;
              }
              break;
          default:
      }
      var newLocation = urlBase;
      window.location = newLocation;
    });


});// end jQuery(document).ready(function()
</script> 
         </div> 
