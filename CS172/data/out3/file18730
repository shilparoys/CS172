              </div> 
              <div class="subchck"> 
               <button type="submit" class="btn submit">Log in</button> 
               <label class="t1-label remember"> <input type="checkbox" value="1" name="remember_me" checked> <span>Remember me</span> </label> 
              </div> 
              <input type="hidden" name="scribe_log"> 
              <input type="hidden" name="redirect_after_login" value="/hashtag/cybersecurity?src=hash&amp;mode=photos"> 
              <input type="hidden" value="8c96b90901b76443d126a7edac170336a28adf83" name="authenticity_token"> 
             </fieldset> 
