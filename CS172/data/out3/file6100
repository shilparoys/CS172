      <div class="align-center"> 
       <h1>2014 Alfred State Football Roster</h1> 
      </div> 
      <div class="roster"> 
       <table cellpadding="0" cellspacing="1" width="100%"> 
        <thead> 
         <tr class="roster-header"> 
          <td class="col1"> No. </td> 
          <td class="col2"> Name </td> 
          <td class="col3"> Pos. </td> 
          <td class="col4"> Cl. </td> 
          <td class="col5"> Ht. </td> 
          <td class="col6"> Wt. </td> 
          <td class="col7"> Hometown/High School </td> 
         </tr> 
        </thead> 
        <tbody> 
         <tr class="roster-row0"> 
          <td data-title="No." class="number"> 1 </td> 
          <td data-title="Name" class="name"> <a href="/sports/fball/2014-15/bios/moore_tavon_vgsj" title="Player's Bio">Tavon Moore</a> </td> 
          <td data-title="Pos."> WB </td> 
          <td data-title="Cl."> Sr. </td> 
          <td data-title="Ht."> 5-10 </td> 
          <td data-title="Wt."> 170 </td> 
          <td data-title="Hometown/High School"> Syracuse, NY / Bishop Grimes </td> 
         </tr> 
