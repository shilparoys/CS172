		function xBoxAlignment(){
			var imgX = document.getElementById('imgCancel');
			var xBox = document.getElementById('xBoxWrapper');
			imgX.style.display = 'inline';   
			imgX.style.left = ((screen.availWidth/2)+270)+'px';
			imgX.style.top  = ((screen.availHeight/2)-230)+'px';
			xBox.style.left = ((screen.availWidth-550)/2)+'px';
			xBox.style.top  = ((screen.availHeight-420)/2)+'px';
		};
	</script> 
