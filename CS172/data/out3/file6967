           <td style="padding-bottom:5px;" class="text_schoolportal"> 
            <table width="100%" border="0" align="right" cellpadding="0" cellspacing="0"> 
             <tbody>
              <tr> 
               <td class="menuBrief"> 
                <table width="95%" border="0" align="right" cellpadding="0" cellspacing="0" dir="rtl"> 
                 <form action="" name="votingFormMain" target="_blank" method="POST"></form> 
                 <tbody>
                  <tr> 
                   <td> 
                    <table width="95%" align="right" border="0" cellspacing="0" cellpadding="0"> 
                     <tbody>
                      <tr> 
                       <td height="28" align="right" class="votetext"> <input name="answer" type="radio" value="a1"> ضعيف </td> 
                      </tr> 
                      <tr> 
                       <td height="28" align="right" class="votetext"> <input name="answer" type="radio" value="a2"> جيد </td> 
