                    </table> </td> 
                  </tr> 
                  <tr> 
                   <td align="center"><a href="javascript:void(vote())" class="votebtn" style="padding-bottom:0px; cursor:pointer;">شارك</a> </td> 
                  </tr>  
                 </tbody>
                </table> <script type="text/javascript" src="Scripts/ajax.js"></script> <script type="text/javascript">
          function vote()
          {
            var ok=0;
            var ans;
            for(var i=0; i<document.votingFormMain.answer.length; i++){
              if(document.votingFormMain.answer[i].checked) 
              {
                ok=1;
                ans="a"+(i+1);
              }
            }
        
            if(ok==0){
              alert('يجب اختيار إجابة أولاً'); 
              return false; 
            }else{
              document.getElementById('tblVoting').style.display='none';
              document.getElementById('waitVoting').style.display='';
              document.getElementById('waitVoting').innerHTML="<br /><br /><br />...من فضلك انتظر";
              ajaxFunction();
              var http = false;
            
              if(navigator.appName == "Microsoft Internet Explorer") {
                http = new ActiveXObject("Microsoft.XMLHTTP");
              } else {
                http = new XMLHttpRequest();
              }
        
              http.open("GET", "vote.php?govote=1&vid=4&answer="+ans);
              http.onreadystatechange=function() {
                if(http.readyState == 4) {
                 document.getElementById('waitVoting').innerHTML = http.responseText;
                }
              }
              http.send(null);
              
            }
          }
        </script> </td> 
              </tr> 
              <tr background="images/boxBG.jpg"> 
               <td align="center"></td> 
              </tr> 
             </tbody>
