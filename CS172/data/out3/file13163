	$(document).ready( function() {
		loadPromo($("#HomepageBanner"), "http://www.cengagebrain.com/shop/en/US/storefront/US?cmd=CLPromoAdWidget&loc=HomepageBanner&source=Internal");
	});

	function loadPromo(container, promoURL) {
		$(container).hide();
		
		$.ajax({ 
			url: 		promoURL,
			dataType: 	'html',
			cache: 		false,
			success:  
				function(data) {
					if (data.trim().length > 0 && $(data).find("html").length == 0) {
						$(container).html(data);
						initModals($(container).find(".yui-panel"));
						$(container).show();
					} else {
						$(container).remove();
					}
				}
		});
	}
	
</script> 
