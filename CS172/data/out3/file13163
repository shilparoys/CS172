   <script type="text/javascript">

	function submitHeaderLogin() {
		var emailInput = $("#loginEmailAddress");
		var passwordInput = $("#loginPassword");
		var emailPlaceholder = emailInput.attr("placeholder");
		var passwordPlaceholder = passwordInput.attr("placeholder");
		var email = emailInput.val();
		var password = passwordInput.val();
		var rememberMe = $("#toggle");
		var isValid = true;
		var pathName = window.location.pathname;
		var pathVars = pathName.split("/");
		document.popUploginForm.ISBN.value = pathVars[pathVars.length - 1];
		document.popUploginForm.origin.value = window.location.href;

		if (email) {
			email = jQuery.trim(email);
		}

		if (email != emailPlaceholder && isValidEmail(email)) {
			document.popUploginForm.login.value = email;
		} else {
			email === "" || email === emailPlaceholder ?
					emailInput.showMsg() : emailInput.showMsg("emailInvalid");
			isValid = false;
		}

		if (password != null && jQuery.trim(password).length > 0 && password != passwordPlaceholder) {
			document.popUploginForm.passwd.value = password;
		} else {
			passwordInput.showMsg();
			isValid = false;
		}

		if (isValid) {
			if (rememberMe && rememberMe.is(":checked")) {
				document.popUploginForm.rememberMe.value = rememberMe.val();
			}

			
				
				
					processSSOLogin();
				
				
			
		}
	}

	$(document).ready(function() {
		if (false) {
			$('#invalidLogin').modal();
		}

		if ("email address".toLowerCase() != "email address".toLowerCase()) {
			$("#loginEmailAddress").val('email address');
		}
	});

</script> 
