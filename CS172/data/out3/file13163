	if (matchMedia) {
		var mq = window.matchMedia("(min-width: 410px)");
		mq.addListener(WidthChange);
		WidthChange(mq);
	}

	// media query change
	function WidthChange(mq) {
		if (mq.matches) {
			// window width is at least 410px so inject large book scroller
			$('.items').html('<!-- These are the large book scoller items -->\
					<div class="largeScroll">\
						<a href="/shop/careerresources.html"><img src="http://images.cengagebrain.com/images/'+bookImgOne+'" alt="'+bookAltOne+'"/></a>\
