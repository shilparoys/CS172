  <script type="text/javascript">
	var pageName = "Home";
	var omniturePageName = $("#omniturePageName").val();
	
	/* You may give each page an identifying name, server, and channel on the next lines. */
	if (omniturePageName) {
		s.pageName = omniturePageName;
	} else if (pageName) {
		s.pageName = pageName; //page Name of respective pages
	} else {
		s.pageName = document.location;	
	}
	
	var prop15 = "";
	
	if (document.getElementById('OmnitureProp15Variable') != null) {
		prop15 = document.getElementById('OmnitureProp15Variable').value;
	}
	
	s.campaign = "";
	s.channel = "Home"; // Function grouping; based on document grouping map
 	s.pageType = ""; // For error pages ONLY
 	s.prop1 = "";
	s.prop2 = "Anonymous"; // Login state; valid values are Anonymous or Registered
	s.prop3 = "";// Non PII Identifier as GUID
	s.prop4 = ""; // sort by|results per page
	s.prop5 = ""; // Product;values isbn|title
	s.prop6 = s.prop2 + ":" + s.pageName; // User Pathing By ; values are "Registered" or "Anonymous": Page Name
	s.prop7 = ""; // Search string; browse/search string value
	s.prop8 = ""; // search Type: browse, search, multi-isbn, filter, page
	s.prop9 = ""; // search result count
	s.prop10 = ""; // Copyright Year
	s.prop11 = ""; // internet index discipline
	s.prop12 = ""; // Search Results Filter 
	s.prop14 = document.URL;
	s.prop15 = prop15;
	s.prop18 = "";
	s.prop20 = ""; // Digital Product Type
	s.prop23 = "";
	s.state = ""; // US State
	s.zip = ""; // US Zip Code
	s.events = ""; // Success Events; should be an event from OmnitureTags.properties file
	s.trackingCode = "";
	s.campaigns = "";
	s.products = ""; // product info; only if data is available for page; if more than 1 product, use comma as separator
	s.purchaseID = ""; // unique order ID, only on order complete page
	s.eVar1 = "";
	s.eVar2 = "Anonymous"; // most recent value of either registered or anonymous
	s.eVar3 = ""; // shipping cost, only on order complete page
	s.eVar4 = ""; // Coupon Code
	s.eVar5 = ""; // Products in Cart;values isbn|title
	s.eVar6 = ""; // internet index discipline
	s.eVar7 = ""; // school name; only if data is available
	s.eVar8 = ""; // Copyright Year
	s.eVar9 = ""; // school Type
	s.eVar10 = ""; // Search Term
	s.eVar11 = s.state; //State
	s.eVar12 = "";// Non PII Identifier as GUID
	s.eVar15 = prop15;
	s.eVar21 = ""; // Order Discount Total
	s.eVar23 = ""; // PayPal, Mastercard, VISA, Discover, American Express
	
	if (s.pageName == "Product Details Page") {
		s.hier1 = ""; //hierarchy using breadcrumb, only for browse, search, filter, product detail view actions
	} else {
		s.hier1 = "";
	}
	
	s.hier2 = "guest"; //member; valid values are guest or member,purchase or member,no purchase
	
	$(document).ready(function(){
		$('#ck_email2').click(function() {
			s.events = "event5";
		});

		$('#ck_facebook2').click(function() {
			s.events = "event5";
		});

		$('#ck_twitter2').click(function() {
			s.events = "event5";
		});

		$('#ck_sharethis2').click(function() {
			s.events = "event5";
		});
	});
	
