	//hide or show the registration input
	var showRegistration = function() {
		$("#register").animate({right: "105%"}, 600, function() {
			$(this).css("visibility","hidden");
		});
		$("#regCode").focus();
	};

	$( ".regIt" ).on( "click", showRegistration);


	$( "#cancelReg" ).on( "click", function() {
		$("#register").css("visibility","visible");
		$("#register").animate({right: "0%"}, 600);
	});



	/* Scrollable Items - This bit of the script looks for a 410px breakpoint and determines which set of scrollable items to display in the bottom book scroller. There are variables set up for the images and alt text associated with each item so you only have to change them once. */

	// Change the book images here. They all link to the same page outlined in the html below
	var bookImgOne = "77348630814441337503294319181704545311.jpg";
