     <div id="ad-desktop"> 
      <script type="text/javascript">
            $(document).ready(function () {

                var loadImmediately = false;
                var isMainstage = false;

                    loadImmediately = true;


                if (DFP.shouldRenderForDevice('desktop')) {
                    DFP.visibilityCheck(function() {
                        DFP.renderForLocation('desktop', loadImmediately, isMainstage);
                    });
                }

            });
        </script> 
