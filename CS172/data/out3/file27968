}
.journeys-module li,
.journeys-module a {
  text-decoration: none;
  display: block;
}
/* Allow bullets for li's
.journeys-module li {
  background: none;
  margin: 0 0 3px;
  padding: 0;
}
*/
.journeys-module a:hover {
  text-decoration: underline;
}
