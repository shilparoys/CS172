  color: #777;
  margin: 5px 0;
  font-weight: 500;
  font-size: 11px;
  line-height: 14px;
  font-family: nyt-franklin, 'helvetica neue', arial, sans-serif;
}
.journeys-module a.item-description:hover {
  color: #777;
}
.journeys-module .item-bold,
.journeys-module .journeys-link {
  color: #000;
}
