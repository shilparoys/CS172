  <div id="stageSnow"></div> 
  <script type="text/javascript">
			
			var count = 100;
			var frameDuration = 20;
			var fallingSpeed = 1.3;
			
			var diameter = 50.0;
			var bodyWidth = 1600;
			var bodyHeight = 1000;
			var sideColumnWidth = 300;
			var fadeoutArea = 100;
			
			var addSnowflake = function() {
				var top = Math.random() * bodyHeight;
				var left = Math.random() * sideColumnWidth * 2.0;
				if (left > sideColumnWidth)
					left = bodyWidth - left + sideColumnWidth;
				var random = 0.0;
				var rotation = 0;
				var rotationSpeed = 0.0;
				var alpha = 1.0;
				var sf = $('<div class="snowflake" />')
					.addClass('snowflakeGray-' + Math.floor(Math.random() * 4))
					.appendTo($('#stageSnow'))
				window.setInterval(function() {
					if (top >= bodyHeight) {
						left = Math.random() * sideColumnWidth * 2.0;
						if (left > sideColumnWidth)
							left = bodyWidth - left + sideColumnWidth;
						top = -diameter;
						alpha = 1.0;
					}
					if (top > bodyHeight - fadeoutArea)
						alpha = (bodyHeight - top) / fadeoutArea;
					var rotate = 'rotate(' + rotation + 'deg)';
					sf.css({
						'top': top,
						'left': left,
						'transform': rotate,
						'-o-transform': rotate,
						'-ms-transform': rotate,
						'-moz-transform': rotate,
						'-webkit-transform': rotate,
						'opacity': alpha,
						'filter': 'alpha(opacity=' + (alpha * 100.0) + ")"
					})
					top = top + fallingSpeed;
					left = left + random;
					rotation = rotation + rotationSpeed;
				}, frameDuration);
				window.setInterval(function() {
					random = random + (Math.random() - 0.5 - 0.2 * random);
					rotationSpeed = rotationSpeed + (Math.random() - 0.5 - 0.2 * rotationSpeed);
				}, (Math.random() + 0.5) * 1000)
			};
			
			$(document).ready(function() {
				for (var i = 0; i < count; i++)
					addSnowflake();
			});
			
		</script> 
