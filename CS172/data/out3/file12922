                     <td colspan="3" valign="top"> 
                      <div align="right">
                       <font class="weather2"><img src="/images/spacer.gif" alt="" width="1" height="19"></font>
                      </div> 
                      <div align="right"> 
                       <script language="javascript1.2" type="text/javascript">
 <!-- Begin
 var days=new Array(8);
 days[1]="Sunday";
 days[2]="Monday";
 days[3]="Tuesday";
 days[4]="Wednesday";
 days[5]="Thursday";
 days[6]="Friday";
 days[7]="Saturday";
 var months=new Array(13);
 months[1]="January";
 months[2]="February";
 months[3]="March";
 months[4]="April";
 months[5]="May";
 months[6]="June";
 months[7]="July";
 months[8]="August";
 months[9]="September";
 months[10]="October";
 months[11]="November";
 months[12]="December";
 var time=new Date();
 var lmonth=months[time.getMonth() + 1];
 var date=time.getDate();
 var lday=days[time.getDay() + 1];
 document.write("&nbsp;<strong><span class=\"weather2\">" + lday + ", " + lmonth + " " + date + "</span></strong>");
