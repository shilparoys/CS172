        // have to add this codes in because the html tags for the div are not applicable in the customTabsRandy.js
        $(document).ready(function () {
            $('#tabsOpenContent > div:not(:first)').hide();

            try {
                var target = document.location.toString().split('#')[1];
                var path = "#" + target;

                // highlight tab
                var targetTab = $('div#tabBox ul li a[href$=' + path + ']').parent('li');
                $(targetTab).addClass('selected');
                $(targetTab).siblings().removeClass("selected");

                // show content
                $(path).show();
            }
            catch (e) { }
        });
    </script> 
