         <!-- Changelinks Scripts --> 
         <script type="text/javascript">// <![CDATA[
 function unhide(divID) {
 var item = document.getElementById(divID);
 if (item) {
 item.className=(item.className=='hidden')?'unhidden':'hidden';
 } }// ]]>
 </script> 
         <script type="text/javascript">// <![CDATA[
jQuery("select").change(function () {
changelinks();
}).change();
function changelinks(){
var countyvals = jQuery("#county").val(),
      hideking = "King";
if (countyvals != hideking){
    jQuery('.King').show();
    jQuery('.callKing').hide();
jQuery("a[href^='http://www.resourcehouse.com/win211/']").not("a[href^='http://www.resourcehouse.com/win211/keyword_search.aspx']")
.each(function(){
this.href = "http://www.resourcehouse.com/win211/" + countyvals + jQuery(this).attr('data-relativeurl');
})}
else {
    jQuery('.King').hide();
    jQuery('.callKing').show();
jQuery("a[href^='http://www.resourcehouse.com/win211/']").not("a[href^='http://www.resourcehouse.com/win211/keyword_search.aspx']")
.each(function(){
this.href = "http://www.resourcehouse.com/win211/" + countyvals + jQuery(this).attr('data-relativeurl');
})}
}
// ]]></script> 
         <script type="text/javascript">// <![CDATA[
jQuery(document).ready(function () {
    jQuery('.King').show();
    jQuery('.callKing').hide();
});
// ]]></script>
