    function run_sikko_latte(){
        var delay_start = 2,
            delay_end   = 8; // seconds
        $.fancybox.init();
        $("#sikkobox").fancybox({
            // 'showCloseButton': false,
            'transitionIn': 'fade',
            'transitionOut': 'fade',
            'overlayColor' : '#000',
            'overlayOpacity' : '0.8',
            'onComplete': function(){
                window.setTimeout(function(){
                    parent.$.fancybox.close();
                }, delay_end*1000);
            },
            'onStart': function(){
            }
        });
        window.setTimeout(function(){
            $("#sikkobox").trigger('click');
        }, delay_start*1000);
    }

