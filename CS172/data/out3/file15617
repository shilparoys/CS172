var preLoad = new Array();
for (iss = 1; iss < pss+1; iss++){
preLoad[iss] = new Image();
preLoad[iss].src = Picture[iss];}

function runSlideShow(){
if (document.all){
document.images._ctl14_PictureBox.style.filter="blendTrans(duration=2)";
document.images._ctl14_PictureBox.style.filter="blendTrans(duration=CrossFadeDuration)";
document.images._ctl14_PictureBox.filters.blendTrans.Apply();}
document.images._ctl14_PictureBox.src = preLoad[jss].src;
document.images._ctl14_PictureBox.title = Caption508[jss];
if (document.getElementById) document.getElementById("_ctl14_CaptionBox").innerHTML= Caption[jss];
if (document.all) document.images._ctl14_PictureBox.filters.blendTrans.Play();
jss = jss + 1;
if (jss > (pss)) jss=1;
}

function manualSwap(nIndex)
{
	document.images._ctl14_PictureBox.src = preLoad[nIndex + 1].src;
	document.getElementById("_ctl14_CaptionBox").innerHTML= Caption[nIndex + 1];
	document.images._ctl14_PictureBox.title = Caption508[nIndex + 1];
	bSlideShow = false;
}

function TimerEvent()
{
	nCounter = nCounter + 21;
	
	if (bScrollLeft)
	{
		document.getElementById('divContainer').scrollLeft -= 2;
	}
	else if (bScrollRight)
	{
		document.getElementById('divContainer').scrollLeft += 2;
	}
	
	if ((nCounter % SlideShowSpeed == 0) && (bSlideShow))
		runSlideShow();
}
</script>  
