  <script type="text/javascript">
	     function SubmitSearch(field, e) {
	         var keycode;
	         if (window.event) {
	             keycode = window.event.keyCode;
	         }
	         else if (e) {
	             keycode = e.which;
	         }
	         else {
	             return true;
	         }

	         if (keycode == 13) {
	             document.getElementById(document.getElementById('searchBtnID').value).click();
	             return false;
	         }
	         else {
	             return true;
	         }
	     }
     </script> 
