  <script type="text/javascript">
	
	var currentMenuItem = null;
	jQuery(document).ready(function(){
		var li = jQuery('#nav').children();
		//jQuery('#nav').children('a').css('color','red');
		
		li.find('a').click(function(){
			//li.find('a').removeClass('menuItemOver');
			//currentMenuItem = jQuery(this).parent();
			//setCurrentMenu();
		});
		
		if(getParameterByName('mi')){
			currentMenuItem = jQuery('#nav').children(':eq(' + getParameterByName('mi') + ')');
		}
		
		if(currentMenuItem != null) setCurrentMenu();
	});
	
	function getParameterByName(name)
	{
		name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
		var regexS = "[\\?&]" + name + "=([^&#]*)";
		var regex = new RegExp(regexS);
		var results = regex.exec(window.location.search);
		
		if(results == null)
			return "";
		else
			return decodeURIComponent(results[1].replace(/\+/g, " "));
	}

	function setCurrentMenu(){
		if(currentMenuItem != null){
			//currentMenuItem.css('font-size','50px');
			currentMenuItem.removeClass('menuItemOver');
			currentMenuItem.find('a:eq(0)').addClass('menuItemOver');
		}
	}
	
