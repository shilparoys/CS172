  <script type="text/javascript">
        function SubmitLogin(field, e) {
            var keycode;
            if (window.event){
                keycode = window.event.keyCode;
                }
            else if (e){
                keycode = e.which;
                }
            else{
                return true;
                }

            if (keycode == 13) {
                document.getElementById('ctl00_MainContent_homePageLogin_loginbutton').click();
                return false;
                }
            else{
                return true;
                }
            }
			
