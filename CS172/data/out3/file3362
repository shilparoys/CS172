    <script type="text/javascript">

$(document).ready(function() {
	var strUpper = "(dvd|cd|usa)";
	var full = "([\\s]+"+strUpper+"|"+strUpper+"[\\s]+)";
	var re = new RegExp(full,"ig");
	$(".catalog-feature,#sub-nav ul li a,.breadcrumbs li,.breadcrumbs li a,.product a,h1 span,#product-info,.product-name,#search-nav ul li,.book-title,.modal-product-name,.item-name").each(function() {
	this.innerHTML = this.innerHTML.replace(re, function(s){return s.toUpperCase();});
	});
});
	

