		<script>
			var $countdowns, tmrCountdowns, DisplayStr = null;
			var DisplayFormat = "<strong>%%D%% days %%H%% hrs, %%M%% mins, %%S%% seconds</strong>";
			
			function calcage(secs, num1, num2) {
				s = ((Math.floor(secs/num1))%num2).toString();
				return s;
			}
			
			function UpdateCountdowns() {
				$countdowns.each(function(){
					var $this = $jq(this);
					var strEvent = $this.attr('data-event');
					var tmEvt = new Date(strEvent);
					var tmNow = new Date();
					
					var tmRemain = new Date(tmEvt-tmNow);
					
					  DisplayStr = DisplayFormat.replace(/%%D%%/g, calcage(Math.floor(tmRemain.valueOf()/1000),86400,100000));
					  DisplayStr = DisplayStr.replace(/%%H%%/g, calcage(Math.floor(tmRemain.valueOf()/1000),3600,24));
					  DisplayStr = DisplayStr.replace(/%%M%%/g, calcage(Math.floor(tmRemain.valueOf()/1000),60,60));
					  DisplayStr = DisplayStr.replace(/%%S%%/g, calcage(Math.floor(tmRemain.valueOf()/1000),1,60));
					
					$this.html(DisplayStr);
					
					tmNow = null;
					tmEvt = null;
					strEvent = null;
					$this = null;
				});
			}
			
			$jq(document).ready(function(){
				$countdowns = $jq('.countdown');
				tmrCountdowns = setInterval(UpdateCountdowns, 1000);
			});
		</script>
		
	</div> --> 
