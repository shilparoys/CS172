           &nbsp;
          </div>
          <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.7.0/underscore-min.js"></script>
          <script type="text/javascript">

// 1/2015 NYTD.jQuery.ready() not firing, using DOMContentLoaded instead
// NYTD.jQuery(document).ready(function() {

document.addEventListener("DOMContentLoaded", function(event) { 

NYTD.jQuery.ajax({
    url: "http://int.nyt.com/applications/catwalk/data/latest.jsonp",
    type: "GET",
    contentType: "application/json",
    cache: true,
    dataType: 'jsonp',
    jsonpCallback: 'catwalkData',

  success: function(data) {
    var i,
        classes = 'show',
        html_string;

    for (i = 0; i < 3; i += 1) {
      if (i === 2) {
        classes += ' last';
      }
      html_string = "<div class='" + classes + "'><a href='" + data[i].url + "/1?src=sfp'><img src='" + data[i].poster_img + "'/><div class='caption'><p>" + data[i].designer + "</p></div></a></div>";
      NYTD.jQuery('#nytint-acol-catwalk-promo').append(html_string);
    }
  }
});
});
</script> 
