  <script type="text/javascript" src="http://www.google.com/jsapi?key=ABQIAAAAt3bHGRhD1-9IAAm0bnrA6BRBY5u8Hu8Z_f_5KHZFhT6l8it_bxSuy8tdhtl2_YtkCUdJatJEgOWrCw"></script> 
  <script type="text/javascript"> 
	Array.prototype.unique = function () {
		var r = new Array();
		o:for(var i = 0, n = this.length; i < n; i++)
		{
			for(var x = 0, y = r.length; x < y; x++)
			{
				if(r[x]==this[i])
				{
					continue o;
				}
			}
			r[r.length] = this[i];
		}
		return r;
	}
	//Code courtesy of http://xhtml.ru/2007/05/03/ajax-feed-api-and-multiple-feeds/
		google.load("feeds", "1");
		var FA = new Array(
"http://arcadiapubliclibrary.blogspot.com/feeds/posts/default", "http://blog.theautry.org/category/libraries-and-archives//feed/", "http://boyleheightshistoryblog.blogspot.com/feeds/posts/default", "http://blog.theautry.org/category/libraries-and-archives//feed/", "http://junginla.blogspot.com/feeds/posts/default", "http://blogs.libraries.claremont.edu/sc/atom.xml", "http://www.curatingla.com/blog-index/rss.xml", "http://onbunkerhill.org/rss.xml", "http://huntingtonblogs.org/", "http://www.sportsletter.org/sportsletter/rss.xml", "http://metroprimaryresources.info/?feed=rss", "http://lmulibrary.typepad.com/lmu-library-news/atom.xml", "http://library.pepperdine.edu/news/index.php/feed/", "http://scgsgenealogy.blogspot.com/feeds/posts/default", "http://sites.uci.edu/ucisca/", "http://clarklibrary.wordpress.com/feed/", "http://blogs.library.ucla.edu/special/feed/", "http://feeds.feedburner.com/libwire-main", "http://feeds.feedburner.com/libwire-main", "http://fromthevcvault.wordpress.com/feed/atom/");
		FA = FA.unique();

	   function initialize() {
			var feedsArr = new Array();
			var numEntr = 3;
			var cnt = FA.length;
		
		for (var k=0; k<FA.length; k++) {
			var feed = new google.feeds.Feed(FA[k]);
			feed.setNumEntries(numEntr);
			feed.setResultFormat(google.feeds.Feed.JSON_FORMAT);
			feed.load(function(result) {
				if (!result.error) {
					for (var i = 0; i < result.feed.entries.length; i++) {
						var entry = result.feed.entries[i];
						var attributes = ["title", "link", "publishedDate", "contentSnippet"];
						var ind = feedsArr.length;
						feedsArr[ind] = new Array();
						feedsArr[ind][0] = Date.parse(entry[attributes[2]]);// for sort entries
						feedsArr[ind][1] = entry[attributes[1]];
						feedsArr[ind][2] = entry[attributes[2]];
						feedsArr[ind][3] = entry[attributes[3]];
						feedsArr[ind][4] = entry[attributes[0]];
						feedsArr[ind][5] = result.feed.title; // site title 
					}
				}
				cnt--;
				if (cnt == 0) {
					feedsArr.sort();
					feedsArr.reverse();
					for (var j = 0; j < feedsArr.length;  j++) {
						$("#blogFeed").append('<li><div><a href="' + feedsArr[j][1] + '">' + feedsArr[j][4] + '</a><br /><span>(source: ' + feedsArr[j][5] + '</a>)</span></div></li>');
					}
				}
			});
		}
		$('#blogFeedContainer').vTicker({
				speed: 800,
				pause: 3000,
				showItems: 5,
				animation: 'fade',
				height: 250,
				direction: 'up'
			 });
	   }
	   google.setOnLoadCallback(initialize);
	</script> 
