        jQuery(document).ready(function ($) {
            // Instantiate the EasyTab 
            $('#tab-container,#tab-socialnetwork-containter').easytabs({
                animate: true,
                animationSpeed: 100
            });
            $('.bxslider').bxSlider({
                mode: 'fade',
                auto: true,
                autocontrols: true,
                pause: 5000,
                slideMargin: 20,
                captions: true
            });
            // Instantiate the ColorBox
            $(".youtube").colorbox({
                iframe: true,
                innerWidth: 640,
                innerHeight: 390
            })

            // Instantiate the CalendarEvents
            $("#eventsDisplay").CalendarEvents({
                user: "ucradmissionssurvey@gmail.com"
                , displayType: "perMonth"
                , apiKey: "AIzaSyC56Z7poGiEDe0507fDXlZOJmMmhtspllg"
                , addLinkType: "link"
                , height: "150"
                , searchCondition: "in"
                , searchCriteria: "International" // View All
                , maxResults: 0
            });

            // Set the selected calendar font-weight
            setSelectedCalendar("View All");


            $("#calSelector-ViewAll").click(function (e) {
                var splitMonthYear = $('.ed-monthName').text().split(" ");
                var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);
                $("#eventsDisplay").html("");
                var searchText = "International";
                $("#eventsDisplay").destroyPlugin;
                $("#eventsDisplay").CalendarEvents({
                    user: "ucradmissionssurvey@gmail.com"
                    , displayType: "perMonth"
                    , apiKey: "AIzaSyC56Z7poGiEDe0507fDXlZOJmMmhtspllg"
                    , addLinkType: "link"
                    , height: "150"
                    , timeMin: currentDate
                    , timeMax: currentDate
                    , searchCriteria: searchText
                    , searchCondition: "in"
                    , maxResults: 0
                    , setContainer: true
                });
                // Don't follow the link to the anchor
                e.preventDefault ? e.preventDefault() : e.returnValue = false;
                // Set as selected
                setSelectedCalendar("View All")
            });

            $("#calSelector-Deadlines").click(function (e) {
                var splitMonthYear = $('.ed-monthName').text().split(" ");
                var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);
                $("#eventsDisplay").html("");
                var searchText = "International Deadline";
                $("#eventsDisplay").destroyPlugin;
                $("#eventsDisplay").CalendarEvents({
                    user: "ucradmissionssurvey@gmail.com"
                    , displayType: "perMonth"
                    , apiKey: "AIzaSyC56Z7poGiEDe0507fDXlZOJmMmhtspllg"
                    , addLinkType: "link"
                    , height: "150"
                    , timeMin: currentDate
                    , timeMax: currentDate
                    , searchCriteria: searchText
                    , searchCondition: "in"
                    , maxResults: 0
                    , setContainer: true
                });
                // Don't follow the link to the anchor
                e.preventDefault ? e.preventDefault() : e.returnValue = false;

                // Set as selected
                setSelectedCalendar("Deadlines")
            });

            $("#calSelector-Counselor").click(function (e) {
                var splitMonthYear = $('.ed-monthName').text().split(" ");
                var currentDate = new Date(splitMonthYear[0] + " 01," + splitMonthYear[1]);
                //alert("monthNameDate value:" + currentDate);
                $("#eventsDisplay").html("");
                var searchText = "International Counselor";
                $("#eventsDisplay").destroyPlugin;
                $("#eventsDisplay").CalendarEvents({
                    user: "ucradmissionssurvey@gmail.com"
                    , displayType: "perMonth"
                    , apiKey: "AIzaSyC56Z7poGiEDe0507fDXlZOJmMmhtspllg"
                    , addLinkType: "link"
                    , height: "150"
                    , timeMin: currentDate
                    , timeMax: currentDate
                    , searchCriteria: searchText
                    , searchCondition: "in"
                    , maxResults: 0
                    , setContainer: true
                });
                // Don't follow the link to the anchor
                e.preventDefault ? e.preventDefault() : e.returnValue = false;
                // Set as selected
                setSelectedCalendar("Counselor")
            });
        });

        setSelectedCalendar = function (cal) {
            switch (cal) {
                case "View All":
                    $("#calSelector-ViewAll").css("font-weight", "bold");
                    $("#calSelector-Counselor").css("font-weight", "normal");
                    $("#calSelector-Deadlines").css("font-weight", "normal");
                    break;
                case "Counselor":
                    $("#calSelector-Counselor").css("font-weight", "bold");
                    $("#calSelector-ViewAll").css("font-weight", "normal");
                    $("#calSelector-Deadlines").css("font-weight", "normal");
                    break;
                case "Deadlines":
                    $("#calSelector-Deadlines").css("font-weight", "bold");
                    $("#calSelector-ViewAll").css("font-weight", "normal");
                    $("#calSelector-Counselor").css("font-weight", "normal");
                    break;
            }
        }


    </script> 
