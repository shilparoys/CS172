var stopvan;
function startSlideShow(){
  stopvan=setTimeout("pic1()",5000)
}
function pic1() {
  document.images.slideCarpool.src="image/FrontPagePhotos/vanpool.jpg";
  stopvan=setTimeout("pic2()",5000);
}

var stopcar;
function pic2() {
  document.images.slideCarpool.src="image/FrontPagePhotos/carpool.jpg";
  stopcar=setTimeout("pic1()",5000);
}

function init() {        	
   MM_preloadImages('image/FrontPagePhotos/vanpool.jpg','image/FrontPagePhotos/transit.jpg','image/FrontPagePhotos/bike_to_work.jpg','image/FrontPagePhotos/work_from_home.jpg','image/FrontPagePhotos/schoolpool.jpg','image/FrontPagePhotos/carpool.jpg','image/FrontPagePhotos/ferry.jpg');
   startSlideShow();
}

function StopSlideShow() {
   clearTimeout(stopcar);
   clearTimeout(stopvan);
}
