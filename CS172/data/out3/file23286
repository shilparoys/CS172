      <script language="javascript">

 function validate(form){


if (isNaN(form.cellphone.value)){
alert("Invalid cell phone number/format enter number without any spaces or characters 55555555555")
form.cellphone.focus()
return false
}

if (form.cellphone.value.length > 0 && form.cellphone.value.length < 10){
alert("Invalid cell phone number/format enter number without any spaces or characters 55555555555")
form.cellphone.focus()
return false
}
if (form.lname.value == "" || form.lname.value.charAt(0) == " "){
alert("Last Name Cannot be blank")
form.lname.focus()
return false
}

if (form.courseid.selectedIndex == 0){
alert("Please select course")
form.lname.focus()
return false
}

if (form.ssn3.value.length != 4){
alert("You must provide the last 4 digits of your social security number")
form.lname.focus()
return false
}
if (form.address1.value.length == 0){
alert("You must provide your address")
form.lname.focus()
return false
}
if (form.city.value.length == 0){
alert("You must provide your city")
form.lname.focus()
return false
}
if (form.state.value.length == 0){
alert("You must provide your state")
form.lname.focus()
return false
}
if (form.zipcode.value.length == 0){
alert("You must provide your zipcode")
form.lname.focus()
return false
}

 }

 </script>   
