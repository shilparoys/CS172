  <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script> 
  <script type="text/javascript" src="//ajax.microsoft.com/ajax/jquery.validate/1.11.1/jquery.validate.min.js"></script> 
  <script type="text/javascript" src="//ajax.microsoft.com/ajax/jquery.validate/1.11.1/additional-methods.min.js"></script> 
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.8.18/jquery-ui.min.js"></script> 
  <script src="/static/gen/core.js"></script> 
  <script src="/static/js/jquery.tipsy.js"></script> 
  <script type="text/javascript">
$(document).ready(function(){
    if(window.location.href.indexOf('iama=P') > -1) {
      $('.student').addClass('hidden');
      $('.parent').removeClass('hidden');
      $("#parent-warning").show();
      $('<div></div>')
          .html("Dear Parent, please register on behalf of your child (i.e., provide your child's name, e-mail address, and birthday, not your own)")
          .dialog({
            title:  "<img class='float-l pr-10' src='/static/img/icons/error.png'  /> Notice",
            dialogClass: "parent-warning-dialog",
            buttons: {
              OK: function() {
                $( this ).dialog( "close" );
              }
            }
          });
    }
    else if(window.location.href.indexOf('iama=S:1') > -1) {
      $('.student').addClass('hidden');
      $('.c-student').removeClass('hidden');
    }
    else if(window.location.href.indexOf('iama=S:9') > -1) {
      $('.student').addClass('hidden');
      $('.graduate').removeClass('hidden');
    }

    //tooltip
    $('.tooltip').tipsy({fade: true, gravity: 'e', opacity:.9});

    $.validator.addMethod(
        "dateMDY",
        function(value, element) {
            return value.match(/^\d{1,2}\/\d{1,2}\/\d{4}$/);
        },
       "Please enter a date in the format dd/mm/yyyy"
    );
    $("#id_signup_form").validate({
      rules: {
       first_name: 'required ascii'
       ,last_name: 'required ascii'
       ,email: 'required email ascii'
       ,email2: {equalTo: '#id_email'}
       ,password: {required:true, minlength:8, maxlength:40, ascii:true}
       ,dob: {required:true, dateMDY:true}
       ,iama: 'required'
       ,country_id: 'required'
      }
      ,messages: {
        dob: {
         dateMDY:
           function(param,element){
             msg = ["Nope", "Keep Trying", "Nadda",
                    "Sorry", "No one\'s home", "Arg", 
                    "Bummer", "Faux pas", "Whoops", "Snafu", 
                    "Blunder"];
            return msg[Math.round(msg.length * Math.random())] + " : mm/dd/yyyy";
          }
        }
      }
      ,submitHandler:
        function(form) {
          $(':submit', $(form)).click(function() {

            return false;
          });
          form.submit();
        }
    });
    
    $("#id_iama").change(function(){

      // show parent warning even after closing pop up
      $("#parent-warning").hide();

      if ('.icon:not(.hidden)') {
        $('.icon').addClass('hidden navOutNext');
        $('.icon').removeClass('navInNext');
      }

      var option_value = $(this).val();

      if (option_value=='S:40') {
        $('.student').removeClass('hidden');
        $('.student').addClass('navInNext');
      }
      else if (option_value=='S:1') {
        $('.c-student').removeClass('hidden');
        $('.c-student').addClass('navInNext');
      }
      else if (option_value=='S:9') {
        $('.graduate').removeClass('hidden');
        $('.graduate').addClass('navInNext');
      }
      else if (option_value=='P:40') {
        $('.parent').removeClass('hidden');
        $('.parent').addClass('navInNext');
      }
      else if (option_value=='C') {
        $('.counselor').removeClass('hidden');
        $('.counselor').addClass('navInNext');
      }

      var role = $(this).val().charAt(0);

      if (role=="P"){

        $('<div></div>')
          .html("Dear Parent, please register on behalf of your child (i.e., provide your child's name, e-mail address, and birthday, not your own)")
          .dialog({
            title:  "<img class='float-l pr-10' src='/static/img/icons/error.png'  /> Notice",
            dialogClass: "parent-warning-dialog",
            buttons: {
              OK: function() {
                $( this ).dialog( "close" );
              }
            }
          });
        $("#parent-warning").show();
      }
      else if (role=="C"){

        window.location.href = 'https://www.collegexpress.com/reg/counselor';r='counselor'
      }

    });

    $("#id_optin_toggle").click(function(){
      $("#id_optin_list").toggle();
      return false;
    });

    if(!cookiesEnabled()){
      $("#cookies-required").append("<p class='error'>Cookies are required to signup. Please enable them to continue.</p>");
    }
  });
</script> 
