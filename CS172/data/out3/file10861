  <script>
    $(function() {

      var pageWidth = $(window).width();

      $('.top_bar .menu-collapse').click(function() {
       $('nav').animate({'right':'0'}, 'slow');
       $('html').addClass('noscroll');
      });

      $('.top_bar #nav-close').click(function() {
       $('nav').animate({'right':'-100%'}, 'slow');
       $('html').removeClass('noscroll');
      });

      $('.top_bar div.active').click(function() {
        if ($(this).hasClass('isDown') ) {
            $('#mystuff_drop').animate({'right':'-100%'}, 'slow');          
            $(this).removeClass('isDown');
        } else {
            $('#mystuff_drop').animate({'right':'0'}, 'slow');  
            $(this).addClass('isDown');
        }
      });

      $('.top_bar #site_search').click(function() {
        if ($(this).hasClass('isDown') ) {
            $('#search-blowout').animate({'left':'-100%'}, 'slow');          
            $(this).removeClass('isDown');
            if ( pageWidth <= 1123 ) {
              $('html').removeClass('noscroll');
            }
        } else {
            $('#search-blowout').animate({'left':'0'}, 'slow');  
            $(this).addClass('isDown');
            if ( pageWidth >= 1123 ) {
              $('#search-blowout form input').select();
            }
            if ( pageWidth <= 1123 ) {
              $('html').addClass('noscroll');
            }
        }
      });

      $('.top_bar  #search-close').click(function() {
        $('.top_bar #search-blowout').animate({'left':'-100%'}, 'slow');
        $('.top_bar #site_search').removeClass('isDown')
        $('html').removeClass('noscroll');
      });

      if(typeof(Modernizr) != "undefined" && Modernizr.input.placeholder){
        $("#body").addClass("has-placeholder");
      }

      // messages
      $("div.error_container").delay(1000).animate({
        left: '+=380'
      }).delay(5000).animate({
        left: '-=380'
      });
      
      // slide up message
      $(window).scroll(function () {
        if ($(this).scrollTop() > 200) {
          if ($.cookie('fb-out') != 'true' ) { 
            $(".slide_up").slideDown(800);
            $(".close_banner").click(function() {
              $(".slide_up").slideUp(500);
              var date = new Date();
              var minutes = 120;
              date.setTime(date.getTime() + (minutes * 60 * 1000));
              $.cookie('fb-out', 'true', { expires: date, path: '/' });
              return false;
            });
          }
        }
      });

      // slide up message logged in
      $(window).scroll(function () {
        if ($(this).scrollTop() > 200) {
          if ($.cookie('fb-in') != 'true' ) { 
            $(".slide_up_in").slideDown(800);
            $(".close_banner").click(function() {
              $(".slide_up_in").slideUp(500);
              $.cookie('fb-in', 'true', { expires: 7, path: '/'});
              return false;
            });
          }
        }
      });

      // dropdown menus
      var megaConfig = {
             interval: 100,
             sensitivity: 7,
             over: function(){
          $(this).find("ul").show();
         },
             timeout: 300,
             out: function(){
          $(this).find("ul").hide();
         }
        };
      
      
      $(".content_category").hoverIntent(megaConfig);

    });

  </script> 
