        {{#compare resultType '===' "PRODUCT"}}
        <div class="item-offer" itemtype="http://schema.org/Offer" itemprop="offers" itemscope="itemscope">
            {{#price}}
            <div class="item-price">
            <meta itemprop="priceCurrency" content="{{priceCurrency}}">
                {{#current}}
                <span class="current" itemprop="price">{{{.}}}</span>
                {{/current}}

                {{#previous}}
                <em class="previous text-alt">
                    <span class="a11y">{{ui.description.prevPrice}}</span>
                    {{{.}}}
                </em>
                {{/previous}}
            </div>
            {{/price}}

            {{#compare shippingPromotion '||' shipIn}}
            <ul class="item-availability text-alt">
                {{#shippingPromotion}}
                <li class="ship-promo">{{{preserveHtml shippingPromotion}}}</li>
                {{/shippingPromotion}}

                {{#shipIn}}
                <li class="ship-time">{{{preserveHtml shipIn}}}</li>
                {{/shipIn}}
            </ul>
            {{/compare}}
        </div>
        {{/compare}}

        {{/if}}
    </div>
</li>
{{/list}}
		
	</script> 
