function SwitchDisplay(DispType)
{
	try
	{
		document.forms[0].DispType[DispType].checked = "true";
	}
	catch (e)
	{
		if (DispType == 0)
			document.forms[0].SaveDispType.value = "calendar";
		else
			document.forms[0].SaveDispType.value = "list";
	}
	ValidateForm();
}
