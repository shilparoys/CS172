jQuery.noConflict();
jQuery(document).ready(function($) {
	help.loadAnswerText();
	help.qaHideShow();
	help.sectionHideShow();
	help.sectionPrint();
	
    //sets topnav icon to green/active
   	setCurrentIcon("Ayuda");
	
	// Handle the ENTER key in the Search box on the page (not the top navigation Search box)
	jQuery("#pageQuery").keypress(function(event) {
		if (event.keyCode == 13) {
			JavaScript:Search('page'); 
			return false;
		}
	});
	
	// Handle clicking the Search button on the page (not the top navigation Search button)
    jQuery("#pageSearchRef").click(function() {
    	JavaScript:Search('page');
    	return false;
    });
    
    // Tracking the search on the page
	jQuery("#pageSearchRef").webtrends.assignSearchButton();
	jQuery("#pageQuery").webtrends.assignSearchInputBox();
	
	// Tracking clicks on Trending Questions section
	jQuery("#trendingQuestionsList .helpQuestion").webtrends.assignTrendingLinks();
	// Tracking clicks on Browse FAQ section
	jQuery("#helpPageBody .helpQuestion").webtrends.assignFaqLinks();
	
	jQuery("#pageQuery").help.addHelpSearchInputBehaviors();
	
}); // end jQuery(document).ready

</script> 
