    function open_taf(page){
        page = page + "&rd=" + Math.random();
        window.open (page, 'adminConsole', 'left=20,top=50,width=500,height=400,scrollbars=yes,resizable=1,status=yes,menubar=no,directories=np');
        return false;
    }
    
    function changeLanguage(value){
        if (value != '') document.location.href = value;
    }	
	
	function launchaction (contenttype, method, objectid,parentobjectid) {
		open_popup('/admin//invoke.cfm?object=' + contenttype + '&objectid=' + objectid + '&parentobjectid=' + parentobjectid + '&method=' + method);
	}
	currentObjectID = 'EN8BA450-E2CA-7800-F21A1634C5F59578';
	currentLanguage = 'en-en';
    
    function open_popup(page){
        page = page + "&rd=" + Math.random();
        window.open (page, 'adminConsole', 'left=20,top=50,width=700,height=625,scrollbars=yes,resizable=1,status=yes,menubar=no,directories=np');
    }			
    
    function popup(mylink, windowname){
		if (! window.focus)return true;
		var href;
		if (typeof(mylink) == 'string')
		   href=mylink;
		else
		   href=mylink.href;
		window.open(href, windowname, 'width=400,height=400,resizable=yes,scrollbars=no');
		return false;
    }
	
    function gotoAnchor(anchorname) {
        document.location.hash = anchorname;
        void(0);
    }
