var tags=new Array ("body","td","p","li","ul","H1","H2","H3","H4","H5","H6","hr","a","DIV","SELECT","INPUT","TEXTAREA","span");
function setFontSize(size) {
	for (j=0;j<tags.length;j++){
		var p = document.getElementsByTagName(tags[j]);
		for (i=0;i<p.length;i++) {
			p[i].style.fontSize = size+"px";
		}
	}
}
</script> 
