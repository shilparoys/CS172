        var isGoogleMapsKeyCorrectlySet = false;
    </script> 
  <script type="text/javascript">
            var googleKeyUnsetMessage = "No s'ha configurat correctament una clau de Google Maps API.";

            function createGoogleMap(idToReplace, x,y, zoom, width, height){
                var divToReplace = document.getElementById(idToReplace);
              if (isGoogleMapsKeyCorrectlySet && GBrowserIsCompatible()) {
                var map = new GMap2(divToReplace);
                  if(height >= 300)
                        map.addControl(new GLargeMapControl());
                  else
                        map.addControl(new GSmallMapControl());
                map.addControl(new GMapTypeControl());
                map.setCenter(new GLatLng(x,y), zoom);
                var centerPoint = new GLatLng(x,y);
                map.addOverlay(new GMarker(centerPoint));
              } else if (!isGoogleMapsKeyCorrectlySet) {
                  divToReplace.innerHTML="<span class='skn-error'>"+googleKeyUnsetMessage+"</span>";
              }
            }

            function createGoogleInput(idToReplace, x,y, idlat, idlong, createPoint, zoom, width, height) {
                var divToReplace = document.getElementById(idToReplace);
                if (isGoogleMapsKeyCorrectlySet && GBrowserIsCompatible()) {
                  var latHidden = document.getElementById(idlat);
                  var lngHidden = document.getElementById(idlong);
                  var map = new GMap2(divToReplace);
                  if(height >= 300)
                        map.addControl(new GLargeMapControl());
                  else
                        map.addControl(new GSmallMapControl());
                  map.addControl(new GMapTypeControl());
                  var centerPoint = new GLatLng(x,y);
                  map.setCenter(centerPoint, zoom);
                  var theMarker;
                  if(createPoint) {
                      theMarker = createDraggableMarker(map, centerPoint, latHidden,lngHidden );
                  }

                  GEvent.addListener(map, "click", function(marker, point) {
                        if(marker){
                            map.removeOverlay(marker);
                            theMarker = null;
                            latHidden.value = '';
                            lngHidden.value = '';
                        }
                        else if(point){
                            if(theMarker){
                                theMarker.setPoint(point);
                            } else{
                                theMarker = createDraggableMarker(map, point, latHidden,lngHidden );
                            }
                            latHidden.value = point.lat();
                            lngHidden.value = point.lng();
                            }
                        }
                    );

                } else if (!isGoogleMapsKeyCorrectlySet) {
                  divToReplace.innerHTML="<span class='skn-error'>"+googleKeyUnsetMessage+"</span>";
                }
            }

            function createDraggableMarker(map, markerPoint, latHidden,lngHidden ){
                if (isGoogleMapsKeyCorrectlySet && GBrowserIsCompatible()) {
                    var theMarker = new GMarker(markerPoint, {draggable: true});
                    map.addOverlay(theMarker);
                    GEvent.addListener(theMarker, "dragend", function() {
                        var point = theMarker.getPoint();
                        latHidden.value = point.lat();
                        lngHidden.value = point.lng();
                        }
                    );
                    return theMarker;
                }
            }
        </script> 
