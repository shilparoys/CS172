  <script type="text/javascript">
 
  

 $(document).ready(function(){


window.onload=my_code();

function my_code(){
search_ajax_way_base("cas");
search_ajax_way_2("casug");
search_ajax_way_3("casgrad");
search_ajax_way_4("cas");
}

    $("#button_find").click(function(event){
    event.preventDefault();
    search_ajax_way_1(this.id);

    });
    
    $(".my_link").click(function(event){
    event.preventDefault();
       
    $("#display_results_1").empty();
        $("#display_results_3").empty();

    search_ajax_way_4(this.id);

    });
   
    
    $("#cas").click(function(event){
 
    
    $("#display_results_3").empty();
      search_ajax_way_base("cas");
      search_ajax_way_4("cas");
    });
    
    
    
$("#cas").live("click", function(){
    $("input[name='search_query']")
                            .attr("value", "");
   
      search_ajax_way_base(this.id);

    });
    

    $("#casug").click(function(event){

    $("#display_results_3").empty();
    $("#display_results_1").empty();
       $("#display_results_4").empty();
      search_ajax_way_2(this.id);

    });
    

    
    
    
   $("#casgrad").click(function(event){
    $("#display_results_1").empty();

     $("#display_results_4").empty();
      search_ajax_way_3(this.id);

    });
    


    $("#search_query").keyup(function(event){
    event.preventDefault();
    search_ajax_way_1(this.id);
    });
     
    });
     
function search_ajax_way_1() {
    $("#search_results_1").show();
  var search_this = $("#search_query").val();
    $.post("../../../php/search-cas.php", {
        blockId: search_this
    }, function (data) {
        $("#display_results_1").html(data);
            
    })
}



function search_ajax_way_base(blockId){
   $("#search_results_1").show();

   var search_this=$("#search_query").val();
   $.post("../../../php/search-cas.php", {searchit : search_this, 'blockId': blockId}, function(data){
      $("#display_results_1").html(data);
        $("#display_results_4").html(data);

   })
  

}

function search_ajax_way_active(blockId){
   $("#search_results_2").show();

   var search_this=$("#search_query").val();
   $.post("../../../php/search-cas.php", {searchit : search_this, 'blockId': blockId}, function(data){
      $("#display_results_2").html(data);

   })
}




function search_ajax_way_3(blockId){
   $("#search_results_3").show();

   var search_this=$("#search_query").val();
   $.post("../../../php/search-cas.php", {searchit : search_this, 'blockId': blockId}, function(data){
      $("#display_results_3").html(data);

   })
}


function search_ajax_way_4(blockId){
   $("#search_results_4").show();

   var search_this=$("#search_query").val();
   $.post("../../../php/search-cas.php", {searchit : search_this, 'blockId': blockId}, function(data){
      $("#display_results_4").html(data);

   })
}





function search_ajax_way_2(blockId){
   $("#search_results_2").show();

   var search_this=$("#search_query").val();
   $.post("../../../php/search-cas.php", {searchit : search_this, 'blockId': blockId},
    function(data){
      $("#display_results_2").html(data);

   })
}






</script> 
  <script type="text/javascript">
  jQuery.noConflict();
  jQuery(window).load(function() {
    switch(window.location.pathname.split("/")[1]) {
      case ("about"):                                  
        jQuery("#about_au").addClass("active");
        break;
      case ("admission"):
        jQuery("#admission").addClass("active");
        break;
      case ("academics"):
        jQuery("#academics").addClass("active");
        break;
      case ("student-life"):
        jQuery("#student_life").addClass("active");
        break;
      case ("arts"):
        jQuery("#arts").addClass("active");
        break;
      default:                
        break;
    };
  });
</script> 
