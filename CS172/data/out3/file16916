       <!-- **************************** START PAGE TITLE ************************************ --> 
       <h1>Homeless</h1> 
       <!-- **************************** END PAGE TITLE ************************************** --> 
      </div> 
      <!--End main content wrapper --> 
      <div id="tier2gallery">
       <!-- begin apple images gallery--> 
       <div id="gallery">
        <!-- begin apple images gallery--> 
        <div id="slides" aria-live="polite">
         <!--images and caption in blue box--> 
         <div>
          <!-- SLIDE 1 --> 
          <a name="slide1"></a> 
          <div class="slide"> 
           <img src="/homeless/images/CHALENG15_Slider.png" width="500" height="275" alt="Graphic with the words: 2015 CHALENG survey on Veterans Homelessness is open.  Veterans, Community, Partners, VA Staff, Take survey "> 
           <div class="description">
