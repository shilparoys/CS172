     <script type="text/javascript">
	$(document).ready(function() {
		$("#find-health-center-fhc-54f39a14b9b8a103667").submit(function(e) {
			e.preventDefault();
			if ($("#find-health-center-fhc-54f39a14b9b8a103667 #fhc-54f39a14b9b8ahealth-zip").val() != '') {
				window.location.href = '/health-center/all/all/' + $("#find-health-center-fhc-54f39a14b9b8a103667 #fhc-54f39a14b9b8ahealth-zip").val();
				return false;
			}
			else if ($("#find-health-center-fhc-54f39a14b9b8a103667 #fhc-54f39a14b9b8ahealth-state").val() != "State") {
				window.location.href = '/health-center/' + $("#find-health-center-fhc-54f39a14b9b8a103667 #fhc-54f39a14b9b8ahealth-state").val() + '/';
				return false;
			}
		});
		$("#fhc-54f39a14b9b8ahealth-state").change(function(e) {
			if ($("#fhc-54f39a14b9b8ahealth-state").val() != "State")
			{
				$("#fhc-54f39a14b9b8ahealth-zip").val("");
			}
		});

		$("#fhc-54f39a14b9b8ahealth-zip").click(function() {
			$("#fhc-54f39a14b9b8ahealth-state").val("State");
		});
        $("#fhc-54f39a14b9b8ahealth-zip").keypress(function (e) {
            var char = (typeof e.which == "undefined") ? e.keyCode : e.which;
            if (!((char===08||char===13)||(char >= 48 && char <= 58))) {
                e.preventDefault();
            }
        });
	});
</script> 
