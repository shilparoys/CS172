	function setupAjax(event, target)
	{ 	
		if (!target)
		{
			target = event.target;
		}	
    	if($(target).hasClass("fp_play_icon")) 
    	{    		
	    	var itemId = $(target).attr("data-id");
			if (itemId == null || itemId == "")
			{
				itemId = $(target).closest("[data-id]").attr("data-id");
			}
			
			var mediaUrl = $(target).attr("data-mediaUrl");
			if (mediaUrl == null || mediaUrl == "")
			{
				mediaUrl = $(target).closest("[data-mediaUrl]").attr("data-mediaUrl");
			}
			
			var netConnectionUrl = $(target).attr("data-netConnectionUrl");
			if (netConnectionUrl == null || netConnectionUrl == "")
			{
				netConnectionUrl = $(target).closest("[data-netConnectionUrl]").attr("data-netConnectionUrl");
			}
			
			var thumbnailUrl = $(target).attr("data-thumbnailUrl");
			if (thumbnailUrl == null || thumbnailUrl == "")
			{
				thumbnailUrl = $(target).closest("[data-thumbnailUrl]").attr("data-thumbnailUrl");
			}
			
			var fallbackUrl = $(target).attr("data-fallbackUrl");
			if (fallbackUrl == null || fallbackUrl == "")
			{
				fallbackUrl = $(target).closest("[data-fallbackUrl]").attr("data-fallbackUrl");
			}
			
			var analyticsId = $(target).attr("data-analyticsId");
			if (analyticsId == null || analyticsId == "")
			{
				analyticsId = $(target).closest("[data-analyticsId]").attr("data-analyticsId");
			}
						
			if (mediaUrl)
			{
				var elementIdentifier = $(target).closest(".easySliderContainer");
				
				var ajaxUrl = "ViewPage.action?siteNodeId=600600";
										
				if(ajaxUrl.indexOf("?") == -1)
				{	
					delim = "?";
				}
				else
				{
					delim = "&";
				}
				$(elementIdentifier).trigger('flowplayerEventPlay');
				var ajaxUrl = ajaxUrl + delim + "width=320&mediaUrl=" + escape(mediaUrl) 
							  + "&netConnectionUrl=" + escape(netConnectionUrl) + "&thumbnailUrl=" + escape(thumbnailUrl) + "&fpAutoPlay=true"
							  + "&fallbackUrl="+fallbackUrl + "&noSplash=true&isInfoglueContent=true&analyticsId=" + escape(analyticsId);
				var targetParent = $(target).parent();
				
				$(this).parent().children(".movie_placeholder").remove();
				$(this).parent().children(".fp_play_icon").remove();
				
				//-------------------------------------------------------
				// FIX for XSITE calls in IE  http://tinyurl.com/ae5wta7
				//-------------------------------------------------------
				
				
				
				var isNiceURI = true;
				
				if(isNiceURI && jQuery.browser.msie && window.XDomainRequest) {
					var xdr 		= new XDomainRequest();
					xdr.timeout 	= 20000;
					
					xdr.onload 		= function() {
						$(targetParent).html(xdr.responseText);
					};
					xdr.onerror 	= function (error) {
						console.log("An error occured calling Ajax service.");
						for (var x in xdr)
						{
							console.log(x + " = " + xdr[x]);
						}
					};
	                xdr.ontimeout 	= function () {
	                	console.log("Timeout calling Ajax service.");
	                };

	                xdr.open("post", ajaxUrl);
	                xdr.send();
				} 
				else 
				{
					$(targetParent).load(ajaxUrl, function(){
						// Do nothing special
					});
				}
			}
    	}
    	else if($(target).hasClass("fp_HTML5_video") ) 
    	{
    		target.webkitEnterFullscreen();
    		target.play();
    	}
	}
</script> 
