			.result(function(event, item) {
				jQuery('#searchText').val(item.suggestion);
				jQuery('#searchFormImageSubmit').click();
			});
		} catch (err) {
			(console && console.log && console.log("Failed to initialize SiteSeeker auto complete", err));
		}
	});
//-->
</script> 
