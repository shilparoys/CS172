  /* suit.view object
     An object for data about the view or state of the current page.
     This is the SUIT technique for carrying server-side values to the client side code. */
  var qs = window.location.search;
  if ((qs.indexOf("%3C") !== -1) && (qs.indexOf("%3C") !== -1)) {
     var ft = qs.substring(10, qs.indexOf("&andor"));
     ft=ft.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, '');
     var nqs = qs.substring(0,10)+ft+qs.substr(qs.indexOf("&andor"));
     window.location.search=nqs;
  } 
   
  //$.holdReady(true);
  suitClass.prototype.view = function() {
    this.pageType = "detail";
    this.view = "event.html";
    this.path = "/event.html";
  }

  function defensexss(idname) {
    var mytext = $("#"+idname).val();
    if (mytext.toLowerCase().indexOf('script') !== -1) {
      mytext = mytext.replace(/(<\s*\/?\s*)script(\s*([^>]*)?\s*>)/gi ,''); //remove script tags
      mytext = mytext.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, ''); //remove special chars
      $("#"+idname).val(mytext);  
    }
  }

  $(document).ready(function(){
   
    $("form").submit(function(){
       defensexss('advfulltext');
       var qs = $.QueryString("fulltext");       
    });
  });
</script> 
