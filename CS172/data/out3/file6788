	var hash = '';

	jQuery.fn.spin = function(opts) 
	{
		this.each(function() 
		{
			var $this = jQuery(this),
			data = $this.data();

			if (data.spinner) 
			{
				data.spinner.stop();
				delete data.spinner;
			}
			if (opts !== false) {
			  data.spinner = new Spinner(jQuery.extend({color: $this.css('color')}, opts)).spin(this);
			}
		});
		return this;
	};
	
	function Desplazar()
	{
		if(!jQuery('#marco-principal').hasClass('traducido-auto'))
			jQuery('#encabezado, #centro-arriba, #marco-principal').addClass('desplazado');
	}
	
	function NoDesplazar()
	{
		if(!jQuery('#marco-principal').hasClass('traducido-auto'))
			jQuery('#encabezado, #centro-arriba, #marco-principal').removeClass('desplazado');
	}
	
	function DesplazarBarraExtra()
	{		
		if(!jQuery('#marco-principal').hasClass('traducido-auto'))
		{
			jQuery('#encabezado').animate({top: '+=40px'}, 1000);
			jQuery('#centro-arriba').animate({marginTop: '+=40px'}, 1000);
		}
		jQuery('#marco-principal').addClass('traducido-auto');
	}
	
	function NoDesplazarBarraExtra()
	{		
		if(jQuery('#marco-principal').hasClass('traducido-auto'))
		{
			jQuery('#encabezado').animate({top: '-=40px'}, 500);
			jQuery('#centro-arriba').animate({marginTop: '-=40px'}, 500);
		}
		jQuery('#marco-principal').removeClass('traducido-auto');
	}
	
	function InicializaTranslator()
	{
		jQuery('.goog-te-menu-value span').bind('click', function(){
			window.setTimeout(function(){
				// SE CARGO EL COMBOBOX DE IDIOMA GOOGLE			
				jQuery('iframe.goog-te-menu-frame').contents().find('.goog-te-menu2-item div').bind('click', function(){
					// CAMBIA EL IDIOMA
					DesplazarBarraExtra();
					window.setTimeout(function(){
						jQuery('iframe.goog-te-banner-frame').contents().find('a.goog-close-link').bind('click', function(){
							// CIERRA EL PANEL
							NoDesplazarBarraExtra();
						});
					}, 500);
				});
			}, 500);
        });
	}
	
	jQuery(document).ready(function() 
	{	
		
		var mostrarOpcionesBuscar = false;
		var opcionesBuscarBase = false;
							
		
		
		function Tactil()
		{
			return (Modernizr.touch || Modernizr.testAllProps('pointerEvents'));
		}
		
		var escondidoMenuArriba = null;
		var abiertoMenuArriba = false;
		var abiertoMenuArribaInterno = false; // Esta abierto un submenu
		var ultimoSubmenu = -1;
		var menuSuperiorGrande = true;
		var menusTactiles = false;
		var estadoAnteriorMenuTactiles = false;
		var estadoAnteriorMenuSuperior = true;
		
		if(jQuery(window).width() <= 640)
		{
			menuSuperiorGrande = false;
			estadoAnteriorMenuSuperior = false;
			mostrarOpcionesBuscar = true;
		
			pasosTutorial = pasosTutorialSinPerfiles;
			resaltadosTutorial = resaltadosTutorialSinPerfiles;
			
		}
		
		if(jQuery(window).width() <= 1024)
		{
			menusTactiles = true;
			estadoAnteriorMenuTactiles = true;
			jQuery('#encabezado .derecha .imagen').addClass('fullres');
			jQuery('#menu-iconos li a div.imagen').addClass('fullres');		
		}
		
		jQuery.fn.slideFadeToggle = function(speed, easing, estado, callback) 
		{					
			if(!enTutorial)
				return this.animate({opacity: 'toggle', height: 'toggle'}, speed, easing, callback);
			else
				return false;
		};
		
		function alternarMenuArriba(estado)
		{
			if(estado == 0 || (estado == -1 && abiertoMenuArriba === true) || (estado == 1 && abiertoMenuArriba === false))
			{
				if(jQuery(window).width() <= 640)
				{
		
					jQuery('#centro-arriba').slideFadeToggle('fast', 'linear');
					if(escondidoMenuArriba === null)
					{
						if(estado == -1)
							escondidoMenuArriba = true;
						else
							escondidoMenuArriba = false;
					}
					else
						escondidoMenuArriba = !escondidoMenuArriba;
				}
				else
				{
					jQuery('#centro-arriba ul.enlaces').slideFadeToggle('fast', 'linear');
					abiertoMenuArriba = !abiertoMenuArriba;
		
					pasosTutorial = pasosTutorialPerfiles;
					resaltadosTutorial = resaltadosTutorialPerfiles;
			
				}
			}
		}
		
		function abrirMenuArriba()
		{
			alternarMenuArriba(1);
		}
		
		function cerrarMenuArriba()
		{
			alternarMenuArriba(-1);
		}
		
		jQuery('a.mostrar-menu').on('click', function () 
		{		
			alternarMenuArriba(0);
			if(abiertoMenuArribaInterno)
			{
				jQuery('#centro-arriba ul.enlaces').slideFadeToggle('fast', 'linear');
				abiertoMenuArribaInterno = !abiertoMenuArribaInterno;
			}
		});	
		
		AcondicionarMenus = function()
		{			
			if(jQuery(window).width() < 560)
				jQuery('#iFrameLogueo').attr('height', '330').attr('width', '100%');
			else
				jQuery('#iFrameLogueo').attr('height', '160').attr('width', '500'); // 200 height?
		
			if(jQuery(window).width() > 640)
			{
				jQuery('#campos-buscador').prependTo('#buscar_global');
				mostrarOpcionesBuscar = opcionesBuscarBase;
				if(mostrarOpcionesBuscar && !jQuery('#entrada_busqueda').data('evento_ligado'))
				{
					jQuery('#entrada_busqueda').data('evento_ligado', true);
					jQuery('#entrada_busqueda').bind('click', function(event){
						event.stopPropagation();		
						if(!jQuery(this).data('focused'))
							procesarFocoBuscar();
						jQuery(this).removeData('focused');
					});	
					jQuery('#entrada_busqueda').bind('focusin', function(){
						jQuery(this).data('focused',true);		
							procesarFocoBuscar();
					}).bind('focusout', function(){			
						window.setTimeout(function(){
							if(!jQuery(document.activeElement).closest('.globo.busqueda').length && !jQuery('.globo.busqueda').data('clicked'))
							{			
								procesarFocoBuscar();
							}
							jQuery('.globo.busqueda').removeData('clicked');
							jQuery(this).removeData('focused');
						}, 50);
					});
				}
				if(jQuery('ul.funciones .busqueda').is(':visible') && !opcionesBuscarBase)
					procesarFocoBuscar();
				if(ultimoSubmenu > -1)
				{
					jQuery('#centro-arriba ul.menu li.padre').eq(ultimoSubmenu).children('ul.enlaces').slideFadeToggle('fast', 'linear');
					ultimoSubmenu = -1;
				}
				if(escondidoMenuArriba !== null && escondidoMenuArriba)
				{
					jQuery('#centro-arriba').slideFadeToggle('fast', 'linear');
					escondidoMenuArriba = false;
				}
				abiertoMenuArriba = false;				
				jQuery('#centro-arriba ul.menu li.padre').unbind('click');
				jQuery('#centro-arriba ul.menu li.padre').children().unbind('click');
				jQuery('#menus-principales').hoverIntent(abrirMenuArriba, cerrarMenuArriba);				
				jQuery('#marco-principal').removeClass('movil');				
			}
			else
			{
				jQuery('#campos-buscador').prependTo('#globo-busqueda');
				mostrarOpcionesBuscar = true;
				if(jQuery('#entrada_busqueda').data('evento_ligado'))
				{
					jQuery('#entrada_busqueda').removeData('evento_ligado');
					jQuery('#entrada_busqueda').unbind('click');	
					jQuery('#entrada_busqueda').unbind('focusin');
					jQuery('#entrada_busqueda').unbind('focusout');					
				}
				
				if(escondidoMenuArriba !== null && !escondidoMenuArriba)
				{
					jQuery('#centro-arriba').slideFadeToggle('fast', 'linear');
					escondidoMenuArriba = true;
				}
				jQuery('#marco-principal').addClass('movil');
				jQuery('#menus-principales').unbind('mouseenter').unbind('mouseleave');
				jQuery('#centro-arriba ul.menu li.padre').bind('click', function(event) {
					if(event.target === this) 
					{
						if(ultimoSubmenu > -1)
							jQuery('#centro-arriba ul.menu li.padre').eq(ultimoSubmenu).children('ul.enlaces').slideFadeToggle('fast', 'linear');
						if(ultimoSubmenu != jQuery(this).index())
						{		
							ultimoSubmenu = jQuery(this).index();
							jQuery(this).children('ul.enlaces').slideFadeToggle('fast', 'linear');							
						}
						else
							ultimoSubmenu = -1;
					}
				});
				
				jQuery('#centro-arriba ul.menu li.padre').children().bind('click', function(event) {
					event.stopPropagation();
				});				
			}
		}		
		
		AcondicionarListasAcordeon = function()
		{			
			jQuery('#marco-principal:not(.multimedios) ul.listado li').each(function(){
				jQuery(this).remove('.expandir');
				var padding = parseInt(jQuery(this).children('.contenedor').css('padding-bottom')) + 5;
				h_entrada_listado = jQuery(this)[0].scrollHeight;				
				h_entrada_listado_m = parseInt(jQuery(this).css('max-height'));
				if(h_entrada_listado > (h_entrada_listado_m + padding))
					jQuery(this).append('<a class="expandir">&bull; &bull; &bull;</a>');
			});
		}
		
		jQuery('ul.acordeon li.padre').hoverIntent(function (){
			jQuery(this).children('ul').animate({opacity: 'toggle', height: 'toggle'}, 'fast', 'linear');
		}, function (){
			jQuery(this).children('ul').animate({opacity: 'toggle', height: 'toggle'}, 'fast', 'linear');
		});
		
		jQuery('#menus-principales').focusin(function()
		{
			if(escondidoMenuArriba === null || escondidoMenuArriba)
				abrirMenuArriba();
		});			
		jQuery('#menus-principales').focusout(function()
		{
			if(!jQuery(document.activeElement).closest('#menus-principales').length && !escondidoMenuArriba)			
				cerrarMenuArriba();
		});
		
		function CerrarGlobo()
		{
			jQuery('ul.funciones li.activo .globo').fadeOut('fast');
			jQuery('ul.funciones li').removeClass('activo');
			jQuery('ul.funciones li input.boton').removeClass('activo');
		};

		
		var traduccionesCargadas = false;		
		var scriptTraducciones1 = document.createElement( 'script' );
		var scriptTraducciones2 = document.createElement( 'script' );		
		scriptTraducciones1.type = 'text/javascript';
		scriptTraducciones1.text = ' \
		function googleTranslateElementInit() { \
			new google.translate.TranslateElement({pageLanguage: \'es\', layout: google.translate.TranslateElement.InlineLayout.SIMPLE, autoDisplay: false}, \'google_translate_element\'); \
			InicializaTranslator(); \
		} ';
		scriptTraducciones2.type = 'text/javascript';
		scriptTraducciones2.src = '//translate.google.com/translate_a/element.js?cb=googleTranslateElementInit';		
		
		function procesarFocoTraducciones()
		{
			CerrarGlobo();
			if(!traduccionesCargadas)
			{
				jQuery('.globo.traducciones .automaticas').append(scriptTraducciones1).append(scriptTraducciones2);
				traduccionesCargadas = true;
			}
			if(jQuery('ul.funciones .traducciones').is(':visible'))
				jQuery('ul.funciones li.menu-lang').removeClass('activo');
			else
			{			
				jQuery('ul.funciones li.menu-lang').addClass('activo');
				jQuery('ul.funciones .traducciones').slideFadeToggle('fast', 'linear', 0);
			}
		};	
		
		function procesarFocoLoguear()
		{
			CerrarGlobo();
			if(jQuery('ul.funciones .login').is(':visible'))
				jQuery('ul.funciones li.menu-loguear').removeClass('activo');			
			loguear(false, 'http%3A%2F%2Fwww.ucr.ac.cr%2F');
		};		

		function procesarFocoBuscar()
		{
			CerrarGlobo();
			if(jQuery('ul.funciones .busqueda').is(':visible'))
			{
				jQuery('ul.funciones li.menu-buscar').removeClass('activo');
				jQuery('ul.funciones li.menu-buscar input.boton').removeClass('activo');
			}
			else
			{
				jQuery('ul.funciones li.menu-buscar').addClass('activo');
				jQuery('ul.funciones li.menu-buscar input.boton').addClass('activo');
				jQuery('ul.funciones .busqueda').slideFadeToggle('fast', 'linear', 0);
			}
		};
		
		jQuery('#menu-lang').click(function(event) {
			event.stopPropagation();
			if(!jQuery(this).data('focused'))
				procesarFocoTraducciones();
			jQuery(this).removeData('focused');
		});
		jQuery('#menu-lang').bind('focusin', function() {
			jQuery(this).data('focused',true);
			procesarFocoTraducciones();
		}).bind('focusout', function() {
			window.setTimeout(function(){
				if(!jQuery(document.activeElement).closest('.globo.traducciones').length && !jQuery('.globo.traducciones').data('clicked'))			
					procesarFocoTraducciones();
				jQuery('.globo.traducciones').removeData('clicked');
				jQuery(this).removeData('focused');
			}, 50);
		});	
		
		jQuery('#menu-loguear, #menu-desloguear').click(function(event){
			event.stopPropagation();
			if(!jQuery(this).data('focused'))
				procesarFocoLoguear();
			jQuery(this).removeData('focused');
		});
		jQuery('#menu-loguear, #menu-desloguear').bind('focusin', function() {
			jQuery(this).data('focused',true);
			procesarFocoLoguear();
		}).bind('focusout', function() {
			window.setTimeout(function(){
				if(!jQuery(document.activeElement).closest('.globo.login').length && !jQuery('.globo.login').data('clicked'))
					procesarFocoLoguear();
				jQuery('.globo.login').removeData('clicked');
				jQuery(this).removeData('focused');
			}, 50);
		});
		
		jQuery('#menu-buscar').click(function(event){
			event.stopPropagation();		
			if(!jQuery(this).data('focused'))
				procesarFocoBuscar();
			jQuery(this).removeData('focused');
		});	
		jQuery('#menu-buscar').bind('focusin', function(){
			jQuery(this).data('focused',true);		
				procesarFocoBuscar();
		}).bind('focusout', function(){			
			window.setTimeout(function(){
				if(!jQuery(document.activeElement).closest('.globo.busqueda').length && !jQuery('.globo.busqueda').data('clicked'))		
					procesarFocoBuscar();
				jQuery('.globo.busqueda').removeData('clicked');
				jQuery(this).removeData('focused');
			}, 50);
		});
		
		if(jQuery(window).width() <= 640 || mostrarOpcionesBuscar)
		{
			if(!jQuery('#entrada_busqueda').data('evento_ligado'))
			{
				jQuery('#entrada_busqueda').data('evento_ligado', true);
				jQuery('#entrada_busqueda').bind('click', function(event){
					event.stopPropagation();		
					if(!jQuery(this).data('focused'))
						procesarFocoBuscar();
					jQuery(this).removeData('focused');
				});	
				jQuery('#entrada_busqueda').bind('focusin', function(){
					jQuery(this).data('focused',true);		
						procesarFocoBuscar();
				}).bind('focusout', function(){			
					window.setTimeout(function(){
						if(!jQuery(document.activeElement).closest('.globo.busqueda').length && !jQuery('.globo.busqueda').data('clicked'))		
							procesarFocoBuscar();
						jQuery('.globo.busqueda').removeData('clicked');
						jQuery(this).removeData('focused');
					}, 50);
				});
			}
			jQuery('#encabezado .derecha ul.menu-iconos li.menu-buscar div.entradas').css('visibility', 'visible');
		}
		
		// Prevenir cambio de foco con mouse en globos:
		jQuery('.globo *, #entrada_busqueda').on('mousedown', function(event) {
			jQuery('.globo').data('clicked',true);
		});
		
		jQuery(document).on('mousedown', function(event) {
			if(!jQuery(event.target).closest('.globo').length)
				CerrarGlobo();
		});

		
				winWidth = jQuery(window).width();
		winHeight = jQuery(window).height();
		hash = window.location.hash.substr(1);		
		
			
		
		
			jQuery('#buscar_global').submit(function() 
			{
				var buscar = jQuery('#entrada_busqueda').val();
				buscar = LumpiarURL(buscar);
				if(buscar != '' && buscar != 'buscar...')
				{
					if(jQuery('input#busqueda_contextual').is(':checked'))
					{
						if(jQuery('#usar_buscar_modulo').val() == 'true')
						{
							BuscarDesdeModulo(buscar);
						}
						else
							jQuery('#buscar_global').attr('action', 'http://www.ucr.ac.cr/portada/buscar/' + buscar + '/');
					}
					else
						jQuery('#buscar_global').attr('action', 'http://www.ucr.ac.cr/buscar/' + buscar + '/');
				}
				else
				{
					alert('Escriba palabras para buscar');
					return false;
				}
			});
			
			jQuery('#busqueda_profunda').click(function() {
				if(jQuery('#busqueda_profunda').is(':checked'))
					jQuery('#busqueda_superficial').val('');
				else
					jQuery('#busqueda_superficial').val('true');
			});		
		
						
		
		jQuery(window).resize(function() {
			winNWidth = jQuery(window).width();
			winNHeight = jQuery(window).height();			
			
			if(Modernizr.mq('(max-width: 640px)'))
				menuSuperiorGrande = false;
			else
				menuSuperiorGrande = true;
			if(estadoAnteriorMenuSuperior != menuSuperiorGrande)
			{
				AcondicionarMenus();
				AcondicionarListasAcordeon();
			}
			estadoAnteriorMenuSuperior = menuSuperiorGrande;
			
			if(Modernizr.mq('(max-width: 1024px)'))
				menusTactiles = true;
			else
				menusTactiles = false;
				
			if(estadoAnteriorMenuTactiles != menusTactiles)
			{		
				if(menusTactiles)
				{
					jQuery('#encabezado .derecha .imagen').addClass('fullres');
					jQuery('#menu-iconos li a div.imagen').addClass('fullres');
				}
				else
				{
					jQuery('#encabezado .derecha .imagen').removeClass('fullres');
					jQuery('#menu-iconos li a div.imagen').removeClass('fullres');
				}
			}			
			estadoAnteriorMenuTactiles = menusTactiles;
			
		});
				
		
		var scrollArriba = true;
		if(jQuery(window).scrollTop() > 40)
		{
			Desplazar();
			scrollArriba = false;
		}
		if(jQuery('body').height() > jQuery(window).height()) 
		{
			setTimeout(function() {
				if (location.hash) {
					jQuery('body, html').animate({scrollTop: (jQuery(window).scrollTop() - 110)}, 500);
				}
			}, 1);
		}
		
				
		jQuery(window).scroll(function()
		{
			if(jQuery(this).scrollTop() > 40 && !enTutorial)
			{				
				if(scrollArriba)
				{
					scrollArriba = false;
					Desplazar();					
									}
			}
			else if(!enTutorial)
			{				
				if(!scrollArriba)
				{
					scrollArriba = true;
					NoDesplazar();					
									}
			}
		});	
			
		
		
		jQuery('#comentarAnon').click(function(){
			CerrarGlobo();
			jQuery('div.agregar-comentario').css('display', 'none');
			jQuery('body,html').animate({scrollTop: jQuery('body,html')[0].scrollHeight});
			jQuery('#comentando').fadeIn('slow');
			jQuery('#comentario').focus();
		});	
		
		jQuery('#entrada_busqueda').focus(function()
		{
			if(jQuery(this).val() == 'buscar...')
				jQuery(this).val('');
		});	
		
		jQuery('#entrada_busqueda').blur(function(){
			if(jQuery(this).val() == '')
				jQuery(this).val('buscar...');
		});

		CerrarCajas = function()
		{
			if(logueando)
			{
				OcultarOverlay();
				jQuery('.globo .login').fadeOut('fast');					
				logueando = false;
			}
			else if(escenario)
			{
				jQuery('.jp-jplayer').jPlayer('pause');
				OcultarOverlay();
				jQuery('div.escenario').fadeOut('fast');
				escenario = false;
			}			
			else if(buscando)
			{				
				OcultarOverlay();
				jQuery('.globo .buscar').fadeOut('fast');
				buscando = false;
			}
			else if(compartiendo)
			{			
				enable_scroll();
				OcultarOverlay();
				jQuery('#compartir').fadeOut('fast');
				compartiendo = false;
			}
			else if(enTutorial)
			{
				cerrarTutorial();
			}
		}

		MostrarOverlay = function()
		{
			jQuery('.overlay').fadeIn('fast');
			jQuery('body').addClass('overlayed');
		}
		
		OcultarOverlay = function()
		{
			jQuery('body').removeClass('overlayed');
			jQuery('.overlay').fadeOut('fast');			
		}
		
		CentrarScroll = function(selector1, selector2)
		{
			var viewportWidth = jQuery(window).width(),
            viewportHeight = jQuery(window).height(),
            elemento = jQuery(selector1),
			elemento2 =  jQuery(selector2),
            elHeight = elemento.height(),
            elOffset = elemento.offset(),
			elHeight2 = 0,
			elOffset2 = 0;
			if(selector2 != '')
			{
				elHeight2 = elemento2.height();
				elOffset2 = elemento2.offset();
			}
			var scrTop = elOffset.top + (elHeight/2) - (viewportHeight/2);
			var scrTop2 = elOffset2.top + (elHeight2/2) - (viewportHeight/2);
			if(scrTop2 > scrTop)
				scrTop = scrTop2;
			jQuery('body,html').animate({scrollTop: scrTop}, 1000);
		}
	
		
		jQuery('.overlay').click(function(){
			CerrarCajas();
		});
		jQuery('a.cerrarFrame').click(function(){ 
			CerrarCajas();
		});
		
		jQuery(document).keyup(function(e) {  // Array of keycodes cerrando globos. Default: Esc (27), 'x' (88), 'c' (67)
			if (e.keyCode == 27) { 
				if(abiertoMenuArriba === true)
					cerrarMenuArriba();
				else
				{
					CerrarGlobo();
					CerrarCajas();
				}
			}  
		});
		
		
		var h_entrada_listado = 0;
		var h_entrada_listado_m = 0;
		var entradaAbierta = null;
		var fondo = '';
		
		AcondicionarMenus();
		AcondicionarListasAcordeon();
		
		jQuery(document).on('click', 'ul.listado li .expandir', function(){
			if(this !== entradaAbierta && entradaAbierta !== null)
			{
				jQuery(entradaAbierta).closest('ul.listado li').animate({'maxHeight':h_entrada_listado_m},500);
				jQuery(entradaAbierta).removeClass('abierto');
			}
			if(!jQuery(this).hasClass('abierto'))
			{				
				entradaAbierta = this;
				h_entrada_listado = jQuery(this).closest('ul.listado li')[0].scrollHeight;
				h_entrada_listado_m = jQuery(this).closest('ul.listado li').css('max-height');
				jQuery(this).closest('ul.listado li').animate({'maxHeight':h_entrada_listado},500);
				jQuery(this).addClass('abierto');
			}
			else
			{
				jQuery(entradaAbierta).closest('ul.listado li').animate({'maxHeight':h_entrada_listado_m},500);
				jQuery(this).removeClass('abierto');
				entradaAbierta = null;
			}
		});
		
		fluidvids.init({
			selector: ['iframe'],
			players: ['www.youtube.com']
		});
	
		var panelEventosActivo = 0;
		
		function CambiarPanelEventos(boton)
		{
	
			nuevo = boton.index();
			if(panelEventosActivo != nuevo)
			{
				jQuery('.actividades-recientes .botones a').css('color', '#666666');
				panelEventosAntes = panelEventosActivo;
				panelEventosActivo = nuevo;				
				jQuery('.actividades-recientes .botones a').eq(panelEventosActivo).css('color', '#f47e46');
				jQuery(('#panel_evento' + panelEventosAntes)).fadeOut('fast');
				jQuery(('#panel_evento' + panelEventosActivo)).fadeIn('fast');
			}
		
		}

		jQuery('.actividades-recientes .botones a').hover(function (){
			if(jQuery(this).css('color') != '#f47e46' && jQuery(this).css('color') != 'rgb(244, 126, 70)')
				jQuery(this).css('color', '#ffff99');
		}, function (){
			if(jQuery(this).css('color') != '#f47e46' && jQuery(this).css('color') != 'rgb(244, 126, 70)')
				jQuery(this).css('color', '#666666');
		});
		
		jQuery('.actividades-recientes .botones span a').click(function (){
			CambiarPanelEventos(jQuery(this));
		});
				
		function iniciarContadorEventos()
		{
			var activo = (panelEventosActivo + 1) % 3;
			var boton = jQuery('.actividades-recientes .botones span a').eq(activo);
			CambiarPanelEventos(boton);		
		}		
		jQuery('.actividades-recientes .botones span a.primero').css('color', '#f47e46');
	
	/**************   Noticias  ***********************/
	
	SlideNoticias = function(direccion)
	{
		var panelWidth = jQuery('.destaque-portada .noticias .panel:nth-child(5)').width();
		if(direccion == 'izquierda')
		{		
			jQuery.when(jQuery('.destaque-portada .noticias .panel').animate({left: -panelWidth}, 500)).then(function () {
				jQuery('.destaque-portada .noticias').append(jQuery('.destaque-portada .noticias .panel:first'));
				jQuery('.destaque-portada .noticias .panel').css('left', 0);
				jQuery('.destaque-portada .noticias .panel:nth-child(5), .destaque-portada .noticias .panel:first-child').css('opacity', '0.2');
				jQuery('.destaque-portada .noticias .panel').removeClass('activo');
				jQuery('.destaque-portada .noticias .panel:nth-child(6)').addClass('activo');
			});
		}
		else
		{		
			jQuery.when(jQuery('.destaque-portada .noticias .panel').animate({left: panelWidth}, 500)).then(function () {
				jQuery('#overlay-destaques-portada').after(jQuery('.destaque-portada .noticias .panel:last'));
				jQuery('.destaque-portada .noticias .panel').css('left', 0);
				jQuery('.destaque-portada .noticias .panel:nth-child(5), .destaque-portada .noticias .panel:first-child').css('opacity', '0.2');
				jQuery('.destaque-portada .noticias .panel').removeClass('activo');
				jQuery('.destaque-portada .noticias .panel:nth-child(6)').addClass('activo');
			});
		}
	}
	
	jQuery(document).on({
		mouseenter: function () {
			if(!jQuery(this).is('.destaque-portada .noticias .panel:nth-child(6)'))
				jQuery(this).css({'opacity': '0.7', 'cursor': 'pointer'});
		},
		mouseleave: function () {
			if(!jQuery(this).is('.destaque-portada .noticias .panel:nth-child(6)'))
				jQuery(this).css({'opacity': '0.2', 'cursor': 'normal'});
		}
	}, '.destaque-portada .noticias .panel');
	
	jQuery('body').on('click', '.destaque-portada .noticias .panel', function (event) {		
		if(!jQuery(this).is('.destaque-portada .noticias .panel:nth-child(6)'))
		{			
			if(jQuery(this).index() > 4 && !enTutorial)
			{
				event.preventDefault();
				SlideNoticias('izquierda');
			}
			else
			{
				event.preventDefault();
				SlideNoticias('derecha');
			}
		}
	});
	
	jQuery('.destaque-portada .noticias .flecha.atras').bind('click', function (){
		if(!enTutorial)
			SlideNoticias('derecha');
	});
		
	jQuery('.destaque-portada .noticias .flecha.delante').bind('click', function (){ 
		if(!enTutorial)
			SlideNoticias('izquierda');
	});
	
	if(Tactil())
	{
		jQuery('.destaque-portada .noticias .panel').hammer().on('swipe', function(event) {	
			if(!enTutorial)
				jQuery(document).stopTime('relojPanelesPortada');			
			if(event.direction == 'right')
				SlideNoticias('derecha');
			else if(event.direction == 'left')
				SlideNoticias('izquierda');
			event.preventDefault();
			if(!enTutorial)
			{
				jQuery(document).everyTime(10000, 'relojPanelesPortada', function(i) {
					iniciarContadorPizarra();
					iniciarContadorNoticias();
					iniciarContadorEventos();

				});	
			}
		});
	}
	
	/***********  Pizarra  ****************/
	var panelPizarraActivo = 0;

	CambiarPanelPizarra = function(anuncio)
	{				
		panelPizarraAntes = panelPizarraActivo;
		panelPizarraActivo = anuncio;
		jQuery('#panel_pizarra' + panelPizarraAntes).fadeOut('fast');
		jQuery('#panel_pizarra' + panelPizarraActivo).fadeIn('slow');
		jQuery('.pizarra .contador a').removeClass('resaltado');
		jQuery('.pizarra .contador a#panel-piz-' + panelPizarraActivo).addClass('resaltado');
	}

	
	jQuery('#multimedios .pizarra .boton.izquierda').click(function (){
		anuncio = (temp =(panelPizarraActivo - 1 % 15)) >= 0 ? temp : 14;
		CambiarPanelPizarra(anuncio);
	});
	
	jQuery('#multimedios .pizarra .boton.derecha').click(function (){		
		anuncio = ((panelPizarraActivo + 1) % 15);
		CambiarPanelPizarra(anuncio);
	});		
	
	jQuery('.destaque-portada .noticias, #multimedios, .destaque-portada .secundarias').hover(function () {
			if(!enTutorial)
				jQuery(document).stopTime('relojPanelesPortada');
			}, function () {
			if(!enTutorial)
			{
				jQuery(document).everyTime(10000, 'relojPanelesPortada', function(i) {
					iniciarContadorPizarra();
					iniciarContadorNoticias();
					iniciarContadorEventos();

				});	
			}
	});
			
	iniciarContadorPizarra = function ()
	{
		var anuncio = (panelPizarraActivo + 1) % 15;
		CambiarPanelPizarra(anuncio);
	}
	
	iniciarContadorNoticias = function()
	{
		SlideNoticias('izquierda');		
	}	
	
	jQuery(document).everyTime(10000, 'relojPanelesPortada', function(i) {
		iniciarContadorPizarra();		
		iniciarContadorNoticias();
		iniciarContadorEventos();

	});		
	
		var tutorialPaso = 0;		
		
		iniciarTutorial = function()
		{
			enTutorial = true;
			jQuery(document).stopTime('relojPanelesPortada');			
			for(i = 0; i < 6; i++)
			{
				if(!jQuery('.destaque-portada .noticias').children().eq(2).hasClass('num-0'))
					jQuery('.destaque-portada .noticias').append(jQuery('.destaque-portada .noticias .panel:first'));
			}
			MostrarOverlay();
			jQuery('#tut-inicio').fadeIn('slow');
			if(jQuery(this).scrollTop() > 40)
			{
				jQuery('#encabezado').removeClass('desplazado');
				jQuery('#centro-arriba').removeClass('desplazado');
				jQuery('#marco-principal').removeClass('desplazado');				
			}
			jQuery('body,html').animate({scrollTop: 0});
			disable_scroll();
		}
		
		if(estaCookieTutorial == undefined)
		{			
			iniciarTutorial();
		}
		
		jQuery(document).on('click', '.tut-paso-anterior', function(e)
		{
			if(tutorialPaso > 0)
			{				
				if(resaltadosTutorial[tutorialPaso] != '')
					jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', 'auto');
				if(tutorialPaso == 4 || tutorialPaso == 6)
				{
					enTutorial = false;
					alternarMenuArriba(0); // jQuery('#centro-arriba ul.enlaces').slideFadeToggle('fast','linear');
					enTutorial = true;
				}
				jQuery(pasosTutorial[tutorialPaso]).fadeOut('fast');
				tutorialPaso--;				
				jQuery(pasosTutorial[tutorialPaso]).fadeIn('slow');
				CentrarScroll(pasosTutorial[tutorialPaso], resaltadosTutorial[tutorialPaso]);
				if(resaltadosTutorial[tutorialPaso] != '')
					jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', '520');
			}
		});

		jQuery(document).on('click', '.tut-paso-siguiente', function(e)
		{
			if(tutorialPaso < (pasosTutorial.length - 2))
			{
				if(resaltadosTutorial[tutorialPaso] != '')
					jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', 'auto');
				if(tutorialPaso == 3 || tutorialPaso == 5)
				{
					enTutorial = false;
					alternarMenuArriba(0); // jQuery('#centro-arriba ul.enlaces').slideFadeToggle('fast','linear');
					enTutorial = true;
				}
				jQuery(pasosTutorial[tutorialPaso]).fadeOut('fast');
				tutorialPaso++;
				CentrarScroll(pasosTutorial[tutorialPaso], resaltadosTutorial[tutorialPaso]);
				jQuery(pasosTutorial[tutorialPaso]).fadeIn('slow');
				if(resaltadosTutorial[tutorialPaso] != '')
					jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', '520');
			}
			else if(tutorialPaso < (pasosTutorial.length - 1))
			{
				jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', 'auto');
				jQuery(pasosTutorial[tutorialPaso]).fadeOut('fast');
				tutorialPaso++;
				jQuery('body,html').animate({scrollTop: 0}, 1000);
				jQuery(pasosTutorial[tutorialPaso]).fadeIn('slow');
			}
		});

		jQuery(document).on('click', '.tut-reiniciar', function(e){
			if(resaltadosTutorial[tutorialPaso] != '')
				jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', 'auto');
			jQuery(pasosTutorial[tutorialPaso]).fadeOut('fast');
			jQuery('#tut-inicio').fadeIn('slow');
			jQuery('body').animate({scrollTop: 0});
			tutorialPaso = 0;			
		});

		jQuery(document).on('click', '.tut-cerrar', function(e){
			cerrarTutorial();
		});
		
		cerrarTutorial = function()
		{
			if(resaltadosTutorial[tutorialPaso] != '')
				jQuery(resaltadosTutorial[tutorialPaso]).css('z-index', 'auto');
			jQuery(pasosTutorial[tutorialPaso]).fadeOut('fast');
			jQuery('body,html').scrollTop(0);
			OcultarOverlay();
			enable_scroll();
			tutorialPaso = 0;
			enTutorial = false;
			if(jQuery('#centro-arriba ul.enlaces').is(':visible'))
				alternarMenuArriba();
			jQuery(document).everyTime(10000, 'relojPanelesPortada', function(i) {
				iniciarContadorPizarra();
				iniciarContadorNoticias();
				iniciarContadorEventos();

			});
			setCookie('_tutorialUCR3_2014', 0, 9999);
		}	
	
		jQuery('#menu-pie li a').click(function (){ 
			if(jQuery(this).attr('href') == 'http://www.ucr.ac.cr/#tutorial')
				iniciarTutorial();
		});

		
						if(hash == 'tutorial' && true)
			iniciarTutorial();
	});	
	

-->
</script> 
