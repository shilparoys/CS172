	<!--	
	var primerLogueo = true;
	var logueando = false;
	var buscando = false;
	var compartiendo = false;
	var escenario = false;
	var ventana_fb = null;
	var ventana_tw = null;
	var enTutorial = false;
	
		function loguear(comentando, retorno)
		{
			var enComentario = '';
			if(!jQuery('ul.funciones .login').is(':visible'))
			{
				jQuery('ul.funciones li.menu-loguear').addClass('activo');
				jQuery('ul.funciones .login').slideFadeToggle('fast', 'linear', 0);
