			{
				jQuery('#comentando_a').css('display', 'block');
				enComentario = '?r=1425261014!comentando'
			}
			else
			{
				jQuery('#comentando_a').css('display', 'none');
			}		
			if(primerLogueo)
			{
				if(retorno != '')
					jQuery('#iFrameLogueo').attr('src', ('https://odi.ucr.ac.cr/loguear.php?oscuro=true&regreso=' + retorno + enComentario));
