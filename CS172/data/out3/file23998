  <script>
						$(document).ready(function() {
				HZ.questions.ViewQuestion.init({"canEditOrDelete": 0});

				if (!UIHelper.isMobileDevice) {
					// make me sticky, oh no, it is parkable
					$moreQuestions = $('.moreQuestions');
					if ($moreQuestions.length > 0 // check more question section exist
						&& $(window).height() > $moreQuestions.outerHeight() + $("#footerContainer").outerHeight() + 61 // check window height is higher than more question section
						&& $(".leftSideContentWithAd").outerHeight() > $(".adSideBar").outerHeight()) // check body left side is higher than more question section
					{
						$moreQuestions.parkable({floatStarts: $moreQuestions.position().top + parseInt($("#mainContent").css("padding-top")) - 61});
					}
				}
			});
		</script> 
