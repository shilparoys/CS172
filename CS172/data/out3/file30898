   <script type="text/javascript">
//configuration
OAS_listpos = 'Leaderboard,Rectangle,SecSponsor,SiteSponsor,TxtLink1,TxtLink2,TxtLink3,TxtLink4,ImpactAd,RectangleBelow';
OAS_url = "http://ads.nj.com/RealMedia/ads/";
OAS_sitepage = "www.nj.com/forums/index.ssf";
OAS_target = "_top";
OAS_query = "rules.html";
//end of configuration

function OAS_AD (pos) {
  if (OAS_version >= 11) OAS_RICH(pos);
  else OAS_NORMAL(pos);
}

OAS_version = 10;
OAS_rn = new String(Math.random()); 
OAS_rns = OAS_rn.substring(2, 11);
function OAS_NORMAL(pos) {
  document.write('<a href="' + OAS_url + 'click_nx.cgi/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" target=' + OAS_target + '>');
  document.write('<img sr' + 'C="' + OAS_url + 'adstream_nx.cgi/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" border=0 /></a>');
}

OAS_version = 11;
if (navigator.userAgent.indexOf('Mozilla/3') != -1 || navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1) OAS_version = 10;
if (OAS_version >= 11) document.write('<script language=JavaScript1.1 sr' + 'C="' + OAS_url + 'adstream_mjx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '?' + OAS_query + '"><\/SCRIPT>');
</script> 
