  <script type="text/javascript"> 
// uses JQuery framework...

function animSlide() {

    var $ImageActive = $('#slideshow IMG.active');
	
    if ( $ImageActive.length == 0 ) $ImageActive = $('#slideshow IMG:last');

    var $ImageNext =  $ImageActive.next().length ? $ImageActive.next()
        : $('#slideshow IMG:first');
 
 


    $ImageActive.addClass('last-active');

    $ImageNext.css({opacity: 0.0})
        .addClass('active')
        .animate({opacity: 1.0}, 1000, function() {
            $ImageActive.removeClass('active last-active');
			
        });
}
 
 
   var timerId = null;    
   
   function startRotation() {         
	   if (timerId) {             return;         }       
	   timerId = setInterval('animSlide()', 6000);   
   }   
   
   /*
   function reStartRotation() {         
	   if (timerId) {             return;         }       
	   timerId = setInterval('animSlide()', 2000);   
   }   
   */
   
   function stopRotation() {         
	   if (!timerId) {             return;         }    
	   clearInterval(timerId);       
   	   timerId = null;     
   } 


$(function() {
	startRotation(); 
	$('#slideshow IMG').hover(stopRotation,startRotation); });

</script> 
