    _CF_checkLocateCall = function(_CF_this)
    {
        //reset on submit
        _CF_error_exists = false;
        _CF_error_messages = new Array();
        _CF_error_fields = new Object();
        _CF_FirstErrorField = null;

        //form element StreetAddressFrom required check
        if( !_CF_hasValue(_CF_this['StreetAddressFrom'], "TEXT", false ) )
        {
            _CF_onError(_CF_this, "StreetAddressFrom", _CF_this['StreetAddressFrom'].value, "The Travel From field is required and should only contain letters, numbers, periods and ampersands");
            _CF_error_exists = true;
        }

        //form element StreetAddressTo required check
        if( !_CF_hasValue(_CF_this['StreetAddressTo'], "TEXT", false ) )
        {
            _CF_onError(_CF_this, "StreetAddressTo", _CF_this['StreetAddressTo'].value, "The Travel To field is required and should only contain letters, numbers, periods and ampersands");
            _CF_error_exists = true;
        }

        //form element Time required check
        if( _CF_hasValue(_CF_this['Time'], "TEXT", false ) )
        {
            //form element Time 'REGEX' validation checks
            if (!_CF_checkregex(_CF_this['Time'].value, /^(0[1-9]|1[012]|[0-9]):([0-5][0-9])$/, true))
            {
                _CF_onError(_CF_this, "Time", _CF_this['Time'].value, "You must enter a time in the following format i.e. 08:42");
                _CF_error_exists = true;
            }

        }else {
            _CF_onError(_CF_this, "Time", _CF_this['Time'].value, "You must enter a time in the following format i.e. 08:42");
            _CF_error_exists = true;
        }

        //form element datepicker required check
        if( _CF_hasValue(_CF_this['datepicker'], "TEXT", false ) )
        {
            //form element datepicker 'DATE' validation checks
            if (!_CF_checkdate(_CF_this['datepicker'].value, true))
            {
                _CF_onError(_CF_this, "datepicker", _CF_this['datepicker'].value, "Date is required and must be properly formatted");
                _CF_error_exists = true;
            }

        }else {
            _CF_onError(_CF_this, "datepicker", _CF_this['datepicker'].value, "Date is required and must be properly formatted");
            _CF_error_exists = true;
        }


        //display error messages and return success
        if( _CF_error_exists )
        {
            if( _CF_error_messages.length > 0 )
            {
                // show alert() message
                _CF_onErrorAlert(_CF_error_messages);
                // set focus to first form error, if the field supports js focus().
                if( _CF_this[_CF_FirstErrorField].type == "text" )
                { _CF_this[_CF_FirstErrorField].focus(); }

            }
            return false;
        }else {
            // run userdefined onSubmit javascript. 
            return checkForm()
            return true;
        }
    }
