  <script>
function flipMoreLess(e) {
	var t = YAHOO.util.Event.getTarget(e);
	var str = t.id;
	var el = "";
	var mid = "";
	if(str.match(/^showMore/)) {
		mid = str.replace("showMore_", "");
		el = new YAHOO.util.Element(t.id + '_Content');
		el.removeClass('hideMore');
		el = new YAHOO.util.Element(t.id);
		el.addClass('hideMore');
		el = new YAHOO.util.Element("showLess_" + mid);
		el.removeClass('hideMore');
	}else{
		mid = str.replace("showLess_", "");
		el = new YAHOO.util.Element("showMore_" + mid);
		el.removeClass('hideMore');
		el = new YAHOO.util.Element(t.id);
		el.addClass('hideMore'); 
		el = new YAHOO.util.Element("showMore_" + mid + '_Content');
		el.addClass('hideMore'); 
	}
}
YAHOO.util.Event.onDOMReady(
	function() {
		var moreLessEls = YAHOO.util.Dom.getElementsByClassName("showMore", "span");
		if(moreLessEls.length > 0) {
			YAHOO.util.Event.addListener(moreLessEls, "click", flipMoreLess);
		}
	}
);
</script>  
