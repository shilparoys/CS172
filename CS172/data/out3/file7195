     <script>
        $(document).ready(function () {

            $( ".schedule" ).each(function () {
               
                var currentdate = new Date();
                var formatdate = (currentdate.getMonth()+1)+"-"+currentdate.getDate()+"-"+currentdate.getFullYear();

                var enddate = $(this).attr("title");
           
                if (formatdate >= enddate)
                {
                    $( this ).remove();
                }
            });
        });
</script> 
