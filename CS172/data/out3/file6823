	var pubTimestamp = 1425258010;

	function reloadIfExpired(timestamp)
	{
		var validityPeriod = 12 * 3600; // 12 hours
		//var validityPeriod = 12; // For debugging only

		var date = new Date();

		var currentTimestamp = Math.floor(date.getTime() / 1000); // Convert ms to s and round to next smallest integer

		//alert("pubTimestamp='" + pubTimestamp + '", currentTimestamp='" + currentTimestamp + "'");

		if (timestamp <= ( currentTimestamp - validityPeriod)) {
			location.reload(false); // forceGet=true => the current page must be reloaded from the server
						// It still reloads the HTML file if forceGet=false (at least on my workstation),
						// but also reuses some content from the cache (e.g. images). So this was
						// configured as a small optimization.
			//alert("Page reloaded!"); // For debugging only
		}
	}

      reloadIfExpired(pubTimestamp);
</script> 
