      <script type="text/javascript">
    $('document').ready(function () {
        $('#PAUbugun-container').quickFlip();

        $('#PAUbugun-navigation li a').each(function () {
            $(this).click(function () {
                $('#PAUbugun-navigation li').each(function () {
                    $(this).removeClass('selected');
                });
                $(this).parent().addClass('selected');
                var flipid = $(this).attr('id').substr(4);
                $('#PAUbugun-container').quickFlipper({}, flipid, 1);

                return false;
            });
        });
    });
</script> 
