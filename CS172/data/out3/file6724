  <script type="text/javascript">
//<![CDATA[
 
function loadPromoCarousel(){
    var promos = (($('#promo-banner li').length));
    var featureArray = new Array();
    

    $('#promo-banner li[data-feature="Yes"]').each(function(){
      featureArray.push($(this));
    });

    var featurePaginationItemSlideTo = 0;

    if (featureArray.length) { 
      $('#promo-banner li').each(function(){
        $(this).removeClass('active');
      });
      var featureItem = featureArray[Math.floor(Math.random()*featureArray.length)];

      $(featureItem).addClass('active');
      featurePaginationItemSlideTo = featureItem.data('slideTo'); //hyphen in data becomes camel case when getting
    }

    for (i = 0; i < promos; i++) {
        var pagiClass = (i == featurePaginationItemSlideTo ? 'active' : '');
        $('.pagination').append('<span class="'+pagiClass+'" data-target="#promo-banner" data-slide-to="'+i+'">'+(i+1)+'</span>');
    }
    
    $('.pagination [data-feature="'+featurePaginationItemSlideTo+'" ]').addClass('active');

    $('.promo-control-prev').on( "click", function() {
        $('.carousel').carousel('prev');
    });
    $('.promo-control-next').on( "click", function() {
        $('.carousel').carousel('next');
    });

  // Initialize fancy box for lightbox viewing
  $("#promo-banner a.video").fancybox({
    'width'  : 576,
    'height' : 435
  });
}

loadPromoCarousel();

//]]>
</script> 
