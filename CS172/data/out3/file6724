// Mobile Javascript functions

pageHasMobileMetadata = 'corpsite-home';
pageIsDebug = null;
pageIsDebug = getParameterByName('viewMobile');
textFromTitle = 'QUT Corporate Site';

if (checkIfEasyEdit()) {
    if (isCoursesPages()) {
        globalDoMobile('course');
    } else if (pageIsDebug == 'true') {
        globalDoMobile('debug');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'standard') {
        globalDoMobile();
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'corpsite-home') {
        globalDoMobile('corpsite-home');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'significant') {
        globalDoMobile('significant');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'section-home') {
        globalDoMobile('section-home');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'landing-page') {
        globalDoMobile('landing-page');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'corpsite-home-new') {
        globalDoMobile('corpsite-home-new');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'standard-responsive') {
        globalDoMobile('standard-responsive');
    } else if (pageHasMobileMetadata && pageHasMobileMetadata == 'significant-responsive') {
        globalDoMobile('significant-responsive');
    } else if (pageHasMobileMetadata == '' || pageHasMobileMetadata == 'no') {
        $(function() {
            $('header, footer').css({width:'100%','min-width':'1100px'});
        });
    }
}
