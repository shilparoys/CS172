  <script type="text/javascript">
$(document).ready(function () {
  function loadAsset(url, selector) {
    var $container = $(selector);
    var onAjaxSuccess = function (data) {
      var $asset = $(data);
      var $images = $asset.find("img");
      if ($images.length) {
        $images.load(function () {
          $images.removeAttr("data-src");
          addToDom($asset);
        }).attr("src", $images.data("src"));
      } else {
        addToDom($asset);
      }
    };
    var addToDom = function ($asset) {
      var _gaq = window._gaq || [];
      var $assetli = $asset.children("li");
      if ($asset.is("ul") && $assetli.length == 1) {
        $container.html($assetli.contents());
      } else {
        $container.html($asset);
      }
      _gaq.push(["_trackEvent", "mega-menu", selector, $asset.data("news-event-id") || "0"]);
    };
    $.ajax({
      url: url,
      success: onAjaxSuccess
    });
    $container.html("<p>Loading..</p>");
  }
  function onNewsMegaMenuOpen() {
    loadAsset("https://www.qut.edu.au/_designs/nested-content-that-needs-a-url/nested-content/news-rss/news-mega-menu-featured-listing", "#featured-news-ajax");
    loadAsset("https://www.qut.edu.au/_designs/nested-content-that-needs-a-url/nested-content/events/news-mega-menu-events-listing", "#event-listing-ajax");
  }
  if ($("#news-mega-menu").parent("li:hover").length) {
    onNewsMegaMenuOpen();
  } else {
    $("#news-mega-menu").parent("li").one("mouseenter", onNewsMegaMenuOpen);
  }
});
</script> 
