      font-weight: bold;
      color: white;
   }
</style> 
  <script type="text/javascript">
//<![CDATA[
document.write([
  '<script src="//maps.googleapis.com/maps/api/js?sensor=false&amp;key=', {
    'qut.edu.au': 'AIzaSyB9XxMmsMyJX3gslLJeiWUGZHctg5KaRbA',
    'www.qut.edu.au': 'AIzaSyB9XxMmsMyJX3gslLJeiWUGZHctg5KaRbA',
    'student.qut.edu.au': 'AIzaSyDeUlp1p82sXab6edw0bOKMkl1S3ZvJVFE',
    'www.student.qut.edu.au': 'AIzaSyDeUlp1p82sXab6edw0bOKMkl1S3ZvJVFE',
    'intranet.qut.edu.au': 'AIzaSyCnRcUTDSYI5sY9jGa9tws28hW41C6WQp4',
    'www.intranet.qut.edu.au': 'AIzaSyCnRcUTDSYI5sY9jGa9tws28hW41C6WQp4'
  }[window.location.host],
  '" type="text/javascript"><\/script>'
].join(''));

//document.write('<script type="text/javascript" src="https://www.qut.edu.au/__data/assets/js_file/0005/311594/labeledmarker_packed.js"><\/script>');   <!-- labelled marker -->
//document.write('<script type="text/javascript" src="https://www.qut.edu.au/__data/assets/js_file/0020/311474/gmaps_util.js"><\/script>');   <!-- gmaps util -->
//]]>
</script> 
  <script type="text/javascript" src="https://www.qut.edu.au/__data/assets/js_file/0005/311594/labeledmarker_packed.js"></script> 
  <script type="text/javascript" src="https://www.qut.edu.au/__data/assets/js_file/0020/311474/gmaps_util.js"></script> 
  <script type="text/javascript">
//<![CDATA[
// Get XML file for markers data
$('.campus-info').hide();
$('#campus-Gardens_Point').show();

if ($.browser.msie) {
    if (parseInt($.browser.version, 10) <= 7) {
        $.ajax({
            type: "GET",
            url: "https://www.qut.edu.au/__data/assets/xml_file/0006/25719/markers.xml?ts=" + Math.floor(new Date().getTime() / 5000),
            dataType: "xml",
            success: parseXml
        });
    } else {
        $.ajax({
            type: "GET",
            url: "https://www.qut.edu.au/__data/assets/xml_file/0006/25719/markers.xml?&1",
            dataType: "xml",
            success: parseXml
        });
    }
} else {
    $.ajax({
        type: "GET",
        url: "https://www.qut.edu.au/__data/assets/xml_file/0006/25719/markers.xml",
        dataType: "xml",
        success: parseXml
    });
}

var markersData = new Array();

//Once XML file retrieved, add each marker record to an array
function parseXml(xml) {
    //find every Marker and add the lat and long
    $(xml).find("marker").each(function () {
        if (($(this).attr("title").match(/(Block$|Block Wing)/i)) || $(this).attr("iconLabel")) {
            var titleLabel = ($(this).attr("iconLabel") != null ? $(this).attr("iconLabel") : $(this).attr("title")).replace(/(Block$|Block Wing)/i, "").replace(/\s/g, "");
            markersData.push({
                latitude: $(this).attr("lat"),
                longitude: $(this).attr("lng"),
                labelText: titleLabel
            });
        }
    });
    //Once array has been constructed, add the google map
    loadGoogleMap('-27.477207', '153.02830', 17);
}

var gMap;
var scrollTimer;
function loadGoogleMap(latitude, longitude, depth) {
    // Basic map options
    var mapOptions = {
        center: new google.maps.LatLng(latitude, longitude),
        zoom: depth,
        mapTypeId: google.maps.MapTypeId.ROADMAP,
        mapTypeControl: false, // Hides map type selection
        scrollwheel: false
    };
    
    gMap = new google.maps.Map(document.getElementById("map_canvas"),
        mapOptions);

    // Following code reduces accidental scrolling/zooming of map whilst scrolling page.
    // Catch scroll event on mobile
    window.onscroll = function (event) {
        gMap.setOptions({scrollwheel: false});
        if (scrollTimer) window.clearTimeout(scrollTimer);
        scrollTimer = window.setTimeout(setScrollableMap, 500);
    }

    // For desktops use mouseover intent
    google.maps.event.addListener(gMap, 'mouseover', function() {
        scrollTimer = window.setTimeout(setScrollableMap, 200);
    });

    google.maps.event.addListener(gMap, 'mouseout', function() {
        if (scrollTimer) window.clearTimeout(scrollTimer);
        gMap.setOptions({scrollwheel: false})
    });

    var buildingIcon =  "https://www.qut.edu.au/__data/assets/image/0006/350583/blue-pin-blank.png";
    $(markersData).each(function () {
        var wOffset = this.labelText.length > 1 ? (this.labelText.length > 2 ? 10.5 : 7) : 3.5;

        new MarkerWithLabel({
           position: new google.maps.LatLng(this.latitude, this.longitude),
           draggable: false,
           map: gMap,
           icon: buildingIcon,
           labelContent:  this.labelText,
           labelAnchor: new google.maps.Point(wOffset, 30.5),
           labelClass: "LabeledMarker_iconMarker" // the CSS class for the label
         });
    });
}

function setScrollableMap() {
   gMap.setOptions({scrollwheel: true})
}

// Handler for clicks in campus buttons
function showMap(theMapId) {
    $('.campus-info').hide();
    //*******Gardens Point*******
    if (theMapId == 'gp-map-link') {
        $('#campus-Gardens_Point').show();
        loadGoogleMap('-27.477207', '153.02830', 17);
        $('#campus-map .view-full-map').attr('href', 'https://www.qut.edu.au/campuses-and-facilities/gardens-point-campus/maps-and-getting-here');
        //*******Kelvin Grove*******
    } else if (theMapId == 'kg-map-link') {
        $('#campus-Kelvin_Grove').show();
        loadGoogleMap('-27.451589', '153.015861', 16);
        $('#campus-map .view-full-map').attr('href', 'https://www.qut.edu.au/campuses-and-facilities/kelvin-grove-campus/maps-and-getting-here');
        //*******Caboolture*******
    } else if (theMapId == 'cab-map-link') {
        $('#campus-Caboolture').show();
        loadGoogleMap('-27.07375', '152.96005', 17);
        $('#campus-map .view-full-map').attr('href', 'https://www.qut.edu.au/campuses-and-facilities/caboolture/maps-and-getting-here');
        //******* Gateways Gardens Point *******
    } else if (theMapId == 'gw-gp-map-link') {
        $('#campus-Gardens_Point').show();
        loadGoogleMap('-27.477207', '153.02830', 17);
        $('#campus-map .view-full-map').attr('href', 'https://www.student.qut.edu.au/about/campuses/gardens-point');
        //******* Gateways Kelvin Grove *******
    } else if (theMapId == 'gw-kg-map-link') {
        $('#campus-Kelvin_Grove').show();
        loadGoogleMap('-27.451589', '153.015861', 16);
        $('#campus-map .view-full-map').attr('href', 'https://www.student.qut.edu.au/about/campuses/kelvin-grove');
        //******* Gateways Caboolture *******
    } else if (theMapId == 'gw-cab-map-link') {
        $('#campus-Caboolture').show();
        loadGoogleMap('-27.07375', '152.96005', 17);
        $('#campus-map .view-full-map').attr('href', 'https://www.student.qut.edu.au/about/campuses/caboolture');
    }
}

//add onclicks for campus tabs
$("#campus-map li a").click(function () {
    $("#campus-map li").removeClass();
    $(this).parent().addClass('current');
    showMap($(this).parent().attr('id'));
    return false;
});;

//add hover for promo
$(".promo-link").hover(function () {
    $(".promo-heading").addClass('hover');
}, function () {
    $(".promo-heading").removeClass('hover');
});

//Add lightbox effect to campus thumbnails
var groupCount = 1;
$('.campus-info').each(function () {
    $(this).find('a').attr('rel', 'group-' + groupCount);
    groupCount++;
});
$('.campus-info a.fancy').fancybox();

//]]>
</script> 
