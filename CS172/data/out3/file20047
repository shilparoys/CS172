        <div class="lfr-portlet-column" id="layout-column_column-2"> 
         <div id="p_p_id_56_INSTANCE_X4ya_" class="portlet-boundary portlet-boundary_56_  portlet-journal-content"> 
          <a id="p_56_INSTANCE_X4ya"></a> 
