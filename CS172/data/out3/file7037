  <script language="javascript">
	function redirect(url)
	{
		window.open(url)  ;
		//return false;
	}
	var id =0;
	function showId(id)
	{   // alert(id);
		document.getElementById(id).style.display = 'block';
		//obj22.style.display = 'block';
		//return false;
	}
	function hideId(id)
	{
		var obj = document.getElementById(id);
		obj.style.display = 'none';
		//return false;
	}
</script> 
