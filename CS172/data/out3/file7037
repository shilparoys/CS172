  <script language="JavaScript">
var tdid  = "";
var countb =0;
function showdiv(tdid,countb){
  //alert(tdid);
   newImage2 = "url(images/btn22.png)";
  var i =0;
  var divid = 'div'+tdid;
  //alert(divid);
  var divid2= "";
  for(i=1;i<=countb;i++){
     var nn = 'b'+i;
    // alert(nn);
    // alert(tdid);
     if(nn !=tdid){
     divid2 = 'div'+nn;
    // alert(divid2);
     document.getElementById(nn).style.backgroundImage= newImage2;
      document.getElementById(divid2).style.display="none";
     }
  }
  newImage = "url(images/btn22r.png)";
  //alert(tdid);
  document.getElementById(tdid).style.backgroundImage= newImage;
  //alert(divid);
   document.getElementById(divid).style.display="block";

}//showdiv('b')

 var tdid2  = "";
function showdiv2(tdid2){
 // alert(tdid2);
   newImage2 = "url(images/btn22.png)";
  var i =0;
  var divid = 'div'+tdid2;
 // alert(divid);
  var divid2= "";
  for(i=1;i<4;i++){
     var nn = 'b2'+i;
    // alert(nn);
     if(nn !=tdid2){
     divid2 = 'div'+nn;
     //alert(divid2);
     document.getElementById(nn).style.backgroundImage= newImage2;
      document.getElementById(divid2).style.display="none";
     }
  }
  newImage = "url(images/btn22r.png)";
  document.getElementById(tdid2).style.backgroundImage= newImage;
   document.getElementById(divid).style.display="block";

}//showdiv2('b')


var picch ="";
var idvar = "";
function chgpic(picch,idvar){
  //alert(picch);
  document.getElementById(idvar).src= 'images/'+picch+'.png';
}
function chgpic2(picch,idvar){
  document.getElementById(idvar).src= 'images/'+picch+'.png';
}

