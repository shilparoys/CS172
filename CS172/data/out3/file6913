					if (elOpenBo3_2 == true) {
						$('#ctrl_Gubun3_2 li div').removeClass('active');
						$('#div_Gubun3_2').slideUp('fast');
						elOpenBo3_2 = false;
					}
					else {
						$('#ctrl_Gubun3_2 li div').addClass('active');
						$('#div_Gubun3_2').slideDown('fast');
						elOpenBo3_2 = true;
					}
				});
				//-->
				</script> 
    </div> 
   </div>
   <!-- /#fnav --> 
