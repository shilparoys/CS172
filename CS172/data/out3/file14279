  <script src="http://www.unomaha.edu/_files/js/jquery.colorbox-min.js"></script> 
  <script>
	$(document).ready(function(){
		$(".iframe").colorbox({iframe:true, width:"80%", height:"80%"});
	});
</script> 
  <script>
	$(document).ready(function(){
		$(".youtube").colorbox({iframe:true, innerWidth:425, innerHeight:268});
	});
</script> 
  <script>
//MODAL-MAKER
;(function($) {
    'use strict';
    var addColorBox = function($el, w, h, useIframe) {
    	$el.colorbox({
			iframe: useIframe, 
			innerWidth: 900, 
			innerHeight: h
		});
	};

	$(function() {
		// youtube non mobile
		$('#content #copy_main .modal-maker').on('click', function() {			
			addColorBox($(this), true);
		});
	});
})(jQuery);
</script> 
