              <li class="feed-item linkedin_share" data-config="{mid:'company:3084'}" data-li-update-position="9" data-li-update-token="0-1425313948643-56481b9ad3f02863708495b1e103c948|13|5970623635839279104" data-li-update-date="1423507603624" data-li-update-id="5970623635839279104">
