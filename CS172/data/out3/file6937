                              <td style="width: 605px" valign="top"> <script language="javascript">

    var Showed=null;
    var N1=null;
    var N2=null;
    var ShowedID=1;
    
   
    
    var Current="N2";
    
      function BiganFade()
    {
    
    N1= document.getElementById("N1");
    N2= document.getElementById("N2");
    
    Showed=N1;
    ShowedID=1;
    
  
    reset();
   
   
    }
    
      
    function autoSwitch()
    {  

    switchTabs(Current);
    
    
//    if(Current=="N1")
//    {
//    switchTabs("N2");
//    }
//    else
//    {
//    switchTabs("N1");
//    }
    
    
    }
    
    
    
    
    function BiganFade2()
    {
    window.setInterval(autoSwitch,10000);
    }
    
   
   var toLost=null;
   var lostValue=100;
   var findValue=0;
   var toFind=null;
   var gdInterval,guInterval=null;
   function switchTabs(me)
   {
   
   try
   {
  window.clearInterval(gdInterval);
  window.clearInterval(guInterval);
  }
  catch(e)
  {
  }

   N1= document.getElementById("N1");
   N2= document.getElementById("N2");
       
   if(me=="N1")
   {
    Current="N2";
    toLost=N2;
    toFind=N1;
    var obj=document.getElementById("Link101");
//    obj.style.fontWeight='bold';
//    obj.style.fontSize='12pt';
    obj.style.background= 'url(images/p1.jpg) repeat-x center center';
//    document.getElementById("Link102").style.fontWeight='normal';
//    document.getElementById("Link102").style.fontSize='10pt';
    document.getElementById("Link102").style.background= 'url(images/p2.jpg) repeat-x center center';
   }
   else
   {
   Current="N1";
   var obj=document.getElementById("Link102");
//   obj.style.fontWeight='bold';
//   obj.style.fontSize='12pt';
   obj.style.background= 'url(images/p1.jpg) repeat-x center center';
//  document.getElementById("Link101").style.fontWeight='normal';
//  document.getElementById("Link101").style.fontSize='10pt';
  document.getElementById("Link101").style.background= 'url(images/p2.jpg) repeat-x center center';
  
   toLost=N1;
   toFind=N2;
   }
    toLost.style.filter = 'alpha(opacity=100)';
     try
    {
    toLost.style.MozOpacity=100;
    }
    catch(e)
    {
    }
    gdInterval=window.setInterval(goDown,100);
   } 
   
    
    function goDown()
    {
    lostValue-=10;
    try
    {
    toLost.style.MozOpacity=lostValue;
    }
    catch(e)
    {
    }
    toLost.style.filter = 'alpha(opacity=' + lostValue + ')';
    if(lostValue<=0)
    {
    lostValue=100;
    window.clearInterval(gdInterval);
    toLost.style.display="none";
    toFind.style.display="block";
    toFind.style.filter = 'alpha(opacity=0)';
     try
    {
    toFind.style.MozOpacity=0;
    }
    catch(e)
    {
    }
    guInterval=window.setInterval(goUp,100);
    }
    
    }
    
    
    
    function goUp()
    {
    findValue+=10;
    try
    {
    toFind.style.MozOpacity=findValue;
    }
    catch(e)
    {
    
    }
    toFind.style.filter = 'alpha(opacity=' + findValue + ')';
    if(findValue>=100)
    {
    findValue=0;
    window.clearInterval(guInterval);
    }
    
    }
    
    
    
    
    
    function reset()
    {
    VTSF=clearInterval(VTSF);
    VTHF=clearInterval(VTHF);
    if(Tv!=0)
      Tv=0;
   ShowFade();
  // Rtimer=window.setInterval(ShowFade,15000);
    }
    
