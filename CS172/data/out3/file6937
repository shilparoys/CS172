                              <div id="UniversitysTab" style="width: 610px; height: 200px;display:none; text-align: center;"> 
                               <script language="javascript">
var PlayFlag=true;
function PusePlay(obj)
{

if(PlayFlag)
{
document.getElementById("MQT").stop();
obj.src="MQButtons/1.gif";
PlayFlag=false;
}
else
{
document.getElementById("MQT").start();
obj.src="MQButtons/2.gif";
PlayFlag=true;
}

}



function MoveUP()
{
document.getElementById("MQT").setAttribute("direction","up");

}


function MoveDown()
{
document.getElementById("MQT").setAttribute("direction","down");

}

</script> 
