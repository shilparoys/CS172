    var VTSF=null;
    var VTHF=null;
    var Tv=0;
    function hideFade()
    {
    
   VTHF=window.setInterval("THF()",75);
    
    }
    
    function TSF()
    {
    if(Tv<100)
    {
    Tv+=10;
    Showed.style.filter = 'alpha(opacity=' + Tv + ')';
    }
    else
    {
    window.setTimeout("hideFade()",12000);
    clearInterval(VTSF);
    }
    }
    
    
    function ShowFade()
    {
     VTSF=window.setInterval("TSF()",75);
    }
    
    function THF()
    {
    if(Tv>0)
    {
    Tv-=10;
    Showed.style.filter = 'alpha(opacity=' + Tv + ')';
    }
    
    else {   
        if(ShowedID==1)
         {
           N1.style.display="none";
           N2.style.display="block";
           Showed=N2;
           ShowedID=2;
         }
            else
         {  
           N2.style.display="none";
           N1.style.display="block";
           Showed=N1;
           ShowedID=1;
         }
         VTSF=clearInterval(VTSF);
         VTHF=clearInterval(VTHF);
        reset();
        
        
        
         } 
    
    }
    
    function tst()
    {
    VTSF=clearInterval("VTSF");
         VTHF=clearInterval("VTHF");
    }
    
    
