                               <script language="javascript">
function CloseAllLoginExceptMe(id,id2)
{
document.getElementById('EmployeeLoginTab').style.display="none";
document.getElementById('StudentLoginTab').style.display="none";
document.getElementById('HospitalLoginTab').style.display="none";
