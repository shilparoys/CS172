function CloseAllTabsExceptMe(id,id2)
{
document.getElementById('PotralLoginTab').style.display="none";
document.getElementById('ScholershipsTab').style.display="none";
document.getElementById('ConfaranceEventsTap').style.display="none";
document.getElementById('WebPartnersTab').style.display="none";
document.getElementById('UniversitysTab').style.display="none";
document.getElementById('JobCornerTab').style.display="none";
document.getElementById(id).style.display="block";

document.getElementById('t1').className='InActiveTab';
document.getElementById('t2').className='InActiveTab';
document.getElementById('t3').className='InActiveTab';
document.getElementById('t4').className='InActiveTab';
document.getElementById('t5').className='InActiveTab';
document.getElementById('t6').className='InActiveTab';
document.getElementById(id2).className='ActiveTab';


}

</script> 
