  // Send event on nav menu link
  $(function() {
    $("nav a").click(function() {
      var href = this.href;
      var nav_done = false;
      var do_nav = function() { if (!nav_done) { document.location = href; nav_done = true; } };
      setTimeout(do_nav, 100);
      ga('send', 'event', 'nav_menu', 'click', href, { hitCallback: do_nav });
      return false;
    });
  });

