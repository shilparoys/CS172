          function readCookie(name) {
	            var nameEQ = name + "=";
	            var ca = document.cookie.split(';');
	            for(var i=0;i < ca.length;i++) {
		            var c = unescape(ca[i]).replace(/\+/g, " ");
		            while (c.charAt(0)==' ') c = c.substring(1,c.length);
		            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	            }
	            return null;
          }
           
          function ReadLastSearchSEO()
          {  
              var lastSearchCookie = readCookie("NYTRELastSearch");
              if(lastSearchCookie)
              {
                var arrLastSearchCookie = lastSearchCookie.split('&');
                for(var i=0; i<arrLastSearchCookie.length; i++)
                {
                    var keyvalue = arrLastSearchCookie[i].split("=");
                    if(keyvalue[0]=="LocationLastSearch")
                    {
                        var lastsearchvalues = keyvalue[1]
                        if( lastsearchvalues != null) 
                        {
                            if(lastsearchvalues.split("::").length>1)
                            {
                                return lastsearchvalues.split("::")[1];
                            }
                            else
                            {
                                return  lastsearchvalues;
                            }
                            
                        }
                    }
                }
                
              }
              return null;
          }         
         
    
        
