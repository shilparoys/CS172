         <li style="display:none;"> <script type="text/javascript">
var SignInOrRegister = "";
function ChangeUserAction()
{
    var PageName = '/my/saved_listings.aspx';
    if(PageName.toLowerCase().indexOf("/detail.aspx") == 0)
    {
        ChangeDetailUserAction();
    }
    else if(PageName.toLowerCase().indexOf("/results.aspx") > -1 || PageName.toLowerCase().indexOf("/resultscompare.aspx") > -1)
    {
        ChangeResultUserAction(PageCache.ListingIds);
    }
    else if(IsDefaultPage())
    {
        window.location.reload(true);
    }
}
function ShowMyList(showMY)
{
    //SAML - this function is no longer needed
    //return;
    var PageName = '/my/saved_listings.aspx';
    var rhtml= "";
    
    if(showMY)
    {
        if(PageName.toLowerCase().indexOf("/results.aspx") > -1) 
        {
            //remove compare cookie
            //RemoveCompareAll(); - called in ToSaved in personalization - Irina
            rhtml += '<li class="my_listings"><a href=javascript:ToSaved("listings","results","' + GetPageName() + '"); target="_parent">My Listings</a> ('+ readListingCountPersCookie() + ')</li>';
            rhtml += '<li class="last"><a href=javascript:ToSaved("searches","results","' + GetPageName() + '"); target="_parent">My Searches</a> ('+ readSearchCountPersCookie() + ')</li>';
            //rhtml += '<li class="last"><a href="javascript:void(0);" onclick="SignOut();" target="_parent">Sign Out</a></li>';
        }
        else if(PageName.toLowerCase().indexOf("/detail.aspx") > -1) 
        {
            //compare cookie is never on detail page if we come from results
            //if we came from compareresults cookie exists - we should leave it
            rhtml += '<li class="my_listings"><a href=javascript:ToSaved("listings","details","' + GetPageName() + '"); target="_parent">My Listings</a> ('+ readListingCountPersCookie() + ')</li>';
            rhtml += '<li class="last"><a href=javascript:ToSaved("searches","details","' + GetPageName() + '"); target="_parent">My Searches</a> ('+ readSearchCountPersCookie() + ')</li>';
            //rhtml += '<li class="last"><a href="javascript:void(0);" onclick="SignOut();" target="_parent">Sign Out</a></li>';
        }
        else if(PageName.toLowerCase().indexOf("/resultscompare.aspx") > -1) 
        {
         
            rhtml += '<li class="my_listings"><a href=javascript:ToSaved("listings","compareresults","' + GetPageName() + '"); target="_parent">My Listings</a> ('+ readListingCountPersCookie() + ')</li>';
            rhtml += '<li class="last"><a href=javascript:ToSaved("searches","compareresults","' + GetPageName() + '"); target="_parent">My Searches</a> ('+ readSearchCountPersCookie() + ')</li>';
            //rhtml += '<li class="last"><a href="javascript:void(0);" onclick="SignOut();" target="_parent">Sign Out</a></li>';
        }
        else 
        {            
            rhtml += '<li class="my_listings"><a href="' + PersonalizationRoot + 'saved_listings.aspx" target="_parent">My Listings</a> ('+ readListingCountPersCookie() + ')</li>';
            rhtml += '<li class="last"><a href="' + PersonalizationRoot + 'saved_searches.aspx" target="_parent">My Searches</a> ('+ readSearchCountPersCookie() + ')</li>';
            //rhtml += '<li class="last"><a href="javascript:void(0);" onclick="SignOut();" target="_parent">Sign Out</a></li>';              
                            
        }
        
        gE("loggedin").innerHTML = rhtml;
        
    }
}
</script> 
