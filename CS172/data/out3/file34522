      <div class="Carousel section"> 
       <!-- Timer (start) --> 
       <style type="text/css">
@media screen,projection{.cufon-canvas{display:inline;display:inline-block;position:relative;vertical-align:middle;font-size:1px;line-height:1px}.cufon-canvas .cufon-alt{display:none}  .cufon-canvas canvas{position:relative}}@media print{.cufon-canvas{padding:0 !important}  .cufon-canvas canvas{display:none}.cufon-canvas .cufon-alt{display:inline}} 
</style> 
       <script type="text/javascript" src="/common/js/jcarousellite-carousel.js"></script> 
       <link rel="stylesheet" href="/common/css/channels.css" type="text/css" media="screen"> 
       <script type="text/javascript">
		  
		  
var slideDecrement = 3;
var slideIncrement = 1;
var selectedSlide = 1;
var slideCounter;
var skipStep;

$(function() { 
    $(".carouselWrapper .carousel").jCarouselLite( {
        btnNext: "#arrowRight",
        btnPrev: "#arrowLeft",
        btnGo:['.carouselWrapper .1','.carouselWrapper .2','.carouselWrapper .3'],
       	speed: 700.0,
		auto: 7000,
		circular: true,
		pause: true, 
		visible: 1,
		scroll: 1, 
		
		
		afterEnd: function(el) {
				if (!skipStep) {		
				var current = ($(".Slide li").index(el));
				if (current<0){current ++;};
				
 
				var nubbins = $(".carouselWrapper #carouselSlideLinks img");
				var count = nubbins.length;
				goImg(current+1,count,'http://cms.nyu.edu');
				
				}else {
				skipStep=false;	
				}
				
				
				
			}
		
		
    });
});




// Change the nav dots 
function goImg(slideId,totalSlides,contextPath){
	if (!skipStep) {
	
	selectedSlide = slideId;
	slideDecrement = selectedSlide-1;
	slideIncrement = selectedSlide;
	}
	for(slideCounter=1;slideCounter<=totalSlides;slideCounter++){
	 	if(slideCounter == slideId) {
	   		document.getElementById(slideId).src = '/common/images/carousel/dotActive.gif';
	  	} else {
	   		document.getElementById(slideCounter).src = '/common/images/carousel/dotInactive.gif';
	  	}
 	}
}

//Rotate back (back button)
function leftRotate(totalSlides,contextPath){	
//alert('dec='+slideDecrement+' inc='+slideIncrement+' selected='+selectedSlide);
	if(selectedSlide != null) {		
				
				if(slideDecrement == 0) {	
							
					slideDecrement = 3;
					
				}
				
				goImg(slideDecrement,totalSlides,contextPath);
				slideIncrement=slideDecrement;				
				slideDecrement--;	
				skipStep=true;		
			}		
	}

// Rotate forward (forward button)
function rightRotate(totalSlides,contextPath){	
	if(selectedSlide != null) {
		slideDecrement=slideIncrement;
		slideIncrement++;
		if(slideIncrement > totalSlides ){
			slideIncrement = totalSlides;
		}	
		goImg(slideIncrement,totalSlides,contextPath);
	} else {
		goImg(3,totalSlides,contextPath);
		slideIncrement = 3;
	}skipStep=true;
}

</script> 
