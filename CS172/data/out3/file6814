  <script src="//www.uta.fi/resurssit/js/slick/slick.min.js"></script> 
  <script>
            $(document).ready(function() {
                $("#topmenu a").each(function() {
                    var title = $(this).html();
                    var href = $(this).attr("href");
                    var pop = $("#topmenupop");
                    var option = $("<option value=\""+href+"\">"+title+"</option>");
                    $(pop).append(option);
                });
                $("#topmenupop").change(function() {
                    var href = $(this).val();
                    $(this).val("/indexen.html");
                    window.location.href = href;
                });
                $("#nocarousel").html($("#carousel").html());
                $("#carousel").show();
                var slides = $("#carousel").children("div").length;
                var rslide = Math.floor(Math.random() * slides);
                $("#carousel").slick({
                    dots: true,
                    initialSlide: rslide
                });
                $("#buttons button").click(function() {
                    var a = $(this).children("a").first();
                    var url = $(a).attr("href");
                    if (url) {
                        window.location = url;
                    }
                });
                $("#hteksti").blur(function() {
                    var val = $(this).val();
                    if (val === '') {
                        $(this).removeClass("search-box-focus");
                        $(this).addClass("search-box-blur-en");
                    }
                });
                $("#hteksti").focus(function() {
                    $(this).removeClass("search-box-blur-en");
                    $(this).addClass("search-box-focus");
                });
                $("#hteksti").blur();
            });
        </script> 
