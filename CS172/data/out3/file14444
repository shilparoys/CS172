					function validateForm(){
						var form=document.getElementById("buscarForm");
						var input=document.getElementById("keyword_id");                                                
						if(input.value==""){
                                                  location.href="index.php?main_page=index&cPath=1";	  
                                                }else{
                                                    form.submit();
                                                }
                                                
					}
