        <script>
	$(document).ready(function(){
					$('#carrito_anim_inside').hide()
			.slideUp();
			});
	var opened=false;
	function muestraDetalle(){
		if(!opened){
			$('#flecha_detall').toggleClass('flecha_right').toggleClass('flecha_down');
			$('#carrito_anim_inside').slideDown(500);
		}else{
			$('#flecha_detall').toggleClass('flecha_right').toggleClass('flecha_down');
			$('#carrito_anim_inside').slideUp(200);
		}
		opened=!opened;
	}
</script> 
