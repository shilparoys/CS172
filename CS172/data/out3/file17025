  <script type="text/javascript" data-policy="always">
 $(window).load(function() {  $.ajax({
	type:'GET',
	url:'/x/6/0/7/40607/.down',
	dataType:'text',
	timeout:1000,
	success:function(data, status, xhr) {
		$.ajax({
			type: 'GET',
			url: '/x/6/0/7/40607/.grace_period',
			dataType: 'text',
			timeout: 1000,
			error: function() {
				var banner_div, alt, src, text_div;

				alt = "OOPS! This site is currently offline. If this is your website, please visit your control panel to sort things out... or we'd be happy to help at support@otherpeoplespixels.com";
				src = "http://static.otherpeoplespixels.com/images/offline.png";
				banner_div = $('<img class="offline-banner" />')
					.attr('alt', alt)
					.error(function () {
						$(this).hide().parent().addClass('no-image');
					}).attr('src', src);

				text_div = $('<div class="offline-text"></div>').text(alt);

				$('<div id="offline" />')
					.append('<div class="offline-screen" />')
					.append(text_div)
					.append(banner_div)
					.appendTo('body');
			}
		});
	}
});
}); 
</script> 
