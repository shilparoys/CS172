  <script type="text/javascript">
	
    //<![CDATA[
	
$('#query').click(ViewGraph);

$('#form1').submit(function(event) {
event.preventDefault();
ViewGraph();
});


    function ViewGraph() {
		site = $('#site').attr("value");
		param = $('#param').attr("value");
		encodeurl = "/water/includes/waterserviceproxy.php?url="+encodeURIComponent("http://waterservices.usgs.gov/nwis/iv/?format=json&site=" + site + "&parameterCd=") + param + "&mode=native";
		if (param == '00060') {DataType = 'streamflow'}
        else if (param == '72019') {DataType = 'groundwater'}
        else if (param == '00045') {DataType = 'rainfall' }
		$.ajax({
          url: encodeurl,
          dataType: 'json',
          data: '',
		  beforeSend : function (json) {
                $('#sitedesc').html('Loading...');
                $('#value').html('');
                $('#date').html('');
                $('#time').html('');
                $('#tz').html('');
                $('#graph').html('');
		  },
          success: function(json){
 			  
           if (typeof json.value.timeSeries == "undefined")
            {
                $('#sitedesc').html('Error: site number may be invalid, it may not be a real-time site, or it may not serve ' + DataType + ' data.');
                $('#value').html('');
                $('#date').html('');
                $('#time').html('');
                $('#tz').html('');
                $('#graph').html('');
            }
            else
            {
				cacheDtTm = new Date().getTime();
                $('#sitedesc').html(json.value.timeSeries[0].sourceInfo.siteName);
                myData = json.value.timeSeries[0].values[0].value[0].value;
                if (myData == -999999)
                {
                   $('#value').html('Not available');
                }
                else if (param == '00060')
                {
                   $('#value').html(Comma(myData) +  ' ft.<sup>3</sup>/sec');
				   myText = '7 day streamflow hydrograph for site ';
                }
                else if (param == '72019')
                {
                   $('#value').html(myData +  ' feet below land surface');
				   myText = '7 day depth to water level for site ';
                }
                else if (param == '00045')
                {
                   $('#value').html(myData +  ' inches');
				   myText = '7 day total rainfall for site ';
                }
				$('#graph').CreateBubblePopup({ innerHtml: '<img src="http://waterdata.usgs.gov/nwisweb/graph?agency_cd=USGS&site_no=' + site + '&parm_cd=' + param + '&period=7&cacheTime=' + cacheDtTm + '" alt="' + myText + site + '" title="' + myText + site + '" >', position:'left',width:'576px',height:'400px'});
                datetime = json.value.timeSeries[0].values[0].value[0].dateTime;
                // Get date
                myDate = datetime.substr(5,2) + '/' + datetime.substr(8,2) + '/' + datetime.substr(0,4);
                $('#date').html(myDate);
                // Get time
                myTime = datetime.substr(11,5);
                $('#time').html(myTime);
                // Get timezone
                myTZ = datetime.substr(23);
                $('#tz').html(myTZ + ' GMT');
                $('#graph').html('<img src="http://waterdata.usgs.gov/nwisweb/graph?agency_cd=USGS&site_no=' + site + '&parm_cd=' + param + '&period=7&cacheTime=' + cacheDtTm + '" height="156" width="225" alt="' + myText + site + '" title="' + myText + site + '" />');
            }
           
        },
          error : function(XMLHttpRequest, textStatus, errorThrown) {
            $('#sitedesc').html('An error occurred. This might be due to entering an invalid site number.');
            $('#value').html('');
            $('#date').html('');
            $('#time').html('');
            $('#tz').html('');
            $('#graph').html('');
          }
        });
        $("#detail_show").show("slow");
    };
    
    function Comma(number) {
        number = '' + number;
        if (number.length > 3)
        {
            var mod = number.length % 3;
            var output = (mod > 0 ? (number.substring(0,mod)) : '');
            for (i=0 ; i < Math.floor(number.length / 3); i++)
            {
                if ((mod == 0) && (i == 0))
                {
                    output += number.substring(mod+ 3 * i, mod + 3 * i + 3);
                }
                else
                {
                    output+= ',' + number.substring(mod + 3 * i, mod + 3 * i + 3);
                }
            }
            return (output);
        }
        else 
        {
            return number;
        }
    }
    
    //]]>
    </script> 
