  <script type="text/javascript">
function sldeUpDown(id,header){
 if($("#"+id).css('display') == 'block'){
 $("#"+id).slideUp('slow');
 $('#'+header).html('<a href="#locate"><img src="images/index_27.jpg" width="50" height="9" alt="Click To Expand"/></a>');
 }
 else {
 $("#"+id).slideDown('slow');
 $('#'+header).html('<img src="images/index_27.jpg" width="50" height="9" alt="Click To Collapse">');
 }
}
function expandAll(){
 $(".demo").slideDown('slow');
 $(".demo1").slideDown('slow');
 $('.header1').html('<img src="images/index_27.jpg" width="50" height="9" alt="Click To Collapse">');
 $('.header2').html('<img src="images/index_27.jpg" width="50" height="9" alt="Click To Collapse">');
}
function colapseAll(){
 $(".demo").slideUp('slow');
 $(".demo1").slideUp('slow');
 $('.header1').html('<img src="images/index_27.jpg" width="50" height="9" alt="Click To Expand">');
 $('.header2').html('<img src="images/index_27.jpg" width="50" height="9" alt="Click To Expand">');
}
</script> 
