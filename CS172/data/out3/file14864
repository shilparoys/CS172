	//Descargas de pdf
	function download_pdf(pdf)
	{
		
	 	if (typeof ga !== 'undefined'){
	 		ga('send', 'event', 'Descargas', 'Descarga PDF', pdf);
	 	}
	}
	//Enviar consulta
	function enviar_consulta(curso){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Consulta sesion informativa', 'Enviar', curso);
	 	}
	}	
	//Inscripcion
	function inscripcion(curso,tipoCurso){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Formulario Inscripcion', 'Ficha', curso + ' | ' + tipoCurso);
	 	}
	}	
	function inscripcion_ok(curso,tipoCurso){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Formulario inscripcion', 'Incripcion OK', curso + ' | ' + tipoCurso);
	 	}
	}			
	//Solicitud info
	function solicitud_informacion(curso,tipoCurso){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Formulario Solicitud informacion', 'Ficha', curso + ' | ' + tipoCurso);
	 	}
	}	
	function solicitud_informacion_ok(curso,tipoCurso){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Formulario Solicitud informacion', 'Incripcion OK ', curso + ' | ' + tipoCurso);
	 	}
	}	
	//busqueda home
	function busqueda_home(palabraclave){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Home', 'Buscar curso home', palabraclave);
	 	}
	}
	//login moodle
	function login_moodle(){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Home', 'Login Moodle', '');
	 	}
	}
