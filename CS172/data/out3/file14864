	function link_ub(){
		
	 	if (typeof ga !== 'undefined'){
			ga('send', 'event', 'UB', 'Web UB');
	 	}
	}
	
	//Opcio Menu
	function opcion_menu(categoria, subcategoria){
		if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Menu', categoria, subcategoria);
		}
	}		
	
	// Matriculacio cursos
	function matriculate_aqui(curso){
		if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Matricula', 'Acceso', curso);
		}
	}
	
	function formulario_matricula(curso){		
		if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Matricula', 'Formulario', curso);
		}
	}
	
	function enviar_formulario_matricula(curso){		
		if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Matricula', 'Enviar', curso);
		}
	}
	
	function outboud_link_social_media(red){
		if (typeof ga !== 'undefined'){
			ga('send', 'event', 'Outbound link', 'Social', red);
		}
	}

	// Consulta Empreses
	function consulta_empresas(tipoConsulta){
