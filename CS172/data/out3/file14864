					var i=1;
					var max=images.length;
					function changeImage()
					{
						$("#link-impacte").attr("href",images[i].link_url);
						$("#link-impacte").attr("target",images[i].link_target);
						$("#img-impacte").attr("src",images[i].src);
						$("#img-impacte").attr("title",images[i].title);
						i++;
						if(i>=max)
							i=0;
					}
					$(document).ready(function(){
						if(max>1)	timer = setInterval("changeImage()", 5000);
					});
				</script> 
