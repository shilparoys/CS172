													  }else{	  
												      
														  jwplayer(tagID).setup({
														    'flashplayer': '/system/modules/il3.ub.edu.presentation/resources/swf/player.swf',
														    'author': 'IL3-UB', 
														    'description': unescape('Miquel%20Escolano.%20Project%20Manager%20de%20IDOM.'),
														    'file': urlVideo,
														    'title': unescape('Miquel%20Escolano.%20Project%20Manager%20de%20IDOM.'),
														    'autostart': 'false',
														    'controlbar': 'bottom',
														    'width': '100%',
														    'height': '390px'
														  });
													  }
													</script> 
            </div> 
           </div> 
          </div> 
