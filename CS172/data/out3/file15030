		$(document).ready(function(){
			$('div#navbar > ul > li > a').click(
				function(event){
				//if(event.target != this) return;

				
								
				$(this).closest("li").toggleClass("firstClicked");
				if( $(this).closest("li").find(".menuEmergente").length > 0  && $(this).closest("li").hasClass("firstClicked") ){					
					event.preventDefault();
					$('div#navbar > ul > li').removeClass("firstClicked");
					$('div#navbar > ul > li').removeClass("backColorFoot");
					$(this).closest("li").addClass("firstClicked");
					$(this).closest("li").toggleClass("backColorFoot");
					$('div#navbar > ul > li').find(".menuEmergente").hide();
					$(this).closest("li").find(".menuEmergente").toggle();
					if($(this).closest("li").find(".menuEmergente").find(".listCol1").length==0){
						var alturaActual = $(this).closest("li").find(".menuEmergente").find(".partir").height();
						if(alturaActual > 460 && alturaActual <= 850){
							$(this).closest("li").find(".partir").easyListSplitter({
						 	  colNumber: 2
							});
						}else if(alturaActual >=851){
							$(this).closest("li").find(".partir").easyListSplitter({
						 	  colNumber: 3
							});
						}
						
						
						
					}				
					
				}else if($(this).closest("li").find(".menuEmergente").length > 0){
					event.preventDefault();
					$(this).closest("li").find(".menuEmergente").hide();
					$(this).closest("li").toggleClass("backColorFoot");
				}
				
				
				
			}
			);			
			
			
						
		});
	</script> 
