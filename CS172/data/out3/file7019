$(document).ready(function() 
{
var showtips= 'True';

	$('a[rel^="prettyPhoto"]').each(function(){
	if($(this).attr("title").length){
	$(this).attr("pptitle", $(this).attr("title"));
	if (showtips =='False')
	{
	$('a[rel^="prettyPhoto"]').each(function(){
	if($(this).attr("title").length){
	$(this).attr("title","");
	}
	});
	}}
});
	
