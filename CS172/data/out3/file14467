  <script type="text/javascript">
  $(document).ready(function() {
    
    var timer;
                      
    $("#slideshow > div:gt(0)").hide();
    
    $("#slideshow")
        .mouseenter(function() {
            if (timer) { clearInterval(timer) }
        })
        .mouseleave(function() {
            timer = setInterval(function() {
                $("#slideshow > div:first")
                    .fadeOut(2000)
                    .next()
                    .fadeIn(2000)
                    .end()
                    .appendTo("#slideshow");
            }, 4000);
        })
        .mouseleave();
                         
    
});
   </script> 
