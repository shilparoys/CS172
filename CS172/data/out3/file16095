		function getLocalStorageLocation() {
			//var domstorage=window.localStorage;
			
			if(TWC && TWC.pco.get("user")){
				var user_localStorage;
				 user_localStorage = TWC.pco.get("user");
				
				if (user_localStorage != undefined) {
					var preferredLoc =user_localStorage.preferredLocation;
					if (preferredLoc)  {
						return preferredLoc.locid;
					}
				} 
			
			}
			
			return null;
		}
