         <script type="text/javascript">
		//<![CDATA[
            jQuery(document).ready(function(){

				jQuery("#virtualKeyboard").mouseenter(function(){
					jQuery("#virtualKeyboard").css("cursor", "move");
				});
					
				jQuery("#virtualKeyboard").mouseleave(function(){
					jQuery("#virtualKeyboard").css("cursor", "default");
				});
					
   				jQuery("#virtualKeyboard").draggable();
   				
   				setupKeyboard();
        	});
            
            function closeKeyboard() {
            	jQuery("#virtualKeyboard").hide();
            }
            
            // Convert array for easy comparison using 'in' operator
			function oc(a)
			{
			    var o = {};
			    for(var i=0;i<a.length;i++)
			    {
			        o[a[i]]='';
			    }
 			    return o;
			}
			
            function setupKeyboard()
            {
                // use the 'id' attribute if available; if not use the 'name' attribute

                // setup the PII fields on this page
		    	var regExp = new RegExp("\\s*","g");
      			var fields = "studentSsn,studentDob";
				fields = fields.replace(regExp, "");
                var elemArr = fields.split(',');
                configVkElements(elemArr);

                // setup initial keyboard focus in the first PII field
                buildKeyboard("keyboard", elemArr[0]);

                var allFields = jQuery("input[id!=KeyboardSelect]");
                var kbFields = oc(elemArr);
                var types = oc(['text', 'radio', 'checkbox', 'select-one']);
			    jQuery("input, select", jQuery("form")).each(function() {
					if(jQuery(this).prop('type') in types) {
				    	jQuery(this).focus(function() {
                            if (jQuery("#virtualKeyboard").is(":visible")) {
                                if (!(jQuery(this).prop('id') in kbFields) || (jQuery(this).prop('id') != jQuery(focusedText).prop('id'))) {
  							        // TODO: how to encapsulate the event name?
				    		        jQuery(focusedText).trigger("virtualKeyboardClosed");
				    		        closeKeyboard();
				    		        focusedText = null;
				    		    }
							}
				    	});
					}
			    });
            }
            
            function virtualKeyboardEnter()
            {
            }
		//]]>
        </script>
