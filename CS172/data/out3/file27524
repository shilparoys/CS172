            jQuery(document).ready(function(){

				jQuery("#studentSsn_question").bind("fafsa.show", function(e) {
					addVkIconAnchor("virtualKeyboard_toggle_studentSsn");
					jQuery("#keyboard_icon_studentSsn").unbind("click.toggleVirtualKeyboard"); // For safety 
					jQuery("#keyboard_icon_studentSsn").bind("click.toggleVirtualKeyboard", function(e) { toggleVirtualKeyboard(this, "virtualKeyboard","studentSsn", e)});
				});

				jQuery("#studentSsn_question").bind("fafsa.hide", function(e) {
					removeVkIconAnchor("virtualKeyboard_toggle_studentSsn");
					jQuery("#keyboard_icon_studentSsn").unbind("click.toggleVirtualKeyboard");
				});
            });
            
            jQuery(window).load(function(e){
            	if (jQuery("#studentSsn").is(":disabled")) {
					removeVkIconAnchor("virtualKeyboard_toggle_studentSsn");
					jQuery("#keyboard_icon_studentSsn").unbind("click.toggleVirtualKeyboard");
            	} else {
					addVkIconAnchor("virtualKeyboard_toggle_studentSsn");
					jQuery("#keyboard_icon_studentSsn").unbind("click.toggleVirtualKeyboard"); // For safety 
					jQuery("#keyboard_icon_studentSsn").bind("click.toggleVirtualKeyboard", function(e) { toggleVirtualKeyboard(this, "virtualKeyboard","studentSsn", e)});
