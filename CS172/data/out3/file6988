  <script type="text/javascript">
function submitForm(containerElement) { 
var theForm = document.createElement('form'); 
var url = window.location.href; theForm.action = 'https://portal.udem.edu.mx/cp/home/login'; theForm.method = 'post'; 
var user = $("#access input[id='user']").attr('value'); 
var pass = $("#access input[id='pass']").attr('value');
theForm.innerHTML = document.getElementById(containerElement).innerHTML; 
var formToSubmit = document.body.appendChild(theForm); 
formToSubmit.elements['user'].value = user;
formToSubmit.elements['pass'].value = pass;
formToSubmit.target = "_blank";
formToSubmit.submit(); 
} 

</script> 
  <script type="text/javascript">// <![CDATA[
	// Search for the terms when the Search button is clicked

	function customSearch() {
	
		var CurrentPath = window.location.hostname;
		var protocol = window.location.protocol;
		var currentSite = window.location.origin; 
		var currentSite2 = protocol +"//"+ CurrentPath; 
		var searchUrl = protocol +"//"+ CurrentPath + "/Esp/Busquedas/Pages/default.aspx?";
		var searchTerm = "&k=" + document.getElementById("searchBox").value;
		//var listParams = "&cs=This%20Site&u=" + currentSite;
		var listParams = "&s=Paginas%20de%20Contenido&u=" + currentSite;
		document.location.href = searchUrl + searchTerm + listParams;

	}

	// Initiate the customSearch function when the Enter key is pressed

	function searchKeyPress(e) {
		// look for window.event in case event isn't passed in
		if (window.event) { e = window.event; }
		if (e.keyCode == 13)
		{
			document.getElementById('searchButton').click();
		}
	}
// ]]></script> 
