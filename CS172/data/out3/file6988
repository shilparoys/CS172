                        //            __doPostBack('lnkConstruyeHtml', 'cambiaBloque');
                    }
                }
            };
            SP.UI.ModalDialog.showModalDialog(options);
            $('.ms-dlgContent').css('top', window.pageYOffset + 10);
            return false;
        });

