  <script type="text/javascript">
	//<![CDATA[
		$(document).ready(function(){
		    displayMenuPrincipal();
					/*
				CARRUSEL DE BANNERS
			*/
			/*
				ATENCIÓN: la función innerXHTML está en el archivo js/recursosFlash.js
			*/
			var xml2str = "<div>" + encodeURIComponent(innerXHTML(document.getElementById('bannersFlash')),true) + "</div>";
			var flashvars = {
			    banners: xml2str
			};
			var params = {
				wmode: "transparent"
			};
			var url = "/templates/pie_abajo_v2/swf/carrusel.swf";
			/*
				ATENCIÓN: no es necesario enlazar con un archivo swfobject.js situado en el directorio js de nuestra plantilla
				porque el plugin avreloaded (para meter vídeos en los artículos) ya lo mete.
			*/

						swfobject.embedSWF(url, "carruselBannersFlash", "342", "209", "8.0.0",undefined,flashvars,params);

			/*
    			MENÚ LÍNEAS DE INVESTIGACIÓN:
				- Sustituye el span contenido en el enlace por una imagen cuyo alt será el texto contenido en el span
				- En el over sustituiremos la imagen por otra
			*/
			$("#menuLineasInv ul li a").map(function(index) {
				var alt = $(this).children("span").text();
			    var srcOut = "/templates/pie_abajo_v2/images/lineas_out_0" + index +"_en.gif";
				var srcOver = "/templates/pie_abajo_v2/images/lineas_over_0" + index +"_en.gif";

				$(this).children("span").replaceWith("<img src='" + srcOut + "' alt='" + alt + "' />");
				$(this).hover (
					function() {
					    $(this).children("img").attr("src",srcOver);
					},
					function() {
					    $(this).children("img").attr("src",srcOut);
					}
				);
			});

			/*
				MENÚ SECORES DE APLICACIÓN:
				- Sustituye el span contenido en el enlace por una imagen cuyo alt será el texto contenido en el span
				- En el over sustituiremos la imagen por otra
			*/
			$("#sectoresAplicacion ul li a").map(function(index) {

    			var alt = $(this).children("span").text();
    			var srcOut = "/templates/pie_abajo_v2/images/sectores_out_0" + index +"_en.gif";
    			var srcOver = "/templates/pie_abajo_v2/images/sectores_over_0" + index +"_en.gif";

    			$(this).children("span").replaceWith("<img src='" + srcOut + "' alt='" + alt + "' />");
       			$(this).hover(
    				function() {
    					$(this).children("img").attr("src",srcOver);
    				},
    				function() {
    					$(this).children("img").attr("src",srcOut);
    				}
    			);
    		});

			/*
				BANNER INDEX BOLETÍN:
				- Sustituir en el over una imagen por otra
			*/
			$("#bannerBoletin").children("a").hover(
				function() {
					var srcOver = "/templates/pie_abajo_v2/images/boletin_over_en.jpg";
					$(this).children("img").attr("src",srcOver);
				},
				function() {
					var srcOut = "/templates/pie_abajo_v2/images/boletin_out_en.jpg";
					$(this).children("img").attr("src",srcOut);
				}
			);

    
            /*   CENTRAR PAGINACIÓN   */
		    var entornoPag = $(".paginacionEnv");// el entorno en que hay que centrar las paginaciones
		    var entornosListaPag = $(".paginacionEnv .interior");// los <div> que contienen los <ul> que representan las paginaciones
																		   // en nuestro caso ES NECESARIO envolver las listas en <div>
		    if(entornoPag.length && entornosListaPag.length) {// comprobamos que existen ambos elementos
		        var centraPaginaciones = new CentrarListaElementosFlotantes(entornoPag,entornosListaPag);
		        centraPaginaciones.centrar();
		    }

		    /*   FORMULARIO DE SUSCRIPCIÓN AL NEWSLETTER   */
		    //var entornoForm = $(".suscripcionNewsletter");

		    //if(entornoForm.html() != null){
	

			/*   MENSAJES DE ESTADO DE LA SUSCRIPCION   */
	
        });
