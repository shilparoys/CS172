        </div> 
        <div id="ctl00_SlideShowBlock1_wGetFrames" frames="%3cxmlroot+OverlayImage%3d%22%7e%2fCanvases%2fMosaic-Blue%2fImages%2foverlay.png%22+OverlayImageName%3d%22%7e%2fCanvases%2fMosaic-Blue%2fImages%2foverlay.png%22%3e%3cframe+Instance%3d%22Top%22+Width%3d%22622%22+Height%3d%22296%22+Top%3d%224%22+Left%3d%224%22+TransitionDuration%3d%221000%22+FirstTransitionDelay%3d%22500%22+TransitionDelay%3d%224000%22+Titles%3d%22false%22+Random%3d%22false%22+SlideControls%3d%22true%22+Transition%3d%22fade%22%3e%3cslide+Path%3d%22%2ffilerequest%2f1664%22+Name%3d%22daily+devotional.jpe%22+Link%3d%22%2fmedia%2fdaily-devotional%22+Title%3d%22daily+devotional.jpe%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1654%22+Name%3d%22card+3.jpg%22+Link%3d%22%2fmedia%2flibrary%22+Title%3d%22card+3.jpg%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1503%22+Name%3d%22time+out+pic.jpg%22+Link%3d%22%2fministry%2ftimeout%22+Title%3d%22time+out+pic.jpg%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1101%22+Name%3d%22HOPEaa.png%22+Link%3d%22%2fministry%2fh.o.p.e-ministry(2)%22+Title%3d%22HOPEaa.png%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1258%22+Name%3d%221Cru.png%22+Link%3d%22%2fministry%2fcru%22+Title%3d%221Cru.png%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1640%22+Name%3d%22image001.jpg%22+Link%3d%22%2fministry%2fteens%2fepic%22+Title%3d%22image001.jpg%22+NewWindow%3d%22false%22+%2f%3e%3cslide+Path%3d%22%2ffilerequest%2f1629%22+Name%3d%22Untitled-1+copy.jpg%22+Link%3d%22%2fministry%2fkids%2folympians%22+Title%3d%22Untitled-1+copy.jpg%22+NewWindow%3d%22false%22+%2f%3e%3c%2fframe%3e%3c%2fxmlroot%3e"></div> 
        <div id="ctl00_SlideShowBlock1_wInlineJavascript"> 
         <script type="text/javascript">

                    function ews_ss_pageload_handlectl00_SlideShowBlock1_wGetFrames() {
                        if ($("#ctl00_SlideShowBlock1_wGetFrames").length == 0)
                            return;
                        if ($("#ctl00_SlideShowBlock1_wGetFrames")[0].getAttribute("frames") == null)
                            return;
                        var xml = $("#ctl00_SlideShowBlock1_wGetFrames")[0].attributes["frames"].value;
                        if (!xml || xml.length == 0)
                            return;
                        xml = unescape(xml);
                        xml = xml.replace(/\+/g, ' '); 

                        if (document.all)
                        {
                          xmlDoc=new ActiveXObject("Microsoft.XMLDOM");
                          xmlDoc.async="false";
                          xmlDoc.loadXML(xml);
                        }
                        else
                        {
                          parser=new DOMParser();
                          xmlDoc=parser.parseFromString(xml,"text/xml");
                        }
                        for (var g = 0; g < xmlDoc.childNodes.length; g++)
                        {
                            xmlRoot = xmlDoc.childNodes[g];
                            for (var t = 0; t < xmlRoot.childNodes.length; t++)
                            {
                                if (xmlRoot.childNodes[t].nodeName.toLowerCase() == "frame")
                                {
                                    var frame = xmlRoot.childNodes[t];
                                    var bRandom = false;
                                    if (frame.attributes.getNamedItem("Random").value == "true" && frame.childNodes.length > 1)
                                        bRandom = true;
                                    if (Boolean(frame.attributes.getNamedItem("Titles").value) && Boolean(frame.attributes.getNamedItem("SlideControls").value))
                                    {
                                        $("#slideshow_ctl00_SlideShowBlock1_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase()).cycle(
                                        { 
                                            fx: frame.attributes.getNamedItem("Transition").value,
                                            speed: parseInt(frame.attributes.getNamedItem("TransitionDuration").value), 
                                            timeout: parseInt(frame.attributes.getNamedItem("TransitionDelay").value), 
                                            random: bRandom,
                                            delay: parseInt(frame.attributes.getNamedItem("FirstTransitionDelay").value),
                                            before: function(a, b, c, d) { slideshowBeforeTransition(a,b,c,d,100); },
                                            after: slideshowAfterTransition,
                                            next: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_next",
                                            pager: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_pager",
                                            prev: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_prev"
                                        });
                                    
                                    }
                                    else if (!Boolean(frame.attributes.getNamedItem("Titles").value) && Boolean(frame.attributes.getNamedItem("SlideControls").value))
                                    {
                                        $("#slideshow_ctl00_SlideShowBlock1_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase()).cycle(
                                        { 
                                            fx: frame.attributes.getNamedItem("Transition").value,
                                            speed: parseInt(frame.attributes.getNamedItem("TransitionDuration").value), 
                                            timeout: parseInt(frame.attributes.getNamedItem("TransitionDelay").value), 
                                            random: bRandom,
                                            delay: parseInt(frame.attributes.getNamedItem("FirstTransitionDelay").value),
                                            after: slideshowAfterTransition,
                                            next: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_next",
                                            pager: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_pager",
                                            previous: "#slideshow_ctl00_SlideShowBlock1_controls_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase() + "_prev"
                                        });                                   
                                    }
                                    else if (!Boolean(frame.attributes.getNamedItem("Titles").value) && !Boolean(frame.attributes.getNamedItem("SlideControls").value))
                                    {
                                        $("#slideshow_ctl00_SlideShowBlock1_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase()).cycle(
                                        { 
                                            fx: frame.attributes.getNamedItem("Transition").value,
                                            speed: parseInt(frame.attributes.getNamedItem("TransitionDuration").value), 
                                            timeout: parseInt(frame.attributes.getNamedItem("TransitionDelay").value), 
                                            random: bRandom,
                                            delay: parseInt(frame.attributes.getNamedItem("FirstTransitionDelay").value),
                                            after: slideshowAfterTransition
                                        });                                    
                                    }
                                    else if (Boolean(frame.attributes.getNamedItem("Titles").value) && Boolean(frame.attributes.getNamedItem("SlideControls").value))
                                    {
                                        $("#slideshow_ctl00_SlideShowBlock1_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase()).cycle(
                                        { 
                                            fx: frame.attributes.getNamedItem("Transition").value,
                                            speed: parseInt(frame.attributes.getNamedItem("TransitionDuration").value), 
                                            timeout: parseInt(frame.attributes.getNamedItem("TransitionDelay").value), 
                                            random: bRandom,
                                            delay: parseInt(frame.attributes.getNamedItem("FirstTransitionDelay").value),
                                            before: function(a, b, c, d) { slideshowBeforeTransition(a,b,c,d,100); },
                                            after: slideshowAfterTransition                                            
                                        });                                    
                                    }     
                                    else
                                    {
                                        $("#slideshow_ctl00_SlideShowBlock1_instance_" + frame.attributes.getNamedItem("Instance").value.toLowerCase()).cycle(
                                        { 
                                            fx: "fade"
                                        });  
                                    }                               
                                    

                                }
                            }
                        }
                        return;
                        
                    }

                    $(document).ready(function()
                    {
                        if(typeof(Sys) != "undefined")
                            Sys.Application.add_load(ews_ss_pageload_handlectl00_SlideShowBlock1_wGetFrames);
                        else
                            ews_ss_pageload_handlectl00_SlideShowBlock1_wGetFrames();
                        
                    });
                </script> 
        </div> 
       </div> 
      </div> 
