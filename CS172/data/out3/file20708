     <td align="left" width="147"> <img border="0" src="../nimages/home_logo_top.gif" width="116" height="17"> </td> 
     <td align="middle" nowrap valign="top" width="550"> <font class="navtext" color="#FDE39B" style="font-size: 7pt">|&nbsp; </font> <font class="navtext" color="#F0ECE0" style="font-size: 7pt"> <a href="../location_map.htm"><font color="#FDE39B">Location Map</font></a><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font> <a href="../news_events.htm"><font color="#FDE39B">News &amp; Events</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font></font> <a href="../search.htm"><font color="#FDE39B">Search</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> | </font></font> <a href="../contact_us.htm"><font color="#FDE39B">Contact Us</font></a><font class="navtext" style="font-size: 7pt"><font class="navtext" color="#FDE39B" style="font-size: 7pt"> |</font></font></font><font face="Verdana" color="#FDE39B"><span style="font-size: 7pt">
        <!--webbot bot="HTMLMarkup" startspan --><script language="JavaScript">

function getAppVersion(){
	appname= navigator.appName;
	appversion = navigator.appVersion;
	majorver = appversion.substring(0, 1);
	if ( (appname == "Netscape") && ( majorver >= 3 ) ) return 1;
	if ( (appname == "Microsoft Internet Explorer") && (majorver >= 4) ) return 1;
	return 0;
}

function chgImg(myNum, myName, logoNum) {
	if (getAppVersion()) {
		document[myName].src = img[myNum].src;
		
		}
}

