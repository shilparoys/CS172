<script>
$(document).ready(fetchNews);

function fetchNews() {
  $.getJSON('http://blogs.westmont.edu/?json', showNews);
}

function showNews(data) {
  var stuff = [];
  $.each(data, function(i, item) {
    stuff.push("<div class='newsitem' onclick=\"location.href='" +
                item.permalink +
                "/?iphone=1'\"' onClick>\n<img class='thumb' src='" + 
                item.thumbnail +
                "'>\n<h3>" +
                item.title +
                "</h3><p>" +
                item.excerpt +
                "</p>\n</div>\n<hr>"
              );
  });
  $('.newswell').html(stuff.join('<br>'));
}
</script>
		

