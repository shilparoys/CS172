$("#territory").change(function(){
//window.location = "/undergraduate/territory/"+this.value;
	h = (this.getAttribute('data-height'))?this.getAttribute('data-height'):600;
	w = (this.getAttribute('data-width'))?this.getAttribute('data-width'):720;

	html = '';
	$(".staff."+this.value).each(function(){
		html+=$(this).html()
	});

	$( "div#territory")
	.html(html)
	.dialog({
		width: w,
		height: h,
		modal: true,
		draggable: false,
		resizable: false,
		open: function(){
			jQuery('.ui-widget-overlay').bind('click',function(){
				jQuery('div#territory').dialog('close');
			});
		}
	})
	;
	return false;
});


</script> 
