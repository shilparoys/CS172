  <script language="javascript" type="text/javascript">
$(document).ready(function(){
	
	$("#qcklnk").click(function(){
		
		$( "#subnavfooter" ).animate({
			  "height": "toggle"
			}, { duration: "slow" });
		
	});	
	$("#subnavfooterclose").click(function(){
		
		$( "#subnavfooter" ).animate({
			  "height": "toggle"
			}, { duration: "slow" });
		
	});
});
</script> 
  <script type="text/javascript" language="javascript">
$(document).ready(function(){
	
	$('#CB_catsearch').click(function(){
		
		if($(this).attr('checked'))
			{
				//alert("Checked");	
				$('#catsearch').html("<input type=\"hidden\" name=\"ask_category\" value=\"\">");
			}
		else
			{
				//alert("Unchecked");	
				$('#catsearch').html("")
			}
		
	});
	
});
</script> 
