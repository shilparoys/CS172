                           <script type="text/javascript">

function InternetAsyncWebPartGetData(unitTermSetGuid, typeTermSetGuid, showEnglishEvents, showFinnishEvents, rssViewID, isUniversityCalendar, listUrl, siteUrl, webPage, unitName, typeName, nrOfItems, currentUI, excludedEventTypes, useAlternativeLayout, containerID) 
{ 

//debugger
	  $.ajax({
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            url: window.location.href.replace(window.location.pathname, '') + '/_layouts/UTUInternet.WebPart/WebService/UTUInternetTodaysEventsService.svc/getEventsList',
            data: JSON.stringify({ 
                unitTermSetGuid:unitTermSetGuid, typeTermSetGuid:typeTermSetGuid, 
                showEnglishEvents:showEnglishEvents, showFinnishEvents:showFinnishEvents, 
                rssViewID:rssViewID, isUniversityCalendar:isUniversityCalendar, listUrl:listUrl, siteUrl:siteUrl, webPage:webPage,
                unitName:unitName, typeName:typeName, nrOfItems:nrOfItems, currentUI:currentUI, excludedEventTypes:excludedEventTypes, 
                useAlternativeLayout:useAlternativeLayout
            }),
            success: function (data) {
                // do something with the data returned, if any

                //debugger
                var containerDiv = document.getElementById(containerID);
                containerDiv.innerHTML = data;

            },
            error: function (msgObj, url, status) {
                SP.UI.Notify.addNotification('Error: ' + status, false);
            }
        });
}


$(document).ready(function() {
//debugger    
InternetAsyncWebPartGetData('84cca1b4-d356-443e-beac-138b31a26a84', 'a958cc48-7412-4c35-9f24-528c5cfaae72', false, true, '', true, '/sites/calendar/lists/utu%20calendar', '', '/fi/Ajankohtaista/Tapahtumat/Sivut/vaitokset.aspx', '', 'Väitös', 11, 1035, '', false, '');
