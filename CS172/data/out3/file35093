						function switchsidesched(SPORTID) {

							$(function() {
								$('.weeklyschedulediv').hide();
								$('#side_weekly_schedule'+SPORTID).show();
								$('.RSID_link').removeClass("rsid_off");
								$('#RSID'+SPORTID).addClass("rsid_off");
							});

						}

