     <script type="text/javascript">
				oz_api = usn.rubicon.apiName;
				oz_site = usn.rubicon.site;
				oz_zone = usn.rubicon.getZone("adv-1");
				oz_ad_slot_size = usn.rubicon.getAdSize("adv-1");
				oz_callback = function(valuation) {
					
					var tier=null, size=null;
					try {
						tier = valuation.estimate.tier;
						size = valuation.estimate.size;
					} catch(e) {/*...*/}
					
					var iframe = document.createElement('iframe');
					iframe.setAttribute("id", "iframe-ad-1");
					iframe.className = "iframe-ad";
					iframe.setAttribute("src", "/ajax/iframe_ad/?tile=1&sitename=usn.travel/bestcruises&params=kw%3Dtravel%3Bkw%3Dbestcruises%3Bkw%3Dsectionfront&tier="+tier+"&size="+size);
					iframe.setAttribute("scrolling", "no");
					iframe.onload = function() {
						checkAdVisibility('iframe-ad-1','adv-1');
						adValidator('adv-1');
					};
					
					document.getElementById("adv-1").appendChild(iframe);
				};
				
				usn.rubicon.callApi();
			</script> 
