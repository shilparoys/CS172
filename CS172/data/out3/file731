         </ul> 
         <div id="tab1"> 
          <h3>Club Events</h3> 
          <p>     </p>
          <div class="event_listing"> 
           <script type="text/javascript">
var academic_feed=new gfeedfetcher("Interurban Center for the Arts", "ica_calendar", "")
academic_feed.addFeed("ICA Calendar", "http://www.greenriver-events.info/public/rss/feed.php?l=8") //Specify "label" plus URL to RSS feed
academic_feed.displayoptions("title snippet") //show the specified additional fields
academic_feed.setentrycontainer("div") //Display each entry as a list (div element)
