  <script>

   // var mySlide = new SimpleCarousel($('slides'), $$('#slides img'), $$('#slides img'), { });

    var carousel = new Fx.Scroll('carousel');
    carousel.index = 0;
    carousel.position = 1;
    carousel.next = function() {
	
        if ($('pick'+(carousel.position+4))) {
            carousel.position += 4;
            carousel.index +=1;
            var e = $('pick'+carousel.position);
            $$('#lights li').each(function(e){e.removeClass('selected')});
            $$('#lights li')[carousel.index].addClass('selected');
            carousel.toElement(e);
        } 
    }
    carousel.prev = function() {
        if ($('pick'+(carousel.position-4))) {
            carousel.position -= 4;
            carousel.index -=1;
            $$('#lights li').each(function(e){e.removeClass('selected')});
            $$('#lights li')[carousel.index].addClass('selected');
            var e = $('pick'+carousel.position);
            if (e) carousel.toElement(e); 
        }
    }
</script> 
