      <div class="tripleBox"> 
       <div class="parsys tripleBox01">
        <div class="globalprograms section"> 
         <link rel="stylesheet" href="/etc/designs/nyu/clientlibs/colorbox.css" type="text/css"> 
         <link rel="stylesheet" href="/etc/designs/nyu/clientlibs/components/globalfinder.css" type="text/css"> 
         <script type="text/javascript" src="/etc/designs/nyu/clientlibs/colorbox.js"></script> 
         <script type="text/javascript">
(function($) {
    // things to note:
    // coordinates are stored in this order, in the jcr:
    // 5-col globe, 3-col globe, 5-col region, 3-col region 
    var NYU = {};
    var size = "3";
    var path = "/content/nyu/en/global/jcr:content/tripleBox01/globalprograms";
    var self;
    
    NYU.GlobalProgramFinder = function(el) {
        this.init = function() {
            self = this;
            
            self.draw();
            self.assignHandlers();
            self.getLocations("globe");
        };
        
        /**
         * Draw default "global" map view
         */
        this.draw = function() {
            var html = "";
            html = '<div id="map-container"><div id="continents"><div id="selectedRegion" />';
            html += '<a href="samerica" id="samerica"><i>South America</i></a>';
            html += '<a href="namerica" id="namerica"><i>North America</i></a>';
            html += '<a href="europe" id="europe"><i>Europe</i></a>';
            html += '<a href="africa" id="africa"><i>Africa</i></a>';
            html += '<a href="asia" id="asia"><i>Asia</i></a>';
            html += '<a href="australia" id="australia"><i>Australia</i></a>';
            html += '</div></div>';
            $('#map-container').replaceWith(html);
        };
        
        /**
         * Draw regional zoomed-in map view
         */
        this.drawRegion = function(r) {
            $('#map-container').replaceWith('<div id="map-container"></div>');
            $("#map-container").addClass(r);
            $("#map-container").append('<a id="backbtn"> &laquo; Go Back to the Global Map</a>');
            $("#backbtn").click(function() {
                $("#map-container").removeClass();
                self.init();
            });
            
            self.getLocations(r);
        };
        
        /**
         * Drawing points/markers and assign lightbox click handler to each
         */
        this.drawPoints = function(points) {
            $.each(points, function(i, p){
                // console.log("Draw: " + p.location + " [" + p.x + "," + p.y + "]");
                $("#map-container").append("<a style='left:" + p.x + "px; top:" + p.y + "px' class='colorbox " + p.marker + "' href='" + path + ".location.html?location=" + p.location + "' title='" + p.name + "'></a>");
            });
            
            $(".colorbox").colorbox({
                close: "&laquo; Go Back to the Global Map",
                iframe: true,
                width: "750px",
                height: "440px",
                opacity: "0.75"
            });
        };
        
        this.assignHandlers = function() {
            // prevent default 'a' action and attach handler to continents
            $("#global-program-finder a").click(function(e) {
                e.preventDefault();
            });
            $('#continents a').click(function(e) {
                self.drawRegion($(this).attr("id"))
            });

            // set mouseover functions
            $("#global-program-finder a").mouseover(function(){
                $("#selectedRegion").addClass($(this).attr("id"));
            });
            $("#global-program-finder a").mouseout(function(){
                $("#selectedRegion").removeClass();
            });
        };
        
        this.getLocations = function(view) {
            //get global locations for this col size & place on map
            $.getJSON("/etc/tags/global/location.3.json", function(locations) {
                $.getJSON("/content/nyu/en/admin/data/globalprogramfinder.2.json", function(programs) {
                    var cities = {};

                    // we have all the programs, so loop through
                    $.each(programs.programs, function(i, program) {
                        if (program.program_city != null) {
                            $.each(program["cq:tags"], function(i, tag) {
                                var location = tag.replace(/global:location\//gi, "");
                                var l = location.split("/");

                                if (view != "globe" && NYU.GlobalProgramFinder.REGIONID2TAG[view] != l[0])
                                    return;
                                if (l.length != 3)
                                    return;
                                if (cities[l[2]] != undefined)
                                    return;

                                var coords = locations[l[0]][l[1]][l[2]]["coords"];
                                var marker = locations[l[0]][l[1]][l[2]]["marker"];

                                var x, y;
                                if (view == "globe") {
                                    x = (size == 5) ? coords[0] : coords[2];
                                    y = (size == 5) ? coords[1] : coords[3];
                                } else {
                                    x = (size == 5) ? coords[4] : coords[6];
                                    y = (size == 5) ? coords[5] : coords[7];
                                }
                                
                                // console.log("Add: " + tag + " [" + x + "," + y + "]");

                                cities[l[2]] = {
                                    "x": x, "y": y,
                                    "marker": marker,
                                    "location": tag,
                                    "name": locations[l[0]][l[1]][l[2]]["jcr:title"] + ", " + locations[l[0]][l[1]]["jcr:title"]
                                };
                            });
                        }
                    });
                    
                    self.drawPoints(cities);
                });
            });
        };
        
        this.init();
    };
    
    NYU.GlobalProgramFinder.REGIONID2TAG = {
        "samerica": "south-america",
        "namerica": "north-america",
        "europe": "europe",
        "asia": "asia",
        "africa": "africa",
        "australia": "australia",
        "globe": "globe"
    };
    
    $(function() {
        var gpf = new NYU.GlobalProgramFinder("global-program-finder");
    });
})(jQuery);
</script> 
