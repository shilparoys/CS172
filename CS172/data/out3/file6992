        <div class="right"> 
         <script type="text/javascript" src="web/js/jquery-1.2.3.pack.js"></script> 
         <script type="text/javascript" src="web/js/jquery.jcarousel.pack.js"></script> 
         <link rel="stylesheet" type="text/css" href="web/css/skin.css"> 
         <link rel="stylesheet" type="text/css" href="web/css/jquery.jcarousel.css"> 
         <!-- MENU script+css  --> 
         <script type="text/javascript">

var mycarousel_itemList = [
    {url: 'web/images/biblio.jpg', title: '',page:'http://www.biblio.uh2c.ac.ma',tar:'_blank'},
	{url: 'web/images/wifilogo.png', title: 'Wifi université',page:'http://www.univcasa.ma/wifi.php',tar:'_blank'},
	{url: 'web/images/master.jpg', title: '',page:'http://mastermarketing.uh2c.ac.ma/',tar:'_blank'},
	{url: 'web/images/career.jpg', title: '',page:'http://careers.uh2c.ac.ma/fr/home/',tar:'_blank'},
    {url: 'web/images/unchain.jpg', title: '',page:'http://www.unchain-vu.net',tar:'_blank'},
    {url: 'web/images/carte.jpg', title: '',page:'carte_jamiati.php',tar:'_self'},
      {url: 'web/images/elearning.jpeg', title: '',page:'http://elearning.univcasa.ma/',tar:'_blank'}
	  
];

function mycarousel_itemVisibleInCallback(carousel, item, i, state, evt)
{
    // The index() method calculates the index from a
    // given index who is out of the actual item range.
    var idx = carousel.index(i, mycarousel_itemList.length);
    carousel.add(i, mycarousel_getItemHTML(mycarousel_itemList[idx - 1]));
};

function mycarousel_itemVisibleOutCallback(carousel, item, i, state, evt)
{
    carousel.remove(i);
};

/**
 * Item html creation helper.
 */
function mycarousel_getItemHTML(item)
{
    return '<a href="'+item.page+'" target="'+item.tar+'"><img src="' + item.url + '" width="154" height="84" alt="' + item.title + '" /></a>';
};


jQuery(document).ready(function() {
    jQuery('#mycarousel').jcarousel({
        wrap: 'circular',
		auto:4,
		scroll: 1,
		vertical: true,
        itemVisibleInCallback: {onBeforeAnimation: mycarousel_itemVisibleInCallback},
        itemVisibleOutCallback: {onAfterAnimation: mycarousel_itemVisibleOutCallback}
    });
});

</script> 
