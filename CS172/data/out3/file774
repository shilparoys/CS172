if(!ismobile()){

var  controller = $.superscrollorama ();

//heady
controller.addTween(
	'#applications',
	(new TimelineLite())
		.append([
			TweenMax.fromTo($('#heady h2'), 1,
				{css:{bottom: -500}, immediateRender:true},
				{css:{bottom: 10}}),
			TweenMax.fromTo($('#practical h2'), 1,
				{css:{top: -500}, immediateRender:true},
				{css:{top: 10}})
		]),
	510,
	-150
);

//dogs
controller.addTween(
	'#know',
	(new TimelineLite())
		.append([
			TweenMax.fromTo($('#dogs1'), 1,
				{css:{top: 540}, immediateRender:true},
				{css:{top: 340}}),
			TweenMax.fromTo($('#dogs2'), 1,
				{css:{top: 540}, immediateRender:true},
				{css:{top: -40}}),
			TweenMax.fromTo($('#dogs3'), 1,
				{css:{top: 880}, immediateRender:true},
				{css:{top: 080}})
		]),
	800,
	200
);

controller.addTween('#woof',  TweenMax.from($('#woof'), .5, {css:{opacity:0}}));
controller.addTween('#dog',  TweenMax.from($('#dog'), .5, {css:{opacity:0}}));


//change the world
controller.addTween(
	'#change',
	(new TimelineLite())
		.append([
			TweenMax.fromTo($('#change1'), 1,
				{css:{top: 800}, immediateRender:true},
				{css:{top: 670}}),
			TweenMax.fromTo($('#change2'), 1,
				{css:{top: 300}, immediateRender:true},
				{css:{top: 100}}),
			TweenMax.fromTo($('#change3'), 1,
				{css:{top: 800}, immediateRender:true},
				{css:{top: 200}})
		]),
	1000,
	200
);
//change the world
controller.addTween(
	'#impact',
	(new TimelineLite())
		.append([
			TweenMax.fromTo($('#make'), .01,
				{css:{top: -210}, immediateRender:true},
				{css:{top: 0}}),
			TweenMax.fromTo($('#an'), .01,
				{css:{top: -40}, immediateRender:true},
				{css:{top: 0}}),
			TweenMax.fromTo($('#impact'), .01,
				{css:{top: -100}, immediateRender:true},
				{css:{top: 0}})
		]),
	400,
	0
);

}

})
