  <script type="text/javascript">
            var weeklist = [ 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday', 'Sunday' ];
            $(function(){
                $.ajax({
                    type    : 'GET',
                    url     : '',
                    cache   : false,
                    success : function (data, status, xhr) {
                        var date = new Date(xhr.getResponseHeader('Date'));
                        var yy = date.getFullYear();
                        var mm = Number( date.getMonth() ) + 1;
                        var dd = date.getDate();
                        $('#getheaddate').text( yy+'.'+mm+'.'+dd+ ' ' + weeklist[ date.getDay() - 1 ] );
                    $('#to_newslink').attr('href', '/today/' + yy + '/');
                    }
                });
            });
        </script> 
  <script type="text/javascript">

function set_facebookitem(){
var formatDD = function(date) {
    var d = new Date(date);
    return ((d.getMonth() + 1) + '月' + d.getDate() + '日' );
};
$.getJSON(
    'http://ajax.googleapis.com/ajax/services/feed/load?callback=?',
    {
        q:'http://www.facebook.com/feeds/page.php?id=430763783673780&format=rss20',
        v:'1.0',
        num:10
    },
    function (data) {
        $.each(data.responseData.feed.entries, function(i, item){
        var plus = '<li>';
        plus += '<p>';
        plus += '<span class="title">' + formatDD(item.publishedDate) + '</span>';
        plus += '<span class="title">' + item.content.substr(0,(80)) + '...</span>';
        plus += '</p>';
        plus += '<p class="more"><a href="' + item.link + '" target="_blank">More<span class="icon--linkBlk"></span></a></p>';
        plus += '</li>';
           $('#facebook__list ul').append( plus );
        });
        $('.facebookslide').bxSlider({
          infiniteLoop: false,
          hideControlOnEnd: true,
          pager:false,
          speed:1500,
          minSlides: 2,
          maxSlides: 4,
          slideWidth: 212,
          slideMargin: 4,
          nextText: '',
          prevText: ''
      });
    }
);
}


//facebook
$(function() {
set_facebookitem();
$('.open_facebook').click( function(){
  $('#facebook__list').html('<ul class="bxslider__social clearfix facebookslide"></ul>');
  set_facebookitem();
});
  

        $('.youtubeslide').bxSlider({
        infiniteLoop: false,
        hideControlOnEnd: true,
        pager:false,
        speed:1500,
        minSlides: 2,
        maxSlides: 4,
        slideWidth: 212,
        slideMargin: 4,
        nextText: '',
        prevText: ''
    });
});
</script> 
