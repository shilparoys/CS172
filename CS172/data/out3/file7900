  <script type="text/javascript">

function moveOnMax(field,nextFieldID){
  if(field.value.length >= field.maxLength){
    document.getElementById(nextFieldID).focus();
  }
}

function isValidSSN() {
    var value = document.application.ssnum1__SS_NUM.value + document.application.ssnum2__SS_NUM.value + document.application.ssnum3__SS_NUM.value;		
    if(document.application.ssnum1__SS_NUM.value != "" || document.application.ssnum2__SS_NUM.value != "" || document.application.ssnum3__SS_NUM.value != ""){
    var re = /^([0-6]\d{2}|7[0-6]\d|77[0-2])([ \-]?)(\d{2})\2(\d{4})$/;
    if (!re.test(value)) {
    return false; 
    }
    
    }
    
    var temp = value;
    if (value.indexOf("-") != -1) { temp = (value.split("-")).join(""); }
    if (value.indexOf(" ") != -1) { temp = (value.split(" ")).join(""); }
    if (temp.substring(0, 3) == "000") {
    return false; 
    }
    if (temp.substring(3, 5) == "00") {
    return false; 
    }
    if (temp.substring(5, 9) == "0000") {
    return false; 
    }
    
    return true;
}

</script> 
