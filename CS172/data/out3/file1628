  <script type="text/javascript">
    $(document).ready(function () {
        $('.overlay').hide();
        $('.popup-box').hide();
        $('.show').click(function () {
            $('.overlay').show();
            $(this).parents('.how-read-bill li').find('.popup-box').show();
            return false;
        });

        $('.close').click(function () {
            $('.overlay').hide();
            $('.popup-box').hide();
            return false;
        });

    });

</script> 
