<style>
li.search .searchbox {
	margin: 3px 0 2px 0;
}
li.search form input#go  {
	margin: 0 0 3px 4px;
}
</style>
<![endif]--> 
    <script type="text/javascript">
var googleResultUrl = 'http://illinois.edu/resources/results.html';
var cx              = '006549799505564222509:-8lddip9q2g';
var cof             = 'FORID:11';

//leave skinId blank to use the campus default
var skinId              = '';

function searchForm() {
    return document.getElementById('illini-bar-search-form');
}
function sendSearch() {
    var search_form = searchForm();
    if (search_form.searchType_people.checked) {
        search_form.cx.value = '';
        search_form.cof.value = '';
        if(search_form.q.value == 'Search')
        {
            search_form.search.value = '';
        }
        else
        {
            search_form.search.value = search_form.q.value;  
        }
        search_form.skinId.value = skinId;
        search_form.action = "http://illinois.edu/ds/search";
        } else if (search_form.searchType_site.checked) {
        search_form.action = 'http://www.grad.illinois.edu/search/site';
        if(search_form.q.value == 'Search')
        {
            search_form.search.value = '';
        }
        else
        {
            search_form.search.value = search_form.q.value;  
        }
        search_form.gq.value = search_form.search.value;
        search_form.cx.value = "015408581927394603623:xzmtw_786-g";
        /* Remove things from form that interfere with Drupal, or are just
           plain useless clutter */
        $('#q').remove();
        $('#search').remove();
        search_form.submit();
        } else {
        search_form.cx.value = cx;
        search_form.cof.value = cof;
        search_form.id = cx;
        search_form.action = googleResultUrl;
    }
    return true;
}
function checkSearchValue(){
    if(searchForm().q.value == 'Search'){searchForm().q.value ='';}
}
</script> 
