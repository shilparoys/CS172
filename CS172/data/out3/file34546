          <script>
			
			function validateOnSubmit() {
			
				if (!Modernizr.input.required) {
					var x = document.loginCareerNet;		
					var errs=0;
					
					if (x.username.value == "")  {errs +=1; }
					if (x.password.value == "")  {errs +=1; }
					
					if (errs >= 1)  alert('Please fill out all mandatory fields with *');
					if (errs == 0)	{ return true; }
					else { return false; }
				}	
			
			}
			
			</script> 
          <div id="formContainer"> 
