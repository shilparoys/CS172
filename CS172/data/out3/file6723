   <!-- SS_BEGIN_SNIPPET(fragment71,scripts)--> 
   <script>
$(function() {
	  
    var question = $( "#question" ),
      usertype = $("input[type=radio][name=usertype]:checked").val(),
			url = "app/answers/list/kw/",
      allFields = $( [] ).add( question ).add( usertype ),
      tips = $( ".validateTips" );
 
    function updateTips( t ) {
      tips
        .text( t )
        .addClass( "ui-state-highlight" );
      setTimeout(function() {
        tips.removeClass( "ui-state-highlight", 1500 );
      }, 500 );
    }

 
    function checkLength( o, n, min, max ) {
      if ( o.val().length > max || o.val().length < min ) {
        o.addClass( "ui-state-error" );
        updateTips( "Length of " + n + " must be between " +
          min + " and " + max + "." );
        return false;
      } else {
        return true;
      }
    }
	
	 function checkType( ) {
	    user = $("input[type=radio][name=usertype]:checked").val();
      if (user == "Yes" ) {
        
				 url= "https://jcu.custhelp.com/" + url;
				
      } else {
        
				url= "https://jcu1.custhelp.com/" + url;
				
      }
			return true;
	
    } 
 
    function checkRegexp( o, regexp, n ) {
      if ( !( regexp.test( o.val() ) ) ) {
        o.addClass( "ui-state-error" );
        updateTips( n );
        return false;
      } else {
        return true;
      }
    }

    $( "#dialog-form" ).dialog({
      autoOpen: false,
      height: 300,
      width: 350,
      modal: true,
      buttons: {
        "Ask a Question": function() {
          var bValid = true;
          allFields.removeClass( "ui-state-error" );
          
          bValid = bValid && checkLength( question, "question", 5, 300 );
          bValid = bValid && checkType();
        
 
          if ( bValid ) {
            $( "#users tbody" ).append( "<tr>" +
              "<td>" + question.val() + "</td>" +
              "<td>" + $("input[type=radio][name=usertype]:checked").val() + "</td>" +
              "<td>" + url + "</td>" +
            "</tr>" );
						window.location = url + question.val() + "/search/1";
            $( this ).dialog( "close" );
						
						//"https://jcu1.custhelp.com/app/answers/list/kw/exam%20timetable/search/1";
          }
        },
        Cancel: function() {
          $( this ).dialog( "close" );
        }
      },
      close: function() {
        allFields.val( "" ).removeClass( "ui-state-error" );
      }
    });
 
    $( "#ask-a-question" )
      .button()
      .click(function() {
        $( "#dialog-form" ).dialog( "open" );
      });
			
  });

  </script> 
   <!-- SS_END_SNIPPET(fragment71,scripts)--> 
