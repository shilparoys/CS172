   <script>
  $(document).ready(function() {

    
	 var width = (window.innerWidth > 0) ? window.innerWidth : screen.width;
	if (width > 600) {
		$("#accordion").accordion({ heightStyle:"content", active:false, collapsible:true});
		}
	else
	  {
		$("#accordion").accordion({ heightStyle:"content", active:false, collapsible:true});
		}
  });
  </script> 
