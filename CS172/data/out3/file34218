    conditionizr.on('ie10', function () {

function overlay() {
    el = document.getElementById("overlay");
    el.style.visibility = (el.style.visibility == "visible") ? "hidden" : "visible";

}
overlay();
    });
    conditionizr.on('ie10touch', function () {

function overlay() {
    el = document.getElementById("overlay");
    el.style.visibility = (el.style.visibility == "visible") ? "hidden" : "visible";
}

overlay();
    });
    conditionizr.on('ie11', function () {

