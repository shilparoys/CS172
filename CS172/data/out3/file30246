   <script type="text/javascript">
function cr_popup(ta_id, ta_name, btn, top, left, position){
	var checkin_date;
	var checkout_date;

	if($.cookie("checkin_date") != null){
		checkin_date = new Date(parseInt($.cookie("checkin_date")));
		$(".checkin-date").datepicker("setDate", checkin_date);
	}
	if($.cookie("checkout_date") != null){
		checkout_date = new Date(parseInt($.cookie("checkout_date")));
		$(".checkout-date").datepicker("setDate", checkout_date);
	}
	
	$('.cr-dates').attr('id', 'taid_' + ta_id);
	$('.cr-iframe').html('');
	$('#cr').removeClass();
	$('#cr').addClass('new');
	if (checkin_date != null && checkout_date != null){
		var checkin = checkin_date.getFullYear() + "-" + (checkin_date.getMonth()+1) + "-" + checkin_date.getDate();
		var checkout = checkout_date.getFullYear() + "-" + (checkout_date.getMonth()+1) + "-" + checkout_date.getDate();
		var iframe_src = "http://www.tripadvisor.com/WidgetEmbed-cdshotelmeta?partnerId=1BC939997ADF43248096C8FF7D34EA0B&display=true&locationId=" + ta_id + "&checkin=" + checkin + "&checkout=" + checkout;
		var the_iframe = '<iframe src="" width="360" height="350" scrolling="no" frameborder="0" src=""></iframe>'
		$('#cr .cr-iframe').html(the_iframe);
		$('#cr .cr-iframe iframe').attr('src', iframe_src);
		
	}
	else {
		$('#cr .change-dates').hide();
		$('#cr #update-dates').css('display','block');
		$('.cr-header').show();
		$('#cr').addClass('no-dates');
	}
	var cr_btn_top = $(btn).offset().top + top;
	var cr_btn_left = $(btn).offset().left + left;
	$('#cr').addClass(position);
	$('#cr').css('top',cr_btn_top);
	$('#cr').css('left',cr_btn_left);
	$('#cr').fadeIn('fast');
	if ($('#cr').hasClass('no-dates')){
		$('#cr .cr-dates .checkin-date').datepicker('show');
	}
}

function close_popup(){
	$('#cr').hide();
	$('.cr-iframe').html('');
}

$(document).ready(function(){	
	$('#cr #update-dates').click(function (){
		if ($.cookie("checkin_date") != null && $.cookie("checkout_date") != null){
			if( $('#cr').hasClass('no-dates') ){
				$('#cr').removeClass('no-dates');
				$('#cr .change-dates').css('display','block');
				$('#cr #update-dates').hide();
				$('.cr-header').hide();
			}
			var in_date = $(this).parent().children(".checkin-date").datepicker("getDate");
			var out_date = $(this).parent().children(".checkout-date").datepicker("getDate");
			var checkin = in_date.getFullYear() + "-" + (in_date.getMonth()+1) + "-" + in_date.getDate();
			var checkout = out_date.getFullYear() + "-" + (out_date.getMonth()+1) + "-" + out_date.getDate();
			var ta_id = $('.cr-dates').attr('id').split('_')[1];
			
			var iframe_src = "http://www.tripadvisor.com/WidgetEmbed-cdshotelmeta?partnerId=1BC939997ADF43248096C8FF7D34EA0B&display=true&locationId=" + ta_id + "&checkin=" + checkin + "&checkout=" + checkout;
			var the_iframe = '<iframe src="" width="360" height="350" scrolling="no" frameborder="0" src=""></iframe>'
			$('#cr .cr-iframe').html(the_iframe);
			$('#cr .cr-iframe iframe').attr('src', iframe_src);	
		}
	});
});
</script> 
