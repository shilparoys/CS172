  <script type="text/javascript">
		$(document).ready(function(){
			setTimeout(function() {
				if ($(".com-count").children().children().html() != "0"){ $(".com-count").show();}
			},700);
			
			setStickyForm();
		});

		function setStickyForm(){
			var stickyDiv = $('div#left-rail'),
				content = $('div#main'),
				stickyDivHeight = stickyDiv.outerHeight(true),
				stickyDivLeft = stickyDiv.offset().left,
				yMin = stickyDiv.offset().top - parseInt(stickyDiv.css('margin-top'), 10),
				yMax = content.offset().top + content.outerHeight(true),
				position = 'default',
				positionCSS = {
					'default': {},
					'fixed': {position: 'fixed', top: 0},
					'bottom': {position: 'absolute', top: yMax - stickyDivHeight}
				};

			function setStickyDivPosition() {
				var w = $(this), x = w.scrollLeft(), y = w.scrollTop();
				if (y >= yMin) {
					if (y + stickyDivHeight < yMax) {
						position = 'fixed';
					}
					else {
						position = 'bottom';
					}
				}
				else {
					position = 'default';
				}
				// Clear all styles, then set appropriately.
				stickyDiv.removeAttr('style');
				stickyDiv.css(positionCSS[position]);
				// Adjust for horizontal scroll.
				if (position === 'fixed' && x > 0) {
					stickyDiv.css({left: stickyDivLeft - x});
				}
			}

			// Only slide the shares box on articles that are long enough.
			if (yMax - stickyDivHeight > yMin) {
				$(window).scroll(setStickyDivPosition);
			}	
		}
	</script> 
