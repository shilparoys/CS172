        <div id="block-block-14" class="block block-block block-javascript"> 
         <div class="block-inner clearfix"> 
          <div class="block-content content no-title"> 
           <script src="/sites/default/files/css_injector_images_image/matchMedia.min.js.txt"></script> 
           <script src="/sites/default/files/css_injector_images_image/jquery.onfontresize.min.js.txt"></script> 
           <script type="text/javascript">
<!--//--><![CDATA[//><!--
/*
 * MIT Commencement website javascript
 * rjw 9/14
 *
 * Requires:
 *   matchMedia polyfill          https://github.com/paulirish/matchMedia.js/
 *   jquery.onfontresize.js       http://www.tomdeater.com/jquery/onfontresize/
 *
 */


jQuery(document).ready(function() {

  var $ = jQuery;

  var sitedomain = "commencement.mit.edu";


  //// BREAKPOINTS
  //// must correspond to style sheet

  var brk_DesktopMenu = "(min-width: 641px)";    /* switch from mobile menu to normal menus */



  ////
  //// insert containers for backgrounds
  ////
  $("body").prepend($('<div id="divbkgimage"></div><div id="divbkgpanel"></div>'));



  ////
  //// move the sidebar content from the main column to the sidebar
  //// (note that the field may not exist if it was left empty)
  ////
  $(".field-name-field-sidetext").each(function() {

    // get content from sidebar field
    //
    var content = $(this).find(".field-item").html();

    // insert content into sidebar block (if it exists)
    //
    $(".block-sidetext-placeholder .block-content").html(content);

    // remove field from main column
    //
    $(this).remove();

    // add class that makes the sidebar block visible
    //
    $("body").addClass("block-sidetext");
  });



  ////
  //// copy sidebar alert to the features panel
  //// (may not exist, or may be more than one)
  ////
  $(".region-sidebar-second .block-alert").each(function() {
    $(this).clone().appendTo(".region-secondary-content");
    if (!($("body").hasClass("block-sidealert"))) {
      $("body").addClass("block-sidealert");
    }
  });



  ////
  //// set height of left sidebar placeholder to fill out column height
  //// (note - this is necessary because of the negative space above the util nav)
  ////
  function _fillsidebar() {
    if ($(".region-sidebar-first").is(":visible")) {

      // find max height of center and right columns
      //
      var h2 = $("#content-column").height();
      var h3 = $(".region-sidebar-second").height();
      var h23 = (h2 > h3 ? h2 : h3);

      // get height of navigation blocks in left sidebar
      //
      var h1 = $(".block-commencement-audience").outerHeight(true) +
               $(".block-commencement-utility").outerHeight(true);

      // calculate target height for filler block
      //
      hf = (h23 > h1 ? (h23 - h1) : 0);

      // fill out left column with placeholder block
      //
      $(".region-sidebar-first .block-placeholder").css('height', hf);
    }
    else {
      $(".region-sidebar-first .block-placeholder").css('height', 0);
    }
  }

  // bind function to window resize
  //
  $(window).resize(_fillsidebar);



  ////
  //// adjust active and active-trail classes in menus
  ////

  // remove all active and active-trail from Event menu
  //
  $(".block-commencement-event li, .block-commencement-event a").removeClass("active active-trail");

  // make sure only one active menu item in menus, and that all parents are marked active-trail
  //
  $(".block-commencement-audience a.active, .block-commencement-utility a.active").parent("li").removeClass("active").addClass("active-trail").parents("li").addClass("active-trail").children("a").removeClass("active").addClass("active-trail");



  ////
  //// mobile menu
  ////

  // create container, toggle button, and menu container
  //
 $mobilemenu = $('<div id="divmobmenu"><div id="divmobtoggle"><a href="#" id="mobtogglelink">Menu</a></div><div id="divmoblinks"><div id="divmobeventlinks"></div><div id="divmobaudiencelinks"></div><div id="divmobutilitylinks"></div><div id="divmobfooterlinks"></div></div></div>');

  // get event menu
  //
  var $eventmenu = $("#block-menu-menu-commencement-event .block-content > ul").clone();
  $mobilemenu.find("#divmobeventlinks").append($eventmenu);

  // get audience menu
  //
  var $audiencemenu = $("#block-menu-menu-commencement-audience .block-content > ul").clone();
  $mobilemenu.find("#divmobaudiencelinks").append($audiencemenu);

  // get utility menu
  //
  var $utilitymenu = $("#block-menu-menu-commencement-utility .block-content > ul").clone();
  $mobilemenu.find("#divmobutilitylinks").append($utilitymenu);

  // get footer menu
  //
  var $footermenu = $("#block-menu-menu-commencement-footer .block-content > ul").clone();
  $mobilemenu.find("#divmobfooterlinks").append($footermenu);

  // set all li with a child ul to openable but closed
  //
  $mobilemenu.find("#divmoblinks li").children("ul").parent().addClass("openable closed");

  // ...but then open up the active trail
  //
  $mobilemenu.find("#divmoblinks a.active, #divmoblinks a.active-trail").parent().removeClass("closed").addClass("open");

  // create opener/closer buttons
  //
  $mobilemenu.find("#divmoblinks li.openable > a").append('<span class="opener">+</span><span class="closer">&ndash;</span>');


  // toggle mobile menu on/off
  //
  $mobilemenu.find('#divmobtoggle a').on('click', function(e) {
    e.preventDefault();
    $('#divmoblinks').slideToggle();
    $(this).toggleClass('active');
  });

  // toggle submenus on/off
  //
  $mobilemenu.find('span.opener').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('closed').addClass('open');
  });
  $mobilemenu.find('span.closer').on('click', function(e) {
    e.preventDefault();
    $(this).parent('a').parent('li').removeClass('open').addClass('closed');
  });

  // insert mobile menu into page
  //
  $("body").prepend($mobilemenu);



  ////
  //// set link classes and open links in new windows
  ////

  // links starting with "http" (but not to this site)
  //
  $("a[href^='http']").not("[href*='"+sitedomain+"']").attr('target','_blank').addClass('external');

  // links starting with "mailto"
  //
  $("a[href^='mailto']").addClass('mailto');

  // links ending with ".pdf"
  //
  $("a[href$='.pdf']").attr('target','_blank').addClass('pdf');

  // links ending with ".ppt"
  //
  $("a[href$='.ppt']").attr('target','_blank').addClass('ppt');

  // links ending with ".doc"
  //
  $("a[href$='.doc']").attr('target','_blank').addClass('doc');



  ////
  //// insert prompt into search box, then
  //// clear search on initial activation
  ////
  var searchtext = 'SEARCH';

  /* fill if empty */
  $('#block-google-appliance-ga-block-search-form .form-text').each( function() {
    if (!this.searchstarted) {
      if (this.value == '') {  // only fill if blank
        this.value = searchtext;
      }
      if (this.value.toLowerCase() != searchtext.toLowerCase()) {  // other value inherited on page reload
        $(this).addClass('active');
        this.searchstarted = true;
      }
    }
  });

  /* check on focus */
  $('#block-google-appliance-ga-block-search-form .form-text').focus( function() {
    if (!this.searchstarted) {
      if (this.value.toLowerCase() == searchtext.toLowerCase()) {  // only clear if it still has original text
        this.value = "";
        $(this).addClass('active');
      }
      this.searchstarted = true;
    }
  });

  ////
  //// change text of search button to "go"
  ////
  $('#block-google-appliance-ga-block-search-form .form-submit').attr('value','Go');



  ////
  //// rewrite links to http to links to https for logged-in users
  ////
  if ($('body').hasClass('logged-in')) {
    $("a[href^='http://"+sitedomain+"']").each(function() {
      var href = $(this).attr('href');
      href = href.replace('http', 'https');
      $(this).attr('href', href);
    });
  }


  ////
  //// responsive embedded iframes for videos, google maps, etc.
  //// adapted from http://css-tricks.com/NetMag/FluidWidthVideo/demo.php
  ////

  // on this site, include all iframes embedded into content area, unless explicitly requested not to
  var $allVideos = $(".content-style iframe, .region-sidebar-second iframe").not(".noresize");

  // initialize
  //
  $allVideos.each(function() {
    $(this)
      // jQuery .data does not work on object/embed elements
      .attr('data-aspectRatio', this.height / this.width)
      .removeAttr('height')
      .removeAttr('width')
      .wrap('<div class="videowrapper"></div>');

    // attempt to give a helpful style sheet to TechTV videos so they will act responsively
    //
    if ($(this).is("iframe[src*='techtv.mit.edu']")) {
      var src = $(this).attr('src');
      src = src.replace('http:','');  // remove http: so that video works through https
      src = src.replace('external_stylesheet=','external_stylesheet=//'+sitedomain+'/sites/default/files/css_injector_images_image/techtvresponsive.css');
      $(this).attr('src', src);
    }
  });

  // react to window resize event
  //
  function _videoresize() {
    $allVideos.each(function() {
      var $el = $(this);
      var newWidth = $el.parent().width();
      $el
        .width(newWidth)
        .height(newWidth * $el.attr('data-aspectRatio'));
    });
  }
  $(window).resize(_videoresize);

  // execute initial video sizing
  //
  //_videoresize();  // will be triggered by window resize below


  ////
  //// Monitor for font resize and call window resize when found
  //// Requires jquery.onfontresize.min.js
  ////
  $(document).bind("fontresize", function () {
    $(window).resize();
  });



  $(window).resize(); // kick off resize-triggered events when page loaded

});
//--><!]]>
</script> 
          </div> 
         </div> 
        </div> 
       </div> 
      </footer> 
     </div>
    </div> 
