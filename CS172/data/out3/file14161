       var ambientListeningIsEnabledForMediaSet = false;
       var ambientListeningIsEnabledForOwner = false;
       var isOwner = false;

       function shouldShowAmbience()
       {
           return ( isOwner || ambientListeningIsEnabledForMediaSet ) && 
                   ambientListeningIsEnabledForOwner && !showingFullSize;
       }

       // Entry point for Flash and Silverlight...
       function captionChanged(text)
       {
           if ( shouldShowAmbience() )
           {
               $(document).trigger("techsmith.screencast.captionchanged", text);
           }
       }     

       function hasCaptions()
       {
           if ( shouldShowAmbience()  )
           {     
               // Open the ambience control.
               $(document).trigger("techsmith.screencast.ambience.open", ["ambience"]);
           }
       }

       $().ready( function() {
           $(document).bind("techsmith.screencast.hascaptions", function (eventTarget) { hasCaptions(); });
           $(document).bind("techsmith.screencast.ambience.hide", function () {
               $("#ambienceAuthorizationContainer:visible").hide();
               $(window).resize();          
           });
           $(document).bind("techsmith.screencast.ambience.show", function () {
               $("#ambienceAuthorizationContainer:hidden").show();
               $(window).resize();
           });
           $(document).bind("techsmith.screencast.ambience.open", function (eventTarget, id) {
               if ( shouldShowAmbience() && isOwner )
               {
                   // Setup owner controls for ambience...
                   $("#ambienceAuthorizationContainer").remove();
                   $("#ambience").after("<div id='ambienceAuthorizationContainer' ><div id='ambienceAuthorization'><a id='ambienceAuthButton' class='button' href='javascript:toggleAmbience();'></a></div></div>");
                   $("#ambience").addClass("ambienceOwner");
                   setupAmbienceAuthText();

                   $("#ambienceAuthorizationContainer").toggle( !$("#ambience").hasClass("hidden") );
               }
           });
           $(document).bind("techsmith.screencast.ambience.close", function () {
               $("#ambienceAuthorizationContainer").remove();
           });

           $(window).resize(resizeEvent);
           $(window).resize();
           $('#mediaTable').css('visibility','visible');
           CSOnLoad();

           $("#clickToPlay:visible").css({"width": 800, "height": 600});

         bindKeyDownNumbers($(".numberOnlyText"));
         bindChangedDimenson($("#embedWidth"), $("#embedHeight"), originalWidth , originalHeight , [ $("#embed") , $("#noIframeEmbed") ] );
          
       });
       
     function resetDimensionsToOriginal()
     {
         resetDimensions( $("#embedWidth"), $("#embedHeight"), originalWidth, originalHeight, [$("#embed"), $("#noIframeEmbed")]);  
     }
     
     function toggleAmbience()
     {     
         PageMethods.SetAmbience( "b198c0fc-d68e-4e5a-9a2b-82dee69088b5", !ambientListeningIsEnabledForMediaSet, OnSetAmbienceSucceeded, OnSetAmbienceFailed ); 
      }

      function OnSetAmbienceSucceeded()
      {
          ambientListeningIsEnabledForMediaSet = !ambientListeningIsEnabledForMediaSet;
          setupAmbienceAuthText();
      }

      function OnSetAmbienceFailed()
      {
          // Display an appropriate error message...
          displayError( "Update to this content failed.  Please try again or <a href=/about.aspx>contact Screencast Support.", false );
          return false;
      }

      function setupAmbienceAuthText()
      {
          if ( ambientListeningIsEnabledForMediaSet )
          {
              $("#ambienceAuthButton").html("Hide this from my viewers");
              $("#ambience").removeClass("ambienceHidden");
          }
          else
          {
              $("#ambienceAuthButton").html("Show this to my viewers");
              $("#ambience").addClass("ambienceHidden");
          }
      }
          
      var canScale = false;
      var originalWidth = 800;
       var originalHeight = 600;
       var aspectRatio = originalWidth / originalHeight;
       var minWidth = 400;
      var minHeight = 300;
      
       /*****************************************************************
        * Begin content scaling section.
        ****************************************************************/           
       // Flag whether the Full size option is on or not.
       var showingFullSize = false;
   
       var boundsWidth;
       var boundsHeight;
      
       // Toggle the Full size option.
       function toggleResizing()
       {
           showingFullSize = !showingFullSize;
           if ( showingFullSize )
           {
               // Reset the content to its original dimensions.
               setDimensions( originalWidth, originalHeight );

               $(document).trigger("techsmith.screencast.ambience.close");
           }
           else
           {
               scaleContent();

               if ( shouldShowAmbience()  )
               {  
                   $(document).trigger("techsmith.screencast.ambience.open", ["ambience"]);
               }
           }
           setupFullSmallIcon();
       }
      
       // Setup the navigation and resizing UI elements based on the scale of the
       // content and whether the Full size option is on or not.
       function setUpResizingUI()
       {
           if ( !canScale || ( boundsWidth > originalWidth && boundsHeight > originalHeight || 
                document.getElementById( "resizeArea" ) == null ) )
           {
               // The content area is larger than the content, no scaling is needed.
               if ( document.getElementById( "resizeArea" ) )
               {
                   document.getElementById( "resizeArea" ).style.display = "none";
               
                   var socials = $("#allSocials");
            
                   if ( socials.length )
                   {
                       $(".socialMedia").width( "100%" );
                       socials.css( "margin", "0 auto" );
                       socials.width( "200px" );
                   }
               }
            
               showingFullSize = false;
           }
           else
           {  
               if ( document.getElementById( "resizeArea" ) )
               {
                   // The content area is smaller than the content, scaling is allowed
                   // so show the Full size option.
                   document.getElementById( "resizeArea" ).style.display = "block";
               
                   var socials = $("#allSocials");
            
                   if ( socials.length )
                   {
                       $(".socialMedia").width( "50%" );
                       $(".socialMedia").css( "position", "relative" );
                       $(".socialMedia").css( "left", "-28px" );
                       socials.css( "margin", "0 0" );
                       socials.width( "auto" );
                       $("#resizeArea").width( "50%" );
                       $("#resizeLink").css( "float", "right" );
                       $("#resizeLink").css( "margin", "0 62px 0 0" );
                   }
                   else
                   {
                       $("#resizeArea").width( "100%" );
                       $("#resizeLink").css( "float", "none" );
                       $("#resizeLink").css( "margin", "auto" );
                   }
               }
            
               setupFullSmallIcon();
           }
       }   
      
       function setupFullSmallIcon()
       {
           // Display the Full/Small size size option text as applicable.
           if ( !canScale )
           {
               document.getElementById( "scaleImage" ).style.display = 'none';
           }
           else if ( showingFullSize )
           {
               if ( document.getElementById( "resizeLink" ) )
               {
                   document.getElementById( "resizeLink" ).title = "small size";
                   document.getElementById( "scaleImage" ).alt = "small size";
                   document.getElementById( "scaleImage" ).src = "/img/view/smallsize.gif";
               }
           }
           else
           {
               if ( document.getElementById( "resizeLink" ) )
               {
                   document.getElementById( "resizeLink" ).title = "full size";
                   document.getElementById( "scaleImage" ).alt = "full size";
                   document.getElementById( "scaleImage" ).src = "/img/view/fullsize.gif";
               }
           }
       }
      
       var resizeTimeout;
       // Set the content to the given dimensions.
       function setDimensions( width, height )
       {
           if ( canScale )
           {
               // Set the dimensions of all items with the embeddedObject class.
               var embeds = $(".embeddedObject");

               //hack for FF 3.6 to make Flash Object Resize Properly on PageLoad
               clearTimeout( resizeTimeout ); 
               resizeTimeout = setTimeout(function(){embeds.width(width);embeds.height(height);}, 0);

               $("#resize").width( width );
            
               $(document).trigger("techsmith.screencast.ambience.resize", [height - $("#ambienceAuthorizationContainer:visible").outerHeight()]);
           }
       }
      
       // Scale the content based on the bounds and content dimensions.  
       function scaleContent()
       {
           var widthCopy = originalWidth;
           var heightCopy = originalHeight;
           var width = widthCopy;
           var height = heightCopy;

           if ( canScale && ( boundsWidth < widthCopy || boundsHeight < heightCopy ) )
           {
               // Sometimes we need to clamp the width to the bounds, sometimes it's
               // the height. The idea is to clamp the dimension that contributes
               // more to the size. That way the lesser dimension will be guaranteed
               // to be scaled inside the bounds

               var widthRatio = widthCopy / boundsWidth;
               var heightRatio = heightCopy / boundsHeight;
               if ( widthRatio > heightRatio )
               {
                   width = boundsWidth;
                   height = boundsWidth * heightCopy / widthCopy;
               }
               else
               {
                   width = widthCopy * boundsHeight / heightCopy;
                   height = boundsHeight;
               }
           }

           setDimensions( width, height );
       }
      
       // Determine the height that is taken up by things other than the content.
       function determineNonContentHeight()
       {
           var h = $("#mediaTable").offset().top;

           h += $("#nav" ).outerHeight(true);           
           h += $("#notifyFlashHelpArea").outerHeight(true);
           h += $("#mediaControls").outerHeight(true);
         
           return h;
       }  
      
       // Determine the bounds available for the content based on the users browser
       // dimensions - removing room for the header and respecting a minimum area. 
       function determineContentBounds()
       {
           var bw,bh;
           if (self.innerHeight) // all except Explorer
           {
               bw = self.innerWidth;
               bh = self.innerHeight;
           }
           else if (document.documentElement && document.documentElement.clientHeight)
               // Explorer 6 Strict Mode
           {
               bw = document.documentElement.clientWidth;
               bh = document.documentElement.clientHeight;
           }
           else if (document.body) // other Explorers
           {
               bw = document.body.clientWidth;
               bh = document.body.clientHeight;
           }

           // Set the bounds, with a little padding.
           boundsWidth = bw - 6 - $("#ambience:visible").outerWidth(true) - 28;
           boundsHeight = bh - determineNonContentHeight() - 6;

           if ( boundsWidth < minWidth )
           {
               boundsWidth = minWidth;
           }

           if ( boundsHeight < minHeight )
           {
               boundsHeight = minHeight;
           }
       }
      
       // Event handler for the window resizing.
       function resizeEvent()
       {
           // Determine the bounds that the content needs to scale to.
           determineContentBounds();
         
           if ( canScale && !showingFullSize )
           {
               // Scale the content if the user has NOT clicked the "full size" option.
               scaleContent();
           }
         
           // Setup the full/small size UI based on whether the content is scaled or not.
           setUpResizingUI();
       }
      
       // Set the handler.
       window.onresize = resizeEvent;
      
       function OnClickToPlay()
       {
           document.getElementById( 'mediaDisplayArea' ).style.display = 'inline';
           document.getElementById( 'clickToPlay' ).style.display = 'none';
       }
      
       // Jump to time in seekable media
       function SeekToTime( t )
       {
           $(document).trigger("techsmith.screencast.seek", t) ;
           window.location.hash = "topOfPage";
           try
           {
               // For IE
               document.getElementById( 'scPlayer' ).jsSeekTime( t );
           }
           catch(e)
           {
             
               try
               {
                   // For Firefox and others
                   document.getElementById( 'scPlayerFF' ).jsSeekTime( t );
               }
               catch(e) {}

           }
       }
      
       // Displays the waiting icon...
       function wait() 
       {
           $( "#waitingArea" ).addClass( "waiting" );
       }

       // Hides the waiting icon...
       function stopWait() 
       {
           $( "#waitingArea" ).removeClass( "waiting" );
       }
      
       function getMediaSetIdForFunction() 
       {
           return document.getElementById("mainContentPlaceholder_action_mediaSetId").value;
      }

      function getMediaGroupIdForFunction() 
      {
          return document.getElementById("mainContentPlaceholder_action_mediaGroupId").value;
      }

      /****************************************************************
       * Begin detail tab functions
       ***************************************************************/
      
      // Event handler for the Edit action of a folder Item
      //
      // Params:
      // mediaGroupId - the id of the folder either that was selected or that the selected item is contained in
      // mediaSetId - the id of the selected item; blank if a folder was selected
      function onClickEdit(containerId, mediaSetId) 
      {
          document.getElementById("mediaDisplayArea").innerHTML = "";
          document.getElementById("mediaDisplayArea").style.display = "none";
          document.getElementById("mainContentPlaceholder_action_mediaSetId").value = mediaSetId;
         document.getElementById("mainContentPlaceholder_action_mediaGroupId").value = containerId;
          loadStylesheet( "/inc/styles/base.css" );
          sc_dialog( "Edit Content Properties", "/controls/lightboxes/settingsmedia.aspx?mediaSetId=" + mediaSetId + "&mediaGroupId=" + containerId, 
             585, 410,
             {
                 "Cancel": function() { cancelMetaData(); },
                 "Save": function() { if (saveAll(false)) { setTimeout( 'doneEditing()', 500); tb_remove(); } }
             } );
          return false;
      }

      // Called after the thickbox for editing has been closed. 
      // We need to reload the page because we hid the media to open
      // the thickbox.
      function doneEditing() 
      { 
          window.location.reload();
      }
      
      // Some CS presentations add events with an OnLoad function (created by the template).  
      // This function will run CS's OnLoad() if it exists.  If it does not exist, no 
      // error will be thrown.
      function CSOnLoad() 
      {
          try
          {
              OnLoad();
          }
          catch(e)
          {}
      }
      
      function onMove( pos )
      {
          var movePos = pos <= 0 ? pos : 0;
          movePos = movePos >= 1 ? movePos : 1;
          var element = document.getElementById( "Navigation" + movePos );

          window.location = element;
      }
      
      function scrollSelectedNavItemIntoView( elem )
      {
          var sl = elem.offsetLeft + ( elem.clientWidth / 2 ) - document.getElementById('mainContentPlaceholder_navigationDiv').offsetLeft - ( document.getElementById('mainContentPlaceholder_navigationDiv').clientWidth / 2 ) + elem.offsetParent.offsetLeft;
          document.getElementById('mainContentPlaceholder_navigationDiv').scrollLeft = sl;
      }
      
      /****************
       * Begin upload thumbnail functions.  The upload thumbnail button is inside the Edit Details lightbox.
       ***************/
      
      // Event handler for the Upload Thumbnail link.  
      function goToThumbnail(mediaSetId) 
      {
          tb_remove();
          
          document.getElementById("mainContentPlaceholder_action_mediaSetId").value = "b198c0fc-d68e-4e5a-9a2b-82dee69088b5";
         document.getElementById("mainContentPlaceholder_action_mediaGroupId").value = "079553aa-ec0c-46c0-a5aa-c54ec27189a8";
          showUploadDialog("Upload Thumbnail", "/controls/lightboxes/upload.aspx?userName=GRCCIT&mediaGroupName=Jing&viewpage=true&mediaSetId=b198c0fc-d68e-4e5a-9a2b-82dee69088b5&thumbnail=true", doneThumbnailing);
          return false;
      }

      function doneThumbnailing() 
      {
          tb_remove();
          onClickEdit( document.getElementById("mainContentPlaceholder_action_mediaGroupId").value, document.getElementById("mainContentPlaceholder_action_mediaSetId").value );
         return false;
     } 
     /**************
      * End thumbnail functions
      *************/
      
     /********************************************************
     * End detail tab functions
     *******************************************************/
              
     /*******************************************************
     * Begin Upload tab functions
     ******************************************************/
      
     // Event handler for the Upload Attachments link.
     function goToUpload() 
     {
         document.getElementById("mediaDisplayArea").innerHTML = "";
         document.getElementById("mediaDisplayArea").style.display = "none";
         document.getElementById("mainContentPlaceholder_action_mediaSetId").value = getMediaSetIdForFunction();
         document.getElementById("mainContentPlaceholder_action_mediaGroupId").value = getMediaGroupIdForFunction();
          showUploadDialog("Upload Attachments", "/controls/lightboxes/upload.aspx?userName=GRCCIT&mediaGroupName=Jing&viewpage=true&mediaSetId=b198c0fc-d68e-4e5a-9a2b-82dee69088b5", doneEditing);
          return false;
      }
      
      /*********************************************************
       * End Upload tab functions
       ********************************************************/
       
      /********************************************************
       * Begin Attachment tab functions
       *******************************************************/
      function DeleteClick( mediaId )
      {
          if( isUserSignedInOrRedirectToSignInPage() )
          {
              if( confirm("Click OK to delete this attachment?") )
              {
                  PageMethods.DeleteMedia( mediaId, OnDeleteSucceeded, OnDeleteFailed );
              }
              else
              {
                  return false;
              }
          }
      }
         
      function DefaultClick( mediaId, mediaSetId )
      {
          if( isUserSignedInOrRedirectToSignInPage() )
          {
              if( confirm("Swap currently displayed content with this attachment. The URL will not change. Current content becomes the attachment. ") )
              {   
                  // There's no need to check for success or failure here.  If an error happens, the code behind will set an error message
                  // in the session and the info bar will show it on reload.
                  PageMethods.SetDefaultMedia( mediaId, mediaSetId, OnDefaultSucceeded, OnDefaultFailed ); 
              }
              else
              {
                  return false;
              }
          }
      }
         
      function OnDeleteSucceeded()
      {
          __doPostBack( "mainContentPlaceholder_attachmentUpdateButton", "" );
       }
       
       function OnDeleteFailed( error )
       {
           displayError( error.get_message(), false );
           return false;
       }
       
       // This function closes the upload lightbox and toggles tab visibility for owners
       function OnDefaultSucceeded()
       {
           window.location.reload();
       }
       
       function OnDefaultFailed()
       {
           displayError( "Unable to replace the currently displayed content with this attachment", false );
           return false;
       }
         
       /*****************************************************
        * End attachment tab functions
        ****************************************************/ 
        
       /*****************************************************
        * Begin owner functions
        ****************************************************/
        
       // Toggles whether or not the template is overriden and
       // tabs are shown to the owner
       function toggleShowDetails()
       {
           PageMethods.ToggleShowDetails( OnDefaultSucceeded, OnDefaultSucceeded );
       }
        
       function showMoreSupportingFiles()
       {
           document.getElementById('moreSupportingFiles').style.display = "inline";
           document.getElementById('moreSupportingLink').style.display = "none";
       }
        
       function hideMoreSupportingFiles()
       {
           document.getElementById('moreSupportingFiles').style.display = "none";
           document.getElementById('moreSupportingLink').style.display = "inline";
       }
        
       /****************************************************
        * End owner functions
        ***************************************************/
       jQuery(function(){
           var tabs = $('#tabs').tabs({
               cookie: {}
           });

           // make sure a tab is selected if the cookie was not set
           var selectedTab = tabs.tabs('option', 'selected');
           if( selectedTab == undefined || selectedTab == null || selectedTab < 0 ){
               tabs.tabs('select', 0); 
           }
       });
   </script> 
