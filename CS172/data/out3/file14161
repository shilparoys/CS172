      /*****************************************************************
      * Begin plugin detection
      * NOTE: 'top' is used below to allow for this control to be run in an
      * iframe.
      ****************************************************************/

      var minFlashVersionMajor = 10;
      var minFlashVersionMinor= 1;
      var minFlashVersionRev = 0;
      var adobeFlashDownloadLink = 'http://get.adobe.com/flashplayer/';

      

      
      
      
         function pluginCheck()
         {
            var flash = checkFlash( minFlashVersionMajor, minFlashVersionMinor, minFlashVersionRev );

            if ( flash == needsUpgrade )
            {
               top.displayWarning( 'You appear to be using an outdated version of the Adobe Flash Player. If you experience problems viewing this content, please try upgrading to the latest version which is available for free at <a href="' + adobeFlashDownloadLink + '" target="_blank">' + adobeFlashDownloadLink + '</a>.', false, 0 );
            }
            else if ( flash == noFlash )
            {
               top.displayError( 'Viewing this content requires Adobe Flash Player.  You can download Adobe Flash Player from <a href="' + adobeFlashDownloadLink + '" target="_blank">' + adobeFlashDownloadLink + '</a>.', false, 0 );
               removeMediaDisplayArea();
            }
         }
      

      

      

      

      
      
      /****************************************************************
      * End plugin detection
      ***************************************************************/
</script> 
