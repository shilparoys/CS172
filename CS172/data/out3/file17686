$(document).on('click', '#RightMenuButton', function() { trackEvent('login'); });
$(document).on('click', '.navBtnSubscribe', function() { trackEvent('subscribe'); });
$(document).on('click', '.channel-article-header', function() { trackEvent('channelheader'); });
$(document).on('click', '.article-body a', function() { trackEvent('inlinelink'); });
$(document).on('click', '.readmoreitem', function() { trackEvent('readmore'); });

function trackEvent(eventtype)
{
    $.ajax({
       type: "POST",
       url: "/rest/track.php",
       data: { incid: 61485, usrid: usrid, tempid: getCookie('incrnd'), abgrp: getCookie('incab'), eventtype: eventtype }
     });
}
</script> 
