         <h2>Event Calendar</h2> 
         <div class="MiniCalendarList"> 
          <div id="LateLoadCalendar" class="LoadingPageContent Loading LoadingCalendar"> 
           <img src="/images/ajax-loader.gif" alt="Loading" width="16" height="16"> 
           <span class="LoadingText">Loading...</span> 
          </div> 
          <div id="CalendarContainer"></div> 
          <script id="CalendarTemplate" type="text/x-handlebars-template">
				<div class="ListEntry">
					<div class="ListEntryDate">{{EventDate}}</div>
					<div class="ListEntryInfo">
						<div class="ListEntryTitle"><a href="{{EventUrl}}">{{EventTitle}}</a></div>
						<div class="ListEntryText">{{{EventDescription}}}</div>
					</div>
				</div>
			</script> 
          <script type="text/javascript">
				$(document).bind("coreLoaded", function (event) {
					core.webMethods.get("getevents", {url:'/calendar/json/eventList.aspx',count:3},
						function (data) {
							calTemplate = Handlebars.compile($("#CalendarTemplate").html());
							for (i = 0; i < data.Data.events.length; i++) {
								thisItem = data.Data.events[i];
								calItem = {
									EventTitle: thisItem.Title.Value,
									EventUrl: thisItem.Links[0].Href,
									EventDescription: thisItem.Content.Value.replace(/\<a.*\>more\<\/a\>/gm, ''),
									EventDate: $.format.date(thisItem.Published, "MMM d")
								}
								$("#CalendarContainer").append(calTemplate(calItem));
							}
							$("#LateLoadCalendar").fadeOut();
						}
					);
				});
			</script> 
         </div> 
         <p><a href="/calendar/" class="More">More Events</a></p> 
        </div> 
