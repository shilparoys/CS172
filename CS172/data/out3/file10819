  <script type="text/javascript">$(document).ready(function() {
 //NAVBAR JQUERY
 $(".submenu").hide();
 $(".navbarlink.navfirst, .navbarlink.navfirst").bind("mouseover", function(){
 $(".submenu.second").hide();
 $(".submenu.third").hide();
 $(".submenu.first").show();
 });
 $(".submenu.first").bind("mouseout", function(){
 $(".submenu.first").hide();
 });
 $(".navbarlink.navsecond").bind("mouseover", function(){
 $(".submenu.first").hide();
 $(".submenu.third").hide();
 $(".submenu.second").show();
 });
 $(".navbarlink.navsecond").bind("mouseout", function(){
 $(".submenu.second").hide();
 });
 $(".navbarlink.navthird").bind("mouseover", function(){
 $(".submenu.first").hide();
 $(".submenu.second").hide();
 $(".submenu.third").show();
 });
 $(".navbarlink.navthird").bind("mouseout", function(){
 $(".submenu.third").hide();
 });
 //mousewheel border at bottom of navbar
 var offset = $("#top_logo").offset();
 $(window).scroll(function () {
 var scrollTop = $(window).scrollTop();
 if (offset.top < scrollTop) {
 $('#top_logo').css('border-bottom', 'solid 2px #e5e5e5');
 } else {
 $('#top_logo').css('border-bottom', 'none');
 }
 });
 //END OF NAVBAR JQUERY
});
</script>
