     <script type="text/javascript">
					/* <![CDATA[ */

						var pause = Number();
						var timer = Number();
						var slides = Number();
						var rotations = Number();	// Sets the number of rotations - always lands on the first slide
						var i = Number();			// Current slide
						var j = Number();			// Next slide
						var timeout;
						
						slides = $("#slideshow > div").size();
						
						
						$('#slideshow div').each(function(arr) {
							idee = arr+1;
							$(this).attr("id", "slide" + idee);
						});
	
						// time in milliseconds
						// 500 = 1/2 a second, 1000 = 1 second etc…
						timer = 700;
						pause = 2*1000;
						
						function changeText() {
							
							if (i <= 1) {
								// End
								$("#slideshow div:last a").html('Play intro again');
								$("#slide"+slides).fadeIn(timer, function() { $("#slideshow div").show(); });
							} else {
								$("#slide"+i).fadeOut(timer, function() { timeout = setTimeout("changeText()", pause); });
							}
							
							i--;
						}
						
						$("#slideshow div:last").append('<a tabindex="2" href="#">Play intro</a>');
						
						$("#slideshow a").click(function() {
							$("#slideshow div").show();
							i = slides;
							changeText();
						 });
						
					/* ]]> */
                    </script> 
