$(function(){
    $('#shell').addClass('carousel');
    
                    var effect = "None";
                    if ( effect.indexOf(' ') != -1) {
                        effect = effect.split(' ');
                        effect = effect[0].toLowerCase() + effect[1];
                    }
                    else {
                        effect = effect.toLowerCase();
                    }
                    $('#slideshow-17').cycle({
                        fx: effect, timeout:8000
                    });
                    $('#controls-17 > button').click( function() {
                        $('#slideshow-17').cycle($(this).attr('class'));
                    });
                
});
</script>
