  <!-- START: JAVASCRIPT ROUTINES FOR SITE SPECIFIC --> 
  <script type="text/javascript" language="javascript"> 
 function image_roll(img_swap, img_name) {
  img_name.src = img_swap;
 }

 function validateform() { 
  if(document.s_crit.last_nme.value=="") { 
    window.alert ("A complete or partial Last Name is Required."); 
    document.s_crit.last_nme.focus(); 
    return false; 
  } 
 if(document.s_crit.birth_mm.value!="") { 
   if(document.s_crit.birth_yy.value=="" || document.s_crit.birth_yy.value.length != 4) { 
      window.alert ("A four (4) digit Birth Year (YYYY) is Required when a Birth Month is specified."); 
      document.s_crit.birth_yy.focus(); 
      return false; 
   } 
 } 
 if(document.s_crit.birth_yy.value!="") { 
   if(document.s_crit.birth_yy.value.length != 4){ 
     window.alert ("Year of Birth has to be four digits."); 
     document.s_crit.birth_yy.focus(); 
     return false; 
   } 
   i=document.s_crit.birth_yy.value * 1;  
   if (i!=document.s_crit.birth_yy.value) { 
      window.alert ("Year of Birth may only contain numbers."); 
      document.s_crit.birth_yy.focus(); 
      return false; 
   } 
   var d = new Date();  
   var curr_year = d.getFullYear(); 
   if (document.s_crit.birth_yy.value<=0 || document.s_crit.birth_yy.value>curr_year) { 
      window.alert ("Year of Birth invalid or out of range."); 
      document.s_crit.birth_yy.focus(); 
      return false; 
   } 
 } 
 if(document.s_crit.death_mm.value!="") { 
   if(document.s_crit.death_yy.value=="" || document.s_crit.death_yy.value.length != 4 ){ 
      window.alert ("A four (4) digit Death Year (YYYY) is Required when a Death Month is specified."); 
      document.s_crit.death_yy.focus(); 
      return false; 
   } 
 } 
 if(document.s_crit.death_yy.value!="") { 
   if(document.s_crit.death_yy.value.length != 4){ 
     window.alert ("Year of Death has to be four digits."); 
     document.s_crit.death_yy.focus(); 
     return false; 
   } 
   i=document.s_crit.death_yy.value * 1;  
   if (i!=document.s_crit.death_yy.value) { 
      window.alert ("Year of Death may only contain numbers."); 
      document.s_crit.death_yy.focus(); 
      return false; 
   } 
   var d = new Date();  
   var curr_year = d.getFullYear(); 
   if (document.s_crit.death_yy.value<=0 || document.s_crit.death_yy.value>curr_year) { 
      window.alert ("Year of Death invalid or out of range."); 
      document.s_crit.death_yy.focus(); 
      return false; 
   } 
 } 
      return true; 
 } 
 function submitForm(){
		var retValue = true;
		retValue = validateform();
		if ( !retValue ){
			return retValue;
		}		var cemElem = document.s_crit.cemetery;
		var selectedValue = cemElem.options[cemElem.selectedIndex].value;
		var url = "/index.html";
		if ( selectedValue != "" ){
			url = url + "?cemetery=" + selectedValue;
		}
		var form = document.s_crit;
		form.action = url;
		form.submit();
		return retValue;
 }
   function externalgLinkMsg() 
   { 
   window.alert ("You are now leaving the Department of Veterans Affairs Website."); 
   } 
 </script> 
  <!-- END: JAVASCRIPT ROUTINES FOR SITE SPECIFIC --> 
