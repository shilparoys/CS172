function f_poisk(n){
var val=document.getElementById("poisk1").value;
var txtin="Сайттан іздеу";
val=val.trim();

if(n==1 && txtin==val ){ document.getElementById("poisk1").value=""; }
if(n==2 && val.length==0){ document.getElementById("poisk1").value="Сайттан іздеу"; }
}
</script> 
