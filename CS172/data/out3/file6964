  <script language="javascript">

if(typeof(_ctx)=="undefined")
{
	_ctx = new Object();
}

_ctx.transpImage = "/images/t.gif";

_ctx.windowOnLoadFirst = new Array();
_ctx.windowOnLoad = new Array();
_ctx.windowOnLoadLast = new Array();

_ctx.oldWindowOnLoad = window.onload;

window.onload = function(e)
{
	if(e==null)
	{
		e = event;
	}
	for(var i=0; i<_ctx.windowOnLoadFirst.length; i++)
	{
		_ctx.windowOnLoadFirst[i](e);
	}
	for(var i=0; i<_ctx.windowOnLoad.length; i++)
	{
		_ctx.windowOnLoad[i](e);
	}
	for(var i=0; i<_ctx.windowOnLoadLast.length; i++)
	{
		_ctx.windowOnLoadLast[i](e);
	}
	if(_ctx.oldWindowOnLoad!=null)
	{
		_ctx.oldWindowOnLoad();
	}
};

function addBookmark()
{
	if (window.external)
	{
		window.external.AddFavorite(window.location.href, "The Lebanese University") ;
	}
}

function toggleOtherLinks()
{
	alert("Coming Soon...");
}

_ctx.language = "US";

</script>
  <script src="/js/formUtils.js"></script>
  <script src="/js/stringUtils.js"></script>
