                            <script language="javascript">
	
	function flipCalendar()
	{
		var calendarContainer = document.getElementById("calendarContainer");
		var eventsContainer = document.getElementById("eventsContainer");
		var c = (calendarContainer.style.display=="none");
		if(!c)
		{
			eventsContainer.style.height = calendarContainer.offsetHeight;
		}
		calendarContainer.style.display = ((c)? "inline":"none");
		eventsContainer.style.display = ((c)? "none":"inline");
		document.getElementById("calendarFlip").innerHTML = ((c)? "Show Events":"Show Calendar");
	}
	
	</script> 
