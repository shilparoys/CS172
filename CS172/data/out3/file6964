function setNewsRirection(dir)
{
	newsDirection = dir;
	if(newsPlayTimer==null)
	{
		animNewsPlay();
	}
}

function toggleNewsPlay()
{
	if(newsPlayTimer==null)
	{
		animNewsPlay();
	}
	else
	{
		animNewsPause();
	}
}

function animNewsPlay()
{
	newsPlayTimer = window.setTimeout("playNews()", newsPlayDelay);
	//document.getElementById("newsPlayerImage").src = "/images/pause.gif";
}

function animNewsPause()
{
	window.clearTimeout(newsPlayTimer);
	newsPlayTimer = null;
	//document.getElementById("newsPlayerImage").src = "/images/play.gif";
}

function setCurrentNews(index)
{
	if(newsPlayTimer!=null)
	{
		window.clearTimeout(newsPlayTimer);
		newsPlayTimer = null;
	}
	document.getElementById("newsIndex" + currentNewsIndex).className = "announcementPage1Off";
	currentNewsIndex = index;
	showCurrentNews();
	animNewsPause();
}

function playNews()
{
	document.getElementById("newsIndex" + currentNewsIndex).className = "announcementPage1Off";
	currentNewsIndex = Math.abs((homePageNews.length + currentNewsIndex + newsDirection) % homePageNews.length);
	showCurrentNews();
}

function showCurrentNews()
{
	var news = homePageNews[currentNewsIndex];

	document.getElementById("currentNewsText").innerHTML = news.text;
	if(homePageNews.length>1)
	{
		document.getElementById("newsIndex" + (currentNewsIndex)).className = "announcementPage1On";
		newsPlayTimer = window.setTimeout("playNews()", newsPlayDelay);
	}

}

_ctx.windowOnLoadLast.push(initNews);

</script> 
