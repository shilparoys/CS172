                 <td id="dateContainer" class="textSmallWhite1" nowrap> <script language="javascript">
												_ctx.serverDateTime = new Date("3/2/2015 4:10:05 AM");
												_ctx.clientDateTime = Date.parse(new Date());

												function setTopPageDate()
												{
													var d = new Date(_ctx.serverDateTime);
													d.setMilliseconds(_ctx.serverDateTime.getMilliseconds() + Date.parse(new Date()) - _ctx.clientDateTime);
													var Dow = new Array("Sunday", "Monday", "Tuesday", "Wednesday", "Thrusday", "Friday", "Saturday");
													var mNames = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");
													var dateRender = "{DD} {d} {MM}, {h}:{minmin} {ampm}&nbsp;Beirut&nbsp;Lebanon (+2 GMT)";
													dateRender = dateRender.replace(/\{d\}/g, d.getDate());
													if(dateRender=="")
													{
														dateRender = "{mm}/{dd}/{yyyy}";
													}
													dateRender = dateRender.replace(/\{dd\}/g, util.string.parseUsing(d.getDate(), 2, 0));
													if(Dow[d.getDay()]!=null)
													{
														dateRender = dateRender.replace(/\{D\}/g, Dow[d.getDay()].substring(0, 2));
														dateRender = dateRender.replace(/\{DD\}/g, Dow[d.getDay()]);
													}
													dateRender = dateRender.replace(/\{m\}/g, d.getMonth()+1);
													dateRender = dateRender.replace(/\{mm\}/g, util.string.parseUsing(d.getMonth() + 1, 2, 0));
													if(mNames[d.getMonth()]!=null)
													{
														dateRender = dateRender.replace(/\{M\}/g, mNames[d.getMonth()].substring(0, 3));
														dateRender = dateRender.replace(/\{MM\}/g, mNames[d.getMonth()]);
													}
													dateRender = dateRender.replace(/\{yy\}/g, d.getYear());
													dateRender = dateRender.replace(/\{yyyy\}/g, d.getFullYear());
													dateRender = dateRender.replace(/\{h\}/g, d.getHours());
													dateRender = dateRender.replace(/\{hh\}/g, util.string.parseUsing(d.getHours(), 2, 0));
													dateRender = dateRender.replace(/\{H\}/g, d.getHours()%24);
													dateRender = dateRender.replace(/\{HH\}/g, util.string.parseUsing(d.getHours()%24, 2, 0));
													dateRender = dateRender.replace(/\{min\}/g, d.getMinutes());
													dateRender = dateRender.replace(/\{minmin\}/g, util.string.parseUsing(d.getMinutes(), 2, 0));
													dateRender = dateRender.replace(/\{s\}/g, d.getSeconds());
													dateRender = dateRender.replace(/\{ss\}/g, util.string.parseUsing(d.getSeconds(), 2, 0));
													dateRender = dateRender.replace(/\{ms\}/g, d.getMilliseconds());
													dateRender = dateRender.replace(/\{ms\}/g, util.string.parseUsing(d.getMilliseconds(), 3, 0));
													dateRender = dateRender.replace(/\{ampm\}/g, ((d.getHours()>12)? "P.M.": "A.M."));
													document.getElementById("dateContainer").innerHTML = dateRender;
													window.setTimeout("setTopPageDate();", 60000);
												}

												_ctx.windowOnLoadLast.push(setTopPageDate);

												</script> </td> 
