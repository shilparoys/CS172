  <script language="javascript">

var currentOpenMenu = null;

function toggleMenu(menuName)
{
	if((currentOpenMenu != null) && (currentOpenMenu!=menuName))
	{
		document.getElementById(currentOpenMenu).style.display = "none";
	}

	document.getElementById(menuName).style.display = ((document.getElementById(menuName).style.display=="none")? "":"none");
	currentOpenMenu = menuName;
}

</script> 
