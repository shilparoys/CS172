                                    <script language="javascript" type="text/javascript">

var currentAnnouncementIndex = null;
var homePageAnnouncements = new Array();
var announcementsPlayTimer = null;
var announcementsPlayDelay = 4000;
var announcementDirection = 1;

function announcementObject(id, text)
{
	this.id = id;
	this.text = text;
}

function initAnnouncements()
{
	var announcement, mainFileName, text;
	text = "<div style=\"direction:rtl;\"><span style=\"direction:rtl\"><a class=\"link2\" style=\"text-decoration:underline\" href=\"/media/announcements/default.aspx?facultyId=0&announcementId=3019\" target=\"announcement\">ØªØ¹ÙÙ Ø§ÙÙØ¯ÙØ±ÙØ© Ø§ÙØ¹Ø§ÙØ© ÙØªØ¹Ø§ÙÙÙØ© ÙÙØ¸ÙÙ Ø§ÙØ¯ÙÙØ© Ø£ÙÙ ØªÙ ÙØµÙ ÙØ±Ø¹ Ø¨ÙØ±ÙØª Ù Ø¬Ø¨Ù ÙØ¨ÙØ§Ù Ø§ÙÙ ÙØ±Ø¹ÙÙ</a></div>";
		announcement = new announcementObject(3019, text);
		homePageAnnouncements.push(announcement);
		text = "<div style=\"direction:rtl;\"><span style=\"direction:rtl\"><a class=\"link2\" style=\"text-decoration:underline\" href=\"/media/announcements/default.aspx?facultyId=0&announcementId=3018\" target=\"announcement\">ØªØ¹ÙÙ Ø±Ø¦Ø§Ø³Ø© Ø§ÙØ¬Ø§ÙØ¹Ø© Ø§ÙÙØ¨ÙØ§ÙÙØ© Ø¹Ù Ø­Ø§Ø¬ØªÙØ§ ÙÙØªØ¹Ø§ÙØ¯ ÙØ¹ Ø£Ø³Ø§ØªØ°Ø© ÙØ£Ø·Ø¨Ø§Ø¡ Ø§Ø®ØªØµØ§ØµÙÙÙ</a></div>";
		announcement = new announcementObject(3018, text);
		homePageAnnouncements.push(announcement);
		text = "<div style=\"direction:rtl;\"><span style=\"direction:rtl\"><a class=\"link2\" style=\"text-decoration:underline\" href=\"/media/announcements/default.aspx?facultyId=0&announcementId=3012\" target=\"announcement\">Ø¬Ø§Ø¦Ø²Ø© Ø§ÙÙØ¬ÙØ³ Ø§ÙÙØ·ÙÙ ÙÙØ¨Ø­ÙØ« Ø§ÙØ¹ÙÙÙØ© ÙÙØªÙÙØ² Ø§ÙØ¹ÙÙÙ Ø§ÙØ¯ÙØ±Ø© Ø§ÙØ®Ø§ÙØ³Ø© - 2015</a><br>(<a class=\"link2\" style=\"text-decoration:underline\" href=\"/files/announcements/annonce_0_3012_3869.doc\" target=\"annonce_0_3012_3869.doc\">CNRS Award application Form</a>)</div>";
		announcement = new announcementObject(3012, text);
		homePageAnnouncements.push(announcement);
		text = "<div style=\"direction:rtl;\"><span style=\"direction:rtl\"><a class=\"link2\" style=\"text-decoration:underline\" href=\"/media/announcements/default.aspx?facultyId=0&announcementId=2797\" target=\"announcement\">Ø§ÙØ¨ÙØ§Ù Ø§ÙØªÙØµÙÙÙ Ø¨Ø§ÙØ³Ø§Ø¹Ø§Øª Ø§ÙÙÙÙØ°Ø© Ø§ÙÙØ§Ø¬Ø¨ Ø§Ø¹ØªÙØ§Ø¯Ù Ø¹ÙØ¯ Ø¥Ø¹Ø¯Ø§Ø¯ Ø¹ÙÙØ¯ Ø§ÙÙØµØ§ÙØ­Ø© ÙÙØ£Ø³Ø§ØªØ°Ø© Ø¹Ù Ø§ÙØ¹Ø§Ù Ø§ÙØ¬Ø§ÙØ¹Ù 2013-2014</a><br>(<a class=\"link2\" style=\"text-decoration:underline\" href=\"/files/announcements/annonce_0_2797_3627.xls\" target=\"annonce_0_2797_3627.xls\">Ø§ÙØ§Ø¹ÙØ§Ù</a>)</div>";
		announcement = new announcementObject(2797, text);
		homePageAnnouncements.push(announcement);
		
	currentAnnouncementIndex = 0;
	if(homePageAnnouncements.length>0)
	{
		showCurrentAnnouncement();
		//toggleAnnouncementsPlay();
	}
}

function setAnnouncementRirection(dir)
{
	announcementDirection = dir;
	if(announcementsPlayTimer==null)
	{
		animAnnouncementPlay();
	}
}

function toggleAnnouncementsPlay()
{
	if(announcementsPlayTimer==null)
	{
		animAnnouncementPlay();
	}
	else
	{
		animAnnouncementPause();
	}
}

function animAnnouncementPlay()
{
	announcementsPlayTimer = window.setTimeout("playAnnouncements()", announcementsPlayDelay);
	//document.getElementById("announcementsPlayerImage").src = "/images/pause.gif";
}

function animAnnouncementPause()
{
	window.clearTimeout(announcementsPlayTimer);
	announcementsPlayTimer = null;
	//document.getElementById("announcementsPlayerImage").src = "/images/play.gif";
}

function setCurrentAnnouncement(index)
{
	if(announcementsPlayTimer!=null)
	{
		window.clearTimeout(announcementsPlayTimer);
		announcementsPlayTimer = null;
	}
	document.getElementById("announcementIndex" + currentAnnouncementIndex).className = "announcementPage1Off";
	currentAnnouncementIndex = index;
	showCurrentAnnouncement();
	animAnnouncementPause();
}

function playAnnouncements()
{
	document.getElementById("announcementIndex" + currentAnnouncementIndex).className = "announcementPage1Off";
	currentAnnouncementIndex = Math.abs((homePageAnnouncements.length + currentAnnouncementIndex + announcementDirection) % homePageAnnouncements.length);
	showCurrentAnnouncement();
}

function showCurrentAnnouncement()
{
	var announcement = homePageAnnouncements[currentAnnouncementIndex];

	document.getElementById("currentAnnouncementText").innerHTML = announcement.text;
	if(homePageAnnouncements.length>1)
	{
		document.getElementById("announcementIndex" + (currentAnnouncementIndex)).className = "announcementPage1On";
		announcementsPlayTimer = window.setTimeout("playAnnouncements()", announcementsPlayDelay);
	}

}

_ctx.windowOnLoadLast.push(initAnnouncements);

