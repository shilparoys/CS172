                               <script language="javascript">
																							function searchFormFocus(elt)
																							{
																								if(elt.value==elt.defaultValue)
																								{
																									elt.value = "";
																								}
																							}
																							function searchFormBlur(elt)
																							{
																								if(elt.value=="")
																								{
																									elt.value = elt.defaultValue;
																								}
																							}
																						</script> 
