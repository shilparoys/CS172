     <script type="text/javascript">

var thisScriptURL = "/community/member_info.php";
var newProjectPageURL = "/community/new_project.php";
var memberSpeciesPageURL = "/community/member_species.php";
var edgeTeamInfoPageURL = "/community/team_info.php";

var pageType = "member";
var memberID = 134;
var id = memberID;
var version = "Publish";
var navigateAwayWarning = "This will navigate to a new page, so any unsaved changes will be lost. Continue?";

function addNewProject()
{
  var answer = confirm(navigateAwayWarning);
  if(!answer)
  {
    return;
  }
  
  document.location.href = newProjectPageURL + "?memberID=" + memberID;
}

function manageRelatedSpecies()
{
  var answer = confirm(navigateAwayWarning);
  if(!answer)
  {
    return;
  }
  
  document.location.href = memberSpeciesPageURL + "?id=" + memberID + "&version=" + version;
}

function editEDGETeamPage()
{
  var answer = confirm(navigateAwayWarning);
  if(!answer)
  {
    return;
  }
  
  document.location.href = edgeTeamInfoPageURL + "?id=" + memberID + "&version=" + version;
}

</script> 
