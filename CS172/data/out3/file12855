              oz_zone = USN.rubicon.getZone('rectangleA');
              oz_ad_slot_size = USN.rubicon.getAdSize('rectangleA');
              oz_callback = function (data) {
                USN.rubicon.injectTargeting(data, 'rectangleA');
              }
              USN.rubicon.callApi('rectangleA');
          </script> 
       </div> 
