    function RewriteLoginLink() {
        var loginLink = $("#loginLink");
       
        if ($(loginLink).length > 0) {
            $(loginLink).attr("href", "#");
            $(loginLink).click(function (e) {
                     window.location = "https://www.dell.com/identity/v2/discovery?wa=wsignin1.0&wtrealm=http%3a%2f%2fwww.dell.com&wreply=http%3a%2f%2fwww.dell.com%2fsupport%2fincidents%2fus%2fen%2f19%2fCareForms%2fProvideInfo%2fCareUnresolved&whr=urn:transaction"
                e.preventDefault();
                
            });
        }
    }

    try {
        Dell.SupportMasthead.CountrySelector.keypress = function () { };
    }
    catch (ex) { }
