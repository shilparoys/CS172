       <script>




jQuery(document).ready(function() {

	var numItems = jQuery('.views-slideshow-pager-field-item').length;
	var itemWidth = 156;
	var actualWidth = numItems * itemWidth;
	var leftOffset = 1;
	var rightOffset = numItems;
	
	
	
	
	
	//alert(jQuery('#widget_pager_bottom_facilities_slideshow-block').html());
	jQuery("#views_slideshow_controls_text_next_facilities_slideshow-block").click(function() {
		
		
		offset = Math.abs(parseInt(jQuery('#widget_pager_bottom_facilities_slideshow-block').css('left')) / itemWidth);
		//alert(leftOffset);
		
		if(leftOffset < numItems){
		
		    if(leftOffset <= (numItems-4)){	
				jQuery('#widget_pager_bottom_facilities_slideshow-block').animate({
		                left: (parseInt(jQuery('#widget_pager_bottom_facilities_slideshow-block').css('left')) - itemWidth) +'px'
		            }, 800);
	        }
	        leftOffset++;    

        }else if(leftOffset == numItems){
	        
	        
	        jQuery('#widget_pager_bottom_facilities_slideshow-block').animate({
	                left: 0
	            }, 800);
	        leftOffset = 1;
        }
        
        
	});
	
	jQuery("#views_slideshow_controls_text_previous_facilities_slideshow-block").click(function() {
	
	
		//alert(leftOffset);
	
			
		if(leftOffset <= 1){	
		
			jQuery('#widget_pager_bottom_facilities_slideshow-block').animate({
                left: '-'+((numItems%4) * itemWidth)+'px'
            }, 800);
	        leftOffset = numItems;
			
        }else{
	        
	       
	        	
	        
	        
	        //alert();
		
			if(parseInt(jQuery('#widget_pager_bottom_facilities_slideshow-block').css('left')) < 0){	
				jQuery('#widget_pager_bottom_facilities_slideshow-block').animate({
		                left: (parseInt(jQuery('#widget_pager_bottom_facilities_slideshow-block').css('left')) + itemWidth) +'px'
		            }, 800);
	        }
			//jQuery('#widget_pager_bottom_facilities_slideshow-block').css('left', '-'+itemWidth+'px');
			//
			leftOffset--;
        }
        
           
	});
	
	

});


</script> 
