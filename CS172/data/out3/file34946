              <script type="text/javascript">

          jQuery(document).ready(function() {
		      jQuery('#search-block-form .custom-search-box').addClass('faded'); 
		      jQuery('#search-block-form .custom-search-box').blur(function (){ if ($(this).value==""){ $(this).value = $(this).defaultValue; } $(this).removeClass('focused');$(this).addClass('faded'); } ); 
		      jQuery('#search-block-form .custom-search-box').focus(function(){ if ($(this).defaultValue==$(this).value) $(this).value = ""; $(this).removeClass('faded');$(this).addClass('focused'); });  
	 
          });

		function focusSearch(formfield){ if (formfield.defaultValue==formfield.value) formfield.value = ""; formfield.className = "focused"; }
		function blurSearch(formfield){ if (formfield.value==""){ formfield.value = formfield.defaultValue; formfield.className = "faded";} }
      </script> 
