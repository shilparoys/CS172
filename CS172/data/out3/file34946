  <script src="sites/rit.edu.gccis/themes/gccis/js/jquery.tools.min.js"></script> 
  <script type="text/javascript">
  //jQuery.noConflict();
  // Code that uses other library's $ can follow here.
  
  jQuery(document).ready(function() {
  //Back to top Code to show button when scrolling reaches 
	//	a certain point
	jQuery(window).scroll(function(){
		var pos = window.pageYOffset;
		scoller = jQuery(".backtotop")
		if(pos >= 400) {
			if(!scoller.hasClass("shown")) {
				scoller.addClass("shown")
				//scoller.animate({"height":"31px","top":"-31px"}, 500)
				scoller.animate({"height":"28px"}, 500)
			}
		} else {
			if(scoller.hasClass("shown")) {
				scoller.removeClass("shown")
				scoller.animate({"height":"0px"}, 500)
				//scoller.animate({"height":"0px","top":"0px"}, 500)
			}
		}
		
		
		
		 
  
  		if (screen.width <= 480) {
	  		jQuery(document).ready(function() {
	  		  jQuery('#block-menu-block-1').click(function() {
	  		    //alert('menu toggle');
			    jQuery('.parent-mlid-0 > ul.menu').toggle();			  
			  })
			});
		
		}
	});
	
	jQuery(".backtotop").click(function(){
		if(jQuery('html, body').offset() != null){
			jQuery('html, body').animate({scrollTop: 0},'slow');
			jQuery(".backtotop").animate({"height":"0px"}, 500);
		}
	});
	
	
	jQuery('.expand_info').click(function() {
                //alert(jQuery(this).parent().next().html());
		jQuery(this).parent().next().slideToggle('slow');
                if(jQuery(this).html() == "Read More..."){
                  jQuery(this).html('Read Less')
                }else{ jQuery(this).html('Read More...'); }


		return false;
	}).next().hide();


	
	
	
	
  });
  
  </script>   
