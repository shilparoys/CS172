  <script>
				$(function(){
					$("#tabs1403365293").tabs({
						create: function( event, ui ) {
							window.setTimeout(checkmenu, 100);
						}
					});

					if (window.location.hash && $(window.location.hash).length){
						$("#tabs1403365293").tabs("select", window.location.hash);
						window.scrollTo(0, $("#tabs1403365293").offset().top );
					}

					//make the tab into a sliders
					if(jQuery().touchslider) $("#tabs1403365293>ul:first").touchslider();
					if(jQuery().hammer) {
						$("#tabs1403365293>div").hammer({
								drag_vertical: false
								,css_hacks:false
								,hold:false
								,drag:false
								,transform:false
								,tap_double:false
								,tap:false
							}).bind("swipe", function(ev){
								ev.stopPropagation();
								var active = $("#tabs1403365293").tabs( "option", "active" );
								var total = $("#tabs1403365293 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs1403365293").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs1403365293").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script> 
