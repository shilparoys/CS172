       <script type="text/javascript">
				function omnitureEmail(target) {
					jQuery.AsMetrics.fireMicroEvent({
						eVar: "eVar21",
						action: "AppleStore Gift Card by Email",
						part: "web",
						feature: "Gift Card Mktg Pg"
					});
					redirect(target);
					return false;
				}
				function omnitureMail(target) {
					jQuery.AsMetrics.fireMicroEvent({
						eVar: "eVar21",
						action: "AppleStore Gift Card by Mail",
						part: "web",
						feature: "Gift Card Mktg Pg"
					});
					redirect(target);
					return false;
				}
				function omnitureItunes(target) {
					jQuery.AsMetrics.fireMicroEvent({
						eVar: "eVar21",
						action: "iTunes Gift Card by Mail",
						part: "web",
						feature: "Gift Card Mktg Pg"
					});
					redirect(target);
					return false;
				}
				function redirect(target) {
					setTimeout(function() {
						window.location.href = target.href;
					}, 1000);
				}
			</script> 
       <div class="section clearfix"> 
        <div class="card-overview"> 
         <h1><img class="ir" src="http://store.storeimages.cdn-apple.com/4428/as-images.apple.com/is/image/AppleInc/aos/published/images/a/os/aos/ycard/aos-ycard-marketing-title?wid=537&amp;hei=66&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400972274820" alt="Easy to give. Fun to receive." width="537" height="66" data-scale-params-2="wid=1074&amp;hei=132&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400972274820"></h1> 
         <p>Depending on which Apple Gift Card you choose to give, they can be used for<br> Apple products, accessories, music, movies, games, apps, and more.</p> 
        </div> 
        <div class="egc-tiles"> 
         <div class="apple-store-gift-cards"> 
          <h2></h2>
          <h2>Apple Store Gift Cards</h2> 
          <p>For Apple hardware and other online and retail products.</p> 
          <a href="/us-hed/personalize/apple?product=PLASTIC_GIFT_CARDS" onclick="return omnitureMail(this)" data-s-object-id="ab95a9a3185e466dafa76e49a57d1ceb" data-evar1="AOS: home/shop_accessories/giftcards | primary-content-1 | Astro Link | 0 | Apple Store Gift Card by Mail - White | PLASTIC_GIFT_CARDS" data-evar30="home/shop_accessories/giftcards/primary-content-1"> <span class="a11y"><h2>Apple Store Gift Cards</h2></span> <img class="ir" src="http://store.storeimages.cdn-apple.com/4428/as-images.apple.com/is/image/AppleInc/aos/published/images/a/os/aos/ycard/aos-ycard-AGC?wid=227&amp;hei=162&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400970743178" alt="Mail" width="227" height="162" data-scale-params-2="wid=454&amp;hei=324&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400970743178"> </a> 
          <a href="/us-hed/personalize/electronic?product=E_GIFT_CARDS" onclick="return omnitureEmail(this)" data-s-object-id="a3e002d568c59196c5ef86369d84e524" data-evar1="AOS: home/shop_accessories/giftcards | primary-content-1 | Astro Link | 1 | Apple Store Gift Card by E-mail - Space Gray | E_GIFT_CARDS" data-evar30="home/shop_accessories/giftcards/primary-content-1"> <span class="a11y"><h2>Apple Store Gift Cards</h2></span> <img class="ir" src="http://store.storeimages.cdn-apple.com/4428/as-images.apple.com/is/image/AppleInc/aos/published/images/a/os/aos/ycard/aos-ycard-EGC?wid=227&amp;hei=162&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400800353140" alt="Email" width="227" height="162" data-scale-params-2="wid=454&amp;hei=324&amp;fmt=png-alpha&amp;qlt=95&amp;.v=1400800353140"> </a> 
         </div> 
