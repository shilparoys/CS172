            <script type="text/javascript">
window.addEvent('domready', function() {
	var myAccordion532 = new Accordion(document.getElements('#accordion_sp1_id532 .toggler'), document.getElements('#accordion_sp1_id532 .sp-accordion-inner'), {
		opacity: false,
				display:-1,
				alwaysHide: true,		
		onActive: function(toggler){
			toggler.addClass('active');
		},
		onBackground: function(toggler){
			toggler.removeClass('active');
		}
	});
});
</script> 
