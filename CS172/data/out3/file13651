  <script language="javascript" type="text/javascript">$(document).ready(function() {
	var scrollTweet = function() {
		$('body').append($('<div id="tweetWidth">' + $('ul#twitter_update_list > li:visible').html() + '</div>').css({
			'white-space':'nowrap',
			'font-size':'12px',
			'display':'none',
			'width':'auto'
		}));
		tweetWidth = $('#tweetWidth').width();
		$('#tweetWidth').remove();
		if ($('ul#twitter_update_list').outerWidth(false) - tweetWidth < 0) {
			$('ul#twitter_update_list > li:visible').animate({
				'left':($('ul#twitter_update_list').outerWidth(false) - tweetWidth + 'px')
			}, Math.abs(($('ul#twitter_update_list').outerWidth(false) - tweetWidth) * 6000 / 170), 'linear', function() {
				setTimeout(showNextTweet, 5000);
			});
		} else if ($('ul#twitter_update_list > li').length > 1) {
			setTimeout(showNextTweet, 5000);
		}
	};
	var showNextTweet = function() {
		$('ul#twitter_update_list > li:first').fadeOut('slow', function() {
			if ($('ul#twitter_update_list > li').length > 1)
				$('ul#twitter_update_list > li:first').after($('ul#twitter_update_list > li:last'));
			$('ul#twitter_update_list > li:first').css({'left':'0px'}).fadeIn('slow');
			setTimeout(scrollTweet, 5000);
		});
	};
	$('ul#twitter_update_list > li').css({'display':'none'});
	$('ul#twitter_update_list > li:first').css({'display':'inline'});
	setTimeout(scrollTweet, 5000);
	
});</script> 
  <script type="text/javascript" src="https://www.certifiedbackground.com/scripts/js/functions.js"></script> 
