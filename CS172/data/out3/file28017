         <script src="http://graphics8.nytimes.com/js/app/movies/widgets/showtime/showtimeWidget.js" language="javascript"></script> 
         <script>
	NYTD.Movies.showtimesWidget = new NYTD.Movies.ShowTimesWidget();
  
  var widget_url= "/svc/movies/mymovietimes.html?output_type=html";
  var zipcode = '';
  var movieid = '';
  if($('zipcode')){zipcode = $('zipcode').getValue(); }
  if($('movieid')){movieid = $('movieid').getValue();}
  if(zipcode ==  '' || movieid == ''){
	var meta = document.getElementsByTagName("meta");
	if(meta != '') {
		for(var i=0;i<meta.length;i++){			
			if(zipcode ==  '' && meta[i].name == "TSZC"){
				zipcode = meta[i].content;
			}
			if(movieid ==  '' && meta[i].name == "MID"){
				movieid = meta[i].content;
			}
		}
	}
  }
	if(zipcode !=  '' && movieid != ''){
		widget_url = widget_url+'&zipcode='+zipcode+'&movie_id='+movieid;
	}else if(zipcode !=  '' && movieid == ''){
		widget_url = widget_url+'&zipcode='+zipcode;
	}
	else if(zipcode ==  '' && movieid != ''){
		widget_url = widget_url+'&movie_id='+movieid;
	}
  NYTD.Movies.showtimesWidget.makeRequest(widget_url, 'GET');
</script> 
