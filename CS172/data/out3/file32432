    (function ($, Drupal, window, document, undefined) {
      $(document).foundation('orbit', {
		  next_on_click: false,
		  timer_speed: 12000, // system var from custom config module, in milliseconds so add the extra zeroes
		  pause_on_hover: false,
		  //resume_on_mouseout: true,
		  slide_number: false
		});		
