	//Left Column Supersleight for PNG transparency in IE6

	$('.roundedBox .top img.box_round_top').supersleight();
	//$('.roundedBox .middle img.box_round_background').supersleight();
	$('.roundedBox .bottom img.box_round_bottom').supersleight();
	$('.roundedBoxBorder .top img.box_round_top').supersleight();
	//$('.roundedBoxBorder .middle img.box_round_background').supersleight();
	$('.roundedBoxBorder .bottom img.box_round_bottom').supersleight();					
	
	//Right Column Supersleight for PNG transparency in IE6
	
	$('#right img.rightTop').supersleight();
	$('#right img.rightBottom').supersleight();																
