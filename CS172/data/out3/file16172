  <script type="text/javascript">
   $(document).ready(
      function() {

      var refreshId = setInterval(function() { $('#feeditem').slideUp("fast").load('/directory/json/feed', function() { $('#feeditem').slideDown("slow") }); }, 15000);
      $('#feeditem').slideUp(1).slideDown("slow");
		

	    $("#id_search").autocomplete({
	        source: function(request, response){
	            $.ajax({
	                url: "/directory/json/search",
	                data: {q: request.term},
	                success: function(data) {
	                    if (data != 'CACHE_MISS')
	                        response($.map(data, function(item) {
	                            return {
	                                label: item[1],
	                                value: item[1],
	                                real_value: item[0]
	                            };
	                        }));
	                },

	                dataType: "json"
	            });
	        },
			select: function(event, ui) { 
				permalink = "/directory/employee/0".replace("0", ui.item.real_value)
				$("#id_search").val(ui.item.label);
				window.location = permalink
				//$("#id_search_form").submit();
				return true;
            },
	        minLength: 3
    	});      	
    	
    	$("#id_search").focusin(function() {
    		if($(this).val() == "Search Directory")
         		$(this).val("");
        });
    });
</script> 
