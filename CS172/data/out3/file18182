              <!-- ##### Begin 404 detection and redirection ##### --> <script type="text/javascript">
// IFRAME BUSTER
 if (top !== self) 
 {
 	// We want to track when this happens, which we do with a custom
 	// ?source=framebuster query added to the URL.
 	// The challenge is, often there's already a ?source=rss on the URL,
 	// which means we have to handle that too.
 	var sourcedomain = 'denverpost.com';
 	var sourceurl = self.document.location.toString();
 	var referrer = document.referrer.split('/')[2];
 	if ( sourceurl.indexOf("www." + sourcedomain) > 0 && referrer.indexOf(sourcedomain) < 0 )
	{
		if ( referrer.indexOf("google") < 0 && referrer.indexOf("facebook") < 0 && referrer.indexOf("images.") < 0  && referrer.indexOf("ow.ly") < 0 && referrer.indexOf("reddit") < 0 && referrer.indexOf("stumbleupon") < 0  )
	 	{
	 		top.location.replace(self.location);
	 		var sourcetag = "skipframe-" + referrer.replace('www.', '');
	 	 	if ( sourceurl.indexOf("?source") > 0 || sourceurl.indexOf("&source") > 0  )
	 	 	{
	 	 		top.location.replace(sourceurl + sourcetag).replace('=rss', '=');
	 	 	}
	 	 	else if  ( sourceurl.indexOf("?") == -1 )
	 	 	{
	 	 		top.location.replace(sourceurl + "?source=" + sourcetag);
	 	 	}
	 	 	else
	 	 	{
	 	 		top.location.replace(sourceurl + sourcetag);
	 	 	}
	 	}
	}
 }


window.onload = function() { 
//$('#ownLocalWidgy').remove(); 
$('#cNewsDiv').remove(); 
}
Array.prototype.shuffle = function() {
    var input = this;
     
    for (var i = input.length-1; i >=0; i--) {
     
        var randomIndex = Math.floor(Math.random()*(i+1)); 
        var itemAtIndex = input[randomIndex]; 
         
        input[randomIndex] = input[i]; 
        input[i] = itemAtIndex;
    }
    return input;
}

// INFINITE ARTICLE SCROLL
// New articles should appear at the base of every article,
// the "base" being halfway down the related-articles div.
var cachebuster = '?date=201502';
var inf_config = {
    property: 'denverpost',
    tid: 'dpArticleBottom',
    article_set: '',
    article_sets: ['news', 'business', 'opinion', 'entertainment', 'style', 'sports'],
    reload_chartbeat: 1,// Future-proofing
    reload_outbrain: 1,
    reload_comments: 0,
    section_lookups: {}
};

// Load the infinite-article script.
// It's in a different location on dev.
var script = document.createElement('script');
script.type = 'text/javascript';
if ( document.location.hash === '#dev' )
{
    script.src = '/infinite.js' + cachebuster;
}
else
{
if ( document.location.hash === '#devbeta' )
{
    inf_config.reload_outbrain = 1;
    script.src = 'http://extras.denverpost.com/app/infinite/infinite-beta.js' + cachebuster;
}
else
{
    script.src = 'http://extras.denverpost.com/app/infinite/infinite.js' + cachebuster;
}
}
if ( document.location.hash !== '#noscroll' ) $('head')[0].appendChild(script);

// &&& PROPERTY-SPECIFIC
var check_href = document.location.href;
var inf_section = 'news';
if ( /business|aerospace|airlines|economy|energy|foodretail|technology|telecom|tourism/i.exec(check_href) ) inf_section = 'business';
else if ( /opinion|editorials|perspective/i.exec(check_href) ) inf_section = 'opinion';
else if ( /comedy|art|theater|television|music|videogames/i.exec(check_href) ) inf_section = 'entertainment';
else if ( /style|fashion|food|fitness|homegarden|petnews|smart|travel/i.exec(check_href) ) inf_section = 'style';
else if ( /sports|broncos|avalanche|paige|kiszla|hochman|nuggets|rockies|rapids|superbowl|colleges|olympics|preps|outdoors|motorsports|cycling|skiing/i.exec(check_href) ) inf_section = 'sports';

inf_config['article_set'] = inf_section;

var script = document.createElement('script');
script.type = 'text/javascript';
script.src = 'http://extras.denverpost.com/cache/article/' + inf_config['property'] + '-' + inf_section + '.js';
if ( document.location.hash !== '#noscroll' ) $('head')[0].appendChild(script);

window.onload = function()
{
    inf_config['articles'] = ar.shuffle();

    // Update inf with the values we've set in inf_config
    if ( typeof inf_config !== 'undefined' )
    {
        for ( var key in inf_config )
        {
            if ( inf_config.hasOwnProperty(key) )
            {
                inf[key] = inf_config[key];
            }
        }
    }

    if ( document.location.hash !== '#noscroll' )
    {
        inf.init();
        inf.checkpoints[0].bottom = inf.get_top();
        $(window).scroll(inf, function()
        {
            run_on_scroll_interval();
        });
        var did_scroll = 0;
        function run_on_scroll_interval() { did_scroll = 1; }
        setInterval(function()
        {
            if ( did_scroll == 1 )
            {
                did_scroll = 0;
                inf.check_scroll();
            }
        }, 500);
    }
}


