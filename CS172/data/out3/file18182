    $('.dpSidebarTab').on('click', function(){
        $(this).siblings('li').removeClass('dpSidebarActiveTab');
        $(this).addClass('dpSidebarActiveTab');
    });
    function dpShowOtherSidebarTab(tabclicked,dumptoshow) {
        if(!$(tabclicked).hasClass('dpSidebarActiveTab')) {
            $('.dpSidebarActiveDump').fadeOut(100).removeClass('dpSidebarActiveDump');
            $(dumptoshow).fadeIn(100).addClass('dpSidebarActiveDump');
