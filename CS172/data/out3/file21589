        <img src="http://content.cpcache.com/si/portals/images/annoying-orange-portal-banner.jpg" alt="" border="0" class="portal-banner"> 
        <h2>Find one-of-a-kind Annoying Orange T-shirts, hoodies, stickers and merchandise.</h2> 
        <div id="fanDesignedContainer" class="clearfix"> 
         <h3 class="fan-designed">Official fan-designed Annoying Orange T-Shirts &amp; more:</h3> 
         <a class="noclick design-your-own-button" href="javascript:launchHelp('/cp/popup/index.aspx?page=create-annoyingorange','height=500,width=630,scrollbars=1')">Design your own Annoying Orange gear. Learn More &gt;</a> 
        </div> 
        <div class="clear">
         &nbsp;
        </div> 
       </div> 
       <script type="text/javascript">
function afterLoad(){
	$(document).ready(function() {
        $('.serpt-portal-layout-right-content').prepend($('#searchresultsh1'));
        $('#searchresultsh1').attr('style','font-size:27px !important');
        $('#searchResultsContainer').prependTo('#hungerGamesSearchResults');
        $('.serpt-header:first').prependTo('#searchResultsContainer');
        $('.serpt-header .align-center').remove();
        $('.serpt-header').removeClass('box-col3').addClass('box-col2');
        $('.serpt-header:last').appendTo('#hungerGamesSearchResults');
		$('.crsl').each(function() {
			var crsl_d = $(this).data();
			var crsl_itemwidth = parseInt($(this).attr('itemwidth'));
			var crsl_imageheight = parseInt($(this).attr('imageheight'));
			var crsl_slide = $(this).find('.crsl-slide:eq(0)');
			var crsl_arrowwidth = 25;
			$(this).children('.crsl-height').height($(this).height());
			crsl_slide.children('.crsl-item').height($(this).height()).width(crsl_itemwidth);
			$(this).children('.crsl-window').width($(this).width()-60);
			//$(this).children('.crsl-arrow').css('background-position','center ' + Math.floor((crsl_imageheight - 27)/2) + 'px');			
			if ($(this).width() < crsl_itemwidth + (crsl_arrowwidth*2)) { $(this).width(crsl_itemwidth + (crsl_arrowwidth*2)); }

			var crsl_itemcount = $(this).find('.crsl-slide').children('.crsl-item').size();
			var crsl_wwidth = $(this).width();
			var crsl_perpage = Math.floor((crsl_wwidth-(crsl_arrowwidth*2)) / (crsl_itemwidth+5)); 
			if (crsl_perpage >= crsl_itemcount) {
				crsl_perpage = crsl_itemcount;
				crsl_arrowwidth = 0;
			} else {
				$(this).children('.crsl-arrow').css('display','block');
				crsl_wwidth = $(this).width() - (crsl_arrowwidth*2);
				$(this).children('.crsl-window').css({left:crsl_arrowwidth,width:crsl_wwidth});			
			}
			var crsl_spacing = crsl_wwidth - (crsl_perpage * crsl_itemwidth);
			var crsl_padding = Math.floor(crsl_spacing/(crsl_perpage*2));
			var crsl_leftover = crsl_spacing - (crsl_perpage*2*crsl_padding);

			if (crsl_leftover > 0) {
				var crsl_windowpad = Math.ceil(crsl_leftover/2);
				crsl_wwidth = crsl_wwidth-(crsl_windowpad*2);
				$(this).children('.crsl-window').css({left:crsl_windowpad+crsl_arrowwidth,width:crsl_wwidth});
			}

			var crsl_totalpages = Math.ceil(crsl_itemcount / crsl_perpage);
			crsl_slide.width(crsl_totalpages * crsl_perpage * (crsl_itemwidth + (crsl_padding * 2)));
			$(this).find('.crsl-item').css({'margin-left':crsl_padding,'margin-right':crsl_padding});

			crsl_d.itemcount = crsl_itemcount;
			crsl_d.currentpage = 1;
			crsl_d.totalpages = crsl_totalpages;

		});
		function crslPage(crsl,direction) {
			crsl_d = crsl.data();
			crsl_d.currentpage += direction;
			if (crsl_d.currentpage < 1) {
				crsl_d.currentpage = 1;
			} else if (crsl_d.currentpage > crsl_d.totalpages) {
				crsl_d.currentpage = crsl_d.totalpages;
			}
			crsl.find('.crsl-slide').stop(true).animate({left: crsl.find('.crsl-window').width() * (crsl.data().currentpage - 1) * -1},500,function() {
				if (crsl_d.currentpage == 1) {
					crsl.children('.crsl-arrow-left').addClass('crsl-inactive');
					crsl.children('.crsl-arrow-right').removeClass('crsl-inactive');
				} else if (crsl_d.currentpage == crsl_d.totalpages) {
					crsl.children('.crsl-arrow-left').removeClass('crsl-inactive');
					crsl.children('.crsl-arrow-right').addClass('crsl-inactive');
				} else {
					crsl.children('.crsl-arrow-left').removeClass('crsl-inactive');
					crsl.children('.crsl-arrow-right').removeClass('crsl-inactive');
				}
			});
		}
		$('.crsl-arrow-left').click(function(){
			crslPage($(this).parents('.crsl'),-1);
		});
		$('.crsl-arrow-right').click(function(){
			crslPage($(this).parents('.crsl'),1);
		});
		
		// replace related topics with handpicked links
		$('#designTopics .browse').replaceWith($('#moreHungerGames').detach().css('display', 'block'));
		
		// move sort container down above search results
		$('.sort-container').insertAfter('#hungerGamesLP');
		
		$('.serp-wrapper').wrap('<div class="trans-wrapper" />');
		$('#serp .serp-wrapper').css('padding','10px');
		
	});
}
function attachOnLoad(fn){
  if (window.addEventListener)
    window.addEventListener("load", fn, false);
  else if (window.attachEvent)
    window.attachEvent("onload", fn);
  else window.onload = fn;
}
attachOnLoad(afterLoad);

</script> 
