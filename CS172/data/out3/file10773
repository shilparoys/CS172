<!--
// Copyright 2009 PropertyBoss Solutions, LLC
// These function scripts may be only used with this rental application
// They may be modified, but this copyright notice must be displayed

// copy Home Address or erase
function copyHomeAddress(copy, prefix) {
if (copy) {
	document.getElementsByName(prefix +'_addr1')[0].value= document.getElementsByName('address1')[0].value
	document.getElementsByName(prefix +'_addr2')[0].value= document.getElementsByName('address2')[0].value
	document.getElementsByName(prefix +'_city')[0].value= document.getElementsByName('city')[0].value
	document.getElementsByName(prefix +'_state')[0].value= document.getElementsByName('state')[0].value
	document.getElementsByName(prefix +'_zip')[0].value= document.getElementsByName('zip')[0].value
}
else {
	document.getElementsByName(prefix +'_addr1')[0].value= ''
	document.getElementsByName(prefix +'_addr2')[0].value= ''
	document.getElementsByName(prefix +'_city')[0].value= ''
	document.getElementsByName(prefix +'_state')[0].value= ''
	document.getElementsByName(prefix +'_zip')[0].value= ''
}
}

// Add more roommate input fields
function addGroup(type) {
	for (i= 2; i < 9; i++) { //check next group item
		element= document.getElementById(type +i)
		if (element == null) return //stop when empty item found
		if (element.style.display == 'none') {
			element.style.display= 'block'
    		return
    	}
	}
}

// validation rules specific to this form
function preCheckForm(theForm) {
  if (document.getElementsByName('applicant_signature')[0].value == 'Test~Form') {
    theForm.submit()
    return
  }

  cell0= document.getElementById('cellphone').value
  cellE= document.getElementById('p1_cell_ph').value
  if (cell0 == cellE && cell0 != '') {
    alert ('Emergency cellphone [' +cellE +'] must be different than Permanent Address [' +cell0 +']')
    document.getElementById('p1_cell_ph').focus()
    return false
  }
  cellG= document.getElementById('p2_cell_ph').value
  if (cell0 == cellG && cell0 != '') {
    alert ('Guarantor cellphone [' +cellG +'] must be different than Permanent Address [' +cell0 +']')
    document.getElementById('p2_cell_ph').focus()
    return false
  }
  CheckForm(theForm)
}
//-->
</script> 
