// Copyright 2009 PropertyBoss Solutions, LLC
// These function scripts may be only used with this application
// They may be modified, but this copyright notice must be displayed

function postLoad() {

// display fields with data
if (document.getElementById('roommate2_ln').value != '')
	document.getElementById('roommate2').style.display= 'block'
if (document.getElementById('roommate3_ln').value != '')
	document.getElementById('roommate3').style.display= 'block'
if (document.application.smoking[1].checked == true)
	document.getElementById('notSmoke').style.display='inline'
if (document.application.alcohol[1].checked == true || document.application.alcohol[2].checked == true)
	document.getElementById('notDrink').style.display='inline'
if (document.application._major.value == 'Other:')
	document.getElementById('otherMajor').style.display='block'
if (document.application.no_ssn.checked)
	document.getElementById('ssn_field').style.display='none'


jscriptEnabled= document.getElementById('jsEnabled')
if (jscriptEnabled != null) jscriptEnabled.style.display= 'none'

// calendar popup
if (typeof scwBaseYear != 'undefined') {
	scwBaseYear= scwDateNow.getFullYear()-50;
	scwDropDownYears= 35;
}

// date picker init:  Entry Date and Date Visited
var currentTime = new Date()
var month = ('0' + String(currentTime.getMonth()+1)).slice(-2)
var day = ('0' + String(currentTime.getDate())).slice(-2)
var year = String(currentTime.getFullYear())
document.getElementById('entrydate').value= month + '/' + day + '/' + year
document.getElementById('visitdate').value= month + '/' + day + '/' + year

// slider init
var slider1= new Slider(document.getElementById("messyDiv"), document.getElementById("messyInput"))
slider1.setMinimum(1)
slider1.setMaximum(5)
slider1.setValue(3)
document.getElementById("messyVal").value= slider1.getValue()
slider1.onchange= function () {
	document.getElementById("messyVal").value= slider1.getValue()
}

var slider2= new Slider(document.getElementById("outgoingDiv"), document.getElementById("outgoingInput"))
slider2.setMinimum(1)
slider2.setMaximum(5)
slider2.setValue(3)
document.getElementById("outgoingVal").value= slider2.getValue()
slider2.onchange= function () {
	document.getElementById("outgoingVal").value= slider2.getValue()
}

var slider3= new Slider(document.getElementById("nocturnalDiv"), document.getElementById("nocturnalInput"))
slider3.setMinimum(1)
slider3.setMaximum(5)
slider3.setValue(3)
document.getElementById("nocturnalVal").value= slider3.getValue()
slider3.onchange= function () {
	document.getElementById("nocturnalVal").value= slider3.getValue()
}

window.onresize= function () {
	slider1.recalculate()
	slider2.recalculate()
	slider3.recalculate()
}
} //end postLoad function
//-->
