      <script type="text/javascript">

function makeSubmitEnabled(){
	document.getElementById('submit_form').disabled='';
}
function flagMissingFields(){
	var d = document.getElementById("first_name");
	d.className = d.className + " error";
	document.getElementById('submit_form').disabled = 1;
	alert("I am an alert box!");
}
    function validateForm() {
        var x = document.getElementById('first_name');
        var xVal = x.value;
        if (xVal == null || xVal == 0 || xVal == "0") {
        	x.className = x.className + " error";
			

            alert("Stop");
        }
        else{
            document.form.submit();
            document.getElementById('submit_form').disabled = 1;
            }
    }

function validateForm2() {
	var inputs = document.getElementsByTagName("input");
	var showAlert = false;
	var errorClassName = "error";
	for (var i=0; i < inputs.length; i++){
		
  		var block = inputs[i];
  		var blockClass = block.getAttribute('class');
   		if (blockClass!=null && (blockClass.indexOf("required") !== -1)){
   			if(block.value == null || block.value == 0 || block.value == "0"){
   				block.className = block.className + " error";
   				showAlert = true;
   				}
   			else{
   				block.classList.remove('error');
   			}	
   		}
	}
	if (showAlert) alert("We apologize but we were unable to process your request due to missing fields.  Please note fields outlined in RED are required.");
	else
            document.FormName.submit();
}

function validateForm3() {
	var inputs = document.getElementsByTagName("input");
	var showAlert = false;
	var errorClassName = "error";
	for (var i=0; i < inputs.length; i++){
		
  		var block = inputs[i];
  		var blockClass = block.getAttribute('class');
   		if (blockClass!=null && (blockClass.indexOf("required") !== -1)){
   			if(block.value == null || block.value == ""){
   				block.className = block.className + " error";
   				showAlert = true;
   				}
   			else{
   				block.className = block.className.replace("error","");
   				//block.classList.remove('error');
   			}	
   		}
	}
	if (showAlert) alert("We apologize but we were unable to process your request due to missing fields.  Please note fields outlined in RED are required.");
	else document.FormName.submit();
}
</script> 
