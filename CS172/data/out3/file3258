      <script language="JavaScript">
	function loadSelectedDate(){
		var date_start = document.FormName.date_start;

		loadTextInToElement(date_start, "arrival");
	}

	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}
</script> 
