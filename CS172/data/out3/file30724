      <script type="text/javascript">
	function toTitleCase(str) {
    		return str.replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();});
	}

	//no showcase for mobile platform
	if (!is_mobile) {	
		var GTC_user_county;
		var GTC_user_state;

		var geoipURL = 'http://geoip.nj.com/api';
		$.ajax({
			url: geoipURL,
			dataType: "json",
			async: false,
			success: function(data){
				GTC_user_county = data.county;
				var geoip_state = data.region;
				GTC_user_state = geoip_state.toUpperCase();
			}
		});

		var showcase_id = $.cookie('showcase_id');
		var resultsURL = 'http://realestate.nj.com/?classification=real estate&temp_type=include&finder=rent&tp=RE_nj&property=nj.com';
		if (GTC_user_county && GTC_user_state == 'NJ' ) {
			resultsURL += '&county=' + GTC_user_county;
		}
		if (showcase_id != null) {
			resultsURL += '&ad_id=' + showcase_id;
		}
		resultsURL += '&v='+Math.round(Math.random()*5);

		$.ajax({
			url: resultsURL,
			success: function(data) {
				jQuery(document).ready(function() {
					$('#finder_results').html(data);
				});
			}
		});
	}
</script> 
