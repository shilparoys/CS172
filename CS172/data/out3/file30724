function POP(u,n,w,h,f){windowFeatures = "width="+w+",height="+h;if (f) windowFeatures += ","+f;popwin = this.open(u, n, windowFeatures);if (!popwin.opener) popwin.opener=self;if (popwin.focus) popwin.focus(); }
function addEvent(obj, evType, fn){ 
 if (obj.addEventListener){ 
   obj.addEventListener(evType, fn, true); 
   return true; 
 } else if (obj.attachEvent){ 
   var r = obj.attachEvent("on"+evType, fn); 
   return r; 
 } else { 
   return false; 
 } 
}

function isValidEmail( e ) {
var oRE  =
 /(^[a-z0-9]([a-z0-9_\-\.]*)@([a-z0-9_\-\.]*)([.][a-z]{3})$)|(^[a-z0-9]([a-z0-9_\.]*)@([a-z0-9_\.]*)(\.[a-z]{3})(\.[a-z]{2})*$)/i;
  //check for valid email
  return oRE.test(e);
}

function classChange(id,newclass) {
	var element = document.getElementById(id);
	element.className = newclass;
}

function verifyAdv(f) {
	var newPage = parseInt(f.page.value);
	var finalPage = parseInt(f.lastPage.value);
	if (newPage > finalPage) {
   		f.page.value = f.thisPage.value;
		f.thisPage.value = "";
		f.lastPage.value = ""; }
}

// Tracking Assistance
function popUpRedirect(id,destination,img,pc) {
   destination = escape(destination);
   window.open( "http://realestate.NJ.com/?tp=RealEstateCore2007&tl=1&category=reporting&aff=nj.com&temp_type=detail&ad_id="+id+'&website='+destination+'&img='+img+'&pc='+pc, "","scrollbars,toolbar=yes,location=yes,status=yes,menubar=yes,titlebar=yes,resizable=1");
}

function dynamicReportingCrumb(adcat_id,cust_id,broker_id,ad_id,destination,pc,type) {
	var date = new Date();
	var time = date.getTime();
	var TRsrc = "http://report-re.NJ.com/images/mdwtc/MDRE_SITE/" + adcat_id + ".gif?aid="+cust_id+";bid="+broker_id+";lid="+ad_id+";pc="+pc + ';r=' + time;
	insertTrackingElement(TRsrc);
}

function dynamicApartmentCrumb(source_id,mgt_co_id,com_id,destination,pc,type) {
	var date = new Date();
	var time = date.getTime();
	var trackingURL = 'http://report-re.NJ.com/images/mdwtc/MDAPT_SITE/' + source_id + '.gif?com_id=' + com_id + ';mgmt_co_id=' + mgt_co_id + ';pc=' + pc + ';r=' + time;
	insertTrackingElement(trackingURL);
}

function insertTrackingElement (url) {
	var element;
	element = document.createElement("img");
	element.setAttribute("src", url);
	element.setAttribute("border", "0");
	element.setAttribute("height", "1");
	element.setAttribute("width", "1");
	document.body.insertBefore(element,null);
}

// End Tracking Assistance

var iFrame;

function showHelp(id) {
	var coords;
	if (id.indexOf('Bot') != -1) {
		id = id.replace("Bot","");
		coords = getCoords(id+'LinkBot');
	}
	else coords = getCoords(id+'Link');

	document.getElementById(id).style.left = (coords.x + 10) + "px";
	document.getElementById(id).style.top = (coords.y + 25) + "px";
	document.getElementById(id).style.display = "block";
	iFrame = document.createElement("iframe");
	iFrame.setAttribute("src", "");
	iFrame.style.position="absolute";
	iFrame.style.border="none";
	iFrame.style.left = document.getElementById(id).offsetLeft + 'px';
	iFrame.style.top = document.getElementById(id).offsetTop + 'px';
	iFrame.style.width = document.getElementById(id).offsetWidth + 'px';
	iFrame.style.height = document.getElementById(id).offsetHeight + 'px';
	iFrame.style.zIndex = "4999";
	document.body.appendChild(iFrame);
}

function hideHelp(id) {
	if (id.indexOf('Bot') != -1) {
		id = id.replace("Bot","");
 	}
 	document.getElementById(id).style.display = "none";
	document.body.removeChild(iFrame);
}

function getCoords() {
	var element = arguments[0];
	element = (typeof element == "object") ? element : document.getElementById(element);
	var coords = {x: 0, y: 0};
	while (element) {
		coords.x += element.offsetLeft;
		coords.y += element.offsetTop;
		element = element.offsetParent;
	}
	return coords;
}

