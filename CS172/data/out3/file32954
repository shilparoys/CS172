  <script>
$(document).ready(function () {

  $("ul.tabs li#li1").click(function () {
    $("ul.tabs li#li2").removeClass("active"); 
    $(this).addClass("active"); 
    $(".tabContainer #tab2").hide(); 
    $(".tabContainer #tab1").fadeIn(); 
    return false;
  });

  $("ul.tabs li#li2").click(function () {
    $("ul.tabs li#li1").removeClass("active"); 
    $(this).addClass("active"); 
    $(".tabContainer #tab1").hide(); 
    $(".tabContainer #tab2").fadeIn(); 
    return false;
  });
	
	$(".emergency a.close").click(function () {
	  $(".emergency").slideUp();
	});
       
});

</script> 
  <script>
$(window).scroll(function()
{
	$end_height = $(document).height() - $(window).height();
	if ( !$('#more-items-loading').is(":visible") && ( ($(window).scrollTop() == $end_height) || ( (navigator.userAgent.match(/iPhone/i)) && $(window).scrollTop()+60 == $end_height) ) ) 
	//adding 60px is for iphone
	{
		$('#more-items-loading').show();
		
		var data_send = { action: 'gz_get_more_posts', sort_by: cur_sort_by, offset: cur_offset, cat: cur_cat };
		
		$.ajax({
			type: 'POST',
			url: '/gazette/wp-admin/admin-ajax.php',
			data: data_send,
			dataType: 'json',
			success: function(data)
			{
				//$('.searchform-wrapper').html('<div style="color:white">'+data.cnt+':'+cur_offset+'</div>');
				if (data.cnt>cur_offset) {
					$('#more-items-loading').hide();
					$('.story-list-more').append(data.html); 
					cur_offset = parseInt(data.cnt);
					dataLayer.push({'eventCategory': 'feed_endless_scroll', 'eventAction': 'feed_latest', 'eventLabel': 'count_'+data.cnt,'event': 'gaEvent'});

				}
				else {
					$('#more-items-loading').html('<center>No more stories to show.</center>');
				} 
			},
			error: function(errorThrown){
				//alert(errorThrown.responseText);
      }
		}); 
		
	}
});

</script> 
  <script>

$(window).scroll(function(){
	if ( getInternetExplorerVersion() != 8 ) {
		var $totalHeight = $('.sticky').outerHeight() + $('#logo').outerHeight() + $('#home-top').outerHeight() + $('#homepage-feature').outerHeight() ; 

		var scroll_top = $(window).scrollTop();      

		if (scroll_top <= $totalHeight) {      
		$('#sticky-filter').hide();
		} 
		if (scroll_top > $totalHeight) {      
		$('#sticky-filter').show();
		} 
	}
});

</script> 
  <script>
var prev_width = $(window).width();
var prev_height = $(window).height();

var inner_size = viewport();
var prev_inner_width = inner_size['width'];

window.onresize = function(event) {
	var w_width = $(window).width();
	
	if ( ( w_width>1242 && prev_width<=1242 ) || ( w_width<=1242 && prev_width>1242 ) ) {
		if ( ( w_width>1242 && prev_width<=1242 ) ) {
			$('#nav-open-btn').hide();
			$('#nav-close-btn').hide();
			$('#nav-open-btn').removeAttr('style');
			$('#nav-close-btn').removeAttr('style');
		}
		prev_width = w_width;
	}
	
	var w_height = $(window).height();
	if ( w_height != prev_height ) {
		if ( use_tinyscroller() ) {
		$('#scrollbar1').tinyscrollbar({invertscroll: true});
		}
		else {
			set_scroller_height();
		}
	}
	
	//For photo description
	inner_size = viewport();
	var inner_width = inner_size['width'];
	if ( inner_width>475 && prev_inner_width<=475 ) {
		$('.article-feature-caption.tablet .toggle-more-less').hide();
		$('.photo-description').removeAttr('style');
		prev_inner_width = inner_width;
	}
	else if ( inner_width<=475 && prev_inner_width>475 ) {
		$('.article-feature-caption.tablet .toggle-more-less').show();
		$('.photo-description').removeAttr('style');
		prev_inner_width = inner_width;
	}
}

</script> 
  <!-- new chartbeat code, put in March 2, 2013 --> 
  <script type="text/javascript">
var _sf_async_config={};
/** CONFIGURATION START **/
_sf_async_config.uid = 2327;
_sf_async_config.domain = "news.harvard.edu"; 

/** CONFIGURATION END **/
(function(){
  function loadChartbeat() {
    window._sf_endpt=(new Date()).getTime();
    var e = document.createElement("script");
    e.setAttribute("language", "javascript");
    e.setAttribute("type", "text/javascript");
    e.setAttribute("src",
       (("https:" == document.location.protocol) ?
         "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" :
         "http://static.chartbeat.com/") +
       "js/chartbeat.js");
    document.body.appendChild(e);
  }
  var oldonload = window.onload;
  window.onload = (typeof window.onload != "function") ?
     loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();
</script> 
  <!-- end chartbeat code -->  
 </body>
