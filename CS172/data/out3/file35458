      //fix bootstrap v2.1.1 dropdown menus in iOS
      $('body').on('touchstart.dropdown', '.dropdown-menu', function (e) { 
          e.stopPropagation(); 
      });
    });
    </script> 
  <!-- InstanceEnd -->  
 </body>
</html>