        var location = (document.location) ? encodeURIComponent(document.location) : '';
        po.src = '//luclive.tk/index.php/chat/getstatus/(click)/internal/(position)/bottom_right/(check_operator_messages)/true/(top)/350/(units)/pixels/(leaveamessage)/true?r=' + refferer + '&l=' + location;
