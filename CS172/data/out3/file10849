           <script type="text/javascript">// <![CDATA[

function extractPageName(hrefString)
 {
var arr = hrefString.split("/");
return  (arr.length<2) ? hrefString : arr[arr.length-2].toLowerCase() + arr[arr.length-1].toLowerCase();
}

function setActiveMenu(arr, crtPage)
 {
 for (var i=0; i<arr.length; i++)
   {
      if(extractPageName(arr[i].href) == crtPage)
        { 
          if (arr[i].parentNode.tagName != "DIV")
            {
               arr[i].className = "activeMenuItem";
               arr[i].parentNode.className = "activeMenuItem";
             }
          }
    }
 }
 
function setPage()
{
 hrefString = document.location.href ? document.location.href : document.location;
// alert(hrefString);
var testId=document.getElementById("wrapNav") ;  /*wrapSectionSecondary_internal.sectionY  */
  if (testId != null){
//    alert("test");
   setActiveMenu(document.getElementById("wrapNav").getElementsByTagName("a"), extractPageName(hrefString));
//alert("test 2");
  }
}
//alert("yes");
setPage();
//alert("no");
