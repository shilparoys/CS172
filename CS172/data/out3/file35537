     <div class="post"> 
      <h3><a href="http://dotsx.usc.edu/newsblog/index.php/main/comments/usc_libraries_name_finalists_for_27th-annual_scripter_award/">USC Libraries Name Finalists for 27th-Annual Scripter Award</a></h3> 
      <div class="details">
       Posted by 
       <span id="eeEncEmail_HTkmApnAWM">.(JavaScript must be enabled to view this email address)</span>
       <script type="text/javascript">
//<![CDATA[
var l=new Array();
var output = '';
l[0]='>';l[1]='a';l[2]='/';l[3]='<';l[4]=' 115';l[5]=' 114';l[6]=' 101';l[7]=' 116';l[8]=' 115';l[9]=' 97';l[10]=' 77';l[11]=' 32';l[12]=' 110';l[13]=' 97';l[14]=' 104';l[15]=' 116';l[16]=' 97';l[17]=' 78';l[18]='>';l[19]='\"';l[20]=' 117';l[21]=' 100';l[22]=' 101';l[23]=' 46';l[24]=' 99';l[25]=' 115';l[26]=' 117';l[27]=' 64';l[28]=' 115';l[29]=' 114';l[30]=' 101';l[31]=' 116';l[32]=' 115';l[33]=' 97';l[34]=' 109';l[35]=' 110';l[36]=':';l[37]='o';l[38]='t';l[39]='l';l[40]='i';l[41]='a';l[42]='m';l[43]='\"';l[44]='=';l[45]='f';l[46]='e';l[47]='r';l[48]='h';l[49]='a ';l[50]='<';
