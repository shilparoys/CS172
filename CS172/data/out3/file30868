       <script type="text/javascript">
(function() {



  function getObject(name) {
    var obj = null;
    // if localStorage is present, use that
    if (('localStorage' in window) && window.localStorage !== null) {
      // easy object property API
      if(typeof localStorage[name] !== null) {
        try {
          obj = JSON.parse(localStorage[name]);
        } catch (e) {
        }
      }
    } else {
      //use cookie
      if( typeof getCookie(name) !== null) {
        try {
          obj = JSON.parse(getCookie(name));
        } catch (e) {
        }
      }
    }
    return obj;
  }

  function deleteObject(name) {
    // if localStorage is present, use that
    if (('localStorage' in window) && window.localStorage !== null) {
      // easy object property API
      try {
        delete localStorage[name];
      } catch (e) {
        localStorage[name] = undefined;
      }
    } else {
      //use cookie
      eraseCookie(name);
    }

  }

  function saveObject(name,objectData) {
    if (('localStorage' in window) && window.localStorage !== null) {
      // easy object property API
      localStorage[name] = JSON.stringify(objectData);
    } else {
      //use cookie
      setCookie(name,JSON.stringify(objectData),29);//29 cause its funny
    }
  }

  function setCookie(name,value,days) {
    if (days) {
      var date = new Date();
      date.setTime(date.getTime()+(days*24*60*60*1000));
      var expires = "; expires="+date.toGMTString();
    } else {
      var expires = "";
    }
    document.cookie = name+"="+value+expires+"; path=/";
  }

  function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i< ca.length;i++) {
      var c = ca[i];
      while (c.charAt(0)==' ') c = c.substring(1,c.length);
      if (c.indexOf(nameEQ) == 0) {
        return c.substring(nameEQ.length,c.length);
      }
    }
    return null;
  }

  function eraseCookie(name) {
    setCookie(name,"",-1);
  }

  function objectCompare(obj1, obj2, loose) {
    // if loose is NOT TRUE, first do a quick comparison of lengths
    // quick & dirty attempt to avoid comparing all property values on large or complex objects
    var count1 = 0, count2 = 0;
    if (!loose){
        for (i in obj1){
        count1++;
        }
        for (i in obj2){
        count2++;
        }
    }

    if (count1 != count2){ // lengths are different, so no need to compare values
        return false;
    }
    else { // lengths are the same (or loose is TRUE), so continue to compare individual property values
        for (i in obj1) {
        if (typeof obj1[i] == 'object' && typeof obj2[i] == 'object') { // both items are objects
            if (!objectCompare(obj1[i], obj2[i], loose)){ // see if objects match
            return false;
            }
        }
        else if (
            loose // loose comparison, 42 == "42", null == '' == undefined
            &&
            !(
            (obj1[i] || '') == (obj2[i] || '') // this will work in all cases EXCEPT ("0" ?= 0|null|undefined), which will evaluate to ("0" ?= ""), which is of course FALSE
            ||
            ( // if either value is a number, try to convert both to numbers and compare - this will work around above exception
                (typeof obj1[i] == 'number' || typeof obj2[i] == 'number')
                &&                Number(obj1[i]) == Number(obj2[i])
            )
            )
        ){ // only one item is an object, or objects don't match
            return false;
        }
        else if (
            !loose // strict comparison, 42 != "42", null != '' != undefined
            &&
            !(obj1[i] === obj2[i])
        ){
            return false;
        }
        }    }

    return true; // all tests passed, objects are equivalent
  }

  var loadedObject = getObject("anon_1");

  
  var newRow = ["22",{"cid":"1116067","ttl":"NJ Advance Media"}];
  if(typeof newRow == 'object') {
    var maxLength = 5;
    if( loadedObject !== null) {
      //see if new row equals an existing row
      //walk the array backwards so pointer doesn't get confused
      for(var l = loadedObject.length - 1; l >= 0; l--){
        if(objectCompare(newRow, loadedObject[l],false)) {
            loadedObject.splice(l,1);
        }
      }
      if(loadedObject.length >= maxLength) {
        loadedObject.pop();
      }
      loadedObject.unshift(newRow);
      saveObject("anon_1",loadedObject);
    }  else {
      loadedObject = [];
      loadedObject.unshift(newRow);
      saveObject("anon_1",loadedObject);
    }
  }
  
})();
</script> 
