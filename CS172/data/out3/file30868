           <script type="text/javascript">
            aiJQuery(document).ready(function() {
                aiJQuery(function() {
                    aiJQuery("div#overlay_signinLink").dialog({
                        autoOpen: false,
                        zIndex: 200010,
                        modal: true,
                        closeText: " ",
                        position: ["center", 50],
                        draggable: false,
                        
                        
                        resizable: false
                    });
                    
                });
                
                aiJQuery( "a#signinLink").click(function() {
                    aiJQuery("div[id^='overlay_']").html( "" );
                    aiJQuery("div[id^='overlay_']").dialog( "close" );
                    
                        aiJQuery.ajax({
                            type: "GET",
                            url: "/jobs/login?rd=http%3A%2F%2Fjobs.nj.com%2Fjobs%2Fnj-advance-media-1116067-cd",
                            
                            success: function(data) {
                                aiJQuery("div#overlay_signinLink").html(data);
                                aiJQuery("div#overlay_signinLink").find("input[type=text],textarea,select").filter(":visible:first").focus(); 
                            },
                            dataType: "html"
                        });
                        aiJQuery("div#overlay_signinLink").dialog("open");
                    
                    return false;
                });
                    
            });
        </script> 
          </div> 
