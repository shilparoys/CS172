                    <script type="text/javascript">
  aiJQuery(document).ready(function() {
    aiJQuery("#autocomplete_location")
    .autocomplete('/jobs/search/locationSuggest', aiJQuery.extend({}, {
      dataType: 'json',
      parse:    function(data) {
        var parsed = [];
        for (key in data) {
          parsed[parsed.length] = { data: [ data[key], key ], value: data[key], result: data[key] };
        }
        return parsed;
      }
    }, { selectFirst:false, minChars:2, cacheLength:0, delay: 250 }))
    .result(function(event, data) { aiJQuery("#location").val(data[1]); });
  });
</script> 
