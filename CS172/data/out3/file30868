aiJQuery.waitingReload = false;
aiJQuery.waitingLocation = false;

aiJQuery(window).focus(function() {
    if(aiJQuery.waitingReload === true) {
        window.location.reload(true);
    } else if(aiJQuery.waitingLocation !== false) {
        window.location.href = aiJQuery.waitingLocation;
    }
});

