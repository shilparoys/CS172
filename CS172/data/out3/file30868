            <script type="text/javascript">
// Put this class on links, and they'll work even inside the div onclick
aiJQuery('.aiPreventOnclick').click(function(event) {
    window.location = aiJQuery(this).attr('href');

    return false;
});

aiJQuery('.aiPreventOnclickNewWindow').click(function(event) {
    event.stopPropagation();
    window.open(aiJQuery(this).attr('href'), '_blank');

    return false;
});
</script> 
