    aiJQuery(document).ready(function(){
        function megaHoverOver(){aiJQuery(this).find(".aiSubNav").stop().fadeTo('fast',1).show();(function(aiJQuery){aiJQuery.fn.calcSubWidth=function(){rowWidth=0;aiJQuery(this).find("ul").each(function(){rowWidth+=aiJQuery(this).width();});};})(aiJQuery);if(aiJQuery(this).find(".row").length>0){var biggestRow=0;aiJQuery(this).find(".row").each(function(){aiJQuery(this).calcSubWidth();if(rowWidth>biggestRow){biggestRow=rowWidth;}});aiJQuery(this).find(".aiSubNav").css({'width':biggestRow});aiJQuery(this).find(".row:last").css({'margin':'0'});}else{aiJQuery(this).calcSubWidth();aiJQuery(this).find(".aiSubNav").css({'width':rowWidth});}} function megaHoverOut(){aiJQuery(this).find(".aiSubNav").stop().fadeTo('fast',0,function(){aiJQuery(this).hide();});}
        var config={
            sensitivity:5,
            interval:25,
            over:megaHoverOver,
            timeout:100,
            out:megaHoverOut};
            aiJQuery("ul.aiTabsLvl1 li .aiSubNav").css({'opacity':'0'});aiJQuery("ul.aiTabsLvl1 li").hoverIntent(config);
        });
</script> 
