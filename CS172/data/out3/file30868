          <div class="" "aiclearfix"=""> 
           <a class="" href="/jobs/search/results?companyId=1116067">View All Jobs From Employer</a> 
          </div> 
          <!-- / aiClearfix --> 
          <div class="aiClearfix"> 
           <form method="post" id="jobSearchQuickForm" name="jobSearchQuickForm" onsubmit="if(aiJQuery(&quot;#location&quot;).val()==&quot;City &amp; State or Zip Code&quot; || aiJQuery(&quot;#autocomplete_location&quot;).val()==&quot;City &amp; State or Zip Code&quot;) {aiJQuery(&quot;#location&quot;).val(&quot;&quot;); aiJQuery(&quot;#autocomplete_location&quot;).val(&quot;&quot;);} return true;" accept-charset="utf-8" action="/jobs/search/advanced"> 
            <div id="aiRunaNewSearch"> 
             <h2>Quick Search</h2> 
             <div class="aiQuickSearch aiQuickSearchBar aiClearfix"> 
              <div class="aiQSHolder"> 
               <div class="aiQuickSearchBarLeft aiqsSearchResults"> 
                <script type="text/javascript">
//<![CDATA[

    function disableLocationFormElements(which)
    {
        var regionFormElements = [
            "region"
        ]

        var locationFormElements = [
            "location",
            "autocomplete_location",
            "radius"
        ]

        switch(which) {

            case "region":

                // Disable region
                aiJQuery.each(regionFormElements, function(key, value) {
                        aiJQuery("#" + value).attr("disabled", "disabled");
                    }
                );

                // Enable location
                aiJQuery.each(locationFormElements, function(key, value) {
                        aiJQuery("#" + value).removeAttr("disabled");
                    }
                );

                break;

            case "location":

                // Enable region
                aiJQuery.each(regionFormElements, function(key, value) {
                        aiJQuery("#" + value).removeAttr("disabled");
                    }
                );

                // Disable location
                aiJQuery.each(locationFormElements, function(key, value) {
                        aiJQuery("#" + value).attr("disabled", "disabled");
                    }
                );

                break;
        }
    }

//]]>
</script> 
                <script type="text/javascript">
//<![CDATA[
function makeMultiSelect() {

    
    
    aiJQuery("#radius").multiSelect({
        selectAll: false,
        selectExclusive: true,
        noneSelected: 'Select Options',
        oneOrMoreSelected: '!'
    });
}
