  <script type="text/javascript">
            
		    function invokeMeMaster() {
   
                  var chkPostBack = 'false';
		        

		           if (chkPostBack == 'false') {
                     
                       $lb.launch({ url: 'images/pop/img.jpg', title: "", options: 'showPrint:false' }); 
		            
                     //in title prop put this: <a href=http://www.asu.edu.jo/asu/userfiles/file/images/img.jpg>   تحميل الأعلان </a>
                     //or like this <a href=http://asu.edu.jo/showfull.aspx\>تكبير حجم الصورة</a>
		        }
		        
		    }
		     // not used
		    function invokeMeMaster3() {
   
                  var chkPostBack = 'false';
		        

		           if (chkPostBack == 'false') {
                     
                       $lb.launch({ url: 'images/pop/king.jpeg', options: 'showPrint:true' }); 
		            
                     
		        }
		        
		    }
		    // not used
		    
		     function invokeMeMaster2() {

		        var chkPostBack = 'false';


                if (chkPostBack == 'false') {
                     $lb.launch({ url: 'images/pop/img.jpg', title: "<a href=http://asu.edu.jo/OneOnly.aspx?id=299&Mode=Ann\\\">للمزيد من المعلومات</a>", options: 'showPrint:true' });
                    $lb.launch({ url:  document.location.replace("http://asu.edu.jo/OneOnly.aspx?id=299&Mode=Ann") });
                // was used in invokeMeMaster    $lb.launch({ url: 'images/pop/eng.jpg', options: 'showPrint:true'});  
                }
            }
            
              

