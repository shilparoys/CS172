    function LaunchFancyBox() {
        // Need to reapply overlay because #popuptrigger is in an update panel and gets
        // rebuilt in the DOM on each update.
        $("#popuptrigger").overlay({
            mask: {
                color: '#ebecff',
                loadSpeed: 1200
            },

            closeOnClick: false
        });

        // Tigger control that references the popup div
        setTimeout('$("#popuptrigger").trigger("click");', 0);
    }
</script> 
