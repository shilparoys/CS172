        function showPopulationDesc() {
        }

        function closeWindow() {
        }

        function showRollTip(msg, e) {
            if (typeof RollTip == "undefined" || !RollTip.ready) return;
            RollTip.reveal(msg, e);
        }

        function hideRollTip() {
            if (typeof RollTip == "undefined" || !RollTip.ready) return;
            RollTip.conceal();
        }


        function link(obj) {
            obj.style.backgroundColor = '#507CD1';
            obj.style.color = 'white';
            return true;
        }

        function link1(obj) {
            obj.style.backgroundColor = '#ffffff';
            obj.style.color = 'Blue';
            return true;
        }

        function linkSOCSearch(obj) {
            obj.style.borderBottom = '1px solid #507CD1';
            obj.style.backgroundColor = '#507CD1';
            obj.style.color = 'white';
            return true;
        }

        function link1SOCSearch(obj) {
            obj.style.borderBottom = '1px solid #5B5BFF';
            obj.style.backgroundColor = '#ffffff';
            obj.style.color = '#5B5BFF';
            return true;
        }

        function cursor_wait() {
            document.body.style.cursor = 'wait';
        }

        function cursor_clear() {
            document.body.style.cursor = 'default';
        }


        function showorhideAboutSOC(id) {
            if (document.getElementById(id).style.display == "none") {
                document.getElementById(id).style.display = "block";
                document.getElementById("helpbtn").innerHTML = "Hide About Schedule of Classes";
                function showorhide(id) {
                    if (document.getElementById(id).style.display == "none") {
                        document.getElementById(id).style.display = "block";
                        document.getElementById("helpbtn").innerHTML = "Hide MySystem Information";

                        /*detectPopupBlocker();*/

                    } else {
                        document.getElementById(id).style.display = "none";
                        document.getElementById("helpbtn").innerHTML = "MySystem Information";
                    }
                }

            } else {
                document.getElementById(id).style.display = "none";
                document.getElementById("helpbtn").innerHTML = "About Schedule of Classes";
            }


            function detectPopupBlocker() {
                var myTest = window.open("about:blank", "", "directories=no,height=1,width=1,menubar=no,resizable=no,scrollbars=no,status=no,titlebar=no,top=0,location=no");
                myTest.close();
                if (!myTest) {
                    document.getElementById("lbl_popupsEnabled").innerHTML = "NOcvc";
                    lbl_popupsEnabled.style.backgroundColor = 'Red';
                    myTest.close();
                } else {
                    lbl_popupsEnabled.style.backgroundColor = '#ffffff';
                    document.getElementById("lbl_popupsEnabled").innerHTML = "YES";
                    myTest.close();
                }

            }
        }
        
    </script> 
