if(window.location.href.indexOf('-p-') != -1)
{
var id = window.location.href.split("-p-")[1].replace(".html","");
var price = document.getElementsByClassName('price')[0].innerHTML.replace("&nbsp;","").replace("&nbsp;","").replace("$","");
var google_tag_params = {
ecomm_prodid: id,
ecomm_pagetype: 'product',
ecomm_totalvalue: price
};
}
if(window.location.href.indexOf('shopping_cart.php') != -1)
{
var id = new Array();
var price  = new Array();
var len = document.getElementsByClassName('product-name').length;
for(i=0;i<len;i++)
{
id.push(document.getElementsByClassName('product-name')[i].getElementsByTagName('a')[0].href.split("-p-")[1].replace(".html",""));
price.push(document.getElementsByClassName('price')[i].getElementsByTagName('b')[0].innerHTML.replace("$",""));
}
var google_tag_params = {
ecomm_prodid: id,
ecomm_pagetype: 'cart',
ecomm_totalvalue: price
};
}
