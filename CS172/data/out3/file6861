  <script language="JavaScript">
function validate()
{
if (document.newuser.txtusername.value=="")
{
alert("Enter Your User Name (Email ID)");
document.newuser.txtusername.focus();
return false;
}
else if(!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(document.newuser.txtusername.value))) 
{
alert("Enter Your Correct Email id");
document.newuser.txtusername.value=""
document.newuser.txtusername.focus();
return false;
}
else if (document.newuser.txtpassword.value=="")
{
alert("Enter Your Password");
document.newuser.txtpassword.focus();
return false;
}

else if (document.newuser.txtname.value=="")
{
alert("Enter Your Name");
document.newuser.txtname.focus();
return false;
}


else if(document.newuser.txtmobile.value=="")
	{
		alert("Enter Your Mobile  No");
		document.newuser.txtmobile.focus();
		return false
	}
	
	else if(document.newuser.txtcourse.value=="")
	{
		alert("Enter Course Interested in");
		document.newuser.txtcourse.focus();
		return false
	}
	else if (document.newuser.txtplace.value=="")
{
alert("Enter Your Place");
document.newuser.txtplace.focus();
return false;
}
	else if (document.newuser.txtState.value=="")
{
alert("Enter Your State");
document.newuser.txtState.focus();
return false;
}
	else if(document.newuser.txtHTW.value=="")
	{
		alert("Enter How you know about our website?");
		document.newuser.txtHTW.focus();
		return false
	}
else
{  
document.newuser.method="post";
document.newuser.action="Admission-Newuser.asp?str=add";
document.newuser.submit();
}
}function getkey(e)
  {
     if (window.event)
           return window.event.keyCode;
     else if (e)
           return e.which;
     else
           return null;
    }

    function numbersonly(e)
    {

        var key = getkey(e); if ((key >=48 && key <=57) || key==8 || key==46 || key==116|| key==9|| key==0) {       
                 return true;
        }
        else
      {
	   if (9==key || 13==key)
	  {
	   return true;
	  }
             alert("Enter Numbers only");
             return false;
     
      }
            	
    }function textonly(e)
    {

        var key = getkey(e);
        if ((key >=65 && key <=90) || (key >=97 && key <=122)  || key==8 || key==46 || key==11 || 69==key || 13==key|| key==0|| key==32) 
        {       
                 return true;
        }
        else
      {
	   
             alert("Enter Text only");
             return false;
     
      }}
   </script> 
