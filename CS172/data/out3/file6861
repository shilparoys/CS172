      <!-- Content Block -->
      <script type="text/javascript">
function IsNumeric(sText)

{
   var ValidChars = "0123456789";
   var IsNumber=true;
   var Char;

 
   for (i = 0; i < sText.length && IsNumber == true; i++) 
      { 
      Char = sText.charAt(i); 
      if (ValidChars.indexOf(Char) == -1) 
         {
         IsNumber = false;
         }
      }
   return IsNumber;
   
   }
   
   function IsText(sText)

{
   var ValidChars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ. abcdefghijklmnopqrstuvwxyz";
   var IsNumber=true;
   var Char;

 
   for (i = 0; i < sText.length && IsNumber == true; i++) 
      { 
      Char = sText.charAt(i); 
      if (ValidChars.indexOf(Char) == -1) 
         {
         IsNumber = false;
         }
      }
   return IsNumber;
   
   }
   function IsEmail(sText)
   {
	   var isEmail;
   if(!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(sText)))
   {
	   
	   isEmail = false;
   }
   else
   {
	   isEmail = true;
   }
   return isEmail;
   }
</script>
      <script language="JavaScript">
function ages(ds,id)
{
	if(document.getElementById(ds).value!="")
{var dat
	dat=document.getElementById(ds).value.split("/")
	var cdat= new Date(dat[1]+'/'+dat[0]+'/'+dat[2])
	document.getElementById(id).value=2015-cdat.getFullYear();
}
}
function validates()
{
	if(document.appform.txtName.value=="")
	{
		alert("Enter Your Name");
		document.appform.txtName.focus();
		return false
	}
	else if(document.appform.txtdob.value=="")
	{
		alert("Select Your Date of Birth");
		document.getElementById("txtdob").focus();
		return false
	}
	else if(document.appform.txtMobile.value=="")
	{
		alert("Enter Your Contact No ");
		document.appform.txtMobile.focus();
		return false
	}
	else if(document.appform.txtEmail.value=="")
	{
		alert("Enter Your Email Id");
		document.appform.txtEmail.focus();
		return false
	}
	else if(!(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(document.appform.txtEmail.value)))
	{
		alert("Enter Your Correct Email Id");
		document.appform.txtEmail.value="";
		document.appform.txtEmail.focus();
		return false
	}
	
	else if(document.appform.txtpassword.value=="")
	{
		alert("Enter Your Password");
		document.appform.txtpassword.focus();
		return false
	}
	else if(document.appform.txtpassword1.value=="")
	{
		alert("Enter Your Confirm Password");
		document.appform.txtpassword1.focus();
		return false
	}
	else if(document.appform.txtpassword1.value!=document.appform.txtpassword.value)
	{
		alert("Enter Your Correct Confirm Password");
		document.appform.txtpassword1.focus();
		return false
	}
	
	else if(document.appform.txtFname.value=="")
	{
		alert("Enter Your Father's Name");
		document.appform.txtFname.focus();
		return false
	}
	else if(document.appform.txtchoice1.value=="")
	{
		alert("Select Your Course Interested ");
		document.appform.txtchoice1.focus();
		return false
	}
	else if(document.appform.txtAddresslin1.value=="")
	{
		alert("Enter Your Address Line1");
		document.appform.txtAddresslin1.focus();
		return false
	}
	
	
	else if(document.appform.txtstate.value=="")
	{
		alert("Enter Your State");
		document.appform.txtstate.focus();
		return false
	}
	
	else if(document.appform.txtCity.value=="")
	{
		alert("Enter Your City");
		document.appform.txtCity.focus();
		return false
	}	
	
	else if(document.appform.txtPincode.value=="")
	{
		alert("Enter Your Pincode");
		document.appform.txtPincode.focus();
		return false
	}	 
	
	
	
	else
	{
		document.appform.txtcourseName.value=getcoursename();		
		document.appform.method="post";
		document.appform.action="online-application.asp?str=add"
		document.appform.submit();
		
	}
}

function getcoursename()
{
	return $("#txtchoice1 option:selected").text();

}
function getkey(e)
  {
     if (window.event)
           return window.event.keyCode;
     else if (e)
           return e.which;
     else
           return null;
    }

    function numbersonly(e)
    {

        var key = getkey(e);
        if ((key >=48 && key <=57) || key==8 || key==46 || key==116|| key==9|| key==0) 
        {       
                 return true;
        }
        else
      {
	   if (9==key || 13==key)
	  {
	   return true;
	  }
             alert("Enter Numbers only");
             return false;
     
      }
            	
    }
	
	function textonly(e)
    {

        var key = getkey(e);
        if ((key >=65 && key <=90) || (key >=97 && key <=122)  || key==8 || key==46 || key==11 || 69==key || 13==key|| key==0|| key==32) {       
                 return true;
        }
        else
      {
	   
             alert("Enter Text only");
             return false;
     
      }}
   </script>
