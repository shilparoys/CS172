       <style type="text/css">@media screen,projection{.cufon-canvas{display:inline;display:inline-block;position:relative;vertical-align:middle;font-size:1px;line-height:1px}.cufon-canvas .cufon-alt{display:none}  .cufon-canvas canvas{position:relative}}@media print{.cufon-canvas{padding:0 !important}  .cufon-canvas canvas{display:none}.cufon-canvas .cufon-alt{display:inline}} </style> 
       <script type="text/javascript" src="/common/js/jcarousellite-carousel.js"></script> 
       <script type="text/javascript">
var slideDecrement = 5;
var slideIncrement = 1;
var selectedSlide;
var slideCounter;

$(function() {
    $(".carouselWrapper .carousel").jCarouselLite( {
        btnNext: "#arrowRight",
        btnPrev: "#arrowLeft",
        btnGo:['.carouselWrapper .1','.carouselWrapper .2','.carouselWrapper .3''.carouselWrapper .1','.carouselWrapper .2','.carouselWrapper .3','.carouselWrapper .4','.carouselWrapper .5'],
        scroll: 1,
        circular: false,
        visible: 1
    });
});

// This method is using for change the image
function goImg(slideId,totalSlides,contextPath){
    selectedSlide = slideId;
    slideDecrement = selectedSlide;
    slideIncrement = selectedSlide;
    for(slideCounter=1;slideCounter<=totalSlides;slideCounter++){
        if(slideCounter == slideId) {
            document.getElementById(slideId).src = '/common/images/carousel/dotActive.gif';
        } else {
            document.getElementById(slideCounter).src = '/common/images/carousel/dotInactive.gif';
        }
    }
}

//This method is using for rotate the slides from left to right.
function leftRotate(totalSlides,contextPath){
    if(selectedSlide != null) {
        if(selectedSlide == slideDecrement){
            if(selectedSlide == 1){
                goImg(slideDecrement,totalSlides,contextPath);
                //slideDecrement = totalSlides;
            } else {
                goImg(slideDecrement-1,totalSlides,contextPath);
            }
            slideDecrement--;
        }else {
            if(slideDecrement == totalSlides){
                goImg(slideDecrement,totalSlides,contextPath);
                slideIncrement=slideDecrement;
                slideDecrement--;
            } else {
                if(slideDecrement == 0) {
                    slideDecrement = 1;
                }
                goImg(slideDecrement,totalSlides,contextPath);
                slideIncrement=slideDecrement;
                slideDecrement--;
            }
        }
    }
}

// This method is using for rotate slides from right to left
function rightRotate(totalSlides,contextPath){
    if(selectedSlide != null) {
        slideDecrement=slideIncrement;
        slideIncrement++;
        if(slideIncrement > totalSlides ){
            slideIncrement = totalSlides;
        }
        goImg(slideIncrement,totalSlides,contextPath);
    } else {
        goImg(2,totalSlides,contextPath);
        slideIncrement = 2;
    }
}

</script> 
