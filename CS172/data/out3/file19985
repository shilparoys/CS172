jQuery(function() {
var theWindow        = jQuery(window),
jQuerybg              = jQuery(".bg"),
aspectRatio      = jQuerybg.width() / jQuerybg.height();
function resizeBg() {
if ( (theWindow.width() / theWindow.height()) < aspectRatio ) {	jQuerybg	.removeClass()	.addClass('bgheight');
} else {	jQuerybg	.removeClass()	.addClass('bgwidth');
}
}
theWindow.resize(function() {	resizeBg();
}).trigger("resize");
jQuery("head").append('<style type="text/css">.bg{position:fixed;z-index:1;left:0;top:0;}.bgwidth{width:100%;}.bgheight{width:100%;}.body{background:none;}</style>');
});
</script>
