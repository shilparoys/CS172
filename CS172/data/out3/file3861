                                    viewportBottom = Y.DOM.docScrollY() + Y.DOM.winHeight();
                                    elemHeight = elem.getY();
                                    if (!loaded && (viewportBottom + loadOffset >= elemHeight)) {
                                        loadImages();
                                    }
                                }
                                function loadImages() {
                                    Y.all('#mediatumblrwall .contents').each(function() {
