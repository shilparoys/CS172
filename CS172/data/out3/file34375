        <div id="block-views-bf18e2f28538ca4413f0de4a0d483121" class="block block-views last even"> 
         <h2 class="block-title">Personal Stories</h2> 
         <div class="content"> 
          <div class="view view-personal-stories-slideshow view-id-personal_stories_slideshow view-display-id-block view-dom-id-b62762880284ba3ccac56d02496c90e1"> 
           <div class="view-content"> 
            <div class="skin-default"> 
             <div class="views-slideshow-controls-top clearfix"> 
              <div id="views_slideshow_controls_text_personal_stories_slideshow-block" class="views-slideshow-controls-text views_slideshow_controls_text"> 
