    $j(document).ready(function () {
        (function($) {

    Zendesk.bindRelatedTopics = function() {
      $('#ticket_subject').blur(function() {
        var currentSearch = this.value;

        $.ajax({
          url: "/categories/search",
          data: {
            query: currentSearch,
            format: "html",
            page: 1,
            per_page: 4,
            for_suggestion: 1,
            xhr: true
          }
        }).done(function(response) {
          if (response.length > 0) {
            $("#topic_suggestions").html(response);
            $("#suggestions_for_new_topic").show();
            $("#topic_suggestions .suggestions a").each(function() { $(this).attr('tabindex', -1) });
          } else {
            $("#suggestions_for_new_topic").fadeOut('fast');
          }
        });
      });
    };

    Zendesk.bindRelatedTopics();

  }(jQuery));

