   <script>

jQuery(document).ready(function(){
	jQuery("#verify_email").keyup(function(){
		if(jQuery(this).val() == jQuery("#email").val()) {
			jQuery("#verify_email_validation").show();
		}
		else {
			jQuery("#verify_email_validation").hide();
		}
	});
	
	jQuery(".form_scholarship").submit(function(){
		if(jQuery("#verify_email").val() == jQuery("#email").val()) {
			return true;
		}
		else {
			alert('Please verify your email address match.');
			return false;
		}
	});
});
</script> 
