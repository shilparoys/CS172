jQuery(document).ready(function() {		
		set_programs("#subjectSelect");
		set_programs("#subjectSelect2");
		jQuery("#subjectSelect").change(function(){
		set_programs("#subjectSelect");
	});

	jQuery("#subjectSelect2").change(function(){
		set_programs("#subjectSelect2");
	});
});

function set_programs(ele) {
	var programs = [];
	var text = jQuery(ele + " option:selected").text();
	if(text == 'Nursing') programs = nursing;
	else if(text == 'Medical Assisting') programs = medical_assisting;
	else if(text == 'Alternative Medicine') programs = alternative_medicine;
	else if(text == 'Dental') programs = dental;
	else if(text == 'Diagnostic Professions') programs = diagnostic;
	else if(text == 'Clinical & Laboratory') programs = clinical;
	else if(text == 'Medical Office') programs = medical_office;
	else if(text == 'Pharmaceutical') programs = pharmaceutical;
	else if(text == 'Rehab Therapy') programs = rehab;
	
	var option = '';
	var selected = 	'';
	
	for (i=0;i<programs.length;i++){
		if(selected == programs[i].key )
 	   		option += '<option value="'+ programs[i].key + '" selected>' + programs[i].val + '</option>';
	   	else
		 option += '<option value="'+ programs[i].key + '">' + programs[i].val + '</option>';
	}
	$('#programsSelect').empty();
	$('#programsSelect').append(option);
	

	$('#programsSelect2').empty();
	$('#programsSelect2').append(option);
}

$(window).load(function(){
	//$("#fixed_bottom_search").slideDown('slow')
});
