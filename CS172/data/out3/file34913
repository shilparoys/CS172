          {
                 var ss = document.createElement("img");
                 ss.width = 1;
                 ss.height = 1;
                 ss.style.visibility = "hidden";
                 ss.src =  mp_protocol + rtaRetailer + ".collect.igodigital.com/collect/ownit/"+ rtaRetailer +"?skus=" + skus.substring(0, skus.length - 1);
                 document.getElementsByTagName("body")[0].appendChild(ss);
          }

         break;
     case "SR":
           rtaSearch = "";
         break;
     default:
         break;
    }

  rtaCartSku=rtaCart;
