    var ig_pageType = "DLP";
    var ig_DepartmentName = "Sweatshirts and Fleece".split(" ").join("_").toLowerCase();
    var ig_SubDeptName = "".split(" ").join("_").toLowerCase();
    var ig_TeamName = "USC Trojans";

    if (ig_TeamName.length == 0)
    {
      ig_TeamName =  "";
    }

    if (ig_TeamName.length == 0)
    {
      ig_TeamName =  "";
    }
    var vSeparator = "|";
    var rtaProductSKU = "";
    var rtaEmail = "";
    var rtaCategory = "";
    var rtaCart = "";
    var rtaCartAmounts = "";
    var rtaCartQuantities = "";
    var rtaCart = "";
    var rtaCartSku="";
    var rtaConvertCart = "";
    var rtaOrderNum = "";
    var rtaCartAmounts = "";
    var rtaCartQuantities = "";
    var rtaSearch = "";
    var rtaTags="PageType=" + ig_pageType;
    var rtaSpecial="keepit";

    if ("True" == "True")
    {
       rtaTags += vSeparator  + "SiteClass=fanatics";  
    }
    else
    {
       rtaTags += vSeparator + "SiteClass=partner";         
    }

    if ("shop.usctrojans.com".indexOf("fanatics.com") > -1)
    {
      rtaTags += vSeparator  + "Psid=10005";
    }
    else
    {
      rtaTags += vSeparator  + "Psid=6269";
    }

    if (ig_TeamName.length > 0)
    {
       rtaTags += vSeparator  + "TeamName=" + ig_TeamName;
    }

    if (ig_DepartmentName.length > 0)
    {
       rtaTags += vSeparator  + "DepartmentName=" + ig_DepartmentName;
    }

    if (ig_SubDeptName.length > 0)
    {
       rtaTags += vSeparator  + "SubdepartmentName=" + ig_SubDeptName;
    }            
    rtaEmail = "";
    if (rtaEmail.length == 0)
    {
        var url = window.location.href;

        regex = /.*e=(\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,3}).*$/; 

        var emFromUrl = url.match(regex); 
        if (emFromUrl && emFromUrl.length > 1)
        {
         rtaEmail = emFromUrl[1];   
        }

    }        
    switch (ig_pageType) {
     case "Home":
         // nothing special to do, rtaRetialre is set up and that is all that is required
         break;
     case "PDP":
         //execute code block 1
            rtaProductSKU = "";
         break;
     case "TLP":          
           rtaCategory = "College";
         break; 
     case "DLP":
           rtaCategory = ig_DepartmentName;
         break;
     case "SDLP": 
           rtaCategory = ig_SubDeptName;
         break;

     case "Hidden":
         rtaCategory = "";
           break;

     case "Browse":
           rtaCategory = "USC_Trojans_Sweatshirts_And_Fleece";
         break;

     case "League":
           rtaCategory = "College";
         break;

     case "MyAccount":
           rtaCategory = ig_pageType;      
  
           var items = jQuery("td.itemnumber").get();

           var skus = "";
           jQuery(items).each(
              function( index ) {
                skus += jQuery.trim(jQuery(this).text()) +  "|";  
              }
           );

          if (skus.length > 0)
          {
            var ss = document.createElement("img");
            ss.width = 1;
            ss.height = 1;
            ss.style.visibility = "hidden";
            ss.src =  mp_protocol + rtaRetailer + ".collect.igodigital.com/collect/ownit/"+ rtaRetailer +"?skus=" + skus.substring(0, skus.length - 1);
            document.getElementsByTagName("body")[0].appendChild(ss);
          }

         break;

     case "Cart":
     case "CheckoutStart":
     case "CheckoutAddressInfo":
     case "CheckoutPaymentInfo":
        // iGoDigital RTA Initialization
        rtaCart= '';
        rtaCartAmounts= '';
        rtaCartQuantities= '';
        if (rtaCart.length > 0)
           {
              rtaCart = rtaCart.substring(0, rtaCart.length - 1);
              rtaCartAmounts = rtaCartAmounts.substring(0, rtaCartAmounts.length - 1);
              rtaCartQuantities = rtaCartQuantities.substring(0, rtaCartQuantities.length - 1);
           }

         break;

     case "CheckoutConfirmation":
         rtaCart= '';
         rtaCartAmounts= '';
         rtaCartQuantities= '';
         rtaConvertCart = "1";
         rtaOrderNum = "";

         if (rtaCart.length > 0)
           {
              rtaCart = rtaCart.substring(0, rtaCart.length - 1);
              rtaCartAmounts = rtaCartAmounts.substring(0, rtaCartAmounts.length - 1);
              rtaCartQuantities = rtaCartQuantities.substring(0, rtaCartQuantities.length - 1);
           }
