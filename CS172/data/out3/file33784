    <h2 class="long">Employment Opportunities</h2> 
    <!-- Start Sidebar --> 
    <div id="sidebar"> 
     <div class="nav"> 
      <h3 class="accessibility">Main Navigation</h3> 
      <ul> 
       <li><a href="/faculty/" title="Faculty Home">Faculty Home<br><span>Teach, learn, grow</span></a></li>
       <li class="currentpage"><h3><dfn>Current page is: </dfn>Employment Opportunities</h3>
        <ul>
         <li><a href="/faculty/applying.html" title="Application and Hiring Process">Application and Hiring Process</a></li>
