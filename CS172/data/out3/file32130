             <script>
	var ms_top_menu_touch = false;
	var ms_top_menu_onblur = false;
	var ms_top_menu_timer_is_on=0;
	function topMenu_MSPointerDown(e)
	  {
			this.focus(); 
			e.stopPropagation(); 
			return false;
	  }
	  function topMenu_MSPointerUp(e)
	  {
			ms_top_menu_touch = true; 
			ms_top_menu_onblur = true;
			e.stopPropagation(); 
			return false;
	  }	 
  var i = 0;
  var z_i = topmenu.length;
  var wid = 92;
    for (i = 0; i < topmenu.length; i++ ) 
    {
      if(topmenu[i].menuList)
      {
        //document.write('<td width="92" valign="middle" background="'+ topmenu[i].color +'" onMouseOut="outLeftMenu('+ topmenu[i].menuID +');" onMouseOver="overLeftMenu('+ topmenu[i].menuID +');"><center><a href="#" >' + topmenu[i].name + '</a></center>');
        if(i != 3)
             wid = 92;
        else
             wid = 88;
        document.write('<td style="width: '+wid+'px; z-index: '+ z_i-- +'; position: relative;" align="center" valign="middle" background="'+ topmenu[i].color +'" >');
		document.write('<div style="width: 100%; position: relative; vertical-align: middle;"><a id="TopMenu_'+topmenu[i].menuID+'" onMouseOut="if (!ms_top_menu_touch){ outLeftMenu('+ topmenu[i].menuID +');} else {ms_top_menu_touch = false; clearTimeout(ms_top_menu_timer_is_on);}" onMouseOver="overLeftMenu('+ topmenu[i].menuID +');" onblur="if(ms_top_menu_onblur){clearTimeout(ms_top_menu_timer_is_on); ms_top_menu_timer_is_on = setTimeout(function(){overLeftMenu(0); ms_top_menu_onblur = false;}, 200);}" onfocus="overLeftMenu('+ topmenu[i].menuID +');" style="-ms-touch-action: none; text-decoration:none; width: 100%; display: inline-block;" href="' + topmenu[i].linkm + '" ><span style="color: ' + topmenu[i].fontcolor + '">' + topmenu[i].name + '</span></a></div>');     
      
	  if (window.navigator.msPointerEnabled) {
		  document.getElementById('TopMenu_'+topmenu[i].menuID).addEventListener("MSPointerDown",topMenu_MSPointerDown,false);
		  document.getElementById('TopMenu_'+topmenu[i].menuID).addEventListener("MSPointerUp",topMenu_MSPointerUp,false);
	  }
	  
	  if(topmenu[i].menuList.length>0)
        {
          CreateMenu(topmenu[i].menuList, topmenu[i].menuID, 'eng');
        }
        document.write('</td>');
      }
      else
      {
        document.write('<td style="width: 92px; position: relative;" align="center" valign="middle" background="'+ topmenu[i].color +'" ><a href="#" >' + topmenu[i].name + '</a></td>');
      }
    }
  </script> 
