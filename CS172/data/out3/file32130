               <script type="text/javascript">           
           function prev_maps(_animateTime)
           {
            var previourPhotoNum = 0;
            if(currentSwtNum == 0)
            {
              previourPhotoNum = $('#map_content .map').length - 1;
            }
            else
            {
              previourPhotoNum = currentSwtNum -  1;
            }
            
            if(nexT_id != -1)
            {
              previourPhotoNum = nexT_id;
              nexT_id = -1;
            }
            
            $("#map_content .map").stop(true, true);
            $("#map_content .map:eq(" + previourPhotoNum +")").css('z-index', 11);
            $("#map_content .map:eq(" + previourPhotoNum +")").css('left', -map_height);
            
            $("#map_content .map:eq(" + currentSwtNum +")").animate({left:''+ map_height +'px'}, _animateTime, function(){ $(this).css('z-index', 10);});
            $("#map_content .map:eq(" + previourPhotoNum +")").animate({left:'0px'}, _animateTime);
            
            currentSwtNum = previourPhotoNum;
          }
          
          function next_maps(_animateTime)
          {
            var nextPhotoNum = 0;
            if(currentSwtNum == ($('#map_content .map').length - 1))
            {
              nextPhotoNum = 0;
            }
            else
            {
              nextPhotoNum = currentSwtNum + 1;
            }
            
            if(nexT_id != -1)
            {
              nextPhotoNum = nexT_id;
              nexT_id = -1;
            }
            
            $("#map_content .map").stop(true, true);
            $("#map_content .map:eq(" + nextPhotoNum +")").css('z-index', 11);
            $("#map_content .map:eq(" + nextPhotoNum +")").css('left', map_height);
            $("#map_content .map:eq(" + currentSwtNum +")").animate({left:'-'+map_height+'px'}, _animateTime, function(){ $(this).css('z-index', 10);});
            $("#map_content .map:eq(" + nextPhotoNum +")").animate({left:'0px'}, _animateTime);
            
            currentSwtNum = nextPhotoNum;
            update_map_current_tip(currentSwtNum);							
          }
          
          function update_map_current_tip(currentSwtNum)
          {
            $('#map_current_tip').html(currentSwtNum + 1);
          }
          
          var currentSwtNum = 0;						
          var map_autoplay = true;
          var set_timer;
          var map_height = 300;
          var nexT_id = -1;
          $(document).ready(function() {                
              
              $('#map_controls .slideLeft').hide();
              $('#map_controls .slideRight').hide();             
              $('#map_content .map:first').css('left', 0);
              $("#map_content").mouseover(
                function()
                {                    
                  $('#map_controls .slideLeft').show();
                  $('#map_controls .slideRight').show();
                }
              )
              $("#map_content").mouseleave(
                function()
                {                    
                  $('#map_controls .slideLeft').hide();
                  $('#map_controls .slideRight').hide();                    
                }
              )
              $("#map_controls ul#slide-list li a").click(
                function()
                {
                  var index = $("#map_controls ul#slide-list li a").index($(this));
                  nexT_id = index;
                  $('#satellite_gallery a:eq(' + index + ')').addClass('show').css({opacity: 1});					
                  
                  $("#map_controls ul#slide-list li").removeClass('current-slide0');
                  $("#map_controls ul#slide-list li").removeClass('current-slide1');
                  $("#map_controls ul#slide-list li").removeClass('current-slide2');
                  $("#map_controls ul#slide-list li").removeClass('current-slide3');
                  $("#map_controls ul#slide-list li").removeClass('current-slide4');
                  $("#map_controls ul#slide-list li").removeClass('current-slide5');
                  $("#map_controls ul#slide-list li").removeClass('current-slide6');
                  $("#map_controls ul#slide-list li:eq(" + index + ")").addClass('current-slide'+index);					
                  if(index >　currentSwtNum)
                  {
                    next_maps(0);
                  }
                  if(index <currentSwtNum)
                  {
                    prev_maps(0);
                  }
                  
                }
              );
              
              if($('#map_content .map').length > 1)
              {
                $('#map_count_total').html($('#map_content .map').length);
                $("#map_content .map:eq(" + currentSwtNum +")").css('z-index', 30);
                update_map_current_tip(currentSwtNum);
                $('#map_controls .slideLeft').click(						
                  function ()
                  {
                    prev_maps(500);
                    $("#map_controls ul#slide-list li").removeClass('current-slide0');
                    $("#map_controls ul#slide-list li").removeClass('current-slide1');
                    $("#map_controls ul#slide-list li").removeClass('current-slide2');
                    $("#map_controls ul#slide-list li").removeClass('current-slide3');
                    $("#map_controls ul#slide-list li").removeClass('current-slide4');
                    $("#map_controls ul#slide-list li").removeClass('current-slide5');
                    $("#map_controls ul#slide-list li").removeClass('current-slide6');
                    $("#map_controls ul#slide-list li:eq(" + currentSwtNum + ")").addClass('current-slide'+currentSwtNum);
                  }
                );								
                $('#map_controls .slideRight').click(			
                  function ()
                  {
                    next_maps(500);
                    $("#map_controls ul#slide-list li").removeClass('current-slide0');
                    $("#map_controls ul#slide-list li").removeClass('current-slide1');
                    $("#map_controls ul#slide-list li").removeClass('current-slide2');
                    $("#map_controls ul#slide-list li").removeClass('current-slide3');
                    $("#map_controls ul#slide-list li").removeClass('current-slide4');
                    $("#map_controls ul#slide-list li").removeClass('current-slide5');
                    $("#map_controls ul#slide-list li").removeClass('current-slide6');
                    $("#map_controls ul#slide-list li:eq(" + currentSwtNum + ")").addClass('current-slide'+currentSwtNum);
                  }
                );
              }
              else
              {
                $('#map_controls').hide();
              }
              
              $('#map_content  .previous-link').click(						
                  function ()
                  {
                    prev_maps(500);
                  }
                );
              if (regionalmapID==4 || regionalmapID==6)
              {
                $("#map_controls ul#slide-list li a:eq(" + regionalmapID + ")").click();
              }              
           });				
        </script> 
