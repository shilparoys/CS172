$(document).ready(function() {
	  // Handler for .ready() called.
	$.ajax({
		url: '/PDADATA/locspc/data/hkweather_playlist_json_e.xml',
		dataType: 'json',
		cache: false,
		error: function(){
		},
		success: function(data){
			$.each(data.Playlist, function (i, obj) {
				if (obj.href.indexOf("PLBdhEGSPvUGWT_7z9qSdJXHxCLBHOom7p") > -1) 
				{
					var cb_upload_time = obj.uploaded;
					var uploaded_date = new Date(cb_upload_time);
					var now = new Date();
					var diff = Math.abs(now - uploaded_date);					
					if(diff < 86400000) //With 24h, 24h to milliseconds is 86400000
					{
						$('#warning_def').hide();
						$('#cb_link_play').attr('href', obj.entry_1_href + '&list=PLBdhEGSPvUGWT_7z9qSdJXHxCLBHOom7p');
						$('#cb_link_play').show();
						$('.topmenutext>tbody>tr>td:eq(5)').css('background-image', 'url(content_elements/bg_pink.gif)');
						$('.topmenutext>tbody>tr>td:eq(5) span').css('color', 'white');
						$('#topic_06 td:first').css('background-color', '#FF5A5A');
						$('#topic_06 td:first').css('border-color', '#FF5A5A');
						$('#topic_06 td:first a').css('border-color', '#FF5A5A');
						$('#topic_06 td:first a').css('color', 'white');
					}
				}
			});
		}
	});
	});
