   newWin.focus()
}

function airpollution()
{
  msgWindow=window.open("http://www.epd-asg.gov.hk/cindex.php","displayWindow3","width=800,height=700, toolbar=1, scrollbars=1, location=1, statusbar=1, menubar=1, resizable=1");
}


function blinkIt() {
 if (!document.all) return;
 else {
   for(i=0;i<document.all.tags('blink').length;i++){
      s=document.all.tags('blink')[i];
      s.style.visibility=(s.style.visibility=='visible')?'hidden':'visible';
   }
 }
}

function TopMenuItem(name, linkm, menuList, menuID, highlighted) {
	this.name = name;
	this.linkm = linkm;
	this.menuList = menuList;
	this.menuID = menuID;
	if (highlighted == 'true') 
  {
		this.color = 'content_elements/bg_pink.gif';
    this.fontcolor = '#FFFFFF';
	}
  else
  {
		this.color = 'content_elements/bg_blue.gif';
    this.fontcolor = '#0000FF';
	}
}

function TopMenuItem(name, linkm, menuList, menuID, highlighted, bgcolor) {
	this.name = name;
	this.linkm = linkm;
	this.menuList = menuList;
	this.menuID = menuID;
	if (highlighted == 'true') 
  {
	  if(bgcolor == "yellow")
    { 
      this.color = 'content_elements/bg_yellow.gif';
      this.fontcolor = '#0000FF';
    }
    else if (bgcolor == "green")
    {
      this.color = 'content_elements/bg_green.gif';
      this.fontcolor = '#0000FF';
    }
    else
    {
		  this.color = 'content_elements/bg_pink.gif';
      this.fontcolor = '#FFFFFF';
	  }
	}
  else
  {
		this.color = 'content_elements/bg_blue.gif';
    this.fontcolor = '#0000FF';
	}
}

function MenuListItem(name, linkb, selected) {
	this.name = name;
	this.linkb = linkb;
	this.selected = selected;
}

function TideHighLow(HL, Time, Height){
	this.HL = HL;
	this.Time = Time;
	this.Height = Height;
}

function CreateMenu(menuName, menuID, lang){
	var menuName = menuName;
	var i = 0;
	var Mwidth = 200;
	var menuID = menuID;
	var FmenuID = "topic_" + menuID;
	var menuColor='';
	if (lang == 'eng'){
		Mwidth = 300;
	}
	
	document.write('<div id=' + FmenuID + ' style="position:absolute; z-index:101;width:' + Mwidth + 'px; visibility:hidden;" onMouseOut="outLeftMenu(' + menuID +');" onMouseOver="overLeftMenu(' + menuID + ');"><table width="' + Mwidth + '" class="otherwxText" border="1" bgcolor="#FFFF70" cellspacing="0" cellpadding="0">');
	for (i = 0; i < menuName.length; i++) {	  
		if (menuName[i].selected == 'true') {
		  if (menuName[i].color != "")
      {
        menuColor = "menuName[i].color";
      }
      else
      {
        menuColor = '#FE5E5E';
      }			
		}
    else
    {
			menuColor = '#99CCFF';
		}
		document.write('<tr><td class="tableborder" bgcolor="' + menuColor + '"><a href="' + menuName[i].linkb + '" style="display: block; border: 3px solid ' + menuColor + '">' + menuName[i].name + '</a></td></tr>');
	}
	document.write('</table></div>');
}

function outLeftMenu(i_menu)
{
    show_hide_DIV(i_menu,false);
}

function overLeftMenu(i_menu)
{
	show_hide_DIV(02,false);
	show_hide_DIV(03,false);
	show_hide_DIV(04,false);
	show_hide_DIV(05,false);
	show_hide_DIV(06,false);
    show_hide_DIV(i_menu,true);
}

function show_hide_DIV(i_layer, b_show) {
  var s_show, obj=MM_find_Obj("topic_" + ((i_layer < 10) ? "0" + i_layer : i_layer));
  var vers=navigator.appVersion; vars=vers.substring(0,1);
  if (obj) {
  	if (obj.style) { obj=obj.style; s_show = b_show ? "visible" : "hidden";}
	else { if (vers>4) {s_show = b_show ? "show" : "hide";}else{s_show="hide";}}
    obj.visibility = s_show;
  }
}

function MM_find_Obj(n, d) { //v4.0
  var p,i,x;  if(!d) d=document; if((p=n.indexOf("?"))>0&&parent.frames.length) {
    d=parent.frames[n.substring(p+1)].document; n=n.substring(0,p);}
  if(!(x=d[n])&&d.all) x=d.all[n]; for (i=0;!x&&i<d.forms.length;i++) x=d.forms[i][n];
  for(i=0;!x&&d.layers&&i<d.layers.length;i++) x=MM_findObj(n,d.layers[i].document);
  if(!x && document.getElementById) x=document.getElementById(n); return x;
}

function MM_findObj(n, d) { //v3.0
  	var p,i,x;  
	if(!d) d=document; 
	if((p=n.indexOf("?"))>0&&parent.frames.length) {
    	d=parent.frames[n.substring(p+1)].document; 
		n=n.substring(0,p);
	}
  	if(!(x=d[n])&&d.all) x=d.all[n]; 
	for (i=0;!x&&i<d.forms.length;i++) 
		x=d.forms[i][n];
  		for(i=0;!x&&d.layers&&i<d.layers.length;i++) 
			x=MM_findObj(n,d.layers[i].document); 
			return x;
}

function genCurrWx_eng(CurrWxData) {

	var Time=CurrWxData[0];
	var Temp=CurrWxData[1];
	var RH=CurrWxData[2];
	var UV=CurrWxData[3];
	var Intensity=CurrWxData[4];
	var SeaTemp=CurrWxData[5];

	document.write('<table width="262" border="0" cellspacing="0" cellpadding="3">');
	document.write('<tr><td bgcolor="#0000FF" class="white_text"><div class="CwxText"><a href="/wxinfo/currwx/current.htm">Current Weather at ' + Time + '</a></div></td></tr>');
	document.write('<tr><td class="tableborder"><div class="BlackText">HKO : Temp <font color="#0000FF">' + Temp + '</font> &deg;C &nbsp;RH <font color="#0000FF">' + RH + '</font> %<br>');

	if(UV != '//'){
		document.write('King\'s Park : UV Index <font color="#0000FF">' + UV + '</font>&nbsp;(<font color="#0000FF">' + Intensity + '</font>)');
	}else{
		document.write('&nbsp;');
	}
	document.write('</div></td></tr></table>');
	document.write('<table width="262" border="0" cellspacing="0" cellpadding="3" style="border-top-style: solid; border-top-width: 3px; border-bottom-style: solid; border-bottom-width: 3px; border-color: #FFFFFF">');
	document.write('<tr><td bgcolor="#0000FF" class="white_text">North Point Sea Surface Temp : ' + SeaTemp + ' &deg;C</td></tr>');
	document.write('</table>');

}

function genAstronTable_eng(AstronData) {
	document.write('<table width="85" height="150" border="0" cellpadding="0" cellspacing="0" background="content_elements/bg_astron.gif">');
	document.write('<tr valign="bottom"><td height="18" width="50" style="padding-left: 4px; font-size:11px;" class="black_text"><a href="/gts/astron2009/sunrise_main_e.htm">Astro</a></td><td class="blue_text" style="font-size:11px;"><a href="javascript:outLeftMenu(100)">Tide</a></td></tr>');
	document.write('<tr><td class="black_text" style="padding-left: 2px;">Sun  <img src="/img/arrow_up.gif" alt=""></td><td align="center" class="blue_text">' + AstronData[0] + '</td></tr>');
	document.write('<tr><td class="black_text" style="padding-left: 2px;">Sun  <img src="/img/arrow_down.gif" alt=""></td><td align="center" class="blue_text">' + AstronData[1] + '</td></tr>');
	document.write('<tr><td class="black_text" style="padding-left: 2px;">Moon<img src="/img/arrow_up.gif" alt=""></td><td align="center" class="blue_text">' + AstronData[2] + '</td></tr>');
	document.write('<tr><td class="black_text" style="padding-left: 2px;">Moon<img src="/img/arrow_down.gif" alt=""></td><td align="center" class="blue_text">' + AstronData[3] + '</td></tr>');
	document.write('</table>');
}

function genTideTable_eng(TideData) {
	document.write('<table width="85" height="150" border="0" cellpadding="0" cellspacing="0" background="content_elements/bg_tide.gif">');
	document.write('<tr valign="bottom"><td height="18" width="50%" align="center" class="blue_text" style="font-size:11px;"><a href="javascript:overLeftMenu(100)">Astro</a></td><td class="black_text" style="padding-left: 12px; font-size:11px;"><a href="/tide/etide_main.htm">Tide</a></td></tr>');

	var i=0;
	for(i=0;i<4;i++){
		if (TideData[i].Time != ':')
			document.write('<tr><td align="center" class="black_text">' + TideData[i].HL + '</td><td align="center" class="blue_text">' + TideData[i].Time + '<br>' + TideData[i].Height + '</td></tr>');
		else
			document.write('<tr><td align="center" class="black_text">' + TideData[i].HL + '</td><td align="center" class="blue_text">' + '-' + '</td></tr>');
	}
	document.write('</table>');
}

