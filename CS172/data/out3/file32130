          <!-- Content 8 --> <script language="JavaScript">
function Wxword(wxterm){
        var tcpage="/informtc/sound/tcname2013e.htm"
        
        if (wxterm!="tc"){
                newWin=window.open("wxinfo/currwx/flw_description/flw_e.htm#"+wxterm,"FLW","Width=760,height=50,Screenx=30,Screeny=180,resizable=yes,scrollbars=yes,status=yes,toolbar=1,location=0,directories=0")
                newWin.focus()
        }
        else{
                newWin=window.open(tcpage,"TC","Width=760,height=3000,Screenx=30,Screeny=180,resizable=yes,scrollbars=yes,status=yes,toolbar=1,location=0,directories=0")
                newWin.focus()
        }
}
</script> 
