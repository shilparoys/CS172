      <script type="text/javascript">
	function updatedate() {
	        var statetofind = document.hshunt.hsstate.value;	       
	        var typetofind;
	        if (document.hshunt.hs_college[0].checked){
	        	typetofind="Highschool";
	        	}
	        if (document.hshunt.hs_college[1].checked){
	        	typetofind="College";
	        	}
	        if (document.hshunt.hs_college[2].checked){
	        	typetofind="Homeschool";
	        	}

		var loadfile = "findhighschoolorcollege.php?state="+statetofind+"&type="+typetofind;

		loadFragmentInToElement('https://secure.westmont.edu/javascript/findhighschool/'+loadfile, 'foundhss');
	}

</script> 
      <script>
function autotab(current,to){
    if (current.getAttribute && 
      current.value.length==current.getAttribute("maxlength")) {
        to.focus() 
        }
}
</script> 
      <script type="text/javascript"> 

function stopRKey(evt) { 
  var evt = (evt) ? evt : ((event) ? event : null); 
  var node = (evt.target) ? evt.target : ((evt.srcElement) ? evt.srcElement : null); 
  if ((evt.keyCode == 13) && (node.type=="text"))  {return false;} 
} 

document.onkeypress = stopRKey; 

</script> 
      <script language="JavaScript">
function testResults (form) {
    TestVar = (form.birthyear.value)
    if (TestVar < 1988)
        alert ("You have entered a birth year which could indicate you are seeking graduate level programs.  Westmont offers strictly undergrauate degrees. Do you wish to continue?");
}
</script> 
      <script type="text/javascript">

function makeSubmitEnabled(){
	document.getElementById('submit_form').disabled='';
}

</script> 
