    $(function(){
        $('#photo-banner .rotate').cycle({
            speed: 1500,
            fx: 'fade',
            timeout: 10000
        });
        $('#photo-banner .rotate-prev').click(function () {
            $('#photo-banner .rotate').cycle('prev');
        });
        $('#photo-banner .rotate-next').click(function () {
            $('#photo-banner .rotate').cycle('next');
        });
        $('.footer-ads').cycle({
            fx:'none',
            timeout: 10000
        });

        var places = $('ul#featuredplaces li');
        $(places[0]).css('display','block');
        $(places[1]).css('display','block');
        if ( $.cookie('union-featuredplaces') == null ){
            $.cookie('union-featuredplaces', '2');
        }
        else if ( $.cookie('union-featuredplaces') < places.length-1){
            nextTile = parseInt($.cookie('union-featuredplaces'))+1;
            $.cookie('union-featuredplaces', nextTile );
        }
        else $.cookie('union-featuredplaces', '2');
        $(places[$.cookie('union-featuredplaces')]).css('display','block');
    });
</script>   
