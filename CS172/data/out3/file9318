var MSIE=navigator.userAgent.indexOf("MSIE"); 
var NETS=navigator.userAgent.indexOf("Netscape"); 
var OPER=navigator.userAgent.indexOf("Opera"); 
 
function setAreaPreset( )
{
   var list=document.customForm.region_presets;
   var area=list.options[list.selectedIndex].value;
   //alert("Feature Not Yet Active:\nRegion lat / lons would be set to: "+area);
   switch (area) {
   case "default" : setArea(90,-90,180,-180);break;
   case "europe" :
     setArea(75,30,45,-20);break;
   case "northamerica" :
     setArea(80,20,-45,-165);break;
   case "arcticcircle" :
     setArea(90,70,180,-180);break;
   case "asia" :
     setArea(55,5,150,45);break;
   case "siberia" : // N.Asia
     setArea(80,45,180,40);break;
   case "indonesia" :
     setArea(25,-15,180,90);break;
   case "australianz" :
     setArea(-5,-55,-170,105);break;
   case "africa" :
     setArea(40,-40,60,-30);break;
   case "indianocean" :
     setArea(25,-60,135,-20);break;
   case "mideast" :
     setArea(45,10,75,20);break;
   case "carribean" :
     setArea(35,10,-60,-90);break;
   case "centralamerica" :
     setArea(30,0,-45,-105);break;
   case "southamerica" :
     setArea(15,-60,-30,-90);break;
   case "pacificocean" :
     setArea(60,-70,-80,130);break;
   case "northpacific" :
     setArea(60,0,-90,130);break;
   case "southpacific" :
     setArea(0,-70,-75,135);break;
   case "atlanticocean" :
     setArea(60,-70,10,-70);break;
   case "northatlantic" :
     setArea(60,0,0,-80);break;
   case "southatlantic" :
     setArea(0,-70,20,-65);break;
   case "arcticcircle" :
     setArea(0,65,180,-180);break;
   case "antarctic" :
     setArea(-65,-90,180,-180);break;
   }
   return;
}
function setArea(n,s,e,w) {
  document.customForm.north.value=n;
  document.customForm.south.value=s;
  document.customForm.east.value=e;
  document.customForm.west.value=w;
  }
function setChecked( checkBox )
{
   checkBox.checked=true;
}

function checkBoxes( f, which ) 
{
   if(which == "all") {    
       f.IDcheck.checked = true;
 	   f.dsFullCheck.checked = true;
   	   f.dsShortCheck.checked = true;
	   f.formatCheck.checked = true;
 	   f.typeCheck.checked = true;
 	   f.keywordsCheck.checked = true;
 	   f.pisCheck.checked = true;
 	   f.notesCheck.checked = true;
 	   f.urlsCheck.checked = true;
 	   f.noFilesCheck.checked = true;
	   f.appTotSizeCheck.checked = true;
	   f.excRestrDSCheck.checked = true;
	   f.yearCheck.checked = true;
   }
   else {
       f.IDcheck.checked = true; 
 	   f.dsFullCheck.checked = false;
   	   f.dsShortCheck.checked = false;
	   f.formatCheck.checked = false;
 	   f.typeCheck.checked = false;
 	   f.keywordsCheck.checked = false;
 	   f.pisCheck.checked = false;
 	   f.notesCheck.checked = false;
 	   f.urlsCheck.checked = false;
 	   f.noFilesCheck.checked = false;
	   f.excRestrDSCheck.checked = false;
	   f.appTotSizeCheck.checked = false;
	   f.yearCheck.checked = false;
   }
}

function validateAndSubmit( f )
{
    //alert("the value of mode is: "+theForm.mode.value);
	var chkIDnum = f.IDcheck.checked;
	var IDnum = f.idnum.value;
	var chkDSfull = f.dsFullCheck.checked;
	var fullname = f.fullname.value;
	var chkDSshort = f.dsShortCheck.checked;
	var chkFormat = f.formatCheck.checked;
	var format = f.format.value;
	var chkYear = f.yearCheck.checked;
        //alert("checks: "+chkYear +chkIDnum+chkDSshort+chkFormat);
	var startYr = f.startYr.value;
	var endYr = f.endYr.value;
	var chkType = f.typeCheck.checked;
	var type = f.type.value;
	var chkKeys = f.keywordsCheck.checked;
	var keyWds = f.keywords.value;
	var chkPIs = f.pisCheck.checked;
	var PINames = f.PInames.value;
	var chkNotes = f.notesCheck.checked;
	var notes = f.notes.value;
	var chkURLs = f.urlsCheck.checked;
	var URLs = f.urls.value;
	var chkNoFiles = f.noFilesCheck.checked;
	var noFiles = f.noFiles.value;
	var chkAppTotSize = f.appTotSizeCheck.value;
	var excRestrDSCheck = f.excRestrDSCheck.checked;
	var excRestr = f.excRestr.value; 
	var east = parseInt(f.east.value);
	var west = parseInt(f.west.value);
	var north = parseInt(f.north.value);
	var south = parseInt(f.south.value);
	//alert(chkIDnum+" "+chkDSfull+" "+chkDSshort+" "+chkFormat+" "+chkYear+" "+chkType);
	if(chkIDnum   ==false && chkDSfull ==false && 
	   chkDSshort ==false && chkFormat ==false && 
	   chkYear    ==false && chkType   ==false && 
	   chkKeys    ==false && chkPIs    ==false && 
	   chkNotes   ==false && chkURLs   ==false &&
	   chkNoFiles ==false && chkAppTotSize==false ) {
	     alert("If you don't check one or more checkboxes\nyou are not including any fields in your report!");
		 return(false);
		 }
		 	 
/*
	//alert('chkIDnum is: '+chkIDnum.checked);
	if( IDnum != "" && chkIDnum == ""  )
	{
			alert("report no. field is non-null. Do you mean to check its box?" );
		        setChecked( f.IDcheck );
			return(false);
	}
*/
 
	if ( chkYear != "" ) {
	  val1 = parseInt( startYr );
	  val2= parseInt( endYr );
	  if( isNaN( val1 ) == true || isNaN( val2 ) == true ) {
	     alert("One of the date fields ("+val1+"-"+val2+") is not a valid 4-digit year.");
	     return(false);
	  }
	  if( (val2 < val1) || (val1 < 1900) || (val1 > 2599) || (val2 < 1900) || (val2 > 2599) ) {
	     alert("Your dates are invalid. Too high, too low, in wrong order.");
	     return(false);
	  }
	}
	if( isNaN( west ) == true || isNaN( east ) == true || 
	   isNaN( north ) == true || isNaN( south ) == true ) {
	   alert("One of the lat / lon fields is not a valid number.");
	   return(false);
	}

	if( (north > 90) || (south < -90) || (south >= north) ) {
		 alert("Your north/south lat/lons are invalid. Please check and try again.");
			 return(false);
	}
	f.east.value = east;
	f.west.value = west;
	f.north.value = north;
	f.south.value = south;

	//alert("west "+west+"east "+east +"north "+north+" south"+south);

	f.submit();
}

function MM_preloadImages() { //v3.0
  var d=document; 
    if(d.images){ 
       if(!d.MM_p) d.MM_p=new Array();
       var i,j=d.MM_p.length,a=MM_preloadImages.arguments; for(i=0; i<a.length; i++)
       if (a[i].indexOf("#")!=0){ 
          d.MM_p[j]=new Image; 
          d.MM_p[j++].src=a[i];
       }
    }
}


// IAGT 2007
//Called to launch coordinate getter in popup
function getCoordsFromMap() {
//Open map as popup
window.open('IAGT/CoordinateAcquisitionTool/coordinateGetter.html','name','height=600,width=532,toolbar=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no');
//return false;
}

// IAGT 2007
//This is called from the popup
function enterCoords(north,south,east,west) {
        document.getElementById("north").value = north;
        document.getElementById("south").value = south;
        document.getElementById("east").value = east;
        document.getElementById("west").value = west;
}

