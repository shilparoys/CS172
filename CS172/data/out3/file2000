  <script type="text/javascript">  
	  
   function checkblank(cString,cLabel,cObj) {  
			//alert("check Blank") ;
			var cTemp="",cGood="",i=0 ;  
			cString=cString+" " ; 
	  
	  	for (i = 0; i < cString.length; i++) {  
	  		cTemp=cString.substring(i,i+1) ;  
	  
	  		if (cTemp != " ") {  
	  			cGood=cGood+cTemp  ;  
	  		}	  
	  
	  		if (cGood == "") {  
	  		
	    			alert("You must enter a " + cLabel + "!")  ;  
	    			eval("document.LOGUSERIN."+cObj+".value=''") ;  
	     			eval("document.LOGUSERIN."+cObj+".focus()") ;
	     			return false ;  
	    		}  
	    	}		  
	    	return true ;  
	  }  
	    
	  function checkblanks() {  
	    //alert("try login") ;
	    if (checkblank(document.LOGUSERIN.username.value,"User Name","username")) {  
	  
	       if (checkblank(document.LOGUSERIN.pword.value,"Password","pword")) {   
	  
	  	     	return true  ;  
	  		}	  
	  	 }	  
	  
	  	 return false ;	  
	  }	  
	    
   </script>
