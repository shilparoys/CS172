   function showobject(htmlobject){		
			htmlobject.style.display = "block";
   }
   function hideobject(htmlobject){		
			htmlobject.style.display = "none";
   }
   function getkeypressnum(e) {
			var keynum;
			//alert("in getkeypress");
			//return 13;
			if(window.event) // IE
				{
				keynum = e.keyCode;
				}
			else if(e.which) // Netscape/Firefox/Opera
				{
				keynum = e.which;
				}
			return keynum;
		}
		</script>
