        <h1 id="site-title"> <span style="float:right;"><script language="javascript" type="text/javascript">
<!-- //hide from older browser
	function validatesearchQT(){
		re = new RegExp("^\\w{2,}");
		var QTvalue = trim(document.SearchForm.QT.value);
		//alert(QTvalue);
		if (!re.test(QTvalue)){
			alert("Please provide at least 2 characters to perform a search");
			document.SearchForm.QT.focus();
			return false;
		} 
		return true;
	}
	function trim(str) {
			var re = /^\s+(.*?)\s+$/;
			return str.replace(re,"$1");
		} 			
	//end of hide -->
</script> 
