  <script type="text/javascript" src="/cgi-bin/scripts/jqColor.js"></script> 
  <script type="text/javascript" src="/cgi-bin/scripts/ui1.2.js"></script> 
  <script type="text/javascript" src="/cgi-bin/scripts/aud1.2.js"></script> 
  <script type="text/javascript">
/*<![CDATA[*/
var h = 240; var w = 0; var th = 10;  var sh = 10; var rotateTimer, rotateDelay

$(function(){
	$('#feature_wrapper').hide();
	var windowWidth, photoCnt, currentPhoto;
	photoCnt = 7;
	currentPhoto = 3;
	var isSmall = false; 

	$(window).resize(function(){
		checkWindowSizeLocal(cutpoint);
	});
	
	function checkWindowSizeLocal(c){
		var w = window.innerWidth	
		if ($.browser.webkit) {w = document.documentElement.clientWidth;}
		if (isSmall == false && w <= c) {
			goSmallLocal();
			isSmall = true;
		} else if (isSmall == true && w > c) {
			goLargeLocal();
			isSmall = false;
		}
	}
	
	function goSmallLocal() {
		//Change ID of features to detach script and allow for custom styling
		$('#features').attr('id','mFeatures').removeAttr('style');
		$('.slide').removeAttr('style');
		$('.story').css({'padding-bottom':'0'});
		$('#content').append('<div id="promotions"><p class="centered"><\/div>');
		$('#nav > li:gt(0) a').appendTo($('#promotions p:first'));
		$('#mNav').clone().insertBefore('#feature_wrapper').attr('id','mMainNav').attr('class','cells');

		$("#mMainNav a.hide, #mMainNav a.show").on('click',function(){
			$(this).siblings("ul").slideToggle(500);
			$(this).toggleClass("hide").toggleClass("show");
			if ($(this).attr('class') == 'hide') 
				$(this).parent('li').css('backgroundColor','#e7ebee');
			else
				$(this).parent('li').css('backgroundColor','#C3D3DF');
		}).siblings().hide();
	}

	function goLargeLocal() {
		$("#qSearchBtn").attr("src","\/cgi-bin\/images\/search_button.jpg");
		$('#nav > li:eq(1)').append($('#promotions a:first'));
		$('#nav > li:last').append($('#promotions a:last'));
		$('#global_clone, #menu_trigger, #srch_trigger, #promotions, #mMainNav').remove();
		$('#mFeatures').attr('id','features');
		initFeatures();
	}

	$("#alertbox").hide().delay(250).slideDown(750, "swing");
	$(window).scroll( function(){$("#nav").css({"top":"0px", "position":"static"}); $(".pagetop").remove();}).resize(function() {initFeatures();}); 
	$.get("/homeimages/home.xml",function(data){ rotate(3, data);});
	$("#features a.slide").css("height","auto");
	if ($("#features a").length>5){
		$("#feature_wrapper").append('<div id="next_feature"><div id="next_bg">&nbsp;<\/div><\/div><div id="prev_feature"><div id="prev_bg" >&nbsp;<\/div><\/div>');  // add controls to the feature area
		$("#features .slide_wrapper:last").prependTo('#features');
		$('#features').css({"left":$('#features').position().left - $("#features a.slide:first").width()/2});
		$("#prev_feature").click(function(){
			if($("#features:animated").length==0){
				$("#features").prepend($("#features .slide_wrapper:last")).css({'left':($("#features a.slide:first").width()*-1.5)});
				$("#features").animate({'left':$("#features a.slide:first").width()/-(2)},300, function(){initFeatures();});
			}
		});
		$("#next_feature").click(function(){
			if($("#features:animated").length==0){
				$("#features").animate({'left':( $("#features a.slide:first").width()*-1.5)},300, function(){
					$("#features").append($("#features .slide_wrapper:first")).css({'left':($("#features a.slide:first").width()/-2)}); initFeatures();
				});
			}
		});
	}

	//initFeatures();
	$(window).load(function() {initFeatures();});//runs again to ensure all images are loaded.
	rotateTimer = setInterval("autoRotate()",6000);
	$("#feature_wrapper").hover(
		function(event){ event.stopPropagation(); clearTimeout(rotateDelay); clearInterval(rotateTimer);},
		function(event){ event.stopPropagation(); clearTimeout(rotateDelay); clearInterval(rotateTimer); rotateTimer = setInterval("autoRotate()",6000);}
	);

	checkWindowSizeLocal(cutpoint);
});

function autoRotate() {	rotateDelay = setTimeout(function(){$("#next_feature").click();},2000);}

function initFeatures(){
	$('#feature_wrapper').show();
	th=0;sh=0;h=150;w=0
	$("#features a.slide").each(function(){th = ($(".title",this).height()>th)?$(".title",this).height():th; sh = ($(".story",this).height()>sh)?$(".story",this).height():sh;});
	$("#features a.slide").each(function(){$(".category",this).css({'padding-top':th-$(".title",this).height()}); });
	$("#features a.slide").each(function(){$(".story",this).css({'padding-bottom':sh-$(".story",this).height()}); h=($(this).outerHeight()>h)?$(this).outerHeight():h; w = $(this).width(); });
	if ($("#features a").length>5){ // arrange for sliding (auto/manual)
		$("#features a.slide").css({"width":(100/$('#features a').length)+"%"}); 
		$("#features").css({"width":($("#features a").length/5)*100+"%","height":(h+10)+"px"});
		image_center=($("#features a.slide:first img:first").height()/2+$("#features a.slide:first img:first").position().top)-10; image_center=image_center>50?image_center:50;
		$("#next_bg, #prev_bg").css({"background-position": "center "+image_center+"px","height":(h+20)+"px"});
	}
}
/*]]>*/
</script> 
