       <div class="grid-box width100 grid-v">
        <div class="module mod-box mod-box-grey  deepest"> 
         <h3 class="module-title">Calendar</h3> 
         <div id="b2j_k2_calendar_container_module_124" class="b2j_k2_calendar_container_module b2j_style-box color-grey"> 
          <input class="b2j_calendar_input" type="text" gldp-id="b2j_date"> 
          <div class="loading"></div> 
          <div class="b2j_calendar" gldp-el="b2j_date" style="width:100%; height:100%;"> 
          </div> 
         </div> 
         <script type="text/javascript">
    var months = JSON.parse('["ianuarie","februarie","martie","aprilie","mai","iunie","iulie","august","septembrie","octombrie","noiembrie","decembrie"]');
    // Add toMysqlFormat function to date
    Date.prototype.toMysqlFormatYearMonthDay = function() {
        return this.getFullYear() + "-" + twoDigits124(1 + this.getMonth()) + "-" + twoDigits124(this.getDate());
    };
    Date.prototype.toMysqlFormatYearMonth = function() {
        return this.getFullYear() + "-" + twoDigits124(1 + this.getMonth());
    };
    Date.prototype.toMysqlFormatYearMonthDayTitle = function() {
        return twoDigits124(this.getDate()) + " " + months[this.getMonth()] + " " + this.getFullYear();
    };
    // add leading 0 to month
    function twoDigits124(d) {
        if(0 <= d && d < 10) return "0" + d.toString();
        return d.toString();
    }
    
    //function font resizer, and some responsive work
    function resize_font124(width){
        var x_times_larger_font = ( width / 500 ) * 18;
        var x_times_larger_font_title = ( width / 500 ) * 14;
        var x_times_larger_font_introtext = ( width / 500 ) * 12;
        var x_times_larger_font_date = ( width / 500 ) * 14;
        if (x_times_larger_font < 10){
            x_times_larger_font = 10;
        }
        if (x_times_larger_font_title < 10){
            x_times_larger_font_title = 10;
        }
        if (x_times_larger_font_introtext < 10){
            x_times_larger_font_introtext = 10;
        }
        if (x_times_larger_font_date < 10){
            x_times_larger_font_date = 10;
        }
        jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .core').css('font-size', Math.floor(x_times_larger_font) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .item_list_view .introtext_view').css('font-size', Math.floor(x_times_larger_font_introtext) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .item_list_view .title_view a').css('font-size', Math.floor(x_times_larger_font_title) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .item_list_view .title_view').css('font-size', Math.floor(x_times_larger_font_title) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .item_list_view .date_view').css('font-size', Math.floor(x_times_larger_font_date) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .monyear span').css('font-size', Math.floor(x_times_larger_font) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_date').css('font-size', Math.floor(x_times_larger_font) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .counts').css('font-size', Math.floor(x_times_larger_font * 0.8) + "px");
        jQuery('#b2j_k2_calendar_container_module_124 .counts').css('line-height', Math.floor(x_times_larger_font * 0.6) + "px");
        if (jQuery('#b2j_k2_calendar_container_module_124').width() < 220){
            jQuery('#b2j_k2_calendar_container_module_124 .counts').hide();
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close').css('width', '12px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close').css('height', '12px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close:hover').css('width', '12px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close:hover').css('width', '12px');
        } else {
            jQuery('#b2j_k2_calendar_container_module_124 .counts').show();
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close').css('width', '18px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close').css('height', '18px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close:hover').css('width', '18px');
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .item_list_view_close:hover').css('width', '18px');
        }
    }
    //variable inits
    var blank_url = '/component/k2/itemlist/date/{year}/{month}/{day}?catid=0';
    var current_date_full = new Date();
        current_date_full.setDate(1);
    var current_date = current_date_full.toMysqlFormatYearMonth(); //2010-08
		
		    
    jQuery(document).ready(function() {
        
        // Render first time
        get_calendar124(current_date);
        // prev button
        jQuery(document).on('click','#b2j_k2_calendar_container_module_124 .b2j_prev', function(){
            current_date_full.setMonth(current_date_full.getMonth()-1);
            current_date = current_date_full.toMysqlFormatYearMonth();
            get_calendar124(current_date);
        });
        //next button
        jQuery(document).on('click', '#b2j_k2_calendar_container_module_124 .b2j_next', function(){
            current_date_full.setMonth(current_date_full.getMonth()+1);
            current_date = current_date_full.toMysqlFormatYearMonth();
            get_calendar124(current_date);
        });
        //month change
        jQuery(document).on('change', '#b2j_k2_calendar_container_module_124 .b2j_month_select', function(){
            var month = twoDigits124(jQuery(this).val());
            current_date_full.setMonth(month);
            current_date = current_date_full.toMysqlFormatYearMonth();
            get_calendar124(current_date);
        });
        //year change
        jQuery(document).on('change', '#b2j_k2_calendar_container_module_124 .b2j_year_select', function(){
            var year = jQuery(this).val();
            current_date_full.setYear(year);
            current_date = current_date_full.toMysqlFormatYearMonth();
            get_calendar124(current_date);
        });
        //Item view close button
        jQuery(document).on('click', '#b2j_k2_calendar_container_module_124 .item_list_view_close', function(){
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default > .item_list_view').remove();
            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default > div').show();
        });       
    });
    // for responsive;
    jQuery(window).resize(function() {
        if(typeof t_id !='undefined'){
            clearTimeout(t_id);
        }
        t_id=setTimeout(function(){
            get_calendar124(current_date);
        }, 500);
    });
    //get calendar main function
    function get_calendar124(current_date){
        jQuery("#b2j_k2_calendar_container_module_124 .loading").show();
        var now_time = Math.floor(new Date().getTime());
        jQuery.get(window.location, {
            now_time: now_time,
            auth: "469537c1e4eb0e21be6326317d424b0f",
            last_refresh: "2015-03-02 02:09:22",
            mid: 124,
            current_date: current_date,
            get_items: 0,
        },   
        //response
        function(data) {
            if (data != ""){
                if(data.indexOf("{b2j_mark1_" + 124 + "}") < 0){
                    alert('jQuery problem');
                    return;
                }
                var startIndex = data.indexOf("{b2j_mark1_" + 124 + "}");
                var endIndex = data.indexOf("{b2j_mark2_" + 124 + "}");
                var data = data.substr(startIndex,endIndex-startIndex);
                var data = data.replace("{b2j_mark1_" + 124 + "}","");
                var data = data.replace("{b2j_mark2_" + 124 + "}","");
                try {
                    var items = JSON.parse(data);
                } catch (err) {
                    alert("Error occured in B2J Calendar module. Make sure caching for module is turned off");
                }
                var item_dates = Array();
                for(var i = 0; i < items.length ; i++){
                    var item = items[i];
                    var item_arr = item.split("*");
                    var date_arr = item_arr[0].split("-");
                    var obj = new Object();
                    obj.date = new Date(date_arr[0], parseInt(date_arr[1])-1, date_arr[2]);
                    obj.counts = item_arr[1];
                    item_dates[i] = obj;
                }
                jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').empty();
                jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').addClass('b2j_calendar');
                jQuery('#b2j_k2_calendar_container_module_124 .b2j_calendar').removeClass('gldp-default');
                jQuery('#b2j_k2_calendar_container_module_124 .b2j_calendar').css('width', jQuery('#b2j_k2_calendar_container_module_124').width()+'px');
                jQuery('#b2j_k2_calendar_container_module_124 .b2j_calendar').css('height', Math.floor(jQuery('#b2j_k2_calendar_container_module_124').width()*8/7)+'px');
                jQuery('#b2j_k2_calendar_container_module_124').css('height', Math.floor(jQuery('#b2j_k2_calendar_container_module_124').width()*8/7)+'px');
                if(!jQuery.isFunction(jQuery.fn.b2j_datepicker)){
                   alert('There seems to be multiple jQuery libraries included or javascript errors exist, fix the issue, so B2J Calendar module can work properly!') ;
                }
                jQuery('#b2j_k2_calendar_container_module_124 .b2j_calendar_input').b2j_datepicker({
                                            dowOffset: 1,
                                            borderSize: 1,
                                            selectableDatesCounts: item_dates,
                                            firstDate: current_date + '-01',
                                            selectableDatesCountsShow: 1,
                                            future_days: 1,
                                            dowNames: ["Dum","Lun","Mar","Mie","Joi","Vin","Sam"], 
                                            monthNames: ["ianuarie","februarie","martie","aprilie","mai","iunie","iulie","august","septembrie","octombrie","noiembrie","decembrie"],
                                            
                                                
                                                selectableDates: item_dates, 
                                            
                                                onClick: (function(el, cell, date, data) {
                                                    if(1 && !cell.find('span').length){
                                                        return;
                                                    }
                                                    jQuery('#b2j_k2_calendar_container_module_124 .loading').show();
                                                    var now_time = Math.floor(new Date().getTime() / 1000);
                                                    jQuery.get(window.location, {			
                                                        now_time: now_time,
                                                        auth: '469537c1e4eb0e21be6326317d424b0f',
                                                        last_refresh: '2015-03-02 02:09:22',
                                                        mid: 124,
                                                        current_date: date.toMysqlFormatYearMonthDay(),
                                                        get_items: 1,
                                                    },   
                                                    //response
                                                    function(data) {
                                                        if (data != ''){
                                                            if(data.indexOf('{b2j_mark1_124}') < 0){
                                                                alert('jQuery problem');
                                                                return;
                                                            }
                                                            var startIndex = data.indexOf('{b2j_mark1_124}');
                                                            var endIndex = data.indexOf('{b2j_mark2_124}');
                                                            var data = data.substr(startIndex,endIndex-startIndex);
                                                            var data = data.replace('{b2j_mark1_124}','');
                                                            var data = data.replace('{b2j_mark2_124}','');
                                                            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default > div').hide();
                                                            var item_list_view = jQuery('<div/>').addClass('item_list_view');
                                                            var item_list_vew_header = jQuery('<div/>').addClass('item_list_view_header').height(jQuery('.monyear.title').height());
                                                            var item_list_vew_footer = jQuery('<div/>').addClass('item_list_view_footer');
                                                            item_list_vew_footer.attr('id', 'footer_124');
                                                            var item_list_vew_close = jQuery('<div/>').addClass('item_list_view_close');
                                                            var item_list_vew_date = jQuery('<div/>').addClass('item_list_view_date').html(date.toMysqlFormatYearMonthDayTitle());
                                                            item_list_vew_date.css('line-height', jQuery('.monyear.title').height() + 'px');
                                                            var ul = jQuery('<ul/>').append(data);
                                                            var ul_parent = jQuery('<div/>').append(ul);
                                                            ul_parent.addClass('overview');
                                                            var ul_parent_parent = jQuery('<div/>').append(ul_parent);
                                                            ul_parent.append(ul);
                                                            ul_parent_parent.addClass('viewport');
                                                            ul_parent_parent.css('height', Math.floor(jQuery('#b2j_k2_calendar_container_module_124').width()*8/7) - jQuery('.monyear.title').height() - 1 +'px');
                                                            item_list_vew_header.append(item_list_vew_close);
                                                            item_list_vew_header.append(item_list_vew_date);
                                                            item_list_vew_footer.html('<div class=\'scrollbar\'><div class=\'track\'><div class=\'thumb\'><div class=\'end\'></div></div></div></div>');
                                                            item_list_vew_footer.append(ul_parent_parent);
                                                            item_list_view.append(item_list_vew_header);
                                                            item_list_view.append(item_list_vew_footer);
                                                            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').prepend(item_list_view);
                                                            resize_font124(jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').width());
                                                            jQuery('#footer_124').tinyscrollbar();
                                                            jQuery('#footer_124').tinyscrollbar_update();
                                                            jQuery('#b2j_k2_calendar_container_module_124 .loading').hide();
                                                        }
                                                    })
                                                })
                                                                                        });
                while(true){
                    if(jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').length>0){
                        jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').css('width', jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').width() + 1);
                        resize_font124(jQuery('#b2j_k2_calendar_container_module_124 .gldp-default').width());
                        if(jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .core:nth-last-child(7)').hasClass('outday')){
                            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .core:not(:nth-last-child(n+8))').remove();
                        }
                        if(jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .core:nth-child(17)').hasClass('outday')){
                            jQuery('#b2j_k2_calendar_container_module_124 .gldp-default .core:nth-child(n+11):nth-child(-n+17)').remove();
                        }
                        jQuery("#b2j_k2_calendar_container_module_124 .loading").hide();
                        break;
                    }
                }
            }
        });
    }
</script> 
