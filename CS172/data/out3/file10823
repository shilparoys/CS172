     <section class="row row-1"> 
      <section class="column column-1 grid-left-0 grid-width-12"> 
       <div id="block-b8b53a09fb3f3975" class="block block-type-text block-fluid-height" data-alias=""> 
        <div class="block-content"> 
         <div class="entry-content">
          <p>LEADER IN ADVOCACY</p>
         </div>
         <!-- .entry-content --> 
        </div>
        <!-- .block-content --> 
       </div>
