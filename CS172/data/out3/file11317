    <section id="catalog" class="catalog"> 
     <a href="#" data-for="showhide-861141442" class="navbutton showhide-link"><span class="hiddentext">View the catalog for major &amp; minor details</span><span class="showtext hidden">Hide the catalog information</span></a>
     <div id="showhide-861141442" class="showhide-content hidden"> 
      <section class="majordetails"> 
       <div class="tabs" id="tabs1928120432">
        <ul>
         <li><a href="#Major">Major</a></li>
         <li><a href="#Minor">Minor</a></li>
