searchJQ(document).ready(function () {	searchJQ('.xcssSearchString').autocomplete({		source: '/Search/AutoComplete',		html: true,		delay: 0	}).data("autocomplete")._renderItem = function (ul, item) {		return $("<li></li>")		.data("item.autocomplete", item)		.append("<a>" + item.label + "</a>")		.appendTo(ul); 	};});</script>
           <script type="text/javascript">
   function faSiteSearch() {
       document.aspnetSearchForm.SearchString.value = $('input[name=SearchString]').val();
       if($('input[name=SearchString]').val() == undefined || $('input[name=SearchString]').val() == "&nbsp;&nbsp;&nbsp;Search"){
           for(i = 0; i < document.aspnetForm.SearchString.length; i++){ 
               if(document.aspnetForm.SearchString[i].value != undefined && document.aspnetForm.SearchString[i].value != "&nbsp;&nbsp;&nbsp;Search"){
                   document.aspnetSearchForm.SearchString.value = document.aspnetForm.SearchString[i].value;
                   break;
               }
               else
                   document.aspnetSearchForm.SearchString.value = document.aspnetForm.SearchString[i].value;
           }
       }
       document.location.href = "/Search/Results?searchPhrase=" + document.aspnetSearchForm.SearchString.value + "&page=1&perPage=10";
   }
   function faSiteSearchTextHook(ev) {
       ev = (window.event ? window.event : ev);
       if (ev.keyCode == 10 || ev.keyCode == 13) {
           faSiteSearch();
           stopEventPropagation(ev);
           return false;
       }
       return true;
   }
</script> 
