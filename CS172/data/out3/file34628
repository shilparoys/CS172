  <script>
function pwdFocus() {
            jQuery('#fakepassword').hide();
            imod.log('click');
            jQuery('.inputLoginPassword').show();
            jQuery('.inputLoginPassword').focus();
        }

        function pwdBlur() {
            if (jQuery('.inputLoginPassword').attr('value') == '') {
                jQuery('.inputLoginPassword').hide();
                jQuery('#fakepassword').show();
            }
        }
</script> 
  <!-- CUFON --> 
  <!-- Cufon: fonts -->   
 </body>
</html>