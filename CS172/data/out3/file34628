		function triggerOpenIdWindow(prmProviderAlias, prmProviderUsername, prmCustomProvider) 
		{
			var url = 'https://securelb.imodules.com/controls/login/OpenIdProxy.aspx?sid=1068&gid=1&trigger=1' + '&oidalias=' + encodeURIComponent(prmProviderAlias);
			if (prmProviderUsername != null) url += '&oidusername=' + encodeURIComponent(prmProviderUsername);
			if (prmCustomProvider != null) url += '&oidcustom=' + encodeURIComponent(prmCustomProvider);
			imod.Browser.OpenWindow(url, 950, 500, 'location=1,menubar=0,toolbar=0,directories=0,status=0,scrollbars=0,resizable=1', 1);

			return false;
		}//]]>
