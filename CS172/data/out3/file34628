	var htmlScraper_cid_5978;
	
	function GetHtmlFromUrlSuccess_cid_5978(data)
	{
		imod.log(data);
	
		if (data.Success)
		{
			htmlScraper_cid_5978.resultElement.parent().append(data.Data[0]);
		}
		else
		{
			htmlScraper_cid_5978.resultElement.parent().append(data.ErrorMessage);
		}
	
		imod.Events.publish('/html_text/loaded');
	}

	function GetHtmlFromUrl_cid_5978(controlId, urlToScrape, startToken, endToken, urlToScriptFile, cacheDuration, resultElement)
	{
		htmlScraper_cid_5978 = new imod.HtmlScraper('/apiservices/HtmlScraper/HtmlScraperService.asmx/GetHtmlFromUrl', controlId, urlToScrape, startToken, endToken, urlToScriptFile, cacheDuration, resultElement, GetHtmlFromUrlSuccess_cid_5978);
		htmlScraper_cid_5978.getHtmlFromUrl();
	}

