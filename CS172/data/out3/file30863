         <div id="article_inset"> 
          <div id="story-package"> 
           <!-- series --> 
           <div id="series"> 
            <script type="text/javascript" src="http://media.nj.com/storypackage/12875896/sp.js"></script> 
           </div> 
           <!-- /series --> 
           <!-- Aka Story Package --> 
