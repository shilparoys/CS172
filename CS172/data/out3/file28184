  <script type="text/javascript">
    // get parameter
    function getParameter(name) {
        name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
        var regexS = "[\\?&]" + name + "=([^&#]*)";
        var regex = new RegExp(regexS);
        var results = regex.exec(window.location.href);

        if (results == null)
            return "";
        else
            return decodeURIComponent(results[1].replace(/\+/g, " "));
    }

    jQuery(document).ready(function() {
        /* buttons click */
        jQuery('#btn_hd').click(function(e) {
            e.preventDefault();

            var cmp = getParameter('CMP');
            var campaignID = getParameter('campaignId');
            var mediaCode = getParameter('MediaCode');
            var debugUser = getParameter('debugUser');
            var cmpStr = '';

            if (cmp != '' && campaignID == '')
                cmpStr = 'CMP=' + cmp;
            else if (cmp == '' && campaignID != '')
                cmpStr = 'CMP=' + campaignID;

            var mediaCodeStr = (mediaCode == '' ? '' : 'MediaCode=' + mediaCode);
            var debugUserStr = (debugUser == '' ? '' : 'debugUser=' + debugUser);
            var redirect = '';

            if (cmpStr == '' && mediaCodeStr == '' && debugUserStr == '')
                redirect = '/hd/253';
            else if (cmpStr == '' && mediaCodeStr == '' && debugUserStr != '')
                redirect = '/hd/253?' + debugUserStr;
            else if (cmpStr == '' && mediaCodeStr != '' && debugUserStr == '')
                redirect = '/hd/253?' + mediaCodeStr;
            else if (cmpStr == '' && mediaCodeStr != '' && debugUserStr != '')
                redirect = '/hd/253?' + mediaCodeStr + '&' + debugUserStr;
            else if (cmpStr != '' && mediaCodeStr == '' && debugUserStr == '')
                redirect = '/hd/253?' + cmpStr;
            else if (cmpStr != '' && mediaCodeStr == '' && debugUserStr != '')
                redirect = '/hd/253?' + cmpStr + '&' + debugUserStr;
            else if (cmpStr != '' && mediaCodeStr != '' && debugUserStr == '')
                redirect = '/hd/253?' + cmpStr + '&' + mediaCodeStr;
            else if (cmpStr != '' && mediaCodeStr != '' && debugUserStr != '')
                redirect = '/hd/253?' + cmpStr + '&' + mediaCodeStr + '&' + debugUserStr;

            window.location.href = redirect;
        });
    });
</script>   
