                    <script type="text/javascript"> 
$().ready(function() {
	var inDesignMode = document.forms[MSOWebPartPageFormName].MSOLayout_InDesignMode.value;
	var $elem = $("#RelatedContentLink a"); 	
	
	if (inDesignMode != "1") { 	
		var baseAddress = $elem.attr('href'); 
		var linkText = "More stories on " + $elem.text(); 
		var primaryTopic = $("#primaryHETopicName a").text();
		$elem.attr('href', baseAddress+'?k=PrimaryTopic:"'+primaryTopic+'"'); 
		$elem.text(linkText+" "+primaryTopic);
	}
	
});
</script> 
                   </div></td> 
