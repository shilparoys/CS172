  <script type="text/javascript">
		function hidePullQuoteIfDefaultValue(){
			if($("body").hasClass("editMode")){
				$(".midTextPullQuoteContainer").show();
			}
			else{
				if($(".midTextPullQuoteInnerBorder .ms-WPBody:contains('Edit this page to modify your web part content.')").length > 0 || $(".midTextPullQuoteInnerBorder .ms-WPBody:empty").length > 0){
					$(".midTextPullQuoteContainer").hide();
				}
				else{
					$(".midTextPullQuoteContainer").show();
				}
			}
		}
		
		if(window.addEventListener){
			window.addEventListener('load',hidePullQuoteIfDefaultValue,false);
		}
		else{
			window.attachEvent('onload',hidePullQuoteIfDefaultValue);
		}
	</script> 
