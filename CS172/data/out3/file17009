           <script type="text/javascript">

	//remove test links in public site
	if (document.location.href.match(/news-room/i) && !document.location.href.match(/\.admin/i)) {
		$('a').filter(function(index) { 
			if ($(this).text() == "Test Link 1" || $(this).text() == "Test Link 2" || $(this).text() == "Test Link 3") {
				$(this).closest('li').remove();
			}
		});
	}

	function setPrimaryTopic() {
		//hide date
		var $dt = $("div#newsPage p.date");
		if ($.trim($dt.text()) == "December 30, 1899")
		$dt.text('');		
		
		//set primary topic links if not in edit mode
		var inDesignMode = document.forms[MSOWebPartPageFormName].MSOLayout_InDesignMode.value;
		if (inDesignMode != 1) {
			var _primaryHeTopicName = $("#primaryHETopicName a").text().trim().replace(/\xA0/g,'');
			var _primaryHeTopicLink = $("#primaryHETopicLink").text().trim().replace(/\xA0/g,'');
			
			var ptLink = "<a href='"+_primaryHeTopicLink+"'>"+_primaryHeTopicName+"</a>";
			var ptNoLink = _primaryHeTopicName;
			//if (_primaryHeTopicName != '' && _primaryHeTopicLink != '') {
			if ($.trim(_primaryHeTopicName) && $.trim(_primaryHeTopicLink)) {
				$("#primaryHETopicLinkDisplay").html(ptLink );
				$("p.learnMoreAbout").html("Learn more about "+ptLink);
			}
			else if (_primaryHeTopicName) {
				$("#primaryHETopicLinkDisplay").html(ptNoLink);
			}		
		}	
	}
	
	setPrimaryTopic();
	
</script> 
