          <div id="twoColumnLeftWide"> 
           <div id="newsPage"> 
            <p class="topic" id="primaryHETopicLinkDisplay"></p> 
            <h1> 
             <!--Title--> Transcripts for Military Personnel </h1> 
            <div style="display:none;" class="contactUsMesgSubj">
              &nbsp; 
            </div> 
            <p class="date"> 
             <!--Formatted Article Date--> December 30, 1899 </p> 
            <div class="newsContent"> 
             <div class="newsImageContainer"> 
              <!-- Thumbnail Large 220x146 --> 
              <div id="ctl00_PlaceHolderMain_ctl07_label" style="display:none">
               Thumbnail Large 220x146
              </div>
              <div id="ctl00_PlaceHolderMain_ctl07__ControlWrapper_RichImageField" class="ms-rtestate-field" style="display:inline" aria-labelledby="ctl00_PlaceHolderMain_ctl07_label">
               <div class="ms-rtestate-field">
                <img alt="" src="/news-room/PublishingImages/220x146-Military-Transcript-Services.jpg" style="border:0px solid">
               </div>
              </div> 
              <!--Image Caption--> 
              <div id="ctl00_PlaceHolderMain_ctl09_label" style="display:none">
               Image Caption
              </div>
              <div id="ctl00_PlaceHolderMain_ctl09__ControlWrapper_RichHtmlField" class="ms-rtestate-field" style="display:inline" aria-labelledby="ctl00_PlaceHolderMain_ctl09_label"></div> 
             </div> 
             <div class="midTextPullQuoteSpacer"></div> 
             <div class="midTextPullQuoteHider hideIfEmpty"> 
              <div class="midTextPullQuoteContainer"> 
               <div class="midTextPullQuoteInnerBorder"> 
                <menu class="ms-SrvMenuUI"> 
                 <ie:menuitem id="MSOMenu_Help" iconsrc="/_layouts/images/HelpIcon.gif" onmenuclick="MSOWebPartPage_SetNewWindowLocation(MenuWebPart.getAttribute('helpLink'), MenuWebPart.getAttribute('helpMode'))" text="Help" type="option" style="display:none"> 
                 </ie:menuitem> 
                </menu> 
