	data_track_addressbar: true
};
addthis.addEventListener('addthis.menu.share', function() {
	if (evt.type == 'addthis.menu.share') {
		alert(evt.data.service);
		if (evt.data.service == 'safety.smart911.com') {
			window.location = 'http://safety.smart911.com/advocate/';
			return false;
		}
	}
});
