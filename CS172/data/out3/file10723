var blurb = [
	[
		'I Signed Up for Smart911...',
		'"To help with a faster emergency response time, if needed, for my twin girls and family."  – Rick – Bartlett, IL',
		'contest1'
	],
	[
		'I Signed Up for Smart911...',
		'"I have 5 children 1 is on a home ventilator & has O2 in his room. It is important to have vital medical history & information on record for responders."  – Dana – Madison, TN',
		'contest2'
	],
	[
		'I Signed Up for Smart911...',
		'"My dog Blaze is part of my family. She can\'t call me if there is an emergency & she can\'t reach the doorknob to let herself out. She needs saving!"  – Amanda – Milton, GA',
		'contest3'
	],
	[
		'I Signed Up for Smart911...',
		'"One month ago today, my brother had a massive heart attack. A stroke left him with Aphasia. We lost him, this service might have saved him."  – Claire – Hermosa Beach, CA',
		'contest4'
	]
];

var bgpos = 0;
window.setInterval(function() {
	var oldpos = bgpos;
	bgpos = (bgpos + 1) % blurb.length;

	console.log("Rotating from "  + oldpos + " to " + bgpos);
	console.log(blurb[oldpos]);
	console.log(blurb[bgpos]);

	// Replace class
	$('#homeSplash').removeClass(blurb[oldpos][2]).addClass(blurb[bgpos][2]);
	// Replace text
	$('#splash-heading-content').html(blurb[bgpos][0]);
	$('#splash-subheading-content').html(blurb[bgpos][1]);
}, 15000);


$('.splash.home').css('backgroundImage', 'url(\'/images/splash/home_contest_main1.jpg\')');

} // end if contestRotation

