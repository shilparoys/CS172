  <script type="text/javascript" src="http://www.drew.edu/ut/wp-content/plugins/drew-shortcode/column.js?ver=1.0"></script> 
  <link rel="stylesheet" id="drewshortcode-css-css" href="http://www.drew.edu/ut/wp-content/plugins/drew-shortcode/drewshortcodes.css" type="text/css" media="all"> 
  <script type="text/javascript" src="http://www.drew.edu/ut/wp-includes/js/jquery/ui/widget.min.js?ver=1.11.2"></script> 
  <script type="text/javascript" src="http://www.drew.edu/ut/wp-includes/js/jquery/ui/tabs.min.js?ver=1.11.2"></script> 
  <script>
				$(function(){
					$("#tabs693843619").tabs({
						create: function( event, ui ) {
							window.setTimeout(checkmenu, 100);
						}
					});

					if (window.location.hash && $(window.location.hash).length){
						$("#tabs693843619").tabs("select", window.location.hash);
						window.scrollTo(0, $("#tabs693843619").offset().top );
					}

					//make the tab into a sliders
					if(jQuery().touchslider) $("#tabs693843619>ul:first").touchslider();
					if(jQuery().hammer) {
						$("#tabs693843619>div").hammer({
								drag_vertical: false
								,css_hacks:false
								,hold:false
								,drag:false
								,transform:false
								,tap_double:false
								,tap:false
							}).bind("swipe", function(ev){
								ev.stopPropagation();
								var active = $("#tabs693843619").tabs( "option", "active" );
								var total = $("#tabs693843619 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs693843619").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs693843619").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script>   
