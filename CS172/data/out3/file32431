			function()
			{ 
					jQuery('.reveal-modal-bg').removeClass("hid-imp");
					jQuery('.reveal-modal-bg').addClass('vis-imp');
					jQuery('#widgetModal-global').foundation('reveal', 'open');
					//modalShowing = true;
					jQuery('div#timeline-loading').hide();					
			}
		);		
	}
	function showStreetView(nid){
		jQuery('div#timeline-loading').show();
		
		var urlContent = '/streetview/' + nid + ' .main';

		jQuery('#widgetModal-global .inner').load(urlContent , 
			function()
			{ 
					jQuery('.reveal-modal-bg').removeClass("hid-imp");
					jQuery('.reveal-modal-bg').addClass('vis-imp');
					jQuery('#widgetModal-global').foundation('reveal', 'open');
					//modalShowing = true;
					jQuery('div#timeline-loading').hide();					
			}
		);
	}
	// getdirections/latlon/to/41.826707,-71.402573/Title
	function showDirections(view_mode){
		
		jQuery('div#timeline-loading').show();
		if(jQuery('.block-views-openlayers-tour-map-block-1').is(':visible') ) {
			jQuery('#widgetModal-global .inner').html('').append(jQuery('#popup iframe').clone());
		} else {
			jQuery('#widgetModal-global .inner').html('').append(jQuery('#locationModal-global iframe').clone());
		}
		t = setTimeout(function(){
			jQuery('.reveal-modal-bg').removeClass("hid-imp");
			jQuery('.reveal-modal-bg').addClass('vis-imp');
			jQuery('div#timeline-loading').hide();	
			jQuery('#widgetModal-global').foundation('reveal', 'open');
		},1000);		
	}
  </script>   
 </body>
</html>