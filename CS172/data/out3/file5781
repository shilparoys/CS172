               <script type="text/javascript">
                              var currentClass = "cartButton";
                              function doflash()
                              {
                              var cartcell = FindElement("cartmenuitem");

                              if (cartcell)
                              {
                              if (currentClass == "cartButton")
                              currentClass = "cartButtonFlash";
                              else
                              currentClass = "cartButton";

                              cartcell.className = currentClass;
                              }
                              }

                              function startflash()
                              {
                              window.setInterval("doflash()",500);
                              }

                              function checkcart(name)
                              {
                              var el = FindElement(name);
                              var cartcell = FindElement("cartmenuitem");
                              if (el != null)
                              {
                              if (el.value != "0")
                              {
                              cartcell.innerHTML = '<a class=\"offButtonLink\" href=\"../cart/index.aspx\">Register Now</a>';
                              startflash();
                              }
                              else
                              {
                              cartcell.innerHTML = '<a class=\"offButtonLink\" href=\"../cart/index.aspx\">Cart: 0 items</a>';
                              }
                              }
                              }
                                            </script> 
