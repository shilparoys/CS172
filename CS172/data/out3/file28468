	var rw_check_ads = $('div.rw_fs_ad_item').length;
	var rw_check_ads_local = $('div#rw_ads_local').children().length;
	var rw_check_ads_global = $('div#rw_ads_global').children().length;

	//alert(rw_check_ads_local);
	//alert(rw_check_ads_global);

	if(rw_check_ads>0){$('.rw_fs_ad_container').css({'display':'block'});
	}

	if(rw_check_ads_local>0){$('div#rw_ads_local').css({'display':'block'});
	}
	
