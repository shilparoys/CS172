         <!-- reorder related items --> 
         <script type="text/javascript">

var str = 'contacts|programs|blogs|events|resources';

(function( $ ){
    	$.fn.jaySort= function(prefix, lst) {
        	for (var x=0; x<lst.length; x++)
		{
			if ( $('#' + prefix + lst[x]).get(0))
           				$(this).append($('#' + prefix + lst[x]));
		}
   	};
})( jQuery );

if ( str != '')
{
	var arr = str.split('|');	
	
	$('#related').jaySort('related-', arr);

}

</script> 
