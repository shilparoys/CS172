    <script>
										var bt_obj_commentform = document.getElementById("box_title_commentform");
										var bc_obj_commentform = document.getElementById("box_content_commentform");
										var bc_link_obj_commentform = document.getElementById("box_close_commentform");
										var bo_link_obj_commentform = document.getElementById("box_open_commentform");
										var init_vis_commentform = "none";
										var bc_vis_commentform = ( init_vis_commentform == "block" ? false : true );
										bo_link_obj_commentform.innerHTML = ( init_vis_commentform == "block" ? " - " : " + " );
										bt_obj_commentform.onclick = tx;
										bc_link_obj_commentform.onclick = tx;
										function tx() { bc_obj_commentform.style.display = ( bc_vis_commentform ? "block" : "none" ); bo_link_obj_commentform.innerHTML = ( bc_vis_commentform ? " - " : " + " ); bc_vis_commentform = !bc_vis_commentform; }
