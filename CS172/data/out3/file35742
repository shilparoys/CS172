      <script type="text/javascript">

function checkRadio() {

    var r = document.colleges.table;

    for (var i = 0; i < r.length; i++) {

        var button = r[i];

        if (button.checked) {

            if(button.value == 'public'){

                window.location.href = '/tool/college/T014-S001-kiplinger-s-best-values-in-public-colleges/index.php';

                return false;

            }

        }

    }

    return true;

}

</script> 
