$(document).ready(function()
{
  var colors = new Object();
  $('div.hiddenCategoryList ul li').each(function() {
    colors[$(this).children("a.main_rightcol_categoriabox_link").html()] = $(this).children("span.main_rightcol_categoriabox_linkline").css("background-color");
  });
  
$('div.homepage_news_itemcontainer span.homepage_news_category span').each(function() {
    if (colors[$(this).children("span").html()]) {
      $(this).css("display", "inline-block");
      $(this).children("span").css("display", "inline-block");
      $(this).css("color", colors[$(this).children("span").html()]);
    }
  });
});
