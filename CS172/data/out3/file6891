		$(this).loopedSlider({
		container: ".homepage_boxscrollinner_loopedslider_container",
		slides: ".homepage_boxscrollinner_loopedslider_slides",
				containerClick: false,
				slidespeed: 500
	});
			
	if ($(this).children(".homepage_boxscrollinner_loopedslider_container").children(".homepage_boxscrollinner_loopedslider_slides").children("div").size() == 1)
			{
				$(this).children(".previous").css("display", "none");
				$(this).children(".next").css("display", "none");
			}
		});
		
