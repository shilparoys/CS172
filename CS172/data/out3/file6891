jQuery(document).ready(function() {
	var tmpHTML = '<li>';
	var i = 1;
	$("ul#homepage_news_carousel").find("div.homepage_news_itemcontainer").each(function(index) {
			if (i % 4 != 0)
				tmpHTML += '<div class="homepage_news_itemcontainer">' + $(this).html() + '</div><hr class="homepage_news_line" />';
			else
				tmpHTML += '<div class="homepage_news_itemcontainer">' + $(this).html() + '</div></li><li>';
			i++;
		});

		tmpHTML += '</li>';
		$("ul#homepage_news_carousel").html(tmpHTML);
	
