      <script type="text/javascript">

	function toggleform(which){
		if (which=="main" || which=="hirek" || which=="sf" || which=="") {
			jQuery("#gs_form").hide();
			jQuery("#ws_form").show();
		}else{
			jQuery("#gs_form").show();
			jQuery("#ws_form").hide();
		}
		jQuery("#where").val(which) ;
		
	}
</script> 
