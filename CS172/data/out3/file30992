  <script type="text/javascript">
$(document).ready(function() {
    // call now
    setTimeout("updatePlaying()",0);	
});
var current;
function updatePlaying(){
    rand = Math.floor(Math.random()*10000000);
    var data = { cachebuster : rand };
    // Do ajax post
    $.get("/hebenowplaying.php", data, function(htmldata, textStatus){
        // Debug to console
        //debug('html result: ', htmldata);
        if(current != htmldata){
            $('#playing').fadeOut(1000);
            $('#playing').hide();
            $('#playing').html(htmldata);
            $('#playing').fadeIn(1000);
            current = htmldata;
        }   
    }, "html");
    // Call ourselves again in 20 seconds
    setTimeout("updatePlaying()",1000*20);
}
</script> 
