  <script type="text/javascript">
    var relpath = '';
    $(document).ready(function(){
        $("#nav ul.cos-menu li.cos-menu-parent").click(
          function( event ) {
              if ( $(event.target).parents("ul.cos-menu-submenu").length > 0 ) return;
              $(this).children("ul").slideToggle(200);
              $(this).toggleClass('expanded');
              
              var url = $(this).find("img").attr("src");
              if ( url.indexOf( "plus.png" ) != -1 ) {
                  $(this).find("img").attr("src", url.replace( "plus.png", "minus.png" ) );
              }
              else {
                  $(this).find("img").attr("src", url.replace( "minus.png", "plus.png" ) );
              }
          } );
    });

    $(document).ready(function(){
        $(".biolink .link a").click(function(){
            $(this).parent().parent().find(".bio").slideToggle(1000);
            if($(this).html() == 'read more...'){
                $(this).html('close');
            } else {
                $(this).html('read more...');
            }
        });		
    });

    $(document).ready(function(){
        var working = false;
        $(".panelbutton").click(function()  {
            if(!working){
                working = true;
                $(".panel:visible").fadeOut(300);
                $(".active").removeClass('active');
                $(this).addClass('active');
                if(this.id == 'panel1button'){
                    $("#panel1").fadeIn(300, function(){working = false});
                } else if(this.id == 'panel2button'){
                    $("#panel2").fadeIn(300, function(){working = false});
                } else if(this.id == 'panel3button'){
                    $("#panel3").fadeIn(300, function(){working = false});
                } else if(this.id == 'panel4button'){
                    $("#panel4").fadeIn(300, function(){working = false});
                }
            }
        });
    });

    var facilityimgs = true;
    $(document).ready(function(){
        $("#facilityimg").click(function(){
            $("div.facilityimgs").fadeOut(0);
            $("div.facilityimgf").fadeOut(0);
            $("#facilityimg").slideToggle(500, function(){
                if(facilityimgs == true){
                    $("img.facilityimgf").show();
                    $("img.facilityimgs").hide();
                    $(this).slideToggle(500, function(){$("div.facilityimgf").fadeIn(300)});
                } else {
                    $("img.facilityimgf").hide();
                    $("img.facilityimgs").show();
                    $(this).slideToggle(500, function(){$("div.facilityimgs").fadeIn(300)});
                }
                facilityimgs = !facilityimgs;
            });
        });
    });

    /*
    $(document).ready(function() {
        $("#infinite").scrollable().circular();
        $("#infinite img").click(function(){
            var thisclicked = this;
            if($("img.facilityimgf").is(':visible')){
                $("div.facilityimgf").hide();
                $("#facilityimg").slideToggle(400, function(){											
                    switchImg(thisclicked);
          	    $("img.facilityimgf").hide();
          	    $("img.facilityimgs").show();
          	    $('#facilityimg').slideToggle(400);
                });
                facilityimgs = true;
            } else {
                switchImg(this);
            }
        });
    });
*/
    
    function switchImg(athis){
        var imgClickedId = athis.id;
        var imgClickedTitle = $(athis).attr("alt");
        $(".facilityimgs").fadeOut(100, function(){
            $(".facilityimglink").html(imgClickedTitle + " &raquo;");
            $(".facilityimglink").attr("href", relpath + 'facilities/' + imgClickedId + ".php");
            $(".facilityimgs").attr("src", relpath + 'images/headers/' + imgClickedId + '.jpg');
            $(".facilityimgf").attr("src", relpath + 'images/headers/' + imgClickedId + '-full.jpg');
        });
        $("img.facilityimgs").fadeIn(200);	
        $("div.facilityimgs").fadeIn(500);	
    }
  </script> 
  <script type="text/javascript">
 $(document).ready(function() {
	 var temp = $("div.events_scroll_block");
	if (temp.length > 1){
		temp.each(function(index, value){
			$(this).hide();
			$('#events_scroll_bar').append('<span class="scroll_dot inactive_button" alt="'+index+'">&nbsp;</span>');
			//console.log(index+" "+value);
		});
		temp.first().show();
		$("span.scroll_dot").first().removeClass("inactive_button").addClass("active_button");
		
	}

	$("span.scroll_dot").click(function(){
		$("span.scroll_dot").removeClass("active_button").addClass("inactive_button");
		$(this).removeClass("inactive_button").addClass("active_button");
		var temp_index = $(this).attr("alt");
		$("div.events_scroll_block").each(function(index, value){
			if(index == temp_index){
				$("div.events_scroll_block").hide();
				$(this).show();
			}
		});
	});


	if($("#admin-menu").index() > 0){
	$("#events_scroll_bar").css({
        position: "absolute",
        top: "629px",
        left: "713px",
        width : "242px"
    });
	}
	if($("#admin-menu").index() < 0){
		$("#events_scroll_bar").css({
	        position: "absolute",
	        top: "563px",
	        left: "713px",
	        width : "242px"
	    });
	}
	
 });

</script> 
  <script type="text/javascript">
$(document).ready(function() {
	if($("div.spotlight_block").length > 1){
		$("div.spotlight_block").hide();
		$("div.spotlight_block").eq(Math.floor(Math.random()*($("div.spotlight_block").length))).show().addClass("spotlight_active");
	//	$("div.spotlight_block").first().show().addClass("spotlight_active");
		$("div#block-block-6").prepend('<div id="spotlight_scrollbar" style="margin-top: 128px; margin-bottom: -170px;opacity:0.4;"><span id="spotlight_left" style="margin-left:-21px;cursor:pointer;"><img src="/cos/sites/rit.edu.cos/themes/cos/images/scroll-arrow-left.png"/></span><span id="spotlight_right" style="float: right; margin-right: -22px;cursor:pointer;"><img src="/cos/sites/rit.edu.cos/themes/cos/images/scroll-arrow-right.png"/></span></div>');	
	}

	$("span#spotlight_right").click(function () {
		if($("div.spotlight_block.spotlight_active").next("div.spotlight_block").index() > -1){
			$("div.spotlight_block.spotlight_active").addClass("old_active");
			$("div.spotlight_block.spotlight_active").next("div.spotlight_block").addClass("spotlight_active");
			$("div.spotlight_block.old_active").hide("slide", { direction: "left" }, 300);
			$("div.spotlight_active.old_active").removeClass("old_active").removeClass("spotlight_active");
			$("div.spotlight_block.spotlight_active").delay(300).show("slide", { direction: "right" }, 300); 
		}
	});
	
	$("span#spotlight_left").click(function () {
		if($("div.spotlight_block.spotlight_active").prev("div.spotlight_block").index() > -1){
			$("div.spotlight_block.spotlight_active").addClass("old_active");
			$("div.spotlight_block.spotlight_active").prev("div.spotlight_block").addClass("spotlight_active");
			$("div.spotlight_block.old_active").hide("slide", { direction: "right" }, 300);
			$("div.spotlight_active.old_active").removeClass("old_active").removeClass("spotlight_active");
			$("div.spotlight_block.spotlight_active").delay(300).show("slide", { direction: "left" }, 300); 
		}
	});

	
	
});
 </script> 
