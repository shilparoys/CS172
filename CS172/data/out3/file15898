      <script type="text/javascript">
            function placePushpin(selectObj) {
                var pp = document.getElementById('pushpin');
                var ppHgt = 32;
                var ppWdt = 24;
                var idx = selectObj.selectedIndex;
                var val = selectObj.options[idx].value;
                if (val == '') {
                    pp.style.left = '0';
                    pp.style.top = '0';
                    pp.style.visibility = 'hidden';                 
                    }else{
                    var coor = val.split(',');
                    var imageHeight = Number(460) + Number(ppHgt); //set the    height of the map image here
                    pp.style.top = (Number(coor[1]) - Number(imageHeight)) + 'px';
                    pp.style.left = coor[0] + 'px';
                    pp.style.visibility = 'visible';
                }
            }
            function displayArea(str) {
                if (str=='') {
                    str = '<span style="font-weight:normal;">Select an area of campus</span>';
                }else{
                    str =  '<span style="font-weight:bold;">' + str + '</span>';
                }
                document.getElementById('displayArea').innerHTML = str; 
            }
            function showMap(selectObj) {
                var idx = selectObj.selectedIndex; 
                var url = selectObj.options[idx].value; 
                document.getElementById('displayMap').src = url;
            }
            function jump(url){
                //parent.location = url;
                location.href=url;
                }
            
        </script> 
      <style title="text/css" type="text/css">
            select {
                background-color: #450084;
                color: #ffffff;
                border: 0;
                padding: 5px;   
            }
            
        </style> 
      <div style="float: right;"> 
       <form> 
        <select onchange="jump(this.options[this.selectedIndex].value);" size="1" style="margin-right: 5px;"> <option selected>-- Jump to a region --</option> <option title="Click to view Bluestone Area" value="regions/bluestone.shtml">Bluestone Area</option> <option title="Click to view Hillside Area" value="regions/hillside.shtml">Hillside Area</option> <option title="Click to view Lake Area" value="regions/lake.shtml">Lake Area</option> <option title="Click to view Memorial Area" value="regions/memorial.shtml">Memorial Area</option> <option title="Click to view North Campus Area" value="regions/morth-campus.shtml">North Campus Area</option> <option title="Click to view Ridge Area" value="regions/ridge.shtml">Ridge Area</option> <option title="Click to view Skyline Area" value="regions/skyline.shtml">Skyline Area</option> <option title="Click to view Village Area" value="regions/village.shtml">Village Area</option> </select> 
