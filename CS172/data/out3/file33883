  <!-- Custom Search Code --> 
  <script type="text/javascript">
        var $SearchBox = $("#SearchText");
        var $value = $SearchBox.val();
        $(document).ready(function () {
            $("#SearchText").addClass('GoogleWatermark');

            $("#SearchText").on('click blur change', function () {
                $(this).toggleClass('GoogleWatermark');
            });

            $SearchBox.bind("enterKey", function (e) {
                window.location = "search.aspx?q=" + $value;
            });
            $SearchBox.keyup(function (e) {
                if (e.keyCode == 13) {
                    e.preventDefault();
                    $(this).trigger("enterKey");
                }
            });
            
        });
    </script> 
