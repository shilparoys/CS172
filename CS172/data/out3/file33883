  <script src="Scripts/Custom_New/jquery.bxslider.min.js"></script> 
  <script src="Scripts/Custom_new/jquery-ui-1.8.11.js" type="text/javascript"></script> 
  <script src="Scripts/calendarEvents/CalendarEvents.js"></script> 
  <script type="text/javascript">
        $(document).ready(function () {
            // set empty string title on image elements in bxSlider
            $('ul.bxslider img').attr('title', '');
            $('.bxslider').bxSlider({
                mode: 'fade',
                speed: 1000,
                captions: true,
                auto: true,
                autoHover: true,
                pause: 10000
            });

            // Second implementation of bx slider here!!
            $('.bxslider2').bxSlider({
                mode: 'horizontal',
                speed: 1000,
                adaptiveHeight: true,
                nextSelector: '#slider-next',
                prevSelector: '#slider-prev',
                nextText: 'Next',
                prevText: 'Previous',
                pause: 5000,
                //minSlides: 2,
                //maxSlides: 4,
                //moveSlides: 2,
                minSlides: 3,
                maxSlides: 3,
                moveSlides: 1,
                slideWidth: 200,
                slideMargin: 20,
                pager: false,
                touchEnabled: true,
                auto: false
            });
            // initialize accordion
            $('.accordionA > li > ul, .accordionA > li > div')
            .click(function (e) {
                e.stopPropagation();
            })
            .parent().filter(':first').addClass('activeAccordion').end()
            .find('> ul, > div').filter(':not(:first)')
            .hide();
            $('.accordionA > li').click(function () {
                var accordionListItem = $('.accordionA > li');
                var selfCheck = $(this).find('div:first, ul:first').is(':visible');
                if (selfCheck) {
                    return;
                }
                accordionListItem.removeClass();
                $(this).addClass('activeAccordion');
                $(this).parent().find(' > li > div:visible, > li > ul:visible').slideToggle();
                $(this).find('> ul:first, > div:first').stop(true, true).slideToggle();
            });
            $('.accordionA > li > a').click(function (e) {
                //alert($(this).text() + ' accordion clicked');
                loadCalendar($(this).text());
                e.preventDefault();
            });

            // Initialize the calendar on the default 1st accordion
            $('.accordionA > li > a:first').click();
        });
        loadCalendar = function (control) {
            var userAcct = "ucrhousingservices@gmail.com";
            var acctApiKey = "AIzaSyCrPnmIYNcnG3taiMW49KnIYwb6m-hgtuI";

            // Check which control
            switch (control) {
                case "Announcements":
                    //$("#calAnnouncements").html("");
                    $("#calAnnouncements").CalendarEvents({
                        user: userAcct
                        , apiKey: acctApiKey
                        , displayType: "perMonth"
                        , searchCriteria: "#housingannouncement"
                        , searchCondition: "in"
                        , searchPath: "description"
                        , height: "225"
                        , addLinkType: "link"
                        , setContainer: false

                    })
                    break;

                case "Upcoming Events":
                    //$("#calUpcomingEvents").html("");
                    $("#calUpcomingEvents").CalendarEvents({
                        user: userAcct
                        , apiKey: acctApiKey
                        , displayType: "perMonth"
                        , searchCriteria: "#housingevent"
                        , searchCondition: "in"
                        , searchPath: "description"
                        , height: "225"
                        , addLinkType: "link"
                        , setContainer: false
                    })
                    break;
            }
            
        }
    </script> 
