                 $(function () {
                     $('.sbcontainer').on('click', function () {                        
                         $('.socialbuttons').stop(true).animate({ width: 'toggle' }, 250)
                     });                     
                     $('html').click(function (event) {
                         if (!$(event.target).closest('.sbcontainer').length) {
                             if ($('.socialbuttons').css("display") != "none")
                                 $('.socialbuttons').stop(true).animate({ width: 'toggle' }, 250)
                         }
                     });
                 });
