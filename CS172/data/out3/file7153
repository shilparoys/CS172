   <script>
     $('#spot').carousel({
         interval: 6000 // in milliseconds  
     });

     $(function () {
         if (($(window).width() > 800)) {
             $('#mainmenu').mouseenter(function () {
                 $('#mainmenu>li ul').stop(true).animate({ height: 400 }, 250);
             })
			.mouseleave(function () {
			    $('#mainmenu>li ul').stop(true).animate({ height: 0 }, 250);
			});
         }
         else {
             $(document.body).click(function (e) {
                 var $box = $('#mainmenu');
                 if (e.target.id !== '#mainmenu' && !$.contains($box[0], e.target)) {
                     $('#mainmenu>li ul').stop(true).animate({ height: 0 }, 250);
                 }
                 else {
                     $('#mainmenu>li ul').stop(true).animate({ height: 400 }, 250);
                 }
             });
         };

     });	
</script> 
