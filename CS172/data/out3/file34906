<script>
$(function(){
	
	nrth = 0;
	west = 0;
	menu = $('#footer div > ul.menu');
	itms = $('#footer div > ul.menu > li');
	clmn = Math.ceil(itms.length/3);
	
	menu.css('position','relative');
	itms.each(function(i){
		
		hght = $(this).prev().height();
		if (hght != null) nrth += hght+20;
		
		if (i != 0 && i % clmn == 0) {
			nrth = 0;
			west += 250;
		}
		
		$(this).css('position','absolute');
		$(this).css('top',nrth+'px');
		$(this).css('left',west+'px');
		
	});
	
});
</script>
