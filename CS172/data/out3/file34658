           <h3>Ask the Office of Faculty Resources a Question</h3> 
           <div id="formContainer" style="width: 354px; margin-bottom: 20px;"> 
            <div class="section topForm"> 
             <script type="text/javascript">
			function validateForm(){
				var x = document.askForm;
				var err = 0;
				
				if (x.name.value == ""){
					err += 1;
				}
				
				if (x.email.value == ""){
					err += 1;
				}
				
				if (x.question.value == ""){
					err += 1;
				}
				
				if (err > 0){
					alert("Please fill out all fields.");
					return false;
				}
				else{
					return true;
				}
			}		
		</script> 
            </div> 
            <form name="askForm" method="post" action="http://www.nyu.edu/v60/formHandlers/ask.ofr.php" onsubmit="return validateForm();"> 
