     <script type="text/javascript">
        $(document).ready(function () {
            $('div.tab-content div table thead').each(function () {
                $(this).find('th:eq(2)').css('width', '56px');
            });
            var currentTab;
        });

        $(document).ready(function () {
            $('td[id^="tab"]').click(function () {                
                // Get the currently viewed term or selected radio button
                var currentSelectedView = getCheckedRadioBtn('pane');

                $('td[id^="tab"]').removeClass("selected");
                // Remove the following if it exists
                $('td[id^="tab"]').removeClass("ie7_class9");
                $('td[id^="tab"]').removeClass("ie7_class10");
                $('td[id^="tab"]').removeClass("ie7_class11");
                // Add the class selected to the current tab
                $(this).addClass("selected");
                $('.tab-content').children('div').hide();
                $('#' + $(this).attr('id') + '-content').show();
                
                // Set the selected radioButton/View
                var selectedView = currentSelectedView + "-";
                setSelectedView(selectedView, getSelectedTab());
            });
        });
        function setSelectedView(sCurrentView, sTab) {
            var view1 = "-p1-p4-p7-p10-p13-";
            var view2 = "-p2-p5-p8-p11-p14-";
            var view3 = "-p3-p6-p9-p12-p15-";
            var viewSelected;
            // Check which view is currently selected
            var v1 = view1.search(sCurrentView);
            if (v1 > 0) {
                viewSelected = "view1";
            }
            var v2 = view2.search(sCurrentView);
            if (v2 > 0) {
                viewSelected = "view2";
            }
            var v3 = view3.search(sCurrentView);
            if (v3 > 0)  {
                viewSelected = "view3";
            }

            // uncheck the currently selected radio
            sCurrentView.checked = false;
            var newViewSelected;

            // Get the group name of the radio
            var group = document.getElementsByName('pane');
            switch (sTab) {
                case "tab1":
                    // Check Current View
                    switch (viewSelected) {
                        case "view1":
                            // Select p1
                            group.item(0).checked = true;
                            newViewSelected = group.item(0).id;
                            break;
                        case "view2":
                            // Select p2
                            group.item(1).checked = true;
                            newViewSelected = group.item(1).id;
                            break;
                        case "view3":
                            // Select p3
                            group.item(2).checked = true;
                            newViewSelected = group.item(2).id;
                            break;
                    }
                    break;
                case "tab2":
                    // Check Current View
                    switch (viewSelected) {
                        case "view1":
                            // Select p4
                            group.item(3).checked = true;
                            newViewSelected = group.item(3).id;
                            break;
                        case "view2":
                            // Select p5
                            group.item(4).checked = true;
                            newViewSelected = group.item(4).id;
                            break;
                        case "view3":
                            // Select p6
                            group.item(5).checked = true;
                            newViewSelected = group.item(5).id;
                            break;
                    }
                    break;
                case "tab3":
                    // Check Current View
                    switch (viewSelected) {
                        case "view1":
                            // Select p7
                            group.item(6).checked = true;
                            newViewSelected = group.item(6).id;
                            break;
                        case "view2":
                            // Select p8
                            group.item(7).checked = true;
                            newViewSelected = group.item(7).id;
                            break;
                        case "view3":
                            // Select p9
                            group.item(8).checked = true;
                            newViewSelected = group.item(8).id;
                            break;
                    }
                    break;
                case "tab4":
                    // Check Current View
                    switch (viewSelected) {
                        case "view1":
                            // Select p10
                            group.item(9).checked = true;
                            newViewSelected = group.item(9).id;
                            break;
                        case "view2":
                            // Select p11
                            group.item(10).checked = true;
                            newViewSelected = group.item(10).id;
                            break;
                        case "view3":
                            // Select p12
                            group.item(11).checked = true;
                            newViewSelected = group.item(11).id;
                            break;
                    }
                    break;
                case "tab5":
                    // Check Current View
                    switch (viewSelected) {
                        case "view1":
                            // Select p13
                            group.item(12).checked = true;
                            newViewSelected = group.item(12).id;
                            break;
                        case "view2":
                            // Select p14
                            group.item(13).checked = true;
                            newViewSelected = group.item(13).id;
                            break;
                        case "view3":
                            // Select p15
                            group.item(14).checked = true;
                            newViewSelected = group.item(14).id;
                            break;
                    }
            }

            return newViewSelected;
        }

        function getSelectedTab() {
            return $('div.tabs td.selected').attr('id');
        }

        function isRadioInTabSelected(sRadio, sTab) {
            var tab1Radios = "-p1-p2-p3-";
            var tab2Radios = "-p4-p5-p6-";
            var tab3Radios = "-p7-p8-p9-";
            var tab4Radios = "-p10-p11-p12-";
            var tab5Radios = "-p13-p14-p15-";
            var inTab = false;

            switch (sTab) {
                case "tab1":
                    if (tab1Radios.search(sRadio) > 0) {
                        inTab = true;
                    }
                    break;
                case "tab2":
                    if (tab2Radios.search(sRadio) > 0) {
                        inTab = true;
                    }
                    break;
                case "tab3":
                    if (tab3Radios.search(sRadio) > 0) {
                        inTab = true;
                    }
                    break;
                case "tab4":
                    if (tab4Radios.search(sRadio) > 0) {
                        inTab = true;
                    }
                    break;
                case "tab5":
                    if (tab5Radios.search(sRadio) > 0) {
                        inTab = true;
                    }
                    break;
            }
            return inTab;
        }

        function getCheckedRadioBtn(sGroupName) {
            var group = document.getElementsByName(sGroupName);

            for (var i = 0; i < group.length; i++) {
                if (group.item(i).checked) {
                    return group.item(i).id;
                } else if (group[0].type !== 'radio') {
                    //if you find any in the group not a radio button return null
                    return null;
                }
            }
        }

        function slideLeft(sRadio, sTab) {
            var group = document.getElementsByName('pane');

            // Check the tab name and dont decrement if already at the lowest p#
            switch (sTab) {
                case "tab1":
                    if (sRadio == "p3") {
                        // Turn off the previous radio (p3)
                        group.item(2).checked = false;
                        // Select P2
                        group.item(1).checked = true;
                    }
                    if (sRadio == "p2") {
                        // Turn off the previous radio (p2)
                        group.item(1).checked = false;
                        // Select P1
                        group.item(0).checked = true;
                    }
                    break;
                case "tab2":
                    if (sRadio == "p6") {
                        // Turn off the previous radio (p6)
                        group.item(5).checked = false;
                        // Select P4
                        group.item(4).checked = true;
                    }
                    if (sRadio == "p5") {
                        // Turn off the previous radio (p5)
                        group.item(4).checked = false;
                        // Select P4
                        group.item(3).checked = true;
                    }
                    break;
                case "tab3":
                    if (sRadio == "p9") {
                        // Turn off the previous radio (p9)
                        group.item(8).checked = false;
                        // Select P8
                        group.item(7).checked = true;
                    }
                    if (sRadio == "p8") {
                        // Turn off the previous radio (p8)
                        group.item(7).checked = false;
                        // Select P7
                        group.item(6).checked = true;
                    }
                    break;
                case "tab4":
                    if (sRadio == "p12") {
                        // Turn off the previous radio (p12)
                        group.item(11).checked = false;
                        // Select P11
                        group.item(10).checked = true;
                    }
                    if (sRadio == "p11") {
                        // Turn off the previous radio (p11)
                        group.item(10).checked = false;
                        // Select P7
                        group.item(9).checked = true;
                    }
                    break;
                case "tab5":
                    if (sRadio == "p15") {
                        // Turn off the previous radio (p15)
                        group.item(14).checked = false;
                        // Select P14
                        group.item(13).checked = true;
                    }
                    if (sRadio == "p14") {
                        // Turn off the previous radio (p14)
                        group.item(13).checked = false;
                        // Select P13
                        group.item(12).checked = true;
                    }
                    break;
            }
            return null;
        }

        function setCheckedRadioBtnLeft(sRadioBtn) {
            // Get the current selected tab
            var currentTab = getSelectedTab();

            // Check if the radio is in the tab
            if (isRadioInTabSelected) {
                // Move as normal
                slideLeft(sRadioBtn, currentTab);
            }
            else {
                // Set the selected view/radio button
                var selectedView = sRadioBtn + "-";
                setSelectedView(selectedView, currentTab);

                slideLeft(sRadioBtn, currentTab);
            }
            return null;
        }

        function slideRight(sRadio, sTab) {
            var group = document.getElementsByName('pane');

            // Check the tab name and dont increment if already at the maximum p#
            switch (sTab) {
                case "tab1":
                    if (sRadio == "p1") {
                        // Turn off the previous radio (p1)
                        group.item(0).checked = false;
                        // Select p2
                        group.item(1).checked = true;
                    }

                    if (sRadio == "p2") {
                        // Turn off the previous radio (p2)
                        group.item(1).checked = false;
                        // Select p3
                        group.item(2).checked = true;
                    }
                    break;
                case "tab2":
                    if (sRadio == "p4") {
                        // Turn off the previous radio (p4)
                        group.item(3).checked = false;
                        // Select p5
                        group.item(4).checked = true;
                    }

                    if (sRadio == "p5") {
                        // Turn off the previous radio (p5)
                        group.item(4).checked = false;
                        // Select p6
                        group.item(5).checked = true;
                    }
                    break;
                case "tab3":
                    if (sRadio == "p7") {
                        // Turn off the previous radio (p7)
                        group.item(6).checked = false;
                        // Select p8
                        group.item(7).checked = true;
                    }

                    if (sRadio == "p8") {
                        // Turn off the previous radio (p8)
                        group.item(7).checked = false;
                        // Select p9
                        group.item(8).checked = true;
                    }
                    break;
                case "tab4":
                    if (sRadio == "p10") {
                        // Turn off the previous radio (p10)
                        group.item(9).checked = false;
                        // Select p11
                        group.item(10).checked = true;
                    }

                    if (sRadio == "p11") {
                        // Turn off the previous radio (p11)
                        group.item(10).checked = false;
                        // Select p12
                        group.item(11).checked = true;
                    }
                    break;
                case "tab5":
                    if (sRadio == "p13") {
                        // Turn off the previous radio (p13)
                        group.item(12).checked = false;
                        // Select p14
                        group.item(13).checked = true;
                    }

                    if (sRadio == "p14") {
                        // Turn off the previous radio (p14)
                        group.item(13).checked = false;
                        // Select p15
                        group.item(14).checked = true;
                    }
                    break;
            }
            return null;
        }

        function setCheckedRadioBtnRight(sRadioBtn) {
            // Get the current selected tab
            var currentTab = getSelectedTab();

            // Check if the radio is in the tab
            if (isRadioInTabSelected(sRadioBtn, currentTab)) {
                // Move as normal
                slideRight(sRadioBtn, currentTab);
            }
            else {
                // Set the selected view/radio button
                var selectedView = sRadioBtn + "-";
                setSelectedView(selectedView, currentTab);

                slideRight(sRadioBtn, currentTab);
            }

            return null;
        }
    </script> 
