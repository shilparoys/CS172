      <h3>UCR Academic Calendar</h3> 
      <div class="padding"> 
       <div class="tabs"> 
        <table style="margin: 0 0 0 0;"> 
         <thead class="tabs gray"> 
          <tr class="tabs gray"> 
           <td id="tab1" class="selected">FIRST DAY</td> 
           <td id="tab2">REGISTRATION</td> 
           <td id="tab3">PAYMENT &amp; DISBURSEMENT</td> 
           <td id="tab4">LAST DAY TO...</td> 
           <td id="tab5">FINALS &amp; GRADES</td> 
          </tr> 
         </thead> 
        </table> 
       </div> 
       <table class="tablebg" style="margin-bottom: 0;"> 
