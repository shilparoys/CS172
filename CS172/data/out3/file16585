  <script type="text/javascript">
$(document).ready(function() {
	$datereviewed = $("meta[name='datereviewed']").attr("content");

	$monthNames = new Array("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");

	$day = $datereviewed.substr(6,2);
	var monthNum = $datereviewed.substr(4,2);
	$month = Number(monthNum - 1);
	$year = $datereviewed.substr(0,4);
	$footerdate = ($monthNames[$month] + " " + $day + ", " + $year);

	$('#sub-footer .small-print').html("Last reviewed/updated " + $footerdate + ".");
});
</script> 
