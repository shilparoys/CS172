  <script type="text/javascript">
  var griffith = {"pers": {"flags": {}, "form": {}, "sessionflags": {}}, "fn": {}};
  var z;
  /* Determine width */
  griffith.fn.retrieveWidthIndex = function(){
    element = document.getElementById('responsive-index');
    if (typeof(lteIE8) === "undefined"){
      if (typeof(window.getComputedStyle) === "undefined"){
        z = parseInt(element.currentStyle['z-index']);
      }else{
        style = window.getComputedStyle(element, "");
        z = parseInt(style.getPropertyValue('z-index'));
      }
    }else{
      // ie8 or lower so set to desktop only
      z = 3;
    }    
    return z;
  };   
  griffith.fn.determineInitialWidth = function(){
    griffith.pers.flags.activeWidthIndex = griffith.fn.retrieveWidthIndex();
    griffith.pers.flags.widthIndex = griffith.fn.retrieveWidthIndex();
  }
  griffith.fn.determineInitialWidth();
  console.log(griffith);
  console.log(griffith.pers.flags.widthIndex);
</script> 
