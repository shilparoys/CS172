      <div id="block-views-Sponsor-block_1" class="clear-block block block-views"> 
       <h2>2015 Library Sponsors</h2> 
       <div class="content">
        <div class="view view-Sponsor view-id-Sponsor view-display-id-block_1 sponsors view-dom-id-1"> 
         <div class="view-content"> 
          <div id="views_slideshow_singleframe_main_Sponsor-block_1" class="views_slideshow_singleframe_main views_slideshow_main">
           <div id="views_slideshow_singleframe_teaser_section_Sponsor-block_1" class="views_slideshow_singleframe_teaser_section">
            <div class="views_slideshow_singleframe_slide views_slideshow_slide views-row-1 views-row-odd" id="views_slideshow_singleframe_div_Sponsor-block_1_0">
             <div class="views-row views-row-0 views-row-first views-row-odd"> 
              <div class="views-field-field-sponsor-type-value"> 
               <span class="field-content"><h3>Copper Level</h3></span> 
              </div> 
              <div class="views-field-field-image-field-fid"> 
               <span class="field-content"><a href="http://www.foodsafetysanitation.com"><img src="http://www.haverfordlibrary.org/sites/default/files/imagecache/news_small/images_news/food-safety-sanitation.png" alt="Food Safety Sanitation" title="Food Safety Sanitation" width="136" height="61" class="imagecache imagecache-news_small imagecache-default imagecache-news_small_default"></a></span> 
              </div> 
             </div> 
            </div> 
            <div class="views_slideshow_singleframe_slide views_slideshow_slide views-row-2 views_slideshow_singleframe_hidden views-row-even" id="views_slideshow_singleframe_div_Sponsor-block_1_1">
             <div class="views-row views-row-0 views-row-first views-row-odd"> 
              <div class="views-field-field-sponsor-type-value"> 
               <span class="field-content"><h3>Bronze Level</h3></span> 
              </div> 
              <div class="views-field-field-image-field-fid"> 
               <span class="field-content"><a href="http://www.holychildacademy.com/"><img src="http://www.haverfordlibrary.org/sites/default/files/imagecache/news_small/images_news/Holy%20Child.jpg" alt="Holy Child Academy" title="Holy Child Academy" width="136" height="75" class="imagecache imagecache-news_small imagecache-default imagecache-news_small_default"></a></span> 
              </div> 
             </div> 
            </div> 
            <div class="views_slideshow_singleframe_slide views_slideshow_slide views-row-3 views_slideshow_singleframe_hidden views-row-odd" id="views_slideshow_singleframe_div_Sponsor-block_1_2">
