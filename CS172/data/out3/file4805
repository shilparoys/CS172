  if (typeof e == 'undefined' && window.event) { e = window.event; }
  if (e.keyCode == 13) {
  document.getElementById('searchButton').click();
  }
  return false;
  }*/

/* A-Z Databases */
  function AZDatabases() {
      var db = document.getElementById("scope_03").value;
      db += document.getElementById("keyword_04").value;
      window.open(db, "_self");
      return false;
  }

  function DBkey(ev) {
      var keynum;
