        <!-- FILTER BOX --> 
        <script type="text/javascript">
    $(document).ready(function () {
        $("#global_search_input_container").hoverIntent({
            interval: 0,
            over: revealFilter,
            timeout: 2000,
            out: hideFilter
        });

        /* FADE IN/OUT */
        function revealFilter() {
            $("#global_search_form").addClass("filter_active");
            $("#filter").fadeTo("fast", 1.0);
        }

        function hideFilter() {
            $("#global_search_form").removeClass("filter_active");
            $("#filter").fadeTo("fast", 0.0)
        }
    });
</script> 
