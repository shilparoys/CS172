/* Set the listbox based on the dropdown */
  function SetMultiSelect() {
      var dd = document.getElementById("scope_01a");
      var cat = dd.options[dd.selectedIndex].text;
      var lb = document.getElementById("scope_01b");
      RemoveCatalogs(lb);
      var opt = new Option("Select Category...", "", false, false);
      lb.options[0] = opt;
      if (dd.selectedIndex == 0) {
          return;
      }

      var lst = getCatalogs(cat);
      for (var i = 0; i < lst.length; i++) {
          opt = new Option(lst[i].name, lst[i].id, false, false);
          lb.options[i + 1] = opt;
      }
  }


/* Remove catalogs from the listbox */
  function RemoveCatalogs(lb) {
    var siz = lb.length;
    for (var i = 0; i < siz; i++) {
      lb.remove(0);
    }
  }


