              <!-- END TABS CONTAINER --> 
              <script type="text/javascript">
    $(document).ready(function () {

        // HIDE ALL CONTENT PANES	
        $(".lspane").hide();

        // ACTIVATE FIRST TAB
        $("ul.tabs li:first").addClass("active").show();

        // SHOW FIRST TAB PANE CONTENT
        $(".lspane:first").show();


        // CLICK EVENT FOR TABS
        $("ul.tabs li").click(function (e) {

            // PREVENT DEFAULT LINK ACTION
            e.preventDefault();

            // REMOVE ACTIVE CLASS FROM ALL ITEMS
            $("ul.tabs li").removeClass("active");

            // ADD ACTIVE CLASS TO CLICKED LI ONLY
            $(this).addClass("active");

            // HIDE ALL CONTENT PANES
            $(".lspane").hide();


            // GET THE HREF ATTR OF THE ANCHOR IN THIS LI TO IDENTIFY CONTENT
            var activeTab = $(this).find("a").attr("href");

            // NOW FADE IN THE ACTIVE TAB CONTENT
            $(activeTab).fadeIn('slow');

        });

    });
</script> 
              <!-- FORM HINTS --> 
              <script type="text/javascript">
    $(document).ready(function () {
        $('.hinter')
        .each(function () {
            $(this).attr("hintText", $(this).val());
        })
        .focus(function () {
            if ($(this).val() == $(this).attr("hintText"))
                $(this).val('');
        })
        .blur(function () {
            if ($(this).val() == "")
                $(this).val($(this).attr("hintText"));
        });

    });
</script> 
              <script src="http://supportforms.epnet.com/eit/scripts/ebscohostsearch.js" type="text/javascript"></script> 
              <script type="text/javascript">
/* Encore Search */
  function EncoreSearch() {
    var kw1 = document.getElementById("keyword_01").value;
    return postwith("http://encore.uri.edu/iii/encore/search?formids=target&lang=eng&suite=def&reservedids=lang%2Csuite&submitmode=&submitname=&I5.x=10&I5.y=13", { target: kw1 }, "POST");
  }


  /* Encore submit function */
  function postwith(to, p, m) {
      var myForm = document.createElement("form");
      myForm.method = m;
      myForm.action = to;
      if (p != null) {
          for (var k in p) {
              var myInput = document.createElement("input");
              myInput.setAttribute("name", k);
              myInput.setAttribute("value", p[k]);
              myForm.appendChild(myInput);
          }
      }
      document.body.appendChild(myForm);
      myForm.submit();
      document.body.removeChild(myForm);
      return false;
  }


/* Integrated Research */
  function IntegratedResearch() {

      var dtext = '<div style="display:none;"><form id="tmpFrm" action="">';

      var kw2 = document.getElementById("keyword_02").value;
      dtext += '<input type="text" name="ebscohostsearchtext" value="' + kw2 + '" />';

      dtext += '<input type="hidden" name="ebscohostwindow" value="1" />';
      dtext += '<input type="hidden" name="ebscohosturl" value="http://search.ebscohost.com/login.aspx?direct=true&site=ehost-live&scope=site&type=1&authtype=ip,cpid&custid=s8475809" />';
      dtext += '<input type="hidden" name="ebscohostsearchsrc" value="db" />';
      dtext += '<input type="hidden" name="ebscohostsearchmode" value="+" />';

      var cats = document.getElementById("scope_01b");
      var cb = 1;
      for (var i = 0; i < cats.length; i++) {
          var ckbox = '<input type="checkbox" name="cbs" id="cb' + cb + '" value="' + cats.options[i].value + '" ';
          if (cats.options[i].selected) {
              ckbox += 'checked="checked" ';
          }
          ckbox += '/>';
          dtext += ckbox;
          cb++;
      }

      dtext += '</form></div>';

      var w = window.open();
      var d = w.document;
      d.open();
      d.write(dtext);
      d.close();

      var myForm = d.getElementById('tmpFrm');
      var ret = ebscoHostSearchGo(myForm);
      w.close();
      return ret;
  }


  function FFkey(ev) {
      var keynum;
      var keychar;

      if (window.event) {
          keynum = ev.keyCode;
      }
      else if (ev.which) {
          keynum = ev.which;
      }

      keychar = String.fromCharCode(keynum);
      if (keychar == "\r") {
          return IntegratedResearch();
      }
      return true;
  }


/* Research Guides */
  function ResearchGuides() {
      //var guid = document.getElementById("scope_02").value;
      var guid = "http://providence.libguides.com/search.php?iid=414&c=0&pid=&search_field=&search="; //gubata changed to eliminate search scope option
      guid += document.getElementById("keyword_03").value;
      window.open(guid, "_self");
      return false;
  }


  function RGkey(ev) {
    var keynum;
    var keychar;


    if (window.event) {
        keynum = ev.keyCode;
    }
    else if (ev.which) {
        keynum = ev.which;
    }

    keychar = String.fromCharCode(keynum);
    if (keychar == "\r") {
        return ResearchGuides();
    }

    return true;
  }

  /* New Journal Finder */

  function NewJournalFinder() {
      var guid = "https://atoz.ebsco.com/titles/searchresults/2812?Find=";
      guid += document.getElementById("keyword_04").value;
      guid += "&GetResourcesBy=";
      var resourcetype = [];
      var els = document.getElementsByName("GetResourcesBy");
      for (var i = 0; i < els.length; i++) {
          if (els[i].checked) {
              resourcetype.push(els[i].value);
          }
      }
      guid += resourcetype + "&SearchType="; //removed resourcetype + from before searchtype
      var searchestype = [];
      var element = document.getElementsByName("SearchType");
      for (var i = 0; i < element.length; i++) {
          if (element[i].checked) {
              searchestype.push(element[i].value);
          }
      }
      guid += searchestype;
      window.open(guid, "_self");

      
      return false;
  }
  /*so that the enter key works for journal guides */
  function JSkey(ev) {
  var keynum1;
  var keychar1;


  if (window.event) {
  keynum1 = ev.keyCode;
  }
  else if (ev.which) {
  keynum1 = ev.which;
  }

  keychar1 = String.fromCharCode(keynum1);
  if (keychar1 == "\r") {
  return NewJournalFinder();
  }

  return true;
  }


  /*so that the enter key works for journal guides 
  function searchKeyPress(e) {
