				newWindow.document.write('<title>Times Health Guide</title>');
				newWindow.document.write(cssString);
				newWindow.document.write('</he'+'ad>');
				newWindow.document.write('<body>'+htmlContent+'</body>');
				newWindow.document.write('</html>');
				newWindow.document.close();
				
            }   
        }
