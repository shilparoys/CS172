  <script type="text/javascript">

var currentFile = ""; 
var currentItem = 0; 
var previousItem = -1; 

var player = null;
function playerReady(thePlayer) {
	player = window.document[thePlayer.id];
	addListeners();
}


function addListeners() {
	if (player) { 
		player.addControllerListener("ITEM", "itemListener"); 
	} else {
		setTimeout("addListeners()",100);
	}
}


function itemListener(obj) { 
	if (obj.index != currentItem) {
 		previousItem = currentItem;
		currentItem = obj.index;
	}
}
var popWin = null;
function createWindow(theUrl,w,h) {   
	if (player) { 
		player.sendEvent("STOP"); 
		deletePlayer('wrapper', 'placeholder1', 'player1');
	}

	document.getElementById("buttonId1").style.visibility = "hidden";
 	var winl = (screen.width-w)/2;
	var wint = (screen.height-h)/2;
	
	settings='height='+h+',width='+w+',top=250,left='+winl+',outerWidth=='+w+',toolbar=no,scrollbars=no,directories=0, statusbar=no,menubar=0,resizable=no,location=0,status=0';
	popWin = window.open(theUrl, "smallWin",settings);
	popWin.focus();
}


function popWinPlay() {
	if ((popWin)&&(! popWin.closed)) { 
		popWin.player.sendEvent('PLAY'); 
	} else { 
		createWindow('/governor/assets/script/mediaplayer/welcome_win.html?'+currentItem); 
	}
}


function initPlayer() {
	document.getElementById("buttonId1").style.visibility = "visible";
	
}

$(document).ready(function() {
	
$("ul#topnav li").click(function() { //Hover over event on list item
	$(this).css({ 'background' : '#4f678e url(/governor/assets/slices/top_nav_active.gif) repeat-x'}); //Add background color + image on hovered list item
	$(this).find("span").show(); //Show the subnav
} , function() { //on hover out...
	$(this).css({ 'background' : 'none'}); //Remove the background
	$(this).find("span").hide(); //Hide the subnav
});
	
});
</script> 
