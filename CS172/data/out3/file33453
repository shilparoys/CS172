function mediaDropDownHandler(objectID){
    var URL = $('#' + objectID).val();
    if(URL != "") {
        newWin = window.open(URL, "newWin");
    }
} // end function

function formHandler(form){
var URL = document.form.site.options[document.form.site.selectedIndex].value;
  newWin = window.open(URL, "newWin");
} // end function
