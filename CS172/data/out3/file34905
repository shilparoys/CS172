	function startTimer(cartIndex, setTimerID) {
		var exp = "document.cartListForm.countdown" + cartIndex + ".value = " +
							"cartList["+cartIndex+"].timer.display(" + cartIndex + ");";
		if(setTimerID)
			timerID = setInterval(exp, 1000);
		else
			setInterval(exp, 1000);
