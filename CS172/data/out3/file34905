	}

	function printAccountInfo()
	{
		document.write(
			'	<tr>',
			'		<th>Name</th>',
			'		<td>' ,shopperInfo.first, ' ',shopperInfo.last, '</td>',
			'	</tr>',
			'	<tr>',
			'		<th>Account Number:</th>' ,
			'		<td>', shopperInfo.account, '</td>',
			'	</tr>',
			'	<tr>',
			'		<th>Email:</th>' ,
			'		<td>', shopperInfo.email, '</td>',
			'	</tr>'
		);
	}

	function printBillingAddr(type)
	{
		for (var j=0; j<addressList_.length; j++){
			if (addressList_[j].addr_type==type) {
				document.write(
					'	<tr>',
					'		<th>', (type=="tb" ? 'Billing': 'Mailing') , ' Address:</th>',
					'		<td>'
				);	
				
				for(var i=0; i < addrOrder.length; i++) {
					var curAddr = eval("addressList_["+j+"].addr"+addrOrder[i].number);
					if(curAddr != "") {
						document.write(
							curAddr, '<br>'
						);
					}
				}
				
				document.write(
					addressList_[j].city, ', ', addressList_[j].state, ' ', addressList_[j].zip, '<br>',
					addressList_[j].country
				);
				
				document.write(
					'		</td>',
					'	</tr>'
				);
			}	
		}	
	}
	
	function printPhone()
	{
		for (var j=0; j<phoneList_.length; j++){
			if (phoneList_[j].phone_type=="td"){
				document.write(
					'	<tr>',
					'		<th>Phone:</th>',
					'		<td>', phoneList_[j].number, '</td>',
					'	</tr>'
				);
			}
		}
	}
	
