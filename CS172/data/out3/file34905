function createCookie(name,value,days) {
	if (days) {
		var date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		var expires = "; expires="+date.toGMTString();
	}
	else var expires = "";
	document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

function eraseCookie(name) {
	createCookie(name,0,-1);
}

var desktopLinkID = ("usc").replace(/^m-/, '');
var mobileLinkID = "m-" + desktopLinkID;

// mobile code case 363228 (for deeplinks etc.)
// returns mobile link
function getMobileLink(url) {
	// do not convert certain conditions
	if(
		url.indexOf("linkID="+desktopLinkID)==-1 ||
		url.indexOf("linkID="+mobileLinkID)>-1 ||
		url.indexOf("SEGetPackageInfo?")>-1 ||
		url.indexOf("fromMobile=1")>-1
	) {
		return url;
	}
	
	// update linkID
	eval('url = url.replace(/linkID='+desktopLinkID+'/g, "linkID='+mobileLinkID+'")');
	
	// updated post url
	if(url.indexOf("&url=")>-1) {
		var postUrl = url.split("&url=");
		var postUrl2 = postUrl[1];
		
		postUrl2 = postUrl2.split("&");
		postUrl2[0] = escape(getMobileLink(unescape(postUrl2[0])));
		
		postUrl[1] = postUrl2.join("&");
		url = postUrl.join("&url=");
	}
	
	// convert ticketcode if displayeventinfo
	if(url.indexOf("SEGetEventInfo?")>-1 && url.indexOf("ticketCode=")>-1) {
		var qs = url.split("ticketCode=");
		qs[1] = qs[1].split("&");
		var t_eventCode = unescape(qs[1][0]).split(":");
		if(t_eventCode[1].indexOf("!M")==-1) {
			t_eventCode[1] += "!M";
			qs[1][0] = t_eventCode.join(":");
			qs[1] = qs[1].join("&");
			url = qs.join("ticketCode=");
		}
	}
	
	return url;
}
// end mb case 363228

function gotoMobile() {
	if(mobileURL!='') {
		eraseCookie("desktopView");
		location.href = mobileURL;
	}
}

