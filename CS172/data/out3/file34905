            </div>  <script>
function changeCal(cal) {
	var calnav = document.getElementById('cal-nav');
		document.getElementById("cal-weekly").style.display = (cal == 'weekly') ? "block" : "none";
		document.getElementById("cal-daily").style.display = (cal == 'daily') ? "block" : "none";
		calnav.className = "cal-" + cal;
}

var _calIsExpanded = false;

function expandCal() {
	var _calButton = document.getElementById('expandCalButton');
	var _calContent = document.getElementById('expandedCalendar');
	if (_calIsExpanded) {
		_calContent.style.display = "none";
		_calIsExpanded = false;
		_calButton.className = '';
	} else {
		_calContent.style.display = "block";
		_calIsExpanded = true;
		_calButton.className = 'nowExpanded';
	}
}
</script> 
