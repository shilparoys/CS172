		function PhoneInfo(phone_type,number) 
		{
			this.phone_type	= phone_type;
			this.number	    = number;
		}
		
		function getPhoneInfoByType(type, phoneList)
		{
			for(var i = 0 ; i < phoneList.length ; i++)
			{
				if(phoneList[i].phone_type == type) 
					return phoneList[i];
			}
			return "";
		}
		
		function getAddrInfoByType(type, addrList)
		{
			for(var i = 0 ; i < addrList.length ; i++)
			{
				if(addrList[i].addr_type == type) 
					return addrList[i];
			}
			return "";
		}
		
		function getBillAddrCount(addrList)
		{
			var counter = 0;
			for(var i = 0 ; i < addrList.length ; i++)
			{
				if(addrList[i].addr_type == "tb") 
					counter++;
				if(addrList[i].addr_type == "fb") 
					counter++;
			}
			return counter;
		}
		
		function getDayPhoneCount(phoneList)
		{
			var counter = 0;
			for(var i = 0 ; i < phoneList.length ; i++)
			{
				if(phoneList[i].phone_type == 'td') 
					counter++;
				if(phoneList[i].phone_type == 'fd') 
					counter++;
			}
			return counter;
		}
		
		function getTicketingPhoneCount(phoneTypeList)
		{
			var counter = 0;
			for(var i = 0 ; i < phoneTypeList.length ; i++)
			{
				if(phoneTypeList[i] == "td" || phoneTypeList[i] == "te") 
					counter++;
			}
			return counter;
		}

		function getFundPhoneCount(phoneTypeList)
		{
			var counter = 0;
			for(var i = 0 ; i < phoneTypeList.length ; i++)
			{
				if(phoneTypeList[i] == "fd" || phoneTypeList[i] == "fe") 
					counter++;
			}
			return counter;
		}

		function getEmailPrefByID(id,emailPrefList)
		{
			for(var i = 0 ; i < emailPrefList.length ; i++)
			{
				if(emailPrefList[i].id == id) 
					return emailPrefList[i];
			}
			return "";
		}

