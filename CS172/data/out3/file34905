	if(hasProperty("reg.integratedPatron")) {
		IPFlag = getPropertyValue("reg.integratedPatron");
	}

	function emailValidateMyAccount(email) {

		var str = email.value; // email string

		var reg1 = /(@.*@)|(\.\.)|(@\.)|(\.@)|(^\.)/; // not valid
		var reg2 = /^.+\@(\[?)[a-zA-Z0-9\-\.]+\.([a-zA-Z]{2,4}|[0-9]{1,4})(\]?)$/; // valid

		if(IPFlag == "0")
		{
			if( isNaN(parseInt(str)) == false )
				return true;
		}

		if (!reg1.test(str) && reg2.test(str)) { // if syntax is valid
			return true;
		}
		window.status = "Please enter a valid Internet e-mail address";

		email.focus();
		email.select();
		return false;
	}

	function formValidate(frm) {
		if(emailValidateMyAccount(frm.email) == false && accntValidate(frm.email) == false) {
			alert("Please enter a valid Internet e-mail address or account number");
			return false;
		}
		if(passwordValidate(frm.password) == false) {
			if(IPFlag)
				alert("Please enter a password between 6 and 15 characters in length");
			else
				alert("Please enter a password between 4 and 12 characters in length");
			return false;
		}
		return true;
	}

	function goOn(action) {
		if(action == "REGISTER") {
				window.location = "/cgi-bin/ncommerce3/EVExecMacro?linkID=usc&evm=regr&account=&pin=&addrReq=&phoneReq=&RSRC=&RDAT=&url=" + escape("/cgi-bin/ncommerce3/SEGetGroupList?linkID=usc&groupCode=&RSRC=&RDAT=&shopperContext=ST");
		} else if(action == "FORGOT") {
			window.location = "/cgi-bin/ncommerce3/EVExecMacro?linkID=usc&evm=regf&addrReq=&phoneReq=&RSRC=&RDAT=&linkSource=&shopperContext=ST";  // 7.10.100 temporary fix
		} else if(action == "OPTIONAL") {
			window.location = "/cgi-bin/ncommerce3/SEGetGroupList?linkID=usc&groupCode=&RSRC=&RDAT=&shopperContext=ST";
		} else if(action == "ACTIVATE") {
			window.location = "/cgi-bin/ncommerce3/EVExecMacro?linkID=usc&evm=rega&returnURL=" + escape("/cgi-bin/ncommerce3/SEGetGroupList?linkID=usc&groupCode=&RSRC=&RDAT=&shopperContext=ST") + "&shopperContext=ST&RSRC=&RDAT=&url=" + escape("/cgi-bin/ncommerce3/SEGetGroupList?linkID=usc&groupCode=&RSRC=&RDAT=&shopperContext=ST");
		} else {
			return false;
		}
	}
	
	function displayWait(action){
		document.open();
		document.writeln('<html><head><title>Please Wait</title>');
		document.writeln('<link rel="stylesheet" type="text/css" href="/evenue/linkID=usc/stylesheets/style.css">');
		document.writeln("<s"+"cript type='javascript'>");
		document.writeln("image1 = new Image();");
		document.writeln("image1.src = '/evenue/linkID=usc/images/waitani.gif';");
		document.writeln("image2 = new Image();");
		document.writeln("image2.src = '/evenue/linkID=usc/stylesheets/style.css';");
		document.writeln("</s"+"cript>");
		document.writeln('</head><body class="nobg">');
		document.writeln('<table cellpadding="25" cellspacing="0" align="center"><tr><td align="center" id="pleasewait">');
		document.writeln('<p><img src="/evenue/linkID=usc/images/waitani.gif" border="0" alt="Please wait..."></p>');
		document.writeln('<h2><marquee width="250px">Retrieving information.  Please wait.</marquee></h2>');
		document.writeln('<p>This process may take up to 3 minutes.<br>Please do not use the back or refresh buttons.</p>');
		document.writeln('</td></tr></table>');
		document.writeln('</body></html>');
		document.close();

		if(action == "REISSUE") {
			window.location = "/cgi-bin/ncommerce3/PAHReissue?linkID=usc&reissue=0";
		} else if(action == "TRF_RET") {
			window.location = "/cgi-bin/ncommerce3/GetUserTktItemList?linkID=usc&trfCancel=0";
		} else if(action == "TRF_CAN") {	
			window.location = "/cgi-bin/ncommerce3/GetUserTktItemList?linkID=usc&trfCancel=1";
		} else if(action == "OH") {
			window.location = "/cgi-bin/ncommerce3/OrderHistory?linkID=usc&ohref=main";
		} else if(action == "DH") {
			window.location = "/cgi-bin/ncommerce3/DonationHistory?linkID=usc";
		} else if(action == "PP") {
			window.location = "/cgi-bin/ncommerce3/PriorityPoints?linkID=usc";
		} else {
			return false;
		}
	}

