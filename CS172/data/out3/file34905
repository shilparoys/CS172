  <script language="JavaScript">
		<!--
			var returnURL = window.location.host + window.location.pathname + window.location.hash + window.location.search;

			if (window.location.pathname.indexOf("OrderWait.d2w") != -1)
			{
				returnURL = window.location.host + "/cgi-bin/ncommerce3/SEGetEventList" + window.location.hash + window.location.search;
			}
			else
			{
				returnURL = window.location.host + window.location.pathname + window.location.hash + window.location.search;
			}
			
			var httpURL   = escape("http://" + returnURL);
			var httpsURL  = escape("https://" + returnURL);

			var common = new Object;
			common.home = "/evenue/linkID=usc/se/";
			common.help = "http://robohelp.paciolan.com/robo/projects/evenue/evenue.htm";
			common.main = "/cgi-bin/ncommerce3/EVExecMacro";
			common.se = "/" + "cgi-bin/ncommerce3/SEGetGroupList";
			common.dp = "/" + "cgi-bin/ncommerce3/DPLanding";
			common.sr = "/" + "cgi-bin/ncommerce3/GetApps";
			common.login = "/cgi-bin/ncommerce3/EVExecMacro";
			common.logout = "/cgi-bin/ncommerce3/REGLogoff";
			common.myAccount = "/cgi-bin/ncommerce3/EVExecMacro";
			common.cart = "/" + "cgi-bin/ncommerce3/SECartRedisplay";
			common.pageName = "MyAccount.html";
			common.hasCart = cart.number != "" && cart.expire > 0 && cart.status != "C" && cart.isPK != "1";
			common.returnURL = escape(window.location);
			common.isStudent = ("0" == "1");
						
			common.presale = "/" + "cgi-bin/ncommerce3/PPUpdateSysConfig" + "?linkID=usc&RSRC=&RDAT=&caller=PS&failureURL=" + httpURL;
			common.promo = "/cgi-bin/ncommerce3/EVExecMacro";
			common.accessibility = "/cgi-bin/ncommerce3/EVExecMacro";

			common.gc = "/cgi-bin/ncommerce3/SEGetGroupList";

			common.search = "/cgi-bin/ncommerce3/EVExecMacro";

				common.loggedIn = false;

			function confirmLogOut()
			{
				if (!common.hasCart) return true;
				return confirm( "You have reservations in your shopping cart." +
						" If you logout, these reservations will be released. Do you really want to logout?");
			}

			function setNavigateReturnURL(url) {
				pos = url.indexOf("://");
				if(pos >= 0)
				{
					// remove leading http(s)://
					url = url.substr(pos+3);
				}
				if(url.indexOf("/") == 0)
				{
					httpURL   = escape(url);
					httpsURL  = escape(url);
				} else {
					httpURL   = escape("http://" + url);
					httpsURL  = escape("https://" + url);
				}
				common.returnURL = escape(url);
			}

			function splitURL(returnURL)
			{	
				if(returnURL.indexOf(escape(escape("&shopperContext=ST"))) == -1)
				{
					var splittedURL = returnURL.split(escape("url="));
					if(splittedURL.length > 1)
					{
						var newURL ="";
						for(var i=1;i<splittedURL.length ;i++ ) newURL+=splittedURL[i];
						return unescape(newURL);
					}
					else 
						return returnURL;
				}
				else
				{
					return escape(common.login);
				}
			}			


			function createNavigateHref(action) {
				navHref = "";

				if(action == "HOME")
				{
					navHref = common.home;
				}
				else if(action == "MAIN")
				{
					navHref = common.main + "?linkID=usc&evm=main";
				}
				else if(action == "SE")
				{
					navHref = common.se + "?linkID=usc&groupCode=&RSRC=&RDAT=&url=" + httpURL;
				}
				else if(action == "DP")
				{
					//Donation Processing
					navHref = common.dp + "?linkID=usc&RSRC=&RDAT=&url=" + httpsURL;
				}
				else if(action == "SS")
				{
					//Student Sales 
					navHref = common.se + "?linkID=usc&groupCode=&RSRC=&RDAT=&url=" + httpURL + "&shopperContext=ST";
				}				
				else if(action == "SR")
				{
					if(regAdminInfo.loginMode != "D" && "usc" != "")
					{
						navHref = common.sr + "?linkID=usc&RSRC=&RDAT=&linkSource=SR&caller=appList";
					}
				}
				else if(action == "ACCOUNT")
				{
					if(regAdminInfo.loginMode != "D" && common.loggedIn)
					{
						navHref = common.myAccount + "?linkID=usc&evm=myac&RSRC=&RDAT=&url=" + httpsURL;
					}
				}
				else if(action == "CART")
				{
					if(common.hasCart)
					{
						navHref = common.cart + "?linkID=usc&RSRC=&RDAT=&cartNum=" + cart.number;
					}
				}
				else if(action == "LOGIN")
				{ 
					if(regAdminInfo.loginMode != "D" && ! common.loggedIn)
					{
						URL_ = unescape(splitURL(httpsURL));
						if(URL_.indexOf("?") == -1)
						{
							URL_ = "https://" + window.location.host + common.main;
						}
						navHref = common.login + "?linkID=usc&evm=myac&entry=" + common.pageName + "&url=" + escape(URL_);
						if(common.pageName == "CartDisplay.html" || common.pageName == "OrderConfirmation.html")
						{
							navHref += "&RSRC=&RDAT=&cartNum=" + cart.number;
						}
					}
					}
				else if(action == "LOGOUT")
				{ 
					if(regAdminInfo.loginMode != "D" && common.loggedIn)
					{
						navHref = common.logout + "?linkID=usc&RSRC=&RDAT=&url=" + "http://" + window.location.host + escape("/cgi-bin/ncommerce3/EVExecMacro?linkID=usc&evm=regl");
					}
				}
				else if(action == "PROMO")
				{ 
					navHref = common.promo + "?linkID=usc&evm=prmo&RSRC=&RDAT=&caller=PR";
				}
				else if(action == "PRESALE")
				{
					//Go to the page where the user inputs the promo code
					navHref = common.presale;
				}
				else if(action == "ACCESSIBILITY")
				{
					//Go to the page where the user inputs the accessible seating request info
					navHref = common.accessibility + "?linkID=usc&evm=seac&url=" + httpURL;
				}				
				else if(action == "GC")
				{
					if(common.isStudent)
					{
						navHref = common.gc + "?linkID=usc&groupCode=&RSRC=&RDAT=&caller=GC&shopperContext=ST&url=" + httpURL;
					}
					else
					{
						navHref = common.gc + "?linkID=usc&groupCode=&RSRC=&RDAT=&caller=GC&url=" + httpURL;
					}
				}
				else if(action == "SEARCH")
				{
					navHref = common.search + "?linkID=usc&evm=setx&RSRC=&RDAT=" + ((common.isStudent) ? "&shopperContext=ST" : "");
				}
				else if(action == "HELP")
				{
					navHref = common.help;
				}

				return navHref;
			}
		//-->
		</script> 
