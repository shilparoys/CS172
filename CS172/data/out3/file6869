jQuery(document).ready(function($){
  jQuery("#init_1").mouseenter(function(){
    jQuery('#vanacms-widget-switcher-wrap').show();
    jQuery('#vanacms-support-wrap').hide();
    jQuery('#vanacms-community-wrap').hide();
  });
  jQuery("#init_2").mouseenter(function(){
    jQuery('#vanacms-widget-switcher-wrap').hide();
    jQuery('#vanacms-community-wrap').hide();
    jQuery('#vanacms-support-wrap').show();
  });
  jQuery("#init_3").mouseenter(function(e){
    jQuery('#vanacms-widget-switcher-wrap').hide();
    jQuery('#vanacms-support-wrap').hide();
    jQuery('#vanacms-community-wrap').show();
  });
  jQuery('#vanacms-widget').mouseleave(function(e){
    jQuery('#vanacms-widget-switcher-wrap').hide();
    jQuery('#vanacms-support-wrap').hide();
    jQuery('#vanacms-community-wrap').hide();
  });

  jQuery(".support-list img").hover(function(){
      jQuery('#share_desc').html($(this).attr('alt'));
  }, function() {
      jQuery('#share_desc').html('&nbsp;');        
  });
});
function increaseFont(){
  jQuery('p,h1,h2,h3,h4,h5,td,li,a').each(function(){
  var oldf = parseInt( jQuery(this).css('font-size') );
  var oldl = parseInt( jQuery(this).css('line-height') );
  jQuery(this).css({
  'font-size' : oldf + 2 + 'px',
  'line-height' : oldl + 2 + 'px'
  });
  });
}
function decreaseFont(){
  jQuery('p,h1,h2,h3,h4,h5,td,li,a').each(function(){
  var oldf = parseFloat( jQuery(this).css('font-size') );
  var oldl = parseFloat( jQuery(this).css('line-height') );
  jQuery(this).css({
  'font-size' : oldf - 2 + 'px',
  'line-height' : oldl - 2 + 'px'
  });
  });
}
</script>  
