  <script type="text/javascript">
        $(document).ready(function(){
			$("#nav ul li").hover(function(){
				$(this).addClass("hover");
				$(".searchSelect").addClass("invisible");
			}, function(){
				$(this).removeClass("hover");
				$(".searchSelect").removeClass("invisible");
			});
			$("div.searchSection:eq(0)").show();
			$("ul#searchNav a:first").addClass("current");
			$("ul#searchNav a").click(function(){
				$("ul#searchNav a").removeClass("current");
				$(this).addClass("current");
				$("div.searchSection").hide();
				var showsearch = $(this).attr("href");
				$(showsearch).show();
				return false;
			});
			$("div.exhibit").hide();
			var promocount = $("div.exhibit").length;
			var randomnumber=Math.floor(Math.random()*promocount);
			$("div.exhibit:eq(" + randomnumber +")").show();
			var importanttext = $("#important .content").text();
			if (importanttext.length > 10) {
				$("#important").show();
			}
			
		});
        </script>
  <script language="JavaScript" type="text/JavaScript">

function pageInit() {
	document.catalogsearch.searchtype.selectedIndex=0;
 	document.forms[0].elements[1].focus();
	var catalogform = document.catalogsearch;
 	if (catalogform) {
    	change_action('catalogsearch', catalogform.selectedIndex);
  }
}
window.onload = pageInit;

</script>
