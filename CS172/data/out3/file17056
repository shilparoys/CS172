  <script language="javascript">
function setCookie(c_name, value) {                
	var sessExpire ="";
	var domain = "comcast.com";
	var c_value = escape(value) + "; expires=" + sessExpire + ( ( domain ) ? ";domain=" + domain : "" );
    document.cookie = c_name + "=" + c_value;
    }
            
function getQuerystring(key, default_) {
	if (default_ == null) default_ = "";
    key = key.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
    var regex = new RegExp("[\\?&]" + key + "=([^&#]*)");
    var qs = regex.exec(window.location.href);
    if (qs == null)
    return default_;
    else
    return qs[1];
    }           
					
	var mobileSite = getQuerystring("v");
	if (mobileSite != null && mobileSite != "") {
	setCookie("MobileSite", mobileSite);
	}

</script> 
