    window.addEvent('domready', function() {    
		if(document.getElementById('icemegamenu')!= null)
		var myMenu = new MenuMatic({id:'icemegamenu',
			effect:'slide & fade',
			duration:600,
			physics:Fx.Transitions.Pow.easeOut,
			hideDelay:1000,
			opacity:95                                            
		});
    });
</script> 
