         <script type="text/javascript">
	Drupal.behaviors.siteMigrationList = function(context) {
		if(context!=='[object HTMLDocument]'){ /*if page load*/
		$('#block-views-site_migration_list-block_3').prepend('<a href="#" title="Click to Close this Box. If you are logged in you may also permanently hide this box. To do so,  visit your account and edit your preferences; unchecking the \'View this Page in the Archived HR Site\' box." style="float:left; position:relative; width:auto; height:auto; margin-right:10px;" id="siteMigrationOldSiteLinkBox" onclick="return false;"><img src="/sites/all/files/images/close.gif" alt="Close this box" /></a>'); 
		$('#siteMigrationOldSiteLinkBox').click(function(){
			$('#block-views-site_migration_list-block_3').remove();
			if($('#section1').text().trim() == ''){
				$('#section1').remove();
			}
		});
	}//end if page load
}//Drupal.behaviors
</script> 
