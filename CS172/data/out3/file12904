  <script>
	function state_select(state) {
		if (state != 'ca') {
			document.getElementById("plan_ca").style.display	= "none";
			document.getElementById("savings_ca").style.display	= "none";
		}
		if (state != 'or') {
			document.getElementById("plan_or").style.display	= "none";
			document.getElementById("savings_or").style.display	= "none";
		}
		if (state != 'wa') {
			document.getElementById("plan_wa").style.display	= "none";
			document.getElementById("savings_wa").style.display	= "none";
		}
		
		document.getElementById("plan_" + state).style.display = "block";
		document.getElementById("savings_" + state).style.display = "block";
	}
</script> 
