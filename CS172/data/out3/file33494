  <script type="text/javascript">
$(function(){
    var nav_width = $('#main-menu').width();
    
    $('#main-menu').children('li').each(function(){
        var left_edge = $(this).position().left;
        var right_edge = left_edge + $(this).width();
        if((nav_width / 2.0) > left_edge){
            $(this).children('ul').css({'left': 0, 'right': 'auto'});
        } else {
            $(this).children('ul').css({'right': 0, 'left': 'auto'});
        }
    });

    $('#main-menu > li > ul').each(function(){
        var sectionCount = $(this).find('.section').length;
        if(sectionCount > 2){
            $(this).addClass('super');
        }
    });

});
</script>   
