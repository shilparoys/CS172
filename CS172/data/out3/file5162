    <!-- Include firstImpression plugin --> 
    <script type="text/javascript" src="http://beprepared.com/skin/frontend/enterprise/beprepared/js/firstImpression.js"></script> 
    <script type="text/javascript" src="http://beprepared.com/skin/frontend/enterprise/beprepared/js/jquery.popupoverlay.js"></script> 
    <script>
// run popup overlay script only when whole page is loaded
jQuery(document).ready(function() {
	// disable popup overlay if viewed on a mobile device (not satisfied with mobile behavior)
	if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
		document.getElementById("newSlide").style.display = "none";	
	} else {
		// run popup overlay script if cookies are enabled
		if (navigator.cookieEnabled) {
			// run popup overlay
			checkUser();
		} else {
			document.getElementById("newSlide").style.display = "none";	
		}
	}
	// evaluate user based on cookies
	function checkUser() {		
		// check if sailthru_hid cookie exists
		if (jQuery.cookie('sailthru_hid') === null) {			
			// create newUser cookie with imediate expiration if newUser cookie does not exist or is expired & run popup overlay
			if ( firstImpression('newUser', 0.00001) ) {
				popupOverlay();
			// if newUser exists and is not expired, hide popup overlay				
			} else {
				document.getElementById("newSlide").style.display = "none";				
			}
		// if sailthru_hid cookie exists, but newUser cookie does not exist or is expired, create newUser cookie with default expiration (2 years) & run popup overlay			
		} else if (jQuery.cookie('sailthru_hid') && firstImpression('newUser')) {
			popupOverlay();
		// if sailthru_hid cookie and newUser cookie exist and is not expired, hide popup overlay
		} else {
			document.getElementById("newSlide").style.display = "none";
		}
	}
	// popup overlay
	function popupOverlay() {
		// delay popup overlay
		setTimeout(function() {					
			// initialize popup overlay
			jQuery('#newSlide').popup({
				autoopen: true,
				focusdelay: 400,
				outline: false,
				vertical: 'bottom',
				backgroundactive: false,
				background: false,
				blur: false,
				scrolllock: false,
				closeelement: '#newSlideCloseButton',
				// update expiration of newUser cookie when user closes popup overlay (1 day)
				onclose: function() {
					firstImpression('newUser', null);
					firstImpression('newUser', 1);
				}
			});						
		}, 3000);
	}
	// update expiration of newUser cookie when user submits popup overlay form
	jQuery('#newSlideForm').on('submit', function () {
		firstImpression('newUser', null);
		firstImpression('newUser');
		// populate vars[acquisition] with 'sc' url parameter
		urlParam();
		// redirect form submit landing page to page where form was submitted
		newSlideFormRedirect();
	});
	// set form redirect to existing url
	function newSlideFormRedirect() {	
		//var x = document.forms["newSlideForm"]["email"].value;	
		var existingUrl = window.location.protocol + "//" + window.location.host + window.location.pathname;
		//var existingUrl = 'http://beprepared.com/newsletter/subscriber/new/email/' + x; 
		var element = document.getElementById("formRedirect");
		element.value = existingUrl;
	}
	// extract 'sc' url parameter
	function GetUrlValue(VarSearch){
		var Uri = window.location.search.substring(1);
		var SearchString = decodeURIComponent(Uri);
		var VariableArray = SearchString.split('&');
		for(var i = 0; i < VariableArray.length; i++){
			var KeyValuePair = VariableArray[i].split('=');
			if(KeyValuePair[0] == VarSearch){
				return KeyValuePair[1];
			}
		}
	}
	// add 'sc' url parameter to vars[acquisition] (in form)
	function urlParam() {
		var name = document.getElementById("acquisition");
		var fname = GetUrlValue('sc');
		if(typeof fname === 'undefined'){
			name.value = "INTR";
		} else {
			name.value = fname;
		}
	}
});
</script> 
