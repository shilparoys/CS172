      function drawVisualization3() {
        // Create and populate the data table.
        var data = google.visualization.arrayToDataTable([
          ['Source', 'Dollars'],
          ['Contributions, Gifts & Grants', 21652075.0000],
          ['Federated Campaigns', 0.0000],
          ['Membership Dues', 0.0000],
		  ['Fundraising Events', 0.0000],
		  ['Related Organizations', 0.0000],
		  ['Government Grants', 4007013.0000]
        ]);
      
	  	  	var formatter = new google.visualization.NumberFormat({prefix: '$',fractionDigits: 0});
  formatter.format(data, 1); // Apply formatter to second column
	  
        // Create and draw the visualization.
        new google.visualization.PieChart(document.getElementById('chart_revenue')).
            draw(data, {title:"Contributions Breakdown",chartArea:{left:40,top:60,width:"95%",height:"95%"},titleTextStyle:{fontSize: 14},fontName: '"Arial"'});
      
	  }
      
      google.setOnLoadCallback(drawVisualization3);
    </script> 
