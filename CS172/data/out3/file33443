       <script type="text/javascript">

onload=function(){

var rightNow = new Date();

var day = rightNow.getDay();

var hour = rightNow.getUTCHours();

var newDisplay = 'none'; // unless we see otherwise

if(day==4) { // day of scheduled outage 

if((hour>= 7) && (hour<= 12)) { //Time is UTC: 6/11 for standard, 7/12 for DST

newDisplay = 'block';

}

}

document.getElementById('scheduledOutage').style.display = newDisplay;

}

</script> 
