    <script type="text/javascript" charset="utf-8">

$j(document).ready(function() {
  //hide the workphone form row
  $j("#formRow_21").hide();
  var workphone = $j("#form_workphonenumber");

  var homephone_area_code = $j("#form_homephonenumber_area_code");
  var homephone_first_three = $j("#form_homephonenumber_first_three");
  var homephone_last_four = $j("#form_homephonenumber_last_four");
  var homephone = $j("#form_homephonenumber"); // 1 input entry for home phone

  if (homephone.length > 0) {
    $j("#form_homephonenumber_area_code").change(function() { update_workphone(true); });
    $j("#form_homephonenumber_first_three").change(function() { update_workphone(true); });
    $j("#form_homephonenumber_last_four").change(function() { update_workphone(true); });
  } else {
    $j("#form_homephonenumber").change(function() { update_workphone(false); });
  }

  var update_workphone = function(home_split) {
    if (home_split) {
      var new_value = homephone_area_code.attr("value") + homephone_first_three.attr("value") + homephone_last_four.attr("value");
    } else {
      var new_value = homephone.attr("value");
    }
    workphone.attr("value", new_value);
  };
});

</script> 
    <script type="text/javascript" charset="utf-8">

$j(document).ready(function() {
  $j("#formRow_1305").hide();
  var age_select = $j("#form_yearofbirth");

  var dob_month = $j("#form_dob_month");
  var dob_day = $j("#form_dob_day");
  var dob_year = $j("#form_dob_year");

  $j("#form_dob_month").change(function() { update_age_select(); });
  $j("#form_dob_day").change(function() { update_age_select(); });
  $j("#form_dob_year").change(function() { update_age_select(); });

  var update_age_select = function() {
    var month = dob_month.attr("value");
    var day = dob_day.attr("value");
    var year = dob_year.attr("value");
    var age = calculate_age_value(month, day, year);
    var new_value = map_age_to_choice_value(age);
    age_select.attr("value", new_value);
  };

  var calculate_age_value = function(month, day, year) {
    //var dob = new Date(year, month, day, 0, 0, 0, 0);
    var today = new Date();
    var age = today.getFullYear()-year;
    if (today.getMonth() < month || (today.getMonth() == month && today.getDate() < day)) { age--; }
    return age;
  };

  var map_age_to_choice_value = function(age) {
    if (age <= 13) { return "97003"; }
    else if (age >= 14 && age <= 15) { return "97004"; }
    else if (age >= 16 && age <= 17) { return "97005"; }
    else if (age >= 18 && age <= 20) { return "97006"; }
    else if (age >= 21 && age <= 22) { return "97007"; }
    else if (age >= 23 && age <= 24) { return "97008"; }
    else if (age >= 25 && age <= 30) { return "97009"; }
    else if (age >= 31 && age <= 35) { return "97011"; }
    else if (age >= 36 && age <= 40) { return "97011"; }
    else if (age >= 41 && age <= 50) { return "97012"; }
    else if (age >= 51 && age <= 60) { return "194125"; }
    else { return "194557"; }
  };
});

</script> 
