function display_short_term(url) {
   window.open(url,"kaartje","toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=0,resizable=0,copyhistory=0,width=222,height=297")
}
function which_chart() {
   now = new Date();
   hour = now.getUTCHours();
   if (hour < 01 || hour > 15) {chart="short_term_morgen1_radar";}
   else
      if (hour > 00 && hour < 04) {chart="short_term_vandaag1_radar";}
      else {chart="short_term_vandaag2_radar";}
// alert(chart);
   return(chart);
}

