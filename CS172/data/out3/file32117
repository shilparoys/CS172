  <script language="javascript">
<!--
/*******************************************************************************

'args.js', by Charlton Rose

Permission is granted to use and modify this script for any purpose,
provided that this credit header is retained, unmodified, in the script.

*******************************************************************************/

// This function is included to overcome a bug in Netscape's implementation
// of the escape () function:
function myunescape (str)
{
	str = '' + str;
	while (true)
	{
		var i = str . indexOf ('+');
		if (i < 0)
			break;
		str = str . substring (0, i) + ' ' + str . substring (i + 1, str . length);
	}
	return unescape (str);
}

// This function creates the args [] array and populates it with data
// found in the URL's search string:

function args_init ()
{
	args = new Array ();
	var argstring = window.location.search;
	if (argstring.charAt(0) != '?') return;
    argstring = argstring.replace(/[^?a-z0-9]/gi,'');

	argstring = argstring.substring(1,argstring.length);
	var argarray = argstring.split ('&');
	var i;
	var singlearg;
	//document.write(argarray.length+"<br>");
	for (i = 0; i < argarray.length; ++i)
	{
		singlearg = argarray[i].split ('=');
		//document.write(i+" - "+argarray[i]+" - "+singlearg.length+"<br>");
		if (singlearg.length != 2) {
		    args[i] = myunescape(singlearg[0]);
			continue;
		}
		var key = myunescape(singlearg[0]);
		var value = myunescape(singlearg[1]);
		args[key] = value;
		//document.write(key+" - "+value+" - "+args[key]+"<br>");
	}
}

function in_array(needle, haystack)
{
	//alert(needle);
    for(var key in haystack)
    {
	    //if(needle === haystack[key])
		//alert(needle+" - "+key);
		if(needle === key)
        {
            return true;
        }
    }

    return false;
}

function isNumeric(n) {
  return !isNaN(parseFloat(n)) && isFinite(n);
}

redirect2 = new Array();
redirect2['Verwachtingen'] = "/waarschuwingen_en_verwachtingen";
redirect2['Vooruitzichten'] = "/waarschuwingen_en_verwachtingen";
redirect2['Waarschuwingen'] = "/waarschuwingen_en_verwachtingen/overzicht_waarschuwingen.html";
redirect2['Kust'] = "/waarschuwingen_en_verwachtingen/waarschuwingen_kust_en_noordzee.html";
redirect2['Noordzee'] = "/waarschuwingen_en_verwachtingen/waarschuwingen_kust_en_noordzee.html";
redirect2['cms'] = "/cms/content";
redirect2['Seismologie'] = "/seismologie/aardbevingen-nederland.html";
redirect2['Zonkracht'] = "/weer/weer_en_gezondheid/zonkracht.html";
redirect2['Colloquium'] = "/research/colloquia/";
redirect2['Ensembleverwachting'] = "/waarschuwingen_en_verwachtingen/ensemble.html";

// Call the args_init () function to set up the args [] array:
args_init ();
