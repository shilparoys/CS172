     <!-- events block--> 
     <article class="grid_3" id="eventsBlock"> 
      <h4>فعاليات</h4> 
      <section class="accordion"> 
       <div class="loader"> 
        <center> 
         <img class="loading-image" src="/sites/all/modules/custom_events/ajax-loader.gif" alt="loading.."> 
        </center> 
       </div> 
       <div id="nitem"></div> 
      </section>
      <!-- accordion end --> 
     </article> 
     <script type="text/javascript">
$(document).ready(function() {

$.ajax({
     url:"http://events.ksu.edu.sa/api/views/frontpage.jsonp?display_id=services_1&callback=eventreader",
     dataType: 'jsonp', // Notice! JSONP <-- P (lowercase)
     beforeSend: function(){
       $('.loader').show();

   },
     success:function(json){
         // do stuff with json (in this case an array)
         console.log("success");
         console.log(json);
         var activeStatus='';
for (var i = 0; i < json.length; i++) {
console.log(json[i]);

if(i == 0)
{
	activeStatus = '';//'active';
}else
{
	activeStatus = '';
}


$("#nitem").append(
'<div class="accordion-content-wrapp">'+
'    <div class="title '+activeStatus+'"><a href="'+json[i].path+'"> '+json[i].ddate+'</a></div>'+
'    <div class="content">'+
'        <p>'+
'		'+json[i].title+''+
'		<br>'+
'		<a style="font-weight:bold;font-size:13px;" target="_blank" href="'+json[i].path+'">اقرأ المزيد</a>'+
'        </p>'+
'    </div>'+
'</div>'
	);
};




     },
     error:function(e){
			console.log("error");
            console.log( e ); 
            $('.loader').hide();
            $('#eventsBlock').hide();

     }  
     ,
     complete: function(){
      $('.loader').hide();
  }    
});
});
</script> 
