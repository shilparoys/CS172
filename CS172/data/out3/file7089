      <div id="newsitems"></div> 
     </article>
     <!-- grid_9 end --> 
     <script type="text/javascript">
$(document).ready(function() {
    $.ajax({
        //url:"http://news.ksu.edu.sa/api/home-page.jsonp?lang=ar",
        url:"http://news.ksu.edu.sa/api/home-page.jsonp?lang=ar&callback=jsonp",
        dataType: 'jsonp', // Notice! JSONP <-- P (lowercase)
        beforeSend: function(){
           $('.newsLoader').show();
        },
        success:function(json){
            console.log("successNews");
            var activeStatus='';
            for (var i = 0; i < json.length; i++) {
                $("#newsitems").append(
                '<article class="grid_3 alpha team home">'+
                '    <a target="_blank" href="'+json[i].url+'">'+
                '       '+json[i].main_ksu_image+''+
                '       <h1>'+json[i].title+'</h1>'+
                '    </a>'+
                '    <div class="team-description"><p>'+
                '       '+json[i].desc+'<br />'+
                '        <a target="_blank" href="'+json[i].url+'" class="visit-anchor">اقرأ المزيد</a>'+
                '    </p></div>'+
                '</article>'
                );
            };
        },
        error:function(e){
            console.log('newError');
            $('.newsLoader').hide();
            $('#newsBlock').hide();

        }  
        ,
        complete: function(){
            $('.newsLoader').hide();
            console.log("comGrabingNews");
        }
    });
});
</script> 
