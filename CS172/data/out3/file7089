  <script type="text/javascript">   

		

			

            $(window).load(function() {

                $('#slider-1, #slider-2').nivoSlider({

                    directionNav: false

                });

            });

            

            $(document).ready(function($){

						

				$('#nav .menu li').each(function() {

				//	alert(0);

					var $li    = $(this);

					var $a     = $li.find('> a');

					var $p     = $li.parents('li');

						prefix = new Array($p.length + 1).join('-');



					var $option = $('<option>')

						.text(prefix + ' ' + $a.text())

						.val($a.attr('href'))                       

						.appendTo("#nav-responsive");



					if ($li.hasClass('active')) {

						$option.attr('selected', 'selected');

					}

				});

				

			








			

             $('#sliderBanner').carouFredSel({

                    items: 1,

                    prev: '.sliderBanner-nav.prev',

                    next: '.sliderBanner-nav.next',

                    auto: true,

                    scroll: 1,

					delay: 7000

                });

                $('#carousel-1').carouFredSel({

                    items: 1,

                    prev: '.carousel-nav-1.prev',

                    next: '.carousel-nav-1.next',

                    auto: false,

                    scroll: 1

                });

				

            $('#facService').carouFredSel({

                items: 4,

                prev: '.facService-nav-1.prev',

                next: '.facService-nav-1.next',

                auto: false,

                scroll: 1

            });

			

                $('#carousel-2').carouFredSel({

                    items: 4,

                    prev: '.carousel-nav-2.prev',

                    next: '.carousel-nav-2.next',

                    auto: false,

                    scroll: 1

                });



            

				


             $('#ksu_twitters').carouFredSel({

                    items: 1,

                   /* prev: '.tweets-nav.prev',

                    next: '.tweets-nav.next',*/

                    auto: true,

                    scroll: 1,

					delay: 12000

                });

				$("#dropMenu").click(function(){

					$('.menu-name-menu-secondry-header-menu .menu').toggle();

				});	

				$(".menu-name-menu-secondry-header-menu .menu a").click(function(){

					$(".menu-name-menu-secondry-header-menu .menu ul").hide();

					$(this).siblings('ul').slideToggle('fast');

					

					if($(this).attr('href') == "#"){

						window.location = "#dropMenu";	

						return false;

					};

					

					

				});

				

            });           

            $(window).resize(function() {

				//alert(screen.width);

				 console.info($(window).width());

				 if( $(window).width() > 640 ) {

					$('.menu-name-menu-secondry-header-menu .menu').show();

					$('.menu-name-menu-secondry-header-menu .menu li ul').removeAttr('style');

					

				}else{

					$('.menu-name-menu-secondry-header-menu .menu, .menu-name-menu-secondry-header-menu .menu ul').hide();

				 }

			});

			

			

			

        </script> 
  <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script> 
  <script type="text/javascript" src="/sites/all/themes/portal/royal-slider/royalslider/jquery.royalslider.min453d.js"></script> 
  <script type="text/javascript">

				var CJquerys = jQuery.noConflict(true);

			</script> 
  <script type="text/javascript"> 

			CJquerys(document).ready(function(CJquerys){

			CJquerys('.home-slider').fadeIn(0, function() {

			CJquerys('#full-width-slider').royalSlider({

			arrowsNav: true,

			loop: true,

			keyboardNavEnabled: true,

			controlsInside: false,

			imageScaleMode: 'fill',

			arrowsNavAutoHide: false,

			autoScaleSlider: true, 

			autoScaleSliderWidth: 1007,     

