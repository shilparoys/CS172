    function PopupCenter(pageURL, title,w,h) {
      var left = (screen.width/2)-(w/2);
      var top = (screen.height/2)-(h/2);
      var targetWin = window.open (pageURL, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width='+w+', height='+h+', top='+top+', left='+left);
    }
    function Mycommand(theForm) {
      theForm.SQLCommand.value = "select name, label, received, status, modified, media, seedsize, reqfile, req_type from requests order by received";
      theForm.submit()
      return true;
    }
    function Mycommand2(theForm) {
      theForm.SQLCommand.value = "select name, label, received, status, modified, media from archreq where received > sysdate-3 order by name;"
      theForm.submit()
      return true;
    }
    function Mycommand3(theForm) {
      theForm.SQLCommand.value = "select network, problem_start, problem_end, reported, resolved,  station, channel, status, resolution, problem from data_problem order by network";
      theForm.submit()
      return true;
    }
    </script> 
