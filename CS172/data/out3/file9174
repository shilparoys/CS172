// It's possible for the tracker to be included multiple times, so check for the variable it
// creates, and if it exists already then log a duplicate
if (typeof(ga) === "undefined") {
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  
  ga('create', 'UA-1860730-1', 'iris.edu');
  ga('send', 'pageview');
} else {
  // Log this as a duplicate
  ga('send', 'event', 'GA Tracker', 'Duplicated');
}
// It's also possible that the older GA tracker is running here.  Log that case as well.
if (typeof(_gaq) !== "undefined") {
