     //---------------------------------------------------------------
     // Process Request
     //---------------------------------------------------------------
    var clicker=0;

    function clickCounter(){

      ++clicker;

      if (clicker == 1){
         return true
      }

      if (clicker > 10){
         clicker=0;
      }

      return false;
   }
   
   function procRequest(){

      if (clickCounter()){
         document.login.submit();
      }
      return true;
  }
  
    // Deactivate cloak --> 
