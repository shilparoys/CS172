  <script type="text/javascript">

    $(document).ready(function () {
        if ($.browser.msie) {
            alert('For optimum viewing please use Internet Explorer 11, FireFox or Chrome web browsers');
        }
    });

    function setDefaultSearchValue() {
        var object = returnObjById("q");

        if (object.value == "")
            object.value = "Search";
    }

    function clearDefaultSearchValue() {
        var object = returnObjById("q");

        if (object.value == "Search")
            object.value = "";
    }

    function validateForm() {
        var object = returnObjById("q");

        if (object.value == "" || object.value.toLowerCase() == "search") {
            return false;
        }

        return true;
    }

    function returnObjById(id) {
        if (document.getElementById)
            var returnVar = document.getElementById(id);
        else if (document.all)
            var returnVar = document.all[id];
        else if (document.layers)
            var returnVar = document.layers[id];
        return returnVar;
    }

    $('#search-text').live("keypress", function (e) {
        if (e.keyCode == 13) {
            searchProducts();
            return false;
        }
    });

    function searchProducts() {
        var searchVal = document.getElementById('search-text').value;
        if (searchVal != '') {
            window.location = '/estore/fullproductlisting.aspx?sortby=productname&searchtxt=' + searchVal;
        }
    }

  

</script> 
