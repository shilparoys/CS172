     <div id="featuredStation"> 
      <div class="sideHeading">
       Featured Station
      </div> 
      <div class="content"> 
       <h3 style="margin-bottom: 3px">G45A</h3> Suttons Bay, MI, USA 
       <br>
       <br> 
       <img style="width: 175px" class="photoCenter" src="http://anf.ucsd.edu/cacheimages/station_photos/G45A_top_pick_8174222239.jpg" alt="Station Photo"> 
       <img class="photoCenter" src="http://maps.google.com/maps/api/staticmap?center=45.0404,-85.6571&amp;zoom=4&amp;size=175x175&amp;maptype=roadmap&amp;markers=color:blue|45.0404,-85.6571&amp;sensor=false&amp;key=ABQIAAAAP28NUn0El9u6czmlS4XN1RRhiHKqT7kxkRg1cRlrAHTEBMYvwhQKsTVk872sFonjAOiVXly18iF0Sw" alt="Station Location"> 
      </div> 
     </div>
     <!-- end featuredStation --> 
     <div class="separator">
      <img src="/static/images/backgrounds/separator.gif" alt="">
     </div> 
    </div>
    <!-- end colLeft --> 
    <div id="colRight"> 
