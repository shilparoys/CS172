  <script type="text/javascript" charset="utf-8">
		Event.observe(window, 'load', function() {
			var locationMenu, storeMenu, initStoreSelector;
			locationMenu = $('locationMenu');
			storeMenu = $('storeMenu');
			initStoreSelector = function(requestedCountryName) {
				return (function(response) {
					var responseDocument = response.responseXML.documentElement;
					var countriesWithStores = responseDocument.getElementsByTagName('country');
					var requestedCountry = null;
					for(var i = 0; i < countriesWithStores.length; i++) {
						var country = countriesWithStores[i];
						if (country.getAttribute('name') == requestedCountryName) {
							requestedCountry = country;
							break;
						}
					}
					new StoreSelector(requestedCountry, locationMenu, storeMenu, false, 'store');
				});
			}
			new Ajax.Request('/autopush/us/retail/storelist/stores.xml', {
				method: 'get',
				onSuccess: initStoreSelector("United States")})

			Event.observe(storeMenu, 'change', function(event) {
				setTimeout(function() {
					locationMenu.clear();
					storeMenu.clear();
					storeMenu.disable();
				}, 20);
			});
		});

		Event.observe(window, 'beforeunload', function() { });
	</script> 
