    <script type="text/javascript">
  
  function submit_search_form(form)
  {
    var q = form.q.value;

    if ((q == null) || (q.length < 5))
    {
      form.submit();
      return false;
    }

    var zip = parseInt(q.substring(0,5));
    if (isNaN(zip) || (zip < 0) || (zip > 99999))
    {
      form.submit();
      return false;
    }

    window.location = "http://screamradius.com/site/location/"+q.substring(0,5);
    return false;
  }

  function keypress_event(e, form)
  {
    var keycode;

    if (window.event)
      keycode = window.event.keyCode;
    else if (e)
      keycode = e.which;
    else return true;

    if (keycode == 13)
      return submit_search_form(form);
    else
      return true;
}


</script> 
