   <script type="text/javascript">
  
var map = null;
var defaultZoom = 14;
var points = [];
var index = 0;

function load()
{
  var latlng = new google.maps.LatLng(39.925833, -75.12);
  var options = {
      zoom: defaultZoom,
      center: latlng,
      mapTypeId: google.maps.MapTypeId.HYBRID
  };
  map = new google.maps.Map(document.getElementById("map_canvas"), options);

  

  

  zoomToFit();
}

function addMarker(latitude, longitude, tooltip, color)
{
  var image = "http://assets.screamradius.com/images/markers/"+color+(index+1)+".png";
  var point = new google.maps.LatLng(latitude, longitude);
  var marker = new google.maps.Marker({
    position: point,
    map: map,
    title: tooltip,
    icon: image
  });

  points[index++] = point;
}

function zoomToFit()
{
  var bounds = new google.maps.LatLngBounds();
  for (var i=0 ; i < points.length ; i++)
    bounds.extend(points[i]);
  map.fitBounds(bounds);
}

</script> 
