          <script type="text/javascript">
                  var student_life_feed=new gfeedfetcher("student_life", "student_life_class", "")
                  student_life_feed.addFeed("Student Life Calendar", "http://www.greenriver-events.info/public/rss/feed.php?l=34") //Specify "label" plus URL to RSS feed
                  student_life_feed.displayoptions("title snippet") //show the specified additional fields
                  student_life_feed.setentrycontainer("div") //Display each entry as a list (li element)
                  student_life_feed.filterfeed(3, "label") //Show 5 entries, sort by label
                  student_life_feed.init() //Always call this last
                </script> 
