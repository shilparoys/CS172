var data = Menu_GetData(item);
if (!data) return;
__disappearAfter = data.disappearAfter;
Menu_Expand(node, data.horizontalOffset, data.verticalOffset);
}
}
function delayMenu_HoverStatic(item)
{
return (function()
{
overrideMenu_HoverStatic(item);
});
}
