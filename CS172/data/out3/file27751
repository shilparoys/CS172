window.addEvent("domready", function() {
	var JTooltips = new Tips($$(".hasTip"), { maxTitleChars: 50, fixed: false});
}); 
				
				geocoder = new google.maps.Geocoder();
				function codeAddress(){
					var address = '753 Walnut St. Camden New Jersey';
					
				 
					  geocoder.geocode( { 'address': '753 Walnut St. Camden New Jersey'}, function(results, status) {
					    if (status == google.maps.GeocoderStatus.OK) {
							var latLong = results[0].geometry.location;
							initialize(latLong);
						
					    } else {
					      alert('Geocode was not successful for the following reason: ' + status);
					    }
					  });
				}
				function initialize(latlng) {
				 
				 
				  var mapOptions = {
				    zoom: 12,
				    center: latlng,
				    mapTypeId: google.maps.MapTypeId.ROADMAP
				  }
				  var map = new google.maps.Map(document.getElementById('map_canvas'),
				                                mapOptions);

				 
				  /*var image = new google.maps.MarkerImage('/images/newsworks-marker-30.png',
				        new google.maps.Size(30.0, 32.0),
				        new google.maps.Point(0, 0),
				        new google.maps.Point(0, 32.0)
				    );
				    var shadow = new google.maps.MarkerImage('/images/shadow-newsworks-marker-30.png',
				        new google.maps.Size(47.0, 32.0),
				        new google.maps.Point(0, 0),
				        new google.maps.Point(0, 32)
				    );
				 */
				  var myLatLng = latlng;
				  var marker = new google.maps.Marker({
				      position: myLatLng,
				      map: map
				   //   icon: image,
					//  shadow: shadow
				  });
				 var infowindow = new google.maps.InfoWindow({
				      content: ''
				  });
				 google.maps.event.addListener(marker, 'click', function() {
				    infowindow.open(map,marker);
				  });
				}

				google.maps.event.addDomListener(window, 'load', codeAddress);

				    
				
				function endStreetView()
				{
					var coordinates = finishMarker.getPoint();
					streetViewClient.getNearestPanorama(coordinates, afterView);	
				}
				function startStreetView()
				{
					var coordinates = icon.getPoint();
					streetViewClient.getNearestPanorama(coordinates, afterView);
				}
					
				function afterView(streetviewData)
				{

					if (streetviewData.code == G_GEO_SUCCESS)
					{
						var streetViewerWindow = document.getElementById('streetView')
						var coordinates = new GLatLng(streetviewData.location.lat, streetviewData.location.lng);
						var panorama = new GStreetviewPanorama(streetViewerWindow);							
						panorama.setLocationAndPOV(coordinates, streetviewData.location.pov);
					}
					else
					{
						alert('Sorry no street view available for this location.');	
					}
					
				}
				function destinationChange()
				{
					var finished = document.getElementById('address').value;
					if (finished == '')
					{
						alert('Please use a valid address to geocode.');
					}
					else
					{
						geocoder.getLatLng(finished, afterGeocode);	
					}
				}
				function afterGeocode(coordinates)
				{
					panel = document.getElementById('directions');
					drivingDir = new GDirections(map, panel);
					finishMarker = new GMarker(coordinates);
					map.addOverlay(finishMarker);
				if(!map.getBounds().contains(coordinates))
				{
					map.panTo(coordinates);
				}
					var waypoints = [icon.getPoint().toUrlValue(),
					finishMarker.getPoint().toUrlValue()];
					drivingDir.loadFromWaypoints(waypoints);
				}		window.addEvent('domready', function(){ $$('dl.tabs').each(function(tabs){ new JTabs(tabs, {}); }); });
