            function popupform(quicksearch, join){
                if (! window.focus)return true;
                    window.open('', join, 'width=650,height=600,scrollbars=yes,toolbar=yes');
                    quicksearch.target=join;
                    return true;
                }
            function ActionDeterminator(){
                if(document.quicksearch.cima.checked == true) {
                    document.quicksearch.action = 'https://www.cima.es/buscar/google';
                }
                if(document.quicksearch.personal.checked == true) {
                    document.quicksearch.method = "POST";
                    document.quicksearch.action = 'https://www.cima.es/buscar/cima';
                }
                return true;
            }
            function play() {
                document.monFlash.SetVariable("player:jsPlay", "");
            }
            function pause() {
                document.monFlash.SetVariable("player:jsPause", "");
            }
            function stop() {
                document.monFlash.SetVariable("player:jsStop", "");
            }
            function enviar(){
                document.buscador.action = "https://www.cima.es/comunicacion/buscar" + "/" + document.buscador.termino.value;
                document.buscador.submit();
            }
            function buscaryear(opcion){
                if(opcion != '#'){
                                            window.location="https://www.cima.es/comunicacion/anio" + "/" + opcion;
                                     }
             }
             function buscaryear2(opcion){
                 if(opcion != '#'){
                                              window.location="https://www.cima.es/comunicacion/cima-en-los-medios/anio" + "/" + opcion;
                                      }
             }
             function buscararea(opcion){
                if(opcion != '#'){
                                              window.location="https://www.cima.es/comunicacion/area" + "/" + opcion;
                                     }
            }
            function ampliar(url){
                var ancho;
                var alto;
                var top;
                var left;
                imagen = new Image();
                imagen.src = url;
                ancho = imagen.width;
                alto = imagen.height;
                top = (screen.height - alto)/2;
                left = (screen.width - alto)/2;
                window.open(url,'Imagen','menubar=0,navigation=0,toolbars=0,location=0,status=0,resizable=0,width=' + ancho + ',height=' + alto + ',top=' + top + ',left=' + left + '');
            }
            function mostrar_cv(id){
                var ancho = ((screen.width) / 2) - 300;
                var alto = ((screen.height) / 2) - 300;
                window.open('https://www.cima.es/cv/id/' + id,'','scrollbars=yes,resizable=yes,width=600,height=500,top=' + alto + ',left=' + ancho);
            }
        </script> 
