   <script type="text/javascript">
		// Checks the size of the browser window and sets the min-height of the footer to fill the remaning space.
		 $(function(){
        $(window).load(resizeFooter);
        $(window).resize(resizeFooter);
        
    });
    function resizeFooter() {
        var windowHeight = window.innerHeight;
        var footerHeight = $("footer").offset();
        var flexFooter = windowHeight - (footerHeight.top);
        $("footer").css("min-height", flexFooter);
    }
    setInterval(resizeFooter,500);

    </script>  
