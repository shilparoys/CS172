                 $(document).ready(function() {
                    var Maximo = 02; 
                    var num = $("#punteo").html();
					
					$("#indice_slide").html( (num*1) +1);
					
                    $("#cargando").css("display", "inline"); 
                    $("#siguiente").click(function(){
                          var num = $("#punteo").html();
                          var sig = num*1 + 1;
                          if(sig>Maximo){
                             sig = 0;
                          }
                          $("#punteo").html('');
                          $("#punteo").html(sig);
					      $("#indice_slide").html(sig+1);
						  $("#contenido-destacado").hide();
                          $("#contenido-destacado").html('');		
                          $("#contenido-destacado").load("nhome/destacado.php", {puntero: sig }, function(){
                               $("#cargando").css("display", "none");
							   $('#contenido-destacado').fadeIn('slow');
                          });
                    });
                    $("#anterior").click(function(){
                          var num = $("#punteo").html();
                          var ant = num*1 - 1;
                          if(ant<0){
                             ant = Maximo;
                          }
                          $("#punteo").html('');
                          $("#punteo").html(ant);
					      $("#indice_slide").html(ant+1);
						  $("#contenido-destacado").hide();						  
                          $("#contenido-destacado").html('');		
                          $("#contenido-destacado").load("nhome/destacado.php", {puntero: ant }, function(){
                               $("#cargando").css("display", "none");
							   $('#contenido-destacado').fadeIn('slow');							   
                          });
                    });
                    $("#contenido-destacado").html('');	
                    $("#contenido-destacado").load("nhome/destacado.php", {puntero: num }, function(){
                          $("#cargando").css("display", "none");
                    });		
                  });
                </script> 
