              <script type="text/javascript">
var tlc_feed=new gfeedfetcher("student_life", "tlc_class", "")
								tlc_feed.addFeed("Teaching & Learning Center Calendar", "http://www.greenriver-events.info/public/rss/feed.php?l=50") //Specify "label" plus URL to RSS feed
								tlc_feed.displayoptions("title snippet") //show the specified additional fields
								tlc_feed.setentrycontainer("div") //Display each entry as a list (li element)
								tlc_feed.filterfeed(3, "label") //Show 5 entries, sort by label
								tlc_feed.init() //Always call this last
</script> 
