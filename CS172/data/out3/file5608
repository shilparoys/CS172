  <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script> 
  <script type="text/javascript">
        jQuery(document).ready(function($){
            var pages = 9;
            var page = 1;

            function next() {
                if( page == pages ) {
                    page = 1;
                } else {
                    page++;
                }
                $.get('/home/carousel/?page=' + page, function(data) {
                    $('#spotlight-wrap').hide('slide', {}, 500, function() {
                        $('#spotlight-wrap').replaceWith(data);
                        $('#spotlight-wrap').show('slide', {'direction': 'right'}, 500);
                    });
                });
                return false;
            }

            var timer = window.setInterval(next, 12000);

            $(document.body).on('click', 'div#spotlight-nav a#prev', function(){
                window.clearInterval(timer);
                if( page > 1 ) {
                    page--;
                } else {
                    page = pages;
                }
                $.get('/home/carousel/?page=' + page, function(data) {
                    $('#spotlight-wrap').hide('slide', {'direction': 'right'}, 500, function() {
                        $('#spotlight-wrap').replaceWith(data);
                        $('#spotlight-wrap').show('slide', {}, 500);
                    });
                });
                return false;
            });

            $(document.body).on('click', 'div#spotlight-nav a#next', function(){
                window.clearInterval(timer);
                next();
            });

        });
    </script> 
  <script type="text/javascript">
        jQuery(document).ready(function($){
            $(".intro-home ~ p").first().after('<a id="get-to-know-see-more" href="#">Read more</a>');
            $("#get-to-know-see-more").click(function() {
                $(".suppress").slideToggle('linear');
                $(this).hide();
                return false;
            });
            $(".suppress").hide();
        });
    </script> 
