	/* search pagination, moved from profileHead.mtml */
	var offset = 0;
	var sLimit = 20;
	var totalResults = 484;
	var thisPage = 1;

	function galleryDirection(nav) {
		var newOffset;
		if ((nav == 'next') && ((offset + sLimit) < totalResults)) {
			newOffset = offset + sLimit;
			document.write('<li class="next last"><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=' + newOffset + '">Next page &raquo;</a></li>');
		}
		if ((nav == 'previous') && ((offset - sLimit) >= 0)) {
			newOffset = offset - sLimit;
			document.write('<li class="first prev"><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=' + newOffset + '">&laquo; Previous page</a></li>');
		}
	}

	function galleryPagination() {
		if (totalResults > sLimit) {
			document.write('<div class="content-pagination"><ul>');
			galleryDirection('previous');
			document.write('<li class="first">1<li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=20">2</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=40">3</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=60">4</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=80">5</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=100">6</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=120">7</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=140">8</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=160">9</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=180">10</a><li><a href="/cgi-bin/mte/mt-search.cgi?affiliate=njo&sfilter_affiliate=njo&tag=politifact-new-jersey%2Cpolitifact%20new%20jersey&limit=20&offset=200">11</a>');
			galleryDirection('next');
			document.write('</ul></div>');
		}
	}

	function tagSearchPagination(ssfUrl) {
		if (totalResults > sLimit) {
			document.write('<div class="content-pagination"><ul>');
			if ((offset - sLimit) >= 0) {
				var prevStr = "";
				if((thisPage) > 2) { prevStr = "-" + (thisPage - 1); }
				document.write('<li class="first prev"><a href="' + ssfUrl + '/tag/politifact-new-jersey/index' + prevStr + '.html">&laquo; Previous page</a></li>');
			}
			document.write('<li class="first">1</li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-2.html">2</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-3.html">3</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-4.html">4</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-5.html">5</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-6.html">6</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-7.html">7</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-8.html">8</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-9.html">9</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-10.html">10</a></li><li><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-11.html">11</a></li>');
			if ((offset + sLimit) < totalResults) {
				document.write('<li class="next last"><a href="' + ssfUrl + '/tag/politifact-new-jersey/index-' + (thisPage + 1) + '.html">Next page &raquo;</a></li>');
			}
			document.write('</ul></div>');
		}
	}


