  <script>
				$(function(){
					$("#tabs1948070220").tabs({
						create: function( event, ui ) {
							window.setTimeout(checkmenu, 100);
						}
					});

					if (window.location.hash && $(window.location.hash).length){
						$("#tabs1948070220").tabs("select", window.location.hash);
						window.scrollTo(0, $("#tabs1948070220").offset().top );
					}

					//make the tab into a sliders
					if(jQuery().touchslider) $("#tabs1948070220>ul:first").touchslider();
					if(jQuery().hammer) {
						$("#tabs1948070220>div").hammer({
								drag_vertical: false
								,css_hacks:false
								,hold:false
								,drag:false
								,transform:false
								,tap_double:false
								,tap:false
							}).bind("swipe", function(ev){
								ev.stopPropagation();
								var active = $("#tabs1948070220").tabs( "option", "active" );
								var total = $("#tabs1948070220 ul:first li").length;
								if(ev.direction == "right"){ //finger from left to right. open left panel
									if (active > 0 ) $("#tabs1948070220").tabs( "option", "active", (active - 1) );
								}
								if(ev.direction == "left"){//finger from right to left. open right panel
									if (active < total ) $("#tabs1948070220").tabs( "option", "active", (active + 1) );
								}
								return false;
							});
					}
				});
			</script> 
  <script>
			$(function(){
				$(".showhide-link").click(function(){
					$t = $(this);
					$t.find(".hiddentext").toggleClass("hidden");
					$t.find(".showtext").toggleClass("hidden");
					$( "#" + $t.context.dataset.for ).toggleClass("hidden");
					return false;
				});
			});
		</script>  
