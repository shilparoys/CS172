            });
            $('.gallery-play-controls .next').click(function(){
                $(this).googlePageviews('http://www.pressofatlanticcity.com/news/breaking/snow-falls-in-south-jersey-a-few-schools-close-in/article_ef2c46cc-bdaa-11e4-8891-3715c256eb86.html');
            });
            $(document).ready(function() {
                $(".gallery-play-controls").show();
                //$("li.tab-image a span").html('<span>Images <em>(5)</em>'); Tab count conterns all image presentation types not just the -none- types so we can't do this here.
                // We'll have to at some point add all true image counts together and JS them into the tab. Until we fix all image areas to only display available assets we'll have to let the tab count be possibly incorrect.
            });
