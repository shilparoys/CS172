		$(function (){
			var searchDefaultValue = $('.search-bar').val();
			$('.search-bar').focus(function(){  
				if($(this).val() == searchDefaultValue){
					$(this).val('');
					$(this).css({'color':'#333'});
				}
				return false;
			});
			$('.search-bar').blur(function(){  
				if($(this).val() == ''){
					$(this).val(searchDefaultValue);
					$(this).css({'color':'#ccc'});
				}
				return false;
			});
		});
	
	</script> 
