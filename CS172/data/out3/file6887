                    .fadein a {display:block;}
                    .fadein img { position:absolute; left:0; top:0px;}
                </style> 
      <script>
                    $(function(){
                        $('.fadein a:gt(0)').hide();
                        setInterval(function(){
                          $('.fadein a:first-child').fadeOut()
                             .next('a').fadeIn()
                             .end().appendTo('.fadein');},
                          5000);
                    });
                    </script> 
