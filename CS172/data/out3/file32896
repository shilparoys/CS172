    window.onload = function () {
        var sp = new StatusPage.page({ page : '0nrlm48qgndm'});
        sp.incidents({
            filter : 'unresolved',
            success: function(data) {
                non_op = [];
                // console.log(data.incidents);
                for (i in data.incidents)
                {
                    non_op.push(data.incidents[i]);
                    $('#huitsr').append("<a href='"+data.incidents[i].shortlink+"' class='case_"+data.incidents[i].impact+"'>"+data.incidents[i].name+"");
                }
                if (non_op.length > 0)
                {
                    $('#huitsr').append("<a class='all_operational' href='http://status.huit.harvard.edu/'>All Other Systems Operational");
                }
                else
                {
                    $('#huitsr').append("<a class='all_operational' href='http://status.huit.harvard.edu/'>All Systems Operational<br />");
                }
            }
        });
