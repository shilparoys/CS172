  <!-- 1.1: en_US - island/content/gotomeetingJavaScript.inc - pages/default/island/content/gotomeetingJavaScript.inc --> 
  <script type="text/javascript">
        <!--
            var validId = false;
            var defaultError;
            var defaultMeetingIdError = "Invalid Meeting ID. Please confirm the ID with the meeting organizer.";

            // Do this here to set up Javascript-enabled pages for real time
            // validation of entered IDs. The default behaviour is to validate
            // post-submission.
            jQuery(document).ready(function() {
                jQuery(".bbReqs").hide();
                jQuery("#errors").hide();
                if (document.forms["meetingForm"].MeetingID.value != "") {
                    verifyMeeting(false);
                }
                defaultError = jQuery("#errorMessage").html();
                defaultMeetingIdError = jQuery("#meetingIdError").html();
                jQuery("#meetingIdError").hide();
                jQuery("#meetingForm").submit(function() {
                    return verifyMeeting(true);
                });
                
            });

            function verifyMeetingId() {
                var meetingId = document.forms["meetingForm"].MeetingID.value;
                if (meetingId) {
                    meetingId = meetingId.replace(/\-/g,'');
                }

                if (meetingId.length > 0 && (isNaN(meetingId) || meetingId.length != 9)) { 
                    jQuery("#meetingIdError").html("Enter a valid meeting ID (ie 000-000-000)");
                    jQuery("#meetingIdError").show();
                    jQuery("#meetingId").css("background-color", "#ffd2d0");
                    jQuery("#errorMessage").html(defaultError);
                    jQuery("#errors").show();
                    validId = false;
                } else {
                    jQuery("#meetingIdError").hide();
                    jQuery("#meetingId").css("background-color", "#fff");
                    jQuery("#errors").hide();
                    validId = true;
                }
                return validId;
            }

            function verifyName() {
                if (document.forms["meetingForm"].Name.value == "") {
                    jQuery("#nameError").show();
                    jQuery("#name").css("background-color", "#ffd2d0");
                    jQuery("#errorMessage").html(defaultError);
                    jQuery("#errors").show();
                    return false;
                }
                jQuery("#nameError").hide();
                jQuery("#name").css("background-color", "#fff");
                if (validId && jQuery("#meetingIdError").css("visibility") == "hidden") {
                    jQuery("#errors").hide();
                }
                return true;
            }

            function verifyMeeting(doSubmit) {
                var form = document.forms["meetingForm"];
                var meetingId = form.MeetingID.value.replace(/\-/g,'');
                if (!doSubmit && meetingId.length == 0) {
                    return true;
                }
                if (!verifyMeetingId()) {
                    return false;
                }
                var call = "/meeting/validate/" + meetingId;
                jQuery.getJSON(call,
                    function(data) {
                        if (data.Status == "OK" || data.Status == "MeetingNotStarted") {
                            validId = true;
                            action = "/join/" + meetingId;
                            if (jQuery.inArray("Native", data.Endpoints) == -1 &&
                                jQuery.inArray("Flash", data.Endpoints) >= 0) {
                                action = "/flash" + action;
                                //jQuery(".bbReqs").show();
                                //document.forms["meetingForm"].requireName.value = true;
                            }
                            document.forms["meetingForm"].action = action;
                            if (doSubmit) {
                                jQuery("#meetingForm").submit();
                            }
                        } else if (data.Status == "Redirect") {
                            if (data.RedirectHost) {
                                window.location = "https://" + data.RedirectHost + "/join/" + data.MeetingId;
                            }
                        } else {
                            var message = "";
                            if (data.Status == "NoSuchMeeting") {
                                message = "No such meeting.";
                            } else if (data.Status == "InvalidMeetingId") {
                                message = "Invalid meeeting ID.";
                            } else if (data.Status == "MeetingExpired") {
                                message = "This meeting has expired.";
                            }
                            jQuery("#meetingIdError").html(message + " Please confirm the ID with the meeting organizer.");
                            jQuery("#meetingIdError").show();
                            jQuery("#meetingId").css("background-color", "#ffd2d0");
                            jQuery("#errors").show();
                            validId = false;
                        }
                    });
            }
        //-->
        </script> 
