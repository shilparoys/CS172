          <script language="JAVASCRIPT">
function valSalesForm(form) {
	var fel = form.SALE_STARTING_ROW_ID;
	if(fel != null)
	{
		if(fel.value.length > "0")
		{
			fel = form.SALE_TKT_SECTION_ID;
			if(fel.options[fel.selectedIndex].value == 0)
			{
				alert("You can only enter Starting Rows if you select a Specific Section.");
				return false;
			}
		}
	}
	var fel = form.SALE_TKT_TICKET_TYPE_ID;
	if(fel != null)
	{
		if(fel.value.length == 0)
		{
			alert("Please select a Primary Ticket Type.");
			fel.focus();
			return false;
		}
	}
	return true;
}
function TicketTypeChange(new_value) {
	var i;
	for(i = 0; i < document.forms.length; i++)
	{
		var form = document.forms[i];
		if(form.name == "RESFORM")
		{
			form.SALE_TKT_TICKET_TYPE_ID.value = new_value;
		}
	}
}
function MustBeTogetherChange(new_value) {
	var i;
	for(i = 0; i < document.forms.length; i++)
	{
		var form = document.forms[i];
		if(form.name == "RESFORM")
		{
			var el = form.SALE_MUST_BE_TOGETHER;
			if(el != null)
			{
				if(new_value == false)
				{
					el.value = "0";
				}
				else
				{
					el.value = "1";
				}
			}
		}
	}
}
</script> 
