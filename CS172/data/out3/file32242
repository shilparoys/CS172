            <script language="JAVASCRIPT" type="text/javascript">

function secKey(id, seckey, secimage, height, width)
{
	this.id = id;
	this.seckey = seckey.toUpperCase();
	this.secimage = secimage;
	this.height = height;
	this.width = width;
}
var secimg = new Array();

function findSec(seckey) {
	for(var i = 0; i < secimg.length; i++)
	{
		if(seckey.toUpperCase() == secimg[i].seckey)
		{
			return secimg[i];
		}
	}
	return null;
}
var last_sec = "";
function clearSelect() {
	nd();
}
function secSelect(seckey) {
	
	// if(last_sec == seckey) { return };
	var sec = findSec(seckey);
	if(sec == null) 
	{
		return;
	}
	var cont = "<" + "IMG SRC=\"" + sec.secimage + "\" WIDTH=\"" + sec.width + "\" HEIGHT=\"" + sec.height + "\" BORDER=0 STYLE=\"filter:dropshadow(color=grey);\">";
	ol_wrap = 1;
	overlib(cont, HAUTO, VAUTO);
}
</script>  
