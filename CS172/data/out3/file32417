         <script type="text/javascript">
	var filterDelay; // add a timeout
	var qval;
	
	$(document).ready(function(){

		jQuery.extend(jQuery.expr[':'], {
			containsIgnoreCase: "(a.textContent||a.innerText||jQuery(a).text()||'').toLowerCase().indexOf((m[3]||'').toLowerCase())>=0"
		});
		jQuery.extend(jQuery.expr[':'], {
			doesNotContainIgnoreCase: "(a.textContent||a.innerText||jQuery(a).text()||'').toLowerCase().indexOf((m[3]||'').toLowerCase())==-1"
		});	

		$("#body-content").prepend(
			$(document.createElement("FORM")).attr({id:"filter-form"}).append(
				$(document.createElement("LABEL")).attr({
					className:"filter-label"
				}).html("Filter by:").append(
					$(document.createElement("INPUT")).attr({
						id:"filter-input",
						type:"text"
					}).bind("keyup", function() {
						clearTimeout(filterDelay); filterDelay = setTimeout(filterList, 250)}
					 ).bind("blur", function() {
					 	clearTimeout(filterDelay); filterList()
					})
				)
			).bind("submit", function(){return false;})
		);
		$("#filter-input").focus();
	});


	function filterList() {
		var q = $("#filter-input").val();
		if(qval != q && q != '') {
		
			$("#body-content li").filter(function(index, q){
				var reg = new RegExp($("#filter-input").val(), "gi");
				return ($(this).find("a").eq(0).text().match(reg) == null);
			}).removeClass("fade-in").addClass("fade-out");
			
			$("#body-content li").filter(function(index, q){
				var reg = new RegExp($("#filter-input").val(), "gi");
				return !($(this).find("a").eq(0).html().match(reg) == null);
			}).removeClass("fade-out").addClass("fade-in");
			
			doFades();
			toggleHeadings();
		}

		if(q == "") {
			$("#body-content li, #body-content h3").show();
		}
		qval = q;
		
	}
	
	function doFades() {
		$("li.fade-out").fadeOut();
		$("li.fade-in").fadeIn();
	}

	function toggleHeadings() {
		$(".gateway-group").each(function(i,el){
			if($(el).find("li.fade-in").length == 0) {
				$(el).find("h3").hide();
			} else {
				$(el).find("h3").show();
			}
		});
	}


</script> 
