	Event.onLoad(function() {
		var surveyJSON           = "",
		    utilityNavController = apple.utilitynav.UtilityNavViewController.singleton($('guide_wrap'),{surveyData:surveyJSON}),
		    utilityNavInit       = {"url":"/us-hed/cart_utility_nav"}
;
		    
				utilityNavController.account.set({
					"content":{
						"signInUrl": "https://secure.store.apple.com/us-hed/sign_in?c=aHR0cDovL3N0b3JlLmFwcGxlLmNvbS91cy1oZWQvaGVscC9wb2xpY2llc3wxYW9zZTUxNjI1MzExYTgwMWVlOTdjYzNhNjliNWFlZTgwNmViYWI2MGI2ZQ&r=SCDHYHP7CY4H9XK2H&s=aHR0cDovL3N0b3JlLmFwcGxlLmNvbS91cy1oZWQvaGVscC9wb2xpY2llc3wxYW9zZTUxNjI1MzExYTgwMWVlOTdjYzNhNjliNWFlZTgwNmViYWI2MGI2ZQ&t=S99KKATD9FP9FHCP4",
						"signOutUrl":"https://secure.store.apple.com/us-hed/signout?c=aHR0cDovL3N0b3JlLmFwcGxlLmNvbS91cy1oZWR8MWFvc2Y1NGJmNWM2NjNmMTUwY2QzZGFiMTJjMjE0MzBiZTYwYmFkNmU0YTc&s=aHR0cDovL3N0b3JlLmFwcGxlLmNvbS91cy1oZWR8MWFvc2Y1NGJmNWM2NjNmMTUwY2QzZGFiMTJjMjE0MzBiZTYwYmFkNmU0YTc",
						"createAppleIdUrl": "https://appleid.apple.com/cgi-bin/WebObjects/MyAppleId.woa/wa/createAppleId?&localang=US-EN&app_id=2083&returnURL=http%3A%2F%2Fstore.apple.com%2Fus-hed%2Fhelp%2Fpolicies",
						"displayAccountName": "true"
					}
				});
			
		    
				utilityNavController.data.setUrl(utilityNavInit.url);
			
	});
</script> 
