	wrapperid: "simplegallery2", //ID of main gallery container,
	dimensions: [963, 200], //width/height of gallery in pixels. Should reflect dimensions of the images exactly
	imagearray: [
		["images/b1z1.jpg", "apply/index.html", ""],
		["images/b2z1.jpg", "learn/visitusc.html", ""],
		["images/b4z1.jpg", "learn/whyusc.html", ""]
	],
	autoplay: true,
	persist: false,
	pause: 4500, //pause between slides (milliseconds)
	fadeduration: 500, //transition duration (milliseconds)
	oninit:function(){ //event that fires when gallery has initialized/ ready to run
	},
	onslide:function(curslide, i){ //event that fires after each slide is shown
		//curslide: returns DOM reference to current slide's DIV (ie: try alert(curslide.innerHTML)
		//i: integer reflecting current image within collection being shown (0=1st image, 1=2nd etc)
	}
})

</script> 
  <script type="text/javascript">
$(document).ready(function(){

   // Deans Scholarship
  $('#deans').hide();
  $('a#deans-show').click(function() {
    $('#deans').slideToggle('slow');
    return false;
  });
  $('a#deans-hide').click(function() {
    $('#deans').slideUp('fast');
    return false;
  });
})
</script> 
