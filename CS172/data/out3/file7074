  <script xmlns:s="blah">
function show_first_tabs(){
//	alert("show_first_tabs");
	var tabs=document.getElementsByName("tab_block");
//	alert("tabs.length="+tabs.length);
	for (var i = 0; i < tabs.length; i++){
		var children = tabs[i].childNodes;
		for (var c = children.length-1; c >=0 ; c--){
			if( children[c].id == "tab"){
				show_tab(children[c].childNodes[0]); c=100; break;
			};
		}
	}
}

function show_tab(_this)
{
//	deselect
	{
		var children1 = _this.parentNode.parentNode.childNodes;
		for (var c = 0; c < children1.length; c++){
			if( children1[c].id == "tab"){
				var tab_children = children1[c].childNodes;
				for (var i = 0; i < tab_children.length; i++){
					if (tab_children[i].className =="tab_title_selected"){
						tab_children[i].className = "tab_title";
					}
				}
			}
		}
	}

	var this_children = _this.parentNode.childNodes;
	var clone;
	for (var i = 0; i < this_children.length; i++){
		if (this_children[i].className =="tab_title"){
			this_children[i].className = "tab_title_selected";
		}

		if (this_children[i].className =="tab_content"){
			clone = this_children[i].cloneNode(true);
			clone.style.display="";
			break;
		}
	}

	
	var children = _this.parentNode.parentNode.childNodes;
	for (var i = 0; i < children.length; i++){
		if (children[i].id == "current"){
			var element = document.createElement("div");			
			element.appendChild( clone );
			children[i].innerHTML = element.innerHTML;
		}
	}
}
</script>
