         <!-- close rw home slide show bg --> 
         <script>
					     
                                /*************************************/
                                /****** gallery slide show **********/
                                /***** THIS IS v3.0  *********/
                                
                                /*************
                                lotsa fixes including the weird animation shift due to not being abs pos
                                also when nav dots clicked they now update the index so not to lose track
                                /**************
                                
                                
                                /**** REQUIREMENTS
                                
                                - a housing div for the entire slide show including the dots this is used to center the dot group
                                - the height of the slideholder and slide should be the same or weird offsets happen
                                - the dot top pos is based on the slideholder
                                - if you wana move the dots above just replace 'prev' with 'next'
                                - you can replace the dots with any image or just use text with no images
                                
                                
                                *****/
                                
                                
                                
                                //position the dots in the center
                                $('.rw_slide_dot_holderzz').each(function (i) {
                                        var rw_slide_dot_center_poszz = ($(this).parent().width() - $(this).width())/2; 
                                        //alert(rw_slide_dot_center_poszz);
                                        $(this).css({'margin-left':rw_slide_dot_center_poszz+'px'});
                                });
                                
                                //LOOP THROUGH ALL AND DEFAULT THEM TO HIDDEN
                                $('.rw_slide_holder_homezz').each(function (i) {$(this).children('.rw_slidezz').css({'display':'none'});
                                });//END THE EACH LOOP
                                
                                
                                //WHEN A NAV DOT IS CLICKED DO THIS
                                $('.rw_slide_dotzz').click(function() {
                                                
					$(this).parent('.rw_slide_dot_holderzz').prev('.rw_slide_holder_homezz').children().fadeOut('slow');
					$(this).parent('.rw_slide_dot_holderzz').prev('.rw_slide_holder_homezz').find('div.rw_slidezz:eq(' + $(this).index() + ')').fadeIn("slow");
					if (window.rw_slide_indexzz < window.rw_slide_lengthzz-1){
								 //alert($(this).index());
								 window.rw_slide_indexzz=$(this).index()+1;
							}else{
								 window.rw_slide_indexzz = 0;
					}
                                                                                      
                                
                                          
                                });//END CLICK FUNCTIONS
                                
				
				
				
                                //THIS IS THE CODE THAT HANDLES THE CLICKED STATE AND THE ROLLOVER STATES
                                $(".rw_slide_dotzz").click(function() {$(this).addClass("selectedzz").siblings().removeClass("selectedzz");});
                                
                                
                               
                                        
                               window.rw_slide_lengthzz = $('.rw_slide_holder_homezz').children('.rw_slidezz').length;
                                        
                                        //alert('slide length: '+window.rw_slide_lengthzz);
                                
                                
                                        if(window.rw_slide_lengthzz > 1){
                                        
                                                window.rw_slide_indexzz = 0;
                                                
                                
                                
                                
                                                function rw_slide_hangtime_stopzz(){
                                                
                                                          
                                                        //hide all siblings
                                                        $('.rw_slide_holder_homezz').children('.rw_slidezz').fadeOut(1500);
                                
                                                         $('.rw_slide_holder_homezz').find('div.rw_slidezz:eq(' + window.rw_slide_indexzz + ')').fadeIn(1500);
                                
							      $('.rw_slide_dot_holderzz').find('div.rw_slide_dotzz:eq(' + window.rw_slide_indexzz + ')').addClass("selectedzz").siblings().removeClass("selectedzz");
							     if (window.rw_slide_indexzz < window.rw_slide_lengthzz-1)
								    window.rw_slide_indexzz++;
							     else{
								    window.rw_slide_indexzz = 0;
							      }
							       clearInterval(window.rw_slide_timer_setzz);
							       window.rw_slide_timer_setzz = setInterval(function(){rw_slide_hangtime_startzz()},7000);
	
                                
                                                                 
                                                 
                                                                      
                                                }; //close rw_slide_hangtime_stopzz function;
                                
                                 rw_slide_hangtime_stopzz();
                                
                                
                                                function rw_slide_hangtime_startzz(){rw_slide_hangtime_stopzz();}; //end function
                                
                                                
                                        
                                
                                                //clearInterval(window.rw_slide_timer_setzz);
                                                window.rw_slide_timer_setzz = setInterval(function(){rw_slide_hangtime_startzz()},7000);
                                
                                
                                
                                        } //close the slide length condition



			</script> 
        </div> 
