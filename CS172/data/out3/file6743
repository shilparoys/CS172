		$("#left h4:first").children("span").toggleClass("block-link-arrow-reverse");
		$("#left .menu:not(:first)").hide();

		$("#left h4").click(function(){
			$(this).next(".menu").slideToggle("slow").siblings(".menu:visible").slideUp("slow");
			$(this).children("span").toggleClass("block-link-arrow-reverse");
			$(this).siblings("h4").children("span").removeClass("block-link-arrow-reverse");
		});
	});
