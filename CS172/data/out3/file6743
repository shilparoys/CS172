     <h3><a title="Фотоотчеты БНТУ" href="/photos.html"><span>Фотоотчеты</span></a></h3> 
     <script src="/modules/mod_slideshow_morfeoshow_for_bntu/js/jquery.easing.1.3.js" type="text/javascript" charset="utf-8"></script> 
     <script language="javascript">
var $ssms = jQuery.noConflict();

$ssms(function() {
    var current = 0;
    var fadeSpeed = 400;
    var animSpeed = 600;
    var easeType = 'easeOutCirc';

    var $thumbScroller = $ssms('#ssms_thumbContainter');
    var $scrollerContainer = $thumbScroller.find('.ssms_container');
    var $scrollerContent = $thumbScroller.find('.ssms_content');
    var $pg_img_links = $ssms('#ssms_pg_img_links');
    var $pg_desc = $ssms('#ssms_pg_desc');
    var $overlay = $ssms('div#ssms #overlay');

    var scrollerContentCnt = $scrollerContent.length;
    var sliderHeight = 310;
    var totalContent = 0;
    var itemHeight = 0;

    var heightMaxDesc = 0;
    for(var i = 0; i < $pg_desc.find('div').length; i++) {
	if ($pg_desc.find('div').eq(i).height() > heightMaxDesc) heightMaxDesc = $pg_desc.find('div').eq(i).height()
    }
    $ssms('#ssms').height(sliderHeight + heightMaxDesc + 40);

    var cnt = 0;
    $thumbScroller.find('img').each(function(){
	var $img = $(this);
	$ssms('<img/>').load(function(){
	    ++cnt;
	    if(cnt == scrollerContentCnt){
		itemHeight = $thumbScroller.find('.content:first').height();
		buildScrollableItems();
	    }
	}).attr('src',$img.attr('src'));
    });

    $scrollerContent.bind('click',function(e){
	var $this = $(this);
	var idx = $this.index();

	if(current==idx) return;

	var $currentLink = $pg_img_links.find('a.ssms_pg_link:eq('+current+')');
	var $nextLink = $pg_img_links.find('a.ssms_pg_link:eq('+idx+')');
	var $currentDesc = $pg_desc.find('div:nth-child('+(current+1)+')');
	var $nextDesc = $pg_desc.find('div:nth-child('+(idx+1)+')');
	current = idx;

	$nextLink.show();
	$currentLink.stop().animate({'left': '110px','opacity':'0'},animSpeed,function(){
	    $(this).hide().css({
		'left' : '75px',
		'opacity' : 0.9,
		'z-index' : 1
	    });
	    $nextLink.css({'z-index':9999});
	});

	$currentDesc.stop().animate({'left':'-40px','opacity':'0'},animSpeed,function(){
	    $(this).hide();
	    $nextDesc.show().stop().animate({'left':'0px','opacity':'1'},animSpeed);
	});
	e.preventDefault();
    });

    function buildScrollableItems(){
	totalContent = (scrollerContentCnt-1)*itemHeight;
	$thumbScroller.css('height',sliderHeight).find('.thumb').fadeTo(fadeSpeed, 0.6).hover(function(){$(this).fadeTo(fadeSpeed, 1)},function(){$(this).fadeTo(fadeSpeed, 0.6);});
    }
    });
</script> 
