  <script type="text/javascript">

	// removing all excess wysiwyg styles for inline content
	function fixContentStyles() {
		var $generic_content = $('html.generic .inner');
		if ( $generic_content.length ) {
			$generic_content.find('img, div, :header, p, table').removeAttr('width height style align');
			$generic_content.find('iframe[src*="youtube"], iframe[src*=vimeo]').remove();
			$generic_content.find('p:has(img[src*=alumwk])').remove();
		}
	}

	function navGridSizing( $grid ) {
		var width = 0;

		$grid.each(function(){
			width = $(this).width();
			$icons_normal = $(this).find('.icon:not(.portrait, .ignore)');
			$icons_portrait = $(this).find('.icon.portrait:not(.ignore)');

			$(this).height(width).find('.btn-inner').height(.85 * width);
			$icons_portrait.find('> span img').height(.30 * width).width((.30 * width) * .66);
			$icons_normal.find('> span').height(.30 * width).find('img').width(.30 * width);				
		});
	}

	function homeNavGrid() {

		var $nav_grid = $('html.home div.nav-grid > a');

  		window.onorientationchange = function() {
			navGridSizing( $nav_grid );
		};

		if ( $nav_grid.length )
			navGridSizing( $nav_grid );

	}

	$(function(){
		
		fixContentStyles();
		homeNavGrid();

	});
	</script> 
 </head> 
