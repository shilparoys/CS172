          <script type="text/javascript">
  var $jqPMA = jQuery.noConflict(); 
  $jqPMA(function() {
    
             $jqPMA('img.image0').data('ad-title', "<a target='_blank' href='http://ryersonrams.ca/HomePage.dbml?DB_OEM_ID=22300&SPLASH_SET=YES&KEY='>Support your Rams</a>");                              
             
             $jqPMA('img.image1').data('ad-title', "<a target='_blank' href='http://www.ryerson.ca/news/events/General_Public/20150305-first-robotics-competition.html'>A Ryerson first</a>");                              
             
             $jqPMA('img.image2').data('ad-title', "<a target='_blank' href='http://www.ryerson.ca/content/ryerson/news/events/General_Public/20150312-cis-final-basketball.html'>Ryerson vs. the Rest</a>");                              
             
             $jqPMA('img.image3').data('ad-title', "<a target='_blank' href='http://ryerson.ca/ryersontoday/data/news/2015/02/20150211-from-the-president-united-way-thank-you.html'>Together, we found a way</a>");                              
             
             $jqPMA('img.image4').data('ad-title', "<a target='_blank' href='http://ryerson.ca/ryersontoday/data/news/2015/02/20150204-the-2015-alumni-achievement-award-recipients.html'><b>Recognizing amazing alumni</b></a>");                              
             
    
    var galleries = $jqPMA('.ad-gallery').adGallery({
        effect: 'fade',width:false, height: false, slideshow: {animate_first_image: false, speed: 5000}
    });
    galleries[0].settings.effect = 'fade'; 
   
    $jqPMA('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    $jqPMA('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = $jqPMA('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script> 
