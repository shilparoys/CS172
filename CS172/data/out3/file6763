                      <div class="uiwRssReader parbase section"> 
                       <!-- 
    **note changes made above in normal non preview css styles
    repeat the style writing above and append X at the end of the component id for preview
     --> 
                       <div id="toolbarObj367977036" class="allToolbarSelector"></div> 
                       <script>                                                      


</script> 
                       <div id="component367977036" class="rssReaderComponent"> 
                        <div class="entireContainer" style="overflow:hidden;"> 
                         <div class="banner" style=""> 
                          <img title="Upcoming Games" alt="Upcoming Games" class="cq-dd-image cStyles" src="/content/ryerson/index/jcr%3acontent/center/uiwcolumns/conContainer_0_0/uiwcolumns_1/conContainer_1_0/uiwrssreader.img.png/1382381208063.png"> 
                          <div style="clear:both;height:0px;"></div> 
                         </div> 
                         <ul class="feedlist"> 
                         </ul> 
                        </div> 
                       </div> 
                       <script>
jQuery(document).ready(function(){
	jQuery.ajax({
		url: "/content/rssProxy.html?rss=http%3A%2F%2Fwww.ryersonrams.ca%2Frss.dbml%3Fdb_oem_id%3D22300%26media%3Dschedules",
		dataType: "json",
		timeout:5000,
		error:function(){
			jQuery("#component367977036 .feedlist").append("<div class=\"unavailable\">Feed Not Available<div>");
		},
		success: function(data,status,jqXHR){
			if(data.success){
				var limit = 3;
				if(limit > data.items.length){
					limit = data.items.length;
				}
				var appendString = "";
				for(var i = 0; i < limit; i++){
				    var alternateClass = "odd";
					if(i%2 == 0){
						alternateClass = "even";
					}
					appendString += "<li class=\"feeditem "+alternateClass+"\">";
					
					
					if(data.items[i].title !=null && jQuery.trim(data.items[i].title).length > 0){
						appendString += "<div class=\"feedtitle\"><a  href=\""+jQuery.trim(data.items[i].link)+"\">"+jQuery.trim(data.items[i].title)+"</a></div>";
                    }
					
                    
					if(data.items[i].description !=null && jQuery.trim(data.items[i].description).length > 0){
						appendString += "<div class=\"feeddescription\">"+jQuery('<div/>').html(jQuery.trim(data.items[i].description)).text()+"</div>";
                    }
					
					
					appendString += "</li>";
					
				}
			jQuery("#component367977036 .feedlist").append(appendString);
			} else{
                jQuery("#component367977036 .feedlist").append("<div class=\"unavailable\">Feed Not Available<div>");
            }
		}
    });

    
});
</script> 
                      </div> 
