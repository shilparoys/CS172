  <script type="text/javascript">
    jQuery(document).ready(function() {
        jQuery("#skip_to_content").click(function(e) {
            if (!jQuery("#skip_to_content_inner").length) {
                jQuery("#main_content_panel").prepend('<a href="" id="skip_to_content_inner" alt="You are now in the main content area" style="position:absolute;left:-10000px;top:auto;width:1px;height:1px;overflow:hidden;">You are now in the main content area</a>');
                jQuery("#skip_to_content_inner").click(function(e) {
                    e.preventDefault();
                });
            }
            jQuery("#skip_to_content_inner").focus();
            e.preventDefault();
        });
    });
</script> 
