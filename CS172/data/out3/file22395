  <script language="JavaScript" type="text/JavaScript">
    function checkinput() {

        //document.f.sidno.value = document.f.sidno.value.toUpperCase();
        var value = document.getElementById("StudentIdTextBox2").value = document.getElementById("StudentIdTextBox2").value.toUpperCase();
        alert("ok");
        alert("value : " + value.length);

        if (document.getElementById("StudentIdTextBox2").value.length > 2 && document.getElementById("StudentIdTextBox2").value.length < 4) {
            str = document.getElementById("StudentIdTextBox2").value.toUpperCase()
            document.getElementById("StudentIdTextBox2").value = str + " ";
            //alert("ok");
        }
        if (document.getElementById("StudentIdTextBox2").value.length > 6 && document.getElementById("StudentIdTextBox2").value.length < 8) {
            str = document.getElementById("StudentIdTextBox2").value
            document.getElementById("StudentIdTextBox2").value = str + " ";
        }
        if (document.getElementById("StudentIdTextBox2").value.length > 12 && document.getElementById("StudentIdTextBox2").value.length < 14) {
            sf();
        }
    }

    function check_form() {
        if (!document.getElementById("StudentIdTextBox2").value) {
            alert('Please input Id Number.');
            document.getElementById("StudentIdTextBox2").focus();
            return false;
        }
        document.form1.submit();
    }
</script> 
