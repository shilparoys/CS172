  <script>

        $(document).ready(function(){

            var windowWidth = $(window).width(); 
            var minTabletWidth = 800; 
            deviceChecker(); // init device check


            function deviceChecker() { //examine device / width 

                if(( /Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) && (windowWidth<minTabletWidth) ){
                    goMobile(); 

                } else {
                    goDesk();

                }
            }

            function goDesk() {
                //remove mobile assets
                $('.wrapper#mobile').remove();

                //insert / display desk assets 
                $('.wrapper#desktop').show();
            }

            function goMobile() {
                //remove desk assets 
                $('#desk_js').remove(); 
                $('#desk_css').remove(); 
                $('.wrapper#desktop').remove();

                //insert mobile assets 
                $('head').append('<link rel="stylesheet" type="text/css" href="css/seeallmobile-4465.css">');
                var mobileScript = $('<script>').attr({src:'js/seeallmobile-4465.js', type: 'text/javascript'}).appendTo('head');
                //alert('this is mobile');
                
                $('.wrapper#mobile').show();
            }


        });


    </script> 
