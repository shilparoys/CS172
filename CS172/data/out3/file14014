			$(document).ready(function(){
				var sm_conf = {					
					"showTimeout": 0,
				};
				$('#mobilemenuroot_ul').smartmenus(sm_conf);
				$('#menu_toggle_but').click(toggleMobileMenu);
			});			

			function toggleMobileMenu(){				
				if( !$('#mobilemenuroot_ul').hasClass('collapsed') ) {
					$('#mobilemenuroot_ul').addClass('collapsed');
				}else{
					$('#mobilemenuroot_ul').removeClass('collapsed');	
				}
			}
