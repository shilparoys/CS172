   <script>

$(document).ready(function()
{
	init();
	$('body').keydown(function(e)
	{
		if (!usernameFirstTime)
		{
			var keyCode = e.which;
			var focusedId = $(':focus').attr('id');
			if ((keyCode == 13) && ((focusedId == 'uid') || (focusedId == 'pswd'))) document.getElementById('theForm').submit();
		}
	});

    $('#uid').keyup(function(e)
    {
        if (e.which == 9) initUsername()
    });
    	    
    $('#pswd').keyup(function(e)
	{
	    if (e.which == 9) initPassword();
	});
    	    
    $('#uid').click(function()
	{
	    initUsername()
	});

    $('#pswd').click(function()
    {
        initPassword()
    });
    $('#pswd').focus(function()
    {
        initPassword()
    });

/*
    $('#forgotUserNameCancelButton').focusout( function()
    {
        $('#username').focus();
    });
    	            
    $('#userNameSentContinueButton').focusout( function()
    {
        $('#userNameSentContinueCancel').focus();
    });
    	            
    $('#passwordResetRequestSentContinueButton').focusout( function()
    {
        $('#passwordResetRequestSentContinueCancel').focus();
    });
*/    	            
});


</script> 
