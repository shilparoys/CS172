   <script>

function clearISWMessages(id)
{
    $('#'+id).hide();
}

function clearClientSideErrors()
{
    clearISWMessages("clientSideErrorMessages");
}

function clearClientSideSuccess()
{
    clearISWMessages("clientSideSuccessMessages");
}

function clearClientSideNotice()
{
    clearISWMessages("clientSideNoticeMessages");
}

function clearAllClientSideMessages()
{
    clearClientSideNotice();
    clearClientSideSuccess();
    clearClientSideErrors();
}

function showClientSideNotice(msg)
{
    $('#clientSideNotice').html('Notice: ' + msg);
    $('#clientSideNoticeMessages').show();
}

function showClientSideSuccess(msg)
{
    $('#clientSideSuccess').html('Success: ' + msg);
    $('#clientSideSuccessMessages').show();
}

function showClientSideError(msg)
{
    $('#clientSideError').html('Error: ' + msg);
    $('#clientSideErrorMessages').show();
    $('#errorList').hide();
}

function displayClientSideErrors(errorMessageList)
{
	var errorMessages = "", i=0;
	for (i = 0; i < errorMessageList.length; i++)
	{
		 errorMessages = "<li>"+errorMessageList[i]+"</li>\n";
	}
    $('#errorList').html(errorMessages);
    $('#errorList').show();
    $('#clientSideErrorMessages').show();
}
</script> 
   <div id="clientSideErrorMessages" class="systemMessage errorMessages hide"> 
    <h4> <b data-icon=":" class="ico-l alert" title="Error"></b> <strong id="clientSideError">The following errors occurred:</strong> 
     <ul id="errorList"></ul> </h4> 
   </div> 
   <div id="clientSideSuccessMessages" class="systemMessage successMessages hide"> 
    <a href="javascript: clearClientSideSuccess()" class="nolink fright" style="background-color: transparent"> <b data-icon="D" class="ico die" title="Close"></b> </a> 
    <h4> <b data-icon="2" class="ico-l check" title="Success"></b> <strong id="clientSideSuccess">The following successes occurred:</strong> </h4> 
   </div> 
   <div id="clientSideNoticeMessages" class="systemMessage notices hide"> 
    <a href="javascript: clearClientSideNotice()" class="nolink fright" style="background-color: transparent"> <b data-icon="D" class="ico die" title="Close"></b> </a> 
    <h4> <b data-icon="i" class="ico-l info" title="Notice"></b> <strong id="clientSideNotice">The following notices occurred:</strong> </h4> 
   </div> 
