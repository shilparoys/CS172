  <script type="text/javascript">

var confirmFocusElement;
var cancelConfirmFocusElement;

function raveModalConfirm(header, message, buttonLabel, confirmAction, cancelAction, messageType, confirmFocusOnReturn, cancelConfirmFocusOnReturn)
{
    raveModalConfirmWithOptions({
        scrollToTop: true,
        header: header,
        message: message,
        confirmButtonLabel: buttonLabel,
        confirmButtonAction: confirmAction,
        confirmButtonFocus: confirmFocusOnReturn,
        cancelButtonAction: cancelAction,
        cancelButtonFocus: cancelConfirmFocusOnReturn,
        messageType: messageType,
        popupModalClass: ''
    });
}

function raveModalAlert(header, message, buttonLabel, messageType, focusOnReturn)
{
    raveModalConfirmWithOptions({
        scrollToTop: true,
        header: header,
        message: message,
        confirmButtonLabel: buttonLabel,
        confirmButtonAction: "_raveModalConfirmCancelAction()",
        confirmButtonFocus: focusOnReturn,
        cancelButtonAction: "_raveModalConfirmCancelAction()",
        cancelButtonFocus: focusOnReturn,
        messageType: messageType,
        popupModalClass: ''
    });
}

function raveModalConfirmWithOptions(options)
{
    if ( options.scrollToTop) $("html, body").animate({ scrollTop: 0 }, "fast");

    $('#_raveModalBackground').css({height: $('BODY').height()});

    if ("DIRE_WARNING" == options.messageType)
    {
        $('#_raveModalConfirm').attr('class', 'popupModal direWarning ' + options.popupModalClass);
        $('#_raveModalConfirmButton').attr('class', 'btn');
        $('#_raveModalCancelButton').attr('class', 'btn fright');
        $('#_raveModalConfirmCancelIcon').attr('class', 'ico diePale x24');
    }
    else
    {
        $('#_raveModalConfirm').attr('class', 'popupModal ' + options.popupModalClass);
    }
    $('#_raveModalConfirmHeader').html(options.header);
    $('#_raveModalConfirmMessage').html(options.message);
    $('#_raveModalConfirmButton').html(options.confirmButtonLabel);
    $('#_raveModalConfirmButton').attr('href', "javascript:_raveModalConfirmAction('"+options.confirmButtonAction+"')");
    $('#_raveModalConfirmButton').show();

    $('#_raveModalConfirmCancel').attr('href', "javascript: _raveModalConfirmCancelAction('"+options.cancelButtonAction+"')");
    $('#_raveModalBackground').show();
    $('#_raveModalConfirm').show();
    $('#_raveModalCancelButton').hide();

    // optional second button
    if( options.cancelButtonLabel != '' && options.cancelButtonLabel != undefined)
    {
        $('#_raveModalCancelButton').html(options.cancelButtonLabel);
        $('#_raveModalCancelButton').attr('href', "javascript: _raveModalConfirmCancelAction('"+options.cancelButtonAction+"')");
        $('#_raveModalCancelButton').show();
    }

    var actualWidth = parseInt($('#_raveModalConfirm').width(), 10);
    $('#_raveModalConfirm').css({marginLeft: "-"+(actualWidth/2)+"px"});

    $('#_raveModalConfirmMessage').css('width', (actualWidth - 45)+'px');
    var messageWidth = parseInt($('#_raveModalConfirmMessage').width(), 10);

    if ($('#_raveModalConfirmHeader').val().length < 1)
    {
        $('#_raveModalConfirmMessage').css('margin-top', '25px');
    }

    confirmFocusElement = options.confirmButtonFocus;
    cancelConfirmFocusElement = options.cancelButtonFocus;

    $('#_raveModalConfirm').focus();
    $('#_raveModalConfirmCancel').focusout( function() {
        $('#_raveModalConfirmButton').focus();
    });
    $('#_raveModalConfirmButton').focusout( function() {
        $('#_raveModalConfirmCancel').focus();
    });
}   /* raveModalConfirm() */

function _raveModalConfirmAction(func)
{
    $('#_raveModalConfirm').hide();
    $('#_raveModalBackground').hide();
    eval(func);
    if ((confirmFocusElement != null) && !$('#'+cancelConfirmFocusElement).is(':disabled'))
        $('#'+confirmFocusElement).focus();
}

function _raveModalConfirmCancelAction(func)
{
    $('#_raveModalConfirm').hide();
    $('#_raveModalBackground').hide();
    if ((cancelConfirmFocusElement != null) && !$('#'+cancelConfirmFocusElement).is(':disabled'))
        $('#'+cancelConfirmFocusElement).focus();
    eval(func);
}

var focusElement;

function _raveModalAJAXGenericContainerOpen(payLoad, className)
{
    // Backward compatible
    _raveModalAJAXGenericContainerOpen(payLoad, className, false);
}

function _raveModalAJAXGenericContainerOpen(payLoad, className, snapToTop)
{
    if ( snapToTop )
    {
        // Snap to the top
        $("html, body").animate({ scrollTop: 0 }, "fast");
    }
    $('#_raveModalAJAXBackground').show();

    if (payLoad != null)
        $('#_raveModalAJAXGenericContainer').html(payLoad);

    if (className != null)
        $('#_raveModalAJAXGenericContainer').attr('class', className);

    $('#_raveModalAJAXGenericContainer').show();
    var actualWidth = parseInt($('#_raveModalAJAXGenericContainer').width(), 10);
    $('#_raveModalAJAXGenericContainer').css({marginLeft: "-"+(actualWidth/2)+"px"});

}

function _raveModalAJAXGenericContainerClose(callBack)
{
    $('#_raveModalAJAXGenericContainer').hide();
    $('#_raveModalAJAXBackground').hide();
    if (callBack != null) callBack();

}

