   <script>
var usernameFirstTime = true;


function initUsername()
{
    if (usernameFirstTime) document.getElementById("uid").value = "";
    usernameFirstTime = false;
}   /* initUsername() */



var passwordFirstTime = true;

function initPassword()
{
    if (passwordFirstTime)
    {
        $('#pswd').val("");
        if (!$.browser.msie)
            document.getElementById("pswd").type = "password";
    }
    passwordFirstTime = false;
}   /* initPassword() */




var localUserLabel = "";

function init()
{
	localUserLabel = $('#uid').val();
    if (!$.browser.msie)
        document.getElementById("pswd").type = "text";
}   /* init() */

var forgotMode = "";

function forgotUserName()
{
	$(':focus').die();
    $('#usernamePasswordLabel').text('Phone number / Email address');
    _setLabelErrorStyleClass(document.getElementById("usernamePasswordLabel"), true);
	forgotMode = "USERNAME";
	if($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').hide();

	document.getElementById("forgotUserNameHeader").style.display = "inline";
    document.getElementById("forgotPasswordHeader").style.display = "none";
    document.getElementById("forgotUserNameInstructions").style.display = "block";
    document.getElementById("forgotPasswordInstructions").style.display = "none";
    document.getElementById("forgotUsernamePasswordForm").action = "/ForgotUsernamePost.do";
    

    document.getElementById("forgotUserNamePasswordBackground").style.height = document.body.offsetHeight+"px";
    document.getElementById("forgotUserNamePasswordBackground").style.display = "block";
    document.getElementById("forgotUserNamePasswordPopupBox").style.display = "block";
    var actualWidth = parseInt(document.getElementById("forgotUserNamePasswordPopupBox").offsetWidth, 10);
    document.getElementById("forgotUserNamePasswordPopupBox").style.marginLeft = "-"+(actualWidth/2)+"px";

    $('#forgotUserNamePasswordPopupBox').focus();

    setforgotUserNamePasswordFocusEvents();

    $('#username').focusout( function() {
        $('#forgotUserNameSubmitButton').focus();
    });

    $('#username').click( function() {
        $('#username').focus();
    });
    
    
    $('#forgotUserNamePasswordBackground').click( function() {
        $('#forgotUserNamePasswordPopupBox').focus();
    });
}   /* forgotUserName() */



function setforgotUserNamePasswordFocusEvents()
{
    $('#forgotUserNameCancelButton').focusout( function() {
        $('#forgotUserNameCancelIcon').focus();
    });
    
    $('#forgotUserNameCancelIcon').focusout( function() {
        $('#username').focus();
    });

}




function forgotUserNamePasswordCancel()
{
    document.getElementById("forgotUserNamePasswordBackground").style.display = "none";
    document.getElementById("forgotUserNamePasswordPopupBox").style.display = "none";
    clearState();

    if($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').show();

    if (($('#uid').val().length > 0) && ($('#uid').val() != localUserLabel))
        $('#pswd').focus();
    else
        $('#uid').focus();
    $('#forgotUserNamePasswordBackground').unbind('click');
}   /* forgotUserNamePasswordCancel() */




function forgotUserNamePasswordPost()
{
	var errMsg = "";
    errMsg += validateNotEmpty_IdentifyOnError("usernamePasswordLabel", "username");
    if (errMsg.length > 0)
    {
        _setLabelErrorStyleClass(document.getElementById("usernamePasswordLabel"), false);
	    raveModalAlert("", errMsg, "Continue", "DIRE_WARNING", "username");
	    return;
    }

    var nameStr = document.getElementById("username").value;
    var possiblePhoneNumber = "";
    if (nameStr.match(/\d/g) != null)
    {
        possiblePhoneNumber =  extractNumericalString(nameStr);
        if ((possiblePhoneNumber.length == 10) && (nameStr.indexOf("@") < 0))
        	document.getElementById("username").value = possiblePhoneNumber;
    }
    
    var successMsg = "";
    jQuery.ajax({
        url: document.getElementById("forgotUsernamePasswordForm").action,
        data: { username: document.getElementById("username").value,
            innocuous: document.getElementById("innocuous").value },
        dataType: 'html',
        complete: function(xhr, textStatus) {
            if (xhr.responseText.indexOf("securityViolation") >= 0)
            {
                location.href="/accessViolation.do";
                return;
            }
            else if (xhr.responseText.indexOf("userNotFound") >= 0)
                errMsg += "User not found";
            else if (xhr.responseText.indexOf("usernameSent") >= 0)
            {
                document.getElementById("forgotUserNamePasswordPopupBox").style.display = "none";
                userNameSent();
                return;
            }
            else if (xhr.responseText.indexOf("userForPasswordNotFound") >= 0)
                errMsg += "User not found";
            else if (xhr.responseText.indexOf("passwordResetRequestSent") >= 0)
            {
                document.getElementById("forgotUserNamePasswordPopupBox").style.display = "none";
            	passwordResetRequestSent();
            	return;
            }
            else
                errMsg += "Unknown error.";
            
            if (errMsg.length == 0)
            {
                raveModalAlert("", successMsg, "Continue", "", "username");
                return
            }
            else
            {
                _setLabelErrorStyleClass(document.getElementById("usernamePasswordLabel"), false);
                raveModalConfirm("", errMsg, "Continue", "", "", "DIRE_WARNING",
                        "username", "username");
            }
        }
    });

}   /* forgotUserNamePasswordPost() */


function restoreBackground()
{
    document.getElementById("_raveModalBackground").style.height = document.body.offsetHeight+"px";
    document.getElementById("_raveModalBackground").style.display = "block";
}


function userNameSent()
{
	if ($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').hide();

    document.getElementById("forgotUserNamePasswordBackground").style.height = document.body.offsetHeight+"px";
    document.getElementById("forgotUserNamePasswordBackground").style.display = "block";
    document.getElementById("userNameSentPopupBox").style.display = "block";
    var actualWidth = parseInt(document.getElementById("userNameSentPopupBox").offsetWidth, 10);
    document.getElementById("userNameSentPopupBox").style.marginLeft = "-"+(actualWidth/2)+"px";

    $('#userNameSentPopupBox').focus();
    
    $('#forgotUserNamePasswordBackground').click( function() {
        $('#userNameSentPopupBox').focus();
    });

    $('#userNameSentContinueButton').focusout( function() {
        $('#userNameSentContinueCancel').focus();
    });

    $('#userNameSentContinueCancel').focusout( function() {
        $('#userNameSentContinueButton').focus();
    });

}   /* userNameSent() */



function userNameSentClose()
{
    document.getElementById("forgotUserNamePasswordBackground").style.display = "none";
    document.getElementById("userNameSentPopupBox").style.display = "none";
    clearState();

    if ($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').show();
}   /* userNameSentClose() */




function forgotPassword()
{
    $('#usernamePasswordLabel').text('User name');
    _setLabelErrorStyleClass(document.getElementById("usernamePasswordLabel"), true);
	forgotMode = "PASSWORD";
	
    if($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').hide();

    document.getElementById("forgotUserNameHeader").style.display = "none";
    document.getElementById("forgotPasswordHeader").style.display = "inline";
    document.getElementById("forgotUserNameInstructions").style.display = "none";
    document.getElementById("forgotPasswordInstructions").style.display = "block";
    document.getElementById("forgotUsernamePasswordForm").action = "/ForgotPasswordPost.do";
    
    document.getElementById("forgotUserNamePasswordBackground").style.height = document.body.offsetHeight+"px";
    document.getElementById("forgotUserNamePasswordBackground").style.display = "block";
    document.getElementById("forgotUserNamePasswordPopupBox").style.display = "block";
    var actualWidth = parseInt(document.getElementById("forgotUserNamePasswordPopupBox").offsetWidth, 10);
    document.getElementById("forgotUserNamePasswordPopupBox").style.marginLeft = "-"+(actualWidth/2)+"px";

    document.getElementById("username").focus();
}   /* forgotPassword() */


function forgotPasswordCancel()
{
    document.getElementById("forgotUserNamePasswordBackground").style.display = "none";
    document.getElementById("forgotPasswordPopupBox").style.display = "none";

    if($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').show();
}   /* forgotPasswordCancel() */



function forgotPasswordPost()
{
    var errMsg = validateNotEmpty_IdentifyOnError("forgotPasswordUsernameLabel", "forgotPasswordUsername");
    if (errMsg.length == 0)
           document.getElementById("forgotPasswordForm").submit();
    document.getElementById("forgotPasswordUsername").focus();
}   /* forgotPasswordPost() */





function passwordResetRequestSent()
{
    if ($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').hide();

    document.getElementById("forgotUserNamePasswordBackground").style.height = document.body.offsetHeight+"px";
    document.getElementById("forgotUserNamePasswordBackground").style.display = "block";
    document.getElementById("passwordResetRequestSentPopupBox").style.display = "block";
    var actualWidth = parseInt(document.getElementById("passwordResetRequestSentPopupBox").offsetWidth, 10);
    document.getElementById("passwordResetRequestSentPopupBox").style.marginLeft = "-"+(actualWidth/2)+"px";

    $('#passwordResetRequestSentPopupBox').focus();
    
    $('#forgotUserNamePasswordBackground').click( function() {
        $('#passwordResetRequestSentPopupBox').focus();
    });

    $('#passwordResetRequestSentContinueButton').focusout( function() {
        $('#passwordResetRequestSentContinueCancel').focus();
    });

    $('#passwordResetRequestSentContinueCancel').focusout( function() {
        $('#passwordResetRequestSentContinueButton').focus();
    });

}   /* passwordResetRequestSent() */




function passwordResetRequestSentClose()
{
    document.getElementById("forgotUserNamePasswordBackground").style.display = "none";
    document.getElementById("passwordResetRequestSentPopupBox").style.display = "none";
    clearState();

    if($.browser.msie && parseInt($.browser.version, 10) < 9) $('embed').show();

    if (($('#uid').val().length > 0) && ($('#uid').val() != localUserLabel))
        $('#pswd').focus();
    else
        $('#uid').focus();

}   /* passwordResetRequestSentClose() */





function clearState()
{
    document.getElementById("username").value = "";
    
	jQuery.ajax({
		type: 'GET',
		url: '/ClearForgottenState.do'
	});
}   /* clearState() */

</script> 
