			    	$(this).attr('href', href.replace('http://webservices.cstv.com/?rtype=CBS-BuyNow&main=', "http://www.replayphotos.com/" + sellPartner +"/purchase.cfm?rM=" + sport).replace("zz", ""));	
			    }
			});
