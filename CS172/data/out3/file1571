	}
	
	$('#footer-links').append("<a href='http://" + window.location.host + "/feedback/" + schoolCode + "-feedback.html' class='csdisclaimerlink'>Feedback</a> | <a class='capitalize' href='http://" + window.location.host + "'>" + pg_settings.domain + "</a>");
	
	if(location.host == "www.fightingillini.com"){
