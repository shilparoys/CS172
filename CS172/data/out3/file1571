  <script>

//AJAX CODE
var getXML = function(url) { 
	var xmlDoc = null; 
	$.ajax({ 
		type: "GET", 
		url: url, 
		dataType: "xml",
		async: false, 
		success: function(xml) { 
			xmlDoc = $(xml); 
			}, 
			error: function(jqXHR, text, errorMsg) { 
			/* alert("Unable to access XML file."); */
			} 
	}); 
	return xmlDoc; 
}
</script> 
