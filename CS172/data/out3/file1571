$(document).ready(function() {
    if( $.cookie('showOnlyOne') ){
        $('#cooked').hide();
    } else {
        $.cookie('showOnlyOne', 'showOnlyOne', { expires: 1,  path: '/'  });
        $('#cooked').show(); 
    }
});

$(window).load(function(){
	favIcon = document.createElement('link');
	favIcon.rel = 'shortcut icon';
	favIcon.href = "http://grfx.cstv.com/graphics/school-logos/" + schoolCode + "-lg.png";
	$("head").append(favIcon);
