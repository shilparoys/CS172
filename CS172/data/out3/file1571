//AJAX CODE

// RECENT INDEX CODE
$(document).ready(function(){
	var link = "/gallerylink/indexes/" + schoolCode + "/recent_index.xml?c=" + (new Date().getTime() + "").slice(0,-5);
	var xmlDoc = getXML(link);	
	var xGal = $(xmlDoc).find('gallery');
	$($(xGal).get().reverse()).each(function(x){ 
		var hideGallery = $(this).find('hide').text();
		var popup = $(this).find("popup_js").text();
		var newLink = popup.replace("window.open('http://onlyfans.cstv.com/schools/" + schoolCode + "/", "http://" + window.location.host + "/");
		newLink = newLink.substring(0, newLink.indexOf("'"));

		var thumb = $(this).find('thumb_url').text();
		var date = $(this).find('create_date').text();
		var description = $(this).find('description').text();
		var title = $(this).find('title').text();
		$("<li class=hide" + hideGallery + "><a href=\"" + newLink + "\" id=\"\" class=\"\">" + "<img class=\"pull-right\" src=\"" + thumb + "\">" + "<h4>" + date + "</h4>" + "<p class='galTitle'>" + title + "</p>" + "<p class='hidden-phone galDescription'>" + description + "</p>" + "</a><div class='clearfix'></div></li>").appendTo("#recent-index");
	});
});
var sCode;
var sNode;
var sName;
var iLink;
var subNode; 
var nodeVal; 
var nuNode;

// INDEX CODE
$(document).ready(function() { 
	var link = "/gallerylink/indexes/" + schoolCode + "/master_index.xml?c=" + (new Date().getTime() + "").slice(0,-5);
	var xmlDoc = getXML(link); 
	xmlDoc.find('summary').each(function(){
		$(this).children().each(function(x){
			sCode = this.nodeName;
			var nameNode = $(this).find('full_name').text();
			
			var season="";
			$(this).find('seasons').each(function(){ 
				$($(this).children().get().reverse()).each(function(){
					a = 1;
					sNode = this.nodeName.replace("season_", "");
					subNode = sNode.replace("20", "");
					nodeVal = parseInt(subNode, 10);
					nuNode = nodeVal + 1;
					if(nuNode < 10){ nuNode = "0" + nuNode;}					 
						if($(this).find("none").text() == "0"){
						"";
					}else if($(this).find("none").text() == "1"){						
						season+= "<li href='#myModal' onclick='indexList(this.id)' id='/gallerylink/indexes/" + schoolCode + "/" + sCode + "-" + sNode + ".xml' data-toggle='modal'><span>" + sNode + "</span>-" + nuNode + " | " +  $(this).find("none").text() + " Gallery" + "</li>";						
					}
					else{						
						season+= "<li href='#myModal' onclick='indexList(this.id)' id='/gallerylink/indexes/" + schoolCode + "/" + sCode + "-" + sNode + ".xml' data-toggle='modal'><span>" + sNode + "</span>-" + nuNode + " | " + $(this).find("none").text() +  " Galleries" + "</li>";						
					}
				}); 
			});season+=""
			
			$('#index').append("<div id=" + sCode + "  class='indy'><h4 class='gal-year-header btn btn-link' data-toggle='collapse' data-target='#" + sCode + (x+1) + "'>" + nameNode + "</h4>" + 
			"<div id='" + sCode + (x+1) + "'class='collapse'><ul id='gal-by-year" + (x+1) + "' class='gal-by-year'>" + season + "<div class='clearfix'></div></ul></div>" + 
			"</div>");			
		});	
	});
});

// MODAL GALLERY CODE
function indexList(id) {
	var link = id;
	var xmlDoc = getXML(link);
	var xGal = $(xmlDoc).find('gallery');
	$($(xGal).get().reverse()).each(function(x){
	var hideGallery = $(this).find('hide').text();
	var popup = $(this).find("popup_js").text();
	var newLink = popup.replace("window.open('http://onlyfans.cstv.com/schools/" + schoolCode + "/", "http://" + window.location.host + "/");
	newLink = newLink.substring(0, newLink.indexOf("'"));
	var thumb = $(this).find('thumb_url').text();
	var date = $(this).find('create_date').text();
	var description = $(this).find('description').text();
	var title = $(this).find('title').text();
	if(thumb < 1){
		"";
	}else{
	$("<li class=hide" + hideGallery + "><a href=\"" + newLink + "\" id=\"\" class=\"\">" + "<img class=\"pull-right\" src=\"" + thumb + "\">" + "<h4>" + date + "</h4>" + "<p class='galTitle'>" + title + "</p>" + "<p class='hidden-phone galDescription'>" + description + "</p>" + "</a><div class='clearfix'></div></li>").appendTo("#gal-list");
	} 		
	});
};
