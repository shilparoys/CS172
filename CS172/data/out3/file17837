           <div class="adSky" align="center"> 
            <div class="centerInner" align="center"> 
             <!-- DFP display ad slot --> 
             <div id="div-gpt-ad-skyscraper" class="ad-wide-skyscraper story-wide-skyscraper"> 
              <p style="font-family: arial;"><small>ADVERTISEMENT</small></p> 
              <script type="text/javascript">
