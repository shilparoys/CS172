        //--------------EE LOOP ENDS -------------------------------

        //now we create the map itself
        //create the map.
        //we pass in the following paramters: the id of the container the map will render in
        //the zoom level
        //Center co-ords
        //and the maptype
        var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 4,
                center: new google.maps.LatLng(41.244772 ,-99.84375),
                mapTypeId: google.maps.MapTypeId.ROADMAP
        });

        //now we create the information window
        //this is the popup when you click on the location pns on the map
        var infowindow = new google.maps.InfoWindow();
        
        
        //define the variables for the pins and a counter for the loop
        var marker, i;
        
        //now we loop through the locations array
        for (i = 0; i < locations.length; i++) { 
                //for each object in the array we create a pin on the google map
                //we pass the latitude, longitude as a LatLng object and the map we oringinally created
                marker = new google.maps.Marker({
                        position: new google.maps.LatLng(locations[i].latitude, locations[i].longitude),
                        map: map
                                        
                });
                
                //now we add event listeners for the pins for when they are clicked
                //when the ins are click the popup window will open
                google.maps.event.addListener(marker, 'click', (function(marker, i) {
                        return function() { 
                                infowindow.setContent('<div class="maps-popup"><img src="'+ 
                                        'http://www.iris.edu/hq/internship/' +'images/avatars/' + locations[i].avatar + '" class="popup-avatar"/>' + 
                                        '<h4 class="intern-name">' + locations[i].screenname + '</h4>'
                                        + '<p>Project Title: ' + locations[i].project_title + '</p>'
                                        + '<p>Institution: ' + locations[i].host + '</p>' +
                                        '<p> Host: ' + locations[i].hostname + '</p>' + 
                                        '<p>' + '<a href="http://www.iris.edu/hq/internship/blogs/user/'+ locations[i].id + '">Read Blog Entries</a></p>' +
                                        '</div>');
                                infowindow.open(map, marker);
                        }
                })(marker, i));
        }


                
                
                
                
                
                
                
        //------------------------------------------------------------
        // TOGGLE PROJECT DESCRIPTION
        //------------------------------------------------------------
        // 
        // This show will toggle the showing and hiding of the oriject descrition
        //
                
        //show hide project description information        
        $(document).ready( function(){
                
                
                var proj = $('.proj-head'),
                desc = $('div.proj-desc');
                
                $('div.proj-desc').hide();
                
                proj.click(function(){$(this).next('div.proj-desc').slideToggle(300);});
        });
</script> 
