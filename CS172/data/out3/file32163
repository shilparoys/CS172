  <script type="text/javascript">
            $(document).ready(function(){
                    jQuery.fn.preventDoubleSubmit = function () {
                            var alreadySubmitted = 0;
                            return jQuery(this).submit(
                                       function(){
                                                    if(alreadySubmitted > 0){
                                                            alreadySubmitted++;
                                                            alert("Please be patient.  We are still processing your original message.");
                                                            return false;
                                                    }else{
                                                            alreadySubmitted++;     
                                                            return true;
                                                    }
                            }); //end jQuery(this).submit
                    };//end jQuery.fn.preventDoubleSubmit

                    jQuery('#form1').preventDoubleSubmit();

                    jQuery('input, textarea').placeholder();

                    jQuery("#webchat").click(function(e){
                              BG.startChatWithIssueId('26', true);
                              return false;
                    });



            });
    </script> 
