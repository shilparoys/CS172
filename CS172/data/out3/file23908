             <div class="richPinInformation" itemscope itemtype="http://schema.org/Recipe"> 
              <h2 class="richPinName" itemprop="name"> <a href="http://www.ourpaleolife.com/2013/02/baked-cinnamon-apples/" rel="nofollow" class="richPinNameLink" itemprop="url"> "Baked" Cinnamon Apples </a> </h2> 
              <div class="richPinTitleWrapper"> 
               <span itemprop="recipeYield"> Serves 1 </span> 
              </div> 
              <div class="richPinSourceWrapper"> 
               <a href="http://www.ourpaleolife.com/2013/02/baked-cinnamon-apples/" rel="nofollow" class="richPinSourceLink"> <img src="https://s-media-cache-ak0.pinimg.com/favicons/ff7afb549adc6cf0d4ee1d7f57cadfa625abee953f1c8b881dcb7998.png?a8ce844b434303aa48afd87a5d0f4b9a" class="richPinSourceFavicon"> Our Paleo Life </a> 
              </div> 
              <div class="richPinMeta "> 
               <div class="richPinTableColumn richPinTableColumnLeft"> 
                <h3 class="richPinRecipeIngredientHeading">Produce</h3> 
                <table class="richPinIngredientTable"> 
                 <tbody>
                  <tr> 
                   <td class="richPinRecipeIngredientAmount"> 1 </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Apple, Sweet </td> 
                  </tr> 
                 </tbody>
                </table> 
                <h3 class="richPinRecipeIngredientHeading">Baking &amp; Spices</h3> 
                <table class="richPinIngredientTable"> 
                 <tbody>
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1 tsp </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Cinnamon, Ground </td> 
                  </tr> 
                  <tr class="underline"> 
                   <td class="richPinRecipeIngredientAmount"> 1 Dash </td> 
                   <td class="richPinRecipeIngredientName" itemprop="ingredients"> Nutmeg, Ground </td> 
                  </tr> 
                 </tbody>
                </table> 
               </div> 
               <div class="richPinTableColumn richPinTableColumnRight"> 
