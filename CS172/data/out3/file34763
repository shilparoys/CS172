    var footerSlider = new hImageSwapper();

    footerSlider.wrapper         = 'footerSlider';  //name of div with class of imageswapper
    footerSlider.animationType   = 'fade'; //slide or fade
    footerSlider.continious      = 1; //0 no, 1 yes  (only matters for sliding)
    footerSlider.animationSpeed  = 40; //slide: lower-slower, fade: lower-faster
    footerSlider.direction       = 1; //1 or -1
    footerSlider.unit            = 'whole';   // each or whole (only matters for sliding)
    footerSlider.xmlurl          = '/library/xml/usc-12-footer-logos.xml' + '?'+ Math.round(Math.random()*100000000);
    footerSlider.wrapperWidth    = 868;
    footerSlider.wrapperHeight   = 77;
    footerSlider.perUnit         = 7; //1 to fit images in horizontally OR enter how many (if in a grid). Fading *must* be set to how many fit.
    footerSlider.timeDelay       = 6500; //set to zero to not autorotate, (Will stop if this is faster than it takes to complete the animation)
    footerSlider.animate         = 1; // 1 yes - 0 no
    footerSlider.showControls    = 0; // 1 yes - 0 no
    
    DomReady.ready(function(){
