       <script>

	var pathname;
	pathname = window.location.pathname;

	if ($('.node-type-landing').length || $('.node-type-section').length) {
		$('#breadcrumbs').hide();
	}
	else {
		$('#block-menu-menu-utilitynav').hide();
	}

	$('a#openglobalnav').hover(
		function () {
		//	$("#open-nav").attr("src", "/sites/all/themes/custom/sls/images/arrow_down_1a1a1a_on.gif");
		},
		function () {
		//	$("#open-nav").attr("src", "/sites/all/themes/custom/sls/images/arrow_down_1a1a1a_off.gif");
		}
	);

	$('a#closeglobalnav').click(function(){
		$('#block-menu-menu-utilitynav').slideUp(500, function(){
			$('#breadcrumbs').show();
		});
	});

	$('#breadcrumbs h1').click(function(){
		$('#breadcrumbs').hide(500);
		$('#block-menu-menu-utilitynav').slideDown(500);
	}); 

</script> 
