     <script type="text/javascript">
    jQuery(document).ready(function ($) {
        //MMA Social Share - email share overlay trigger.
        jQuery("a.socialshare-email").trigger("click");
        var viewportHeight = $(window).height();
        if(viewportHeight != null)
            document.getElementById("hdnHeight").value = viewportHeight;  
        //desktop
        $("#emailshare").addClass("emailshare-desktop").removeClass("emailshare-tablet");
        $("#message").addClass("message-desktop").removeClass("message-tablet");
        $(".txt-fld").addClass("txt-fld-desktop").removeClass("txt-fld-tablet");
        $("#emailshare-header").addClass("emailshare-header-desktop").removeClass("emailshare-header-tablet");
        $("span.lblCaptcha").addClass("lblCaptcha-desktop").removeClass("lblCaptcha-tablet");
        $("img.captcha").removeClass("captcha-tablet");

        //tablet
        if (viewportHeight < 769) {
            $("#emailshare").addClass("emailshare-tablet").removeClass("emailshare-desktop");
            $("#message").addClass("message-tablet").removeClass("message-desktop");
            $(".txt-fld").addClass("txt-fld-tablet").removeClass("txt-fld-desktop");
            $("#emailshare-header").addClass("emailshare-header-tablet").removeClass("emailshare-header-desktop");
            $("span.lblCaptcha").addClass("lblCaptcha-tablet").removeClass("lblCaptcha-desktop");
            $("img.captcha").addClass("captcha-tablet");
        }
    });

    $(function () {
        $('a[rel*=leanModal]').leanModal({ top: GetTopVal(), closeButton: ".modal_close" });
    });

    //mmaSocialShare: leedan Jan. 2013
    function share(type) {
        var url = "http://www.metmuseum.org" + window.location.pathname;
        var title = escape(window.document.title);
        var windowFeatures = GetCenterDimensions();
        switch (type) {
            case 'facebook':
                window.open('http://www.facebook.com/sharer.php?u=' + url + '&t=' + title, 'myWindow', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Facebook']);
                break;
            case 'twitter':
                title = title + ":&via=metmuseum";
                window.open("https://twitter.com/share?url=" + url + "&text=" + title, 'sharer', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Twitter']);
                break;
            case 'pinterest':
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Pinterest']);
                selectPinImage();
                break;
            case 'tumblr':
                window.open("http://www.tumblr.com/share/link?url=" + encodeURIComponent(url) + "&name=" + encodeURIComponent(title) + "&description='&nbsp;'", 'sharer', "'" + windowFeatures + "'");
                _gaq.push(['_trackEvent', 'mmaSocialShare', 'Click', 'Tumblr']);
                break;
        }
    }

    //Viewport smaller with height smaller than 675px move top of popup modal to '0'
    //Note - mma social share popup supports smallest viewport hieght of 600px; so small tablet size.
    function GetTopVal(){
        if ($(window).height() < 675)
            return 0;
        else
            return 50;
    }

    function GetCenterDimensions() {
        var windowFeatures;
        var leftPosition, topPosition;
        //Allow for borders.
        leftPosition = (window.screen.width / 2) - ((604 / 2) + 10);
        //Allow for title and status bars.
        topPosition = (window.screen.height / 2) - ((604 / 2) + 50);
        var windowFeatures = "status=no,height=520,width=604,resizable=yes,left=" + leftPosition + ",top=" + topPosition + ",screenX=" + leftPosition + ",screenY=" + topPosition + ",toolbar=no,menubar=no,scrollbars=no,location=no,directories=no,dependent=yes,fullscreen=no";
        return windowFeatures;
    }
</script> 
