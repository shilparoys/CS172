  <script type="text/javascript">
            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-3239288-2']);
            _gaq.push(['_setDomainName', 'metmuseum.org']);
            _gaq.push(['_setAllowLinker', true]);
            _gaq.push(['_setAllowAnchor', true]);
            _gaq.push(['_trackPageview']);
            _gaq.push(['_trackPageLoadTime']);

            (function () {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();
        </script> 
  <!-- tracks file downloads and links as Events
        reference: http://www.freshandnew.org/2011/07/fixing-document-download-and-link-tracking-with-the-google-analytics-asynchronous-tracking-code/
        --> 
  <script type="text/javascript">

            $(document).ready(function () {

                $('a').mouseup(function () {
                    href = $(this).attr('href');
                    if (href != null) {
                        href_lower = href.toLowerCase();
                        if (href_lower.substr(-3) == "pdf" || href_lower.substr(-3) == "xls" || href_lower.substr(-3) == "doc" || href_lower.substr(-4) == "ashx" ||
  			            href_lower.substr(-3) == "mp3" || href_lower.substr(-3) == "mp4" || href_lower.substr(-3) == "flv" ||
  			            href_lower.substr(-3) == "txt" || href_lower.substr(-3) == "csv" || href_lower.substr(-3) == "zip") {
                            _gaq.push(['_trackEvent', 'Downloads', href_lower.substr(-3), href]);
                        }
                        if (href_lower.substr(0, 4) == "http") {
                            var domain = document.domain.replace("www.", '');
                            if (href_lower.indexOf(domain) == -1) {
                                href = href.replace("http://", '');
                                href = href.replace("https://", '');
                                _gaq.push(['_trackEvent', 'Outbound Traffic', href]);
                            }
                        }
                    }
                });

            });

        </script> 
