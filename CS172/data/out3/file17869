	if(window_location.indexOf('/classifieds/') < 0 && window_location.indexOf('/life/celebrations/') < 0  && window_location.indexOf('LIFE07') < 0)
	{
		var tbn_mbl_cookieName = "stopMobileRedirect";
		if(jQuery.browser.mobile) // mobile browsers
		{
			//check the URL parameter
			if(getURLParameter(tbn_mbl_cookieName) == 1)
			{
				// set the cookie
				setCookie(tbn_mbl_cookieName,1, 1);
			}
			else
			{
				if(!getCookie(tbn_mbl_cookieName))
				{
					articleParam = isArticle ? '?articleRedirect=1' : '';
					window.location = navigator.userAgent.toLowerCase().indexOf("android") > -1 || navigator.userAgent.match(/iPhone/i) ?
					'http://mobile.buffalonews.com'+articleParam : 'http://m.buffalonews.com';
				}
			}
		}
		function getURLParameter(name)
		{
			return decodeURI(
				(RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,null])[1]
			);
		}
	}
