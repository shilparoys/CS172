			function onYouTubeIframeAPIReady() {
				ytplayer = {};
				if(mobile){
					$('.ytvideoplayer').each(function(){
						if(!ytplayer) return false;
						this.setAttribute('style', false);
						ytplayer[this.id] = new YT.Player(this.id, {
							height: this.getAttribute('data-height'),
							width: this.getAttribute('data-width'),
							videoId: this.getAttribute('data-video'),
						});
					});	
				} else {
					$('.ytvideoplayer').click(function(){
						if(!ytplayer) return false;
						this.setAttribute('style', false);
						ytplayer[this.id] = new YT.Player(this.id, {
							height: this.getAttribute('data-height'),
							width: this.getAttribute('data-width'),
							videoId: this.getAttribute('data-video'),
							events: {
								'onReady': function(event) {
							    	event.target.playVideo();
								}
							}
						});
					});	
				}
			}
