      <script type="text/javascript">     var xmlhttpNotificationCenter = false;   var notificationTitleArray = new Array();   var notificationURLArray = new Array();   var nextNotification = 1;   var notificationCenterDelay = 5000;      xmlhttpNotificationCenter=GetXmlHttpObjectSearch();   if (xmlhttpNotificationCenter==null) {      alert ("Sorry, but I couldn't create an XMLHttpRequest");   }     if (xmlhttpNotificationCenter) {    xmlhttpNotificationCenter.onreadystatechange = setnotificationTitleArray;    xmlhttpNotificationCenter.open("GET", "/page/xml.notificationCenter", true);    xmlhttpNotificationCenter.send(null);   }   else {    alert("Sorry, but I couldn't create an XMLHttpRequest");   }     function GetXmlHttpObjectSearch() {    if (window.XMLHttpRequest) {      /* code for IE7+, Firefox, Chrome, Opera, Safari */      return new XMLHttpRequest();    }    if (window.ActiveXObject) {      /* code for IE6, IE5 */      return new ActiveXObject('Microsoft.XMLHTTP');    }    return null;   }     function setnotificationTitleArray() {    if (xmlhttpNotificationCenter.readyState == 4) {     if (xmlhttpNotificationCenter.status == 200) {      if (xmlhttpNotificationCenter.responseXML) {       var allNotifications = xmlhttpNotificationCenter.responseXML.getElementsByTagName("note");       for (var i=0; i<allNotifications.length; i++) {        var curSortType = allNotifications[i].getElementsByTagName("sortType")[0].firstChild.nodeValue;        if ( curSortType == 'Latest' ) {         var curID = allNotifications[i].getElementsByTagName("FK_noteID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/notificationCenter.click?noteID='+curID;        } else if ( curSortType == 'Most Clicked' ) {         var curID = allNotifications[i].getElementsByTagName("FK_contentID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         var curCatID = allNotifications[i].getElementsByTagName("FK_catID")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/content.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html?nav='+curCatID;        } else if ( curSortType == 'Most Commented Article' ) {         var curID = allNotifications[i].getElementsByTagName("FK_contentID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         var curCatID = allNotifications[i].getElementsByTagName("FK_catID")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/content.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html?nav='+curCatID;        } else if ( curSortType == 'Most Commented Poll' ) {         var curID = allNotifications[i].getElementsByTagName("FK_pollID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/polls.detail/id/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else if ( curSortType == 'Most Commented Topic' ) {         var curID = allNotifications[i].getElementsByTagName("FK_topicID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/mboard.detail/fNav/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else if ( curSortType == 'Most Commented Blog' ) {         var curID = allNotifications[i].getElementsByTagName("FK_blogID")[0].firstChild.nodeValue;         var curTitle = allNotifications[i].getElementsByTagName("title")[0].firstChild.nodeValue;         notificationTitleArray[i] = curTitle;         notificationURLArray[i] = '/page/blogs.detail/display/'+curID+'/'+curTitle.replace(/[^0-9a-zA-Z]/g,'-')+'.html';        } else {         notificationTitleArray[i] = '';         notificationURLArray[i] = '';        }       }       setNotification(0, rotateNotifications);      }     }    }   }         function rotateNotifications() {    setTimeout('setNotification("'+nextNotification+'",rotateNotifications)', notificationCenterDelay);    nextNotification++;    if ( nextNotification > notificationTitleArray.length -1 ) {     nextNotification = 0;    }   }       function setNotification(chosen,functionName) {    document.getElementById('notificationNumber').innerHTML = (parseInt(chosen)+1)+') ';    document.getElementById('notificationTitle').innerHTML = notificationTitleArray[chosen];    document.getElementById('notificationURL').href = notificationURLArray[chosen];    document.getElementById('notificationURL').title = notificationTitleArray[chosen];    functionName();   }  </script> 
