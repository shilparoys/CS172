    <script language="javascript">    var xmlhttp;        function getBreaking() {     xmlhttp=GetXmlHttpObject();     if (xmlhttp==null) {        alert ("Sorry, but I couldn't create an XMLHttpRequest");        return;     }          var url="/cached/layout.breakingNews.html";     url += '?sid='+Math.random();       xmlhttp.onreadystatechange=stateChanged;     xmlhttp.open('GET',url,true);     xmlhttp.send(null);          }       function GetXmlHttpObject() {     if (window.XMLHttpRequest) {       /* code for IE7+, Firefox, Chrome, Opera, Safari */       return new XMLHttpRequest();     }     if (window.ActiveXObject) {       /* code for IE6, IE5 */       return new ActiveXObject('Microsoft.XMLHTTP');     }     return null;    }        function stateChanged() {     if (xmlhttp.readyState==4) {      if (xmlhttp.status == 200) {       document.getElementById('breakingNewsAjax').innerHTML = '';       document.getElementById('breakingNewsAjax').innerHTML=xmlhttp.responseText;      }     }    }        getBreaking();      </script> 
    <script language="javascript">

	var showMobileAd = 0;
	
	/* check for apple product */
	var apple = {};
	apple.userAgent = navigator.userAgent;
	apple.device = false;
	apple.products = ["iPhone", "iPod", "iPad"];
	for (var d = 0; d < apple.products.length; d++) {
		var t = apple.products[d];
		apple[t] = !!apple.userAgent.match(new RegExp(t, "i"));
		apple.device = apple.device || apple[t];
	}
	
	
	
	/* cookie manipulation */
	function getCookie(cookieName) {
		var i,x,y,ARRcookies=document.cookie.split(";");
		for ( i=0; i<ARRcookies.length; i++ ) {
			x = ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			y = ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			x = x.replace(/^\s+|\s+$/g,"");
			if ( x == cookieName ) {
				return unescape(y);
			}
		}
	}
	 
	function setCookie(cookieName,value,exdays) {
		var cookieName = 'mobileIphoneAppAdClose';
		var expireDate = new Date();
		expireDate.setDate(expireDate.getDate() + exdays);
		/* expireDate.setTime(expireDate.getTime() + 60000); for testing - set to 1 minutes */
		var cookieValue=escape(value) + ((exdays==null) ? "" : "; expires="+expireDate.toUTCString());
		document.cookie = cookieName + "=" + cookieValue;
	}
	/* END cookie manipulation */
	 
 

	if ( apple.device ) {
		var checkForCookie = getCookie("mobileIphoneAppAdClose");
		if (checkForCookie!=null && checkForCookie!="") {
			showMobileAd = 0;
		} else {
			showMobileAd = 1;
		}
	}
	
	function closeAd(){
		setCookie("mobileIphoneAppAdClose",'adClosed',14);
	}


	/* create display ad */
	if ( showMobileAd ) {
		var msg = "\n Enjoy a better mobile experience by downloading the free \'Messenger News\' app today! Would you like to download the app now? \n\n";
		if (confirm(msg)) {
                                                  closeAd();
			location.replace("/iPhone");
		} else {
			closeAd();
		}
	}
</script> 
    <script language="javascript">

	var showMobileAndroidAd = 0;
	
	/* check for apple product */
	var android = {};
	android.userAgent = navigator.userAgent;
	android.device = false;
	android.products = ["android"];
	for (var d = 0; d < android.products.length; d++) {
		var t = android.products[d];
		android[t] = !!android.userAgent.match(new RegExp(t, "i"));
		android.device = android.device || android[t];
	}
	
	
	
	/* cookie manipulation */
	function getCookie(cookieName) {
		var i,x,y,ARRcookies=document.cookie.split(";");
		for ( i=0; i<ARRcookies.length; i++ ) {
			x = ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			y = ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			x = x.replace(/^\s+|\s+$/g,"");
			if ( x == cookieName ) {
				return unescape(y);
			}
		}
	}
	 
	function setCookie(cookieName,value,exdays) {
		var cookieName = 'mobileAndroidAppAdClose';
		var expireDate = new Date();
		expireDate.setDate(expireDate.getDate() + exdays);
		/* expireDate.setTime(expireDate.getTime() + 60000); for testing - set to 1 minutes */
		var cookieValue=escape(value) + ((exdays==null) ? "" : "; expires="+expireDate.toUTCString());
		document.cookie = cookieName + "=" + cookieValue;
	}
	/* END cookie manipulation */
	 
 

	if ( android.device ) {
		var checkForCookie = getCookie("mobileAndroidAppAdClose");
		if (checkForCookie!=null && checkForCookie!="") {
			showMobileAd = 0;
		} else {
			showMobileAd = 1;
		}
	}
	
	function closeAd(){
		setCookie("mobileAndroidAppAdClose",'adClosed',14);
	}


	/* create display ad */
	if ( showMobileAd ) {
		var msg = "\n Enjoy a better mobile experience by downloading the free \'Messenger News\' app today! Would you like to download the app now? \n\n";
		if (confirm(msg)) {
			closeAd();
			location.replace("/Android");
		} else {
			closeAd();
		}
	}
</script> 
