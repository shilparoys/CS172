ocument.body.parentNode.scrollWidth) && (viewportwidth > document.body.parentNode.clientWidth)) {    window_width = viewportwidth;   } else {    if (document.body.parentNode.clientWidth > document.body.parentNode.scrollWidth) {     window_width = document.body.parentNode.clientWidth;    } else {     window_width = document.body.parentNode.scrollWidth;    }   }   var popUpDiv = document.getElementById(popUpDivVar);   window_width=window_width/2-150;   popUpDiv.style.left = 20 + '%';  }  function popup(windowname) {   blanket_size(windowname);   window_pos(windowname);   toggle('blanket');   toggle(windowname);    }  </script> 
