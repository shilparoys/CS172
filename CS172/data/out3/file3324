     <script type="text/javascript">
	function calculateCost(){
	var total=0.0;
	
	
	var guest1minor = document.form1.guest1minor;
			
	for (x=0; x<=5; x++){
		total += calculateCostPerArea("liId"+x);
		}
	//total = calculateCostPerArea("liId2");
	
	total = looksLikeMoney(total);

	loadTextInToElement(total, "total_cost");
	}
	
	function calculateCostPerArea(repeateAreaIndex){
		var areaTotal=0.0;
		var parking_cost=25;
		if(document.getElementsByName('attendee_first_name_'+repeateAreaIndex)[0].value=="") return 0;


		if(document.getElementsByName('weekend_'+repeateAreaIndex)[0].checked)
					areaTotal += 15;
		if(document.getElementsByName('lunch_'+repeateAreaIndex)[0].checked)
					areaTotal += 15;
		if(document.getElementsByName('package_'+repeateAreaIndex)[0].checked)
					areaTotal += 25;
		if(areaTotal>25) areaTotal = 25;
					
		//var springSing = document.getElementsByName('tickettype_'+repeateAreaIndex);

		//if(springSing[0].checked) areaTotal += 25;
		//if(springSing[1].checked) areaTotal += 10;
		

		//if(document.getElementsByName('vip_parking_'+repeateAreaIndex)[0] != null && 		
		//	document.getElementsByName('vip_parking_'+repeateAreaIndex)[0].checked)
		//	areaTotal += parking_cost;

		//alert ("cost is "+areaTotal);
		
		
		return areaTotal;
	}
	
	
	
	function calculateonecost2(guestminor, tickettype){
		if(tickettype[2].checked){
		return 10;
		}
		if(tickettype[0].checked&&guestminor.checked==false){
		return 35;
		}
		if(tickettype[1].checked&&guestminor.checked==false){
		return 25;
		}
		if(tickettype[0].checked&&guestminor.checked==true){
		return 15;
		}
		if(tickettype[1].checked&&guestminor.checked==true){
		return 10;
		}
		return 0;
	}



	function loadTextInToElement(fragment_text, element_id) {
 		var element = document.getElementById(element_id);
		element.innerHTML = fragment_text;
	}
	function looksLikeMoney(total) {
		var total = (parseInt((total+.001)*100))/100;
		var niceTotal = "$"+total;
		if(niceTotal.substring(niceTotal.length-2,niceTotal.length-1)=="."){
			niceTotal=niceTotal+"0";
		}
 		return niceTotal;
	}

	</script> 
     <script type="text/javascript">

function makeSubmitEnabled(){
	document.getElementById('submit_form').disabled='';
}

</script> 
     <h1>Parents Weekend: March 20 and 21, 2015 </h1> 
     <p>This popular annual event provides parents a glimpse of their student's life at Westmont. We invite you to take advantage of the activities offered throughout the weekend, including an update from the president, mini-seminars, connecting with other parents and of course, Spring Sing, Westmont's biggest musical tradition. <strong> All events, times and locations are available on the <a href="http://www.westmont.edu/parents/parents-weekend.html" target="_blank">Parents Weekend Schedule</a>. Online registration deadline is midnight on March 13.</strong> </p> 
     <div> 
     </div> 
     <form class="forms" id="form1" name="form1" method="post" action="https://secure.westmont.edu/forms/alumni/parents_weekend/index_real_processor.php" onsubmit="document.getElementById('submit_form').disabled = 1;"> 
      <fieldset> 
       <legend>Parents Weekend Costs</legend> 
       <ol> 
        <li> 
         <ul> 
          <li> Parents Weekend Registration: $15 per adult - Includes all weekend events and a Saturday continental breakfast; it does not include the Saturday Parents lunch or meals in the D.C.</li> 
          <li> Saturday Parents Lunch: $15 per person </li> 
          <li> OR Parents Weekend Discounted Package: $25 - Includes all weekend events, the Saturday continental breakfast plus the Saturday Parents lunch (excludes meals in the D.C.)</li> 
          <li> For Spring Sing tickets and parking information, please visit the <a href="http://www.westmont.edu/_offices/campuslife/SpringSingTicketInformation.html" target="_blank">Spring Sing Webpage</a> </li> 
         </ul> </li> 
       </ol>
