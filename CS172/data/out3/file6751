  <script type="text/javascript">
	function validatesss(){
		if(document.frmsrch.search.value=="" || document.frmsrch.search.value=="Please Enter The Text" || document.frmsrch.search.value=="Search Site")
		{
			document.frmsrch.search.style.backgroundColor ="#cfe8ff";
			document.frmsrch.search.value="Please Enter The Text";
			document.frmsrch.search.focus();
			return false;
		}
	}
	function emptysss()
	{
		document.frmsrch.search.value="";
		return false;
	}

	</script> 
  <script type="text/javascript" src="http://www.ub.bw/js/ub.js"></script> 
