  <script type="text/javascript">
            function sendAuthn(name, value, formID){
                $('body').removeClass('ui-loading');
                try{
                    var oForm = document.forms[formID];
                            
                    var found = $(oForm).find("#name");
                    if (found.length == 0) {
                        $(oForm).append("<input type='hidden' name='"+name+"' value='"+value+"'/>");
                    }
                    else {
                        found[0].val(value);
                    }
                    oForm.submit();
                }catch(e){
                    alert('Error....\n'+e.message);
                }
            }
            $(document).ready(function() {
                // disable ajax nav
                $.mobile.ajaxLinksEnabled = false;
                $(document).bind( "pagebeforeload", function( event, data ){
                    event.preventDefault();
                    data.deferred.resolve( data.absUrl, data.options, page );
                });
                $('.x509_tls_module').click(function() {
                    if ($(this).attr("disabled")!="true") {
                        var oForm = document.forms["AuthNForm"];
                        $(oForm).append("<input type='hidden' name='adAS_oldaction' value='"+$(oForm).attr("action")+"'/>");
                        $(oForm).attr("action", "https://identidad.usal.es/adas/themes/usal/tls/index.php");
                        oForm.submit();
                        //$('#externalauth').load("https://identidad.usal.es/adas/themes/usal/tls/js-tls.php");
                    }
                });                    
            });
        </script> 
