        <tr> 
         <td scope="row" width="10">&nbsp;</td> 
         <td width="*%"> 
          <div class="div_content" id="div_content">
           <br>
           <h2 style="padding-bottom: 0px; text-align: center; padding-top: 0px; padding-left: 0px; margin: 0px; padding-right: 0px"> &nbsp;</h2> 
           <h2 style="padding-bottom: 0px; text-align: center; padding-top: 0px; padding-left: 0px; margin: 0px; padding-right: 0px"> <a href="http://e-tour2011.rudn.ru" target="_blank"><img alt="" src="pictur.php?t_id=2572" style="border-top: 0px solid; height: 32px; border-right: 0px solid; border-bottom: 0px solid; border-left: 0px solid; width: 202px"></a></h2> 
           <h2 style="padding-bottom: 0px; text-align: center; padding-top: 0px; padding-left: 0px; margin: 0px; padding-right: 0px"> <a href="http://e-tour.rudn.ru/" target="_blank"><img alt="" src="/pictur.php?t_id=3608" style="border-top: 0px solid; height: 32px; border-right: 0px solid; border-bottom: 0px solid; border-left: 0px solid; width: 202px"></a></h2> 
           <h2 style="padding-bottom: 0px; text-align: center; padding-top: 0px; padding-left: 0px; margin: 0px; padding-right: 0px"> &nbsp;</h2> 
           <table border="0" cellpadding="1" cellspacing="1" style="width: 100%;"> 
            <tbody> 
