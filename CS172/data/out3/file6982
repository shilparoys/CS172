		 var doc_options = '<option value="na">Selecciona una opción</option><option value="http://www.lasalle.mx/oferta-educativa/doctorado/educacion/">Doctorado en Educación</option><option value="http://www.lasalle.mx/oferta-educativa/doctorado/administracion/">Doctorado en Administración</option>';
		 
		 jQuery(document).on('change','input[type=radio][name=oferta_radio]', function(e) {
			 e.preventDefault();
			 var cur_val = jQuery('input[type=radio][name=oferta_radio]:checked').val();

			 if(cur_val == 1) {
				 jQuery('#oferta_select').html(lic_options);
			 }
			 if(cur_val == 2) {
				 jQuery('#oferta_select').html(pre_options);
			 }
			 if(cur_val == 3) {
				 jQuery('#oferta_select').html(esp_options);
			 }
			 if(cur_val == 4) {
				 jQuery('#oferta_select').html(mae_options);
			 }
			 if(cur_val == 5) {
				 jQuery('#oferta_select').html(cur_options);
			 }
			 if(cur_val == 6) {
				 jQuery('#oferta_select').html(doc_options);
			 }
		 });

		 jQuery(document).on('submit','.ow_form', function(e) {
			 e.preventDefault();
			 var target_page = jQuery('#oferta_select').val();
       	  	
			if(target_page != 'na') {
				 window.location.assign(target_page);
			 } 
			 else if ( jQuery('#radio5').prop('checked') && target_page === 'na' ) {
			 	window.location.assign('http://lasalle.mx/oferta-educativa/cursos-y-diplomados/');	
			 }
       	  	//default go to listview content
			else {
				window.location.assign('http://lasalle.mx/oferta-educativa/#list_view_content');	
			}
		 });
		</script> 
