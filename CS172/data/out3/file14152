  <script type="text/javascript">
	$(document).ready(function(){
	
	$("#accord-main1 dd").hide();
	$("#accord-main1 dt").addClass("open");
	$("#accord-main1 dt").eq(0).addClass("close");
	$("#accord-main1 dd").eq(0).show();
	
	$("#accord-main1 dt").hover(
	  function () {
	$(this).addClass('feedback');
	  },
	  function () {
	    $(this).removeClass('feedback');
	  }
	); // end feedback for .faq data term
	
	$("#accord-main1 dt").click(function(){
		$(this).next("dd").slideToggle("fast")
		.siblings("dd:visible").slideUp("fast");
		$(this).toggleClass("close");
		$(this).siblings("dt").removeClass("close");
	});
	
	$("#accord-main2 dd").hide();
	$("#accord-main2 dt").addClass("open");
	//$("#accord-main1 dt").eq(0).addClass("close");
	//$("#accord-main1 dd").eq(0).show();
	
	$("#accord-main2 dt").hover(
	  function () {
	$(this).addClass('feedback');
	  },
	  function () {
	    $(this).removeClass('feedback');
	  }
	); // end feedback for .faq data term
	
	$("#accord-main2 dt").click(function(){
		$(this).next("dd").slideToggle("fast")
		.siblings("dd:visible").slideUp("fast");
		$(this).toggleClass("close");
		$(this).siblings("dt").removeClass("close");
	});

	
	//SCRIPT FOR SCROLLING BACK TO TOP
	$('a.top').click(function(){
	     $('html, body').animate({scrollTop: '0px'}, 300);
	     return false;
	});
	
	//BASIC TOOLTIP
	$('a').tooltip({
		track: true,
		delay: 0,
		showURL: false,
		showBody: " - ",
		fade: 250
	});


});
</script> 
