             <div id="ctl00_cplhMainContent_Polls1_poll_footer" class="poll_footer"></div> 
             <style>
    .poll_list_item
    {
       list-style: none;
    }
</style> 
             <script type="text/javascript">
      	  
    function UpdateLoad() {
	  
        var answer_id_value = $('#ctl00_cplhMainContent_Polls1_rblPollAnswers input:checked').val();
    var primary_background = '#ba131e'		
    var primary_text = '#ffffff'		
    if(typeof answer_id_value === 'undefined'){answer_id_value = 0;};
			
    $.ajax({
        type: "POST",
        url: "/services/get_polls.ashx",
        data: { answer_id: answer_id_value },
        dataType: "json",
        success:      function (data) {
            $.each(data, function (index, elem) {				
                $('.poll_submit_button_container')	
                $('.poll_item').css('display', 'none');
                $('#poll_page_link').css('display', 'block');
                $('.poll_submit_button_container').css('display', 'none');				
                var $chart = $( ".poll_list_item" );
                $chart.append(data[index].answer_text)
					  .append( '<li><div class="poll_bar" style="background:' + primary_background + ';' + 'color:' + primary_text + '; width:' + data[index].pct + '%;">' + '<div class="poll_pct_label">' + data[index].pct + '% </div></div></li>')
                $('.poll_bar').css('min-width', '20px');
            });
        }});			
}
	  
function review_answer() {
	
    var answer_id_value = $('#ctl00_cplhMainContent_Polls1_rblPollAnswers input:checked').val();			
    if(typeof answer_id_value === 'undefined'){answer_id_value = "0";};
	
    if (answer_id_value != "0") {UpdateLoad()}		
};

</script> 
