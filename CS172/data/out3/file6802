  <script type="text/javascript">
	function mycarousel_initCallback(carousel) {
    jQuery('#mycarousel-prev').bind('click', function() {
   	   _currentPage = _currentPage +1;
   	   if(_currentPage > _pages)
   	   {
   	   _currentPage = 1;
   	     carousel.scroll(1);
   	   }
        carousel.next();
        $('#txtPage').html(_currentPage);
        return false;
        
    });

    jQuery('#mycarousel-next').bind('click', function() {
		_currentPage = _currentPage -1;
   	   if(_currentPage <=0 )
   	   {
   	     _currentPage = _pages;
   	     carousel.scroll(_pages*_scrlCount);
   	   }
        carousel.prev();
        $('#txtPage').html(_currentPage);
        return false;
    });
};

var _scrlCount = 3;
var _currentPage = 1;
var _pages = 0;
$(document).ready(function() {
		$('#txtPage').before('<a href="#" id="mycarousel-next" class="arrow back">&lt;</a>');
		$('#txtPages').after('<a href="#" id="mycarousel-prev" class="arrow forward">&gt;</a>');
		$("div .wrapper").attr("id","mycarousel");
		$("div .wrapper").addClass("jcarousel-skin-tango");
		
		$("div .wrapper").css("overflow","hidden");
		_pages =Math.ceil( $("#spotlights ul>li").length/_scrlCount);
		$('#txtPage').html(_currentPage);
		$('#txtPages').html(_pages);

jQuery("#mycarousel").jcarousel({
        scroll: _scrlCount,
        wrap: 'last',
        rtl:false,
        initCallback: mycarousel_initCallback,

        buttonNextHTML: null,
        buttonPrevHTML: null
    });	                   
});

</script> 
  <!--End SpotLights--> 
