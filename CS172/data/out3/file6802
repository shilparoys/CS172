$(this).css("background-image","none");
});

$('div.menu-vertical>ul.root>li.static>ul.static>li:not(:has(> ul)) > a').each(function(){
$(this).css("background-image","none");
});

$("div.menu-vertical>ul.root>li.static>ul.static").each(function(){
$(this).hide();
$(this).css("margin-left","10px");
});
	$("#"+_divID+" ul li").last().css({"font-family":"Arial","font-size":"13","margin-top":"-1px","font-weight":"bold"});}
	});
