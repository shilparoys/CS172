     <script type="text/javascript">

	function showPagedContent (selector, which) 
	{
		$(selector).each( function (index, elem) {
			// Show the previous page.
			if ( which == 'prev' ) {
				which = typeof(elem.current_page) !== 'undefined' ? Math.max(1, elem.current_page - 1) : 1;
				showPagedContent(selector, which);
			
			// Show the next page.
			} else if ( which == 'next' ) {
				which = typeof(elem.current_page) !== 'undefined' ? Math.min($(elem).find('.page_content').length, elem.current_page + 1) : 1;
				showPagedContent(selector, which);
			
			// Show the specified page.
			} else {
				// Show the specified page.
				elem.className = elem.className.replace(/\s*show_page_[0-9]+/g, '');
				$(elem).addClass('show_page_' + which);
				
				// Set the page number.
				elem.current_page = which;
				
				// Update the pagination links.
				if ( $(elem).find('.pagination') ) {
					$(elem).find('.pagination').removeClass('last_page');				
					if ( $(elem).find('.page_content').length == which ) {
						$(elem).find('.pagination').addClass('last_page');
					};
				};
			};
		});
		
		return false;
	};
	
</script> 
