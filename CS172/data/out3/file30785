    <script type="text/javascript" src="http://media.nj.com/static/classifieds/merchandise/js/jquery.blockUI.js"></script> 
    <script type="text/javascript">
$(document).ready(function() { 

$.blockUI.defaults.css.top= '25%'; 
$.blockUI.defaults.css.textAlign= 'left'; 
$.blockUI.defaults.css.border='none', 
$.blockUI.defaults.css.backgroundColor='none', 
$.blockUI.defaults.css.cursor='pointer';

$('#s2fxclose').click($.unblockUI);
$('#report_s2fxclose').click($.unblockUI);
$('#s2fxclose_t').click($.unblockUI);
$('.close_wrap .close').click($.unblockUI);	

$("#sendForm").submit(function(event) {
	event.preventDefault(); 
	$('.error').css('visibility','hidden');

	var submit = true;
	var sender = $('#YOUR_NAME').val();
	var email = $('#FROM').val();
	var destination = $('#TO').val();
	var code = $('#code').val();

	if (sender == "") { 
		$('#name_error').css('visibility','visible');
		submit = false;
	}
	if (email.search(/^(\w([-._\w]*\w)*@(\w[-_\w]*\w\.)+\w{2,9})$/)  ) { 
		$('#from_error').css('visibility','visible');
		submit = false;
	}
	if (destination.search(/^(\w([-._\w]*\w)*@(\w[-_\w]*\w\.)+\w{2,9})$/)  ) { 
		$('#email_error').css('visibility','visible');
		submit = false;
	}
	if (code == "") { 
		$('#captcha_error').css('visibility','visible');
		submit = false;
	}
	if (submit) { 
		url = '/forge/data_entry';
		$.post( url, 
			$("#sendForm").serialize(),
			function( data ) {
				if (data.status == 'success') {
					$.unblockUI;
					$.blockUI({message: $('#sendtofriend_thanks')});
					utag.view({page_name:"classifieds_nj /merch send to a friend thank you page", "custom_page_url":"http://classifieds.nj.com/classifieds_send_to_friend_thanks"});
				} else {
					$('#captcha_error').css('visibility','visible');
					getCaptcha();
				}
			},
			'json'
		);
	} 
});

$("#reportForm").submit(function(event) {
	event.preventDefault(); 
	$('.error').css('visibility','hidden');

	var submit = true;
	var code = $('#report_code').val();

	if (!$('input[name=report]:checked').val()) {
    		$('#report_error').css('visibility','visible');
		submit = false;
	}
	if (code == "") { 
		$('#report_captcha_error').css('visibility','visible');
		submit = false;
	}
	if (submit) { 
		url = '/forge/data_entry';
		$.post( url, 
			$("#reportForm").serialize(),
			function( data ) {
				if (data.status == 'success') {
					$.unblockUI;
					$.blockUI({message: $('#sendtofriend_thanks')});
				} else {
					$('#report_captcha_error').css('visibility','visible');
					getCaptcha();
				}
			},
			'json'
		);
	} 
});
});

function launcher(id) {
	$('#send_ad_id').val(id);
	getCaptcha();
	$.blockUI({message: $('#sendtofriend')});
	utag.view({page_name:"classifieds_nj /merch send to a friend", "custom_page_url":"http://classifieds.nj.com/classifieds_send_to_friend"});
} 

function launch_report(id) {
	$('#report_ad_id').val(id);
	getCaptcha();
	$.blockUI({message: $('#reportad')});
} 

function getCaptcha() {
	var date = new Date();
	var time = date.getTime();
	var captchaImgSrc = 'http://www.nj.com/cgi-bin/prxy/captcha/nph-captcha.cgi/captcha/img/get?key=' + time;
	$('#captcha_id').val(time);
	$('#captcha_img').attr('src',captchaImgSrc); 
	$('#code').val('');
	$('#report_captcha_id').val(time);
	$('#report_captcha_img').attr('src',captchaImgSrc); 
	$('#report_code').val('');
}
</script> 
