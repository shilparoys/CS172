      <script src="http://www.mapquestapi.com/sdk/js/v7.0.s/mqa.toolkit.js?key=Gmjtd%7Clu6y2gu7ng%2C80%3Do5-lwbsg"></script> 
      <script type="text/javascript">
	var myPoiCollection = new MQA.ShapeCollection();


function startMap(){
	var mapWindow = document.getElementById('mapWindow');
	var options = {};
	if (mapWindow) {
		getResults();
		if (!myPoiCollection.getSize()) {
			options={
				elt:document.getElementById('mapWindow'),
				zoom:8,
				latLng:{lat:40.281921, lng:-74.699203}
			};
		} else {
			options={
				elt:document.getElementById('mapWindow'),
				collection: myPoiCollection,
				bestFitMargin: 50
			};
		}
		window.map = new MQA.TileMap(options);
		MQA.withModule('viewoptions', function() {
			map.addControl(
				new MQA.ViewOptions()
			);
		});
		MQA.withModule('largezoom', function() {
			map.addControl(
				new MQA.LargeZoom(),
				new MQA.MapCornerPlacement(MQA.MapCorner.TOP_LEFT, new MQA.Size(5,5))
			);
		});
	}
}

function getResults() {
	if (listingAdId.length) {
		for (var j=0; j < listingAdId.length; j++) {
			poiAdId=listingAdId[j];
			poiIndex=listingAdIndex[j];
			poiStreet=listingStreet[j];
			poiCity=listingCity[j];
			poiState=listingState[j];
			poiPostalcode=listingZip[j];
			poiDateTime = listingDateTime[j];
			poiText = listingText[j];
			poiLat=listingLat[j] * 1;
			poiLng=listingLng[j] * 1;
			poiDetailLink=listingDetailLink[j];

			if (poiLat != '' && poiLng != '' && !isNaN(poiLat) && !isNaN(poiLng)) {
				poiLatLng = new MQA.LatLng();
				poiLatLng.setLatLng(poiLat,poiLng);
			} else {
				continue;
			}

			imageCount=listingImages[j].length;
			propertyDetails = poiDetailLink;

			str = '<div class="map_info_box">';
			str += '<div class="addy_bar">';
			str += '<span class="pop_addr">' + poiStreet + '<br />' + poiCity + ', ' + poiState + ' ' + poiPostalcode + '</span>';
			str += '</div>';

			str += '<div class="more_details">';

			str += '<div class="garage_details">';
			str += '<span style="float: left; font-weight: bold;">Date/Time:</span>';
			str += '<span style="float: right; color: #fcef22; width: 136px;">' + poiDateTime + '</span>';
			str += '</div>';

			str += '<div class="garage_details">';
			str += '<span style="font-weight: bold;">Details: </span>';
			str +='<span>';
			if (imageCount > 0) {
				str += '<img src="http://media.nj.com/static/classifieds/merchandise/images/icon_camera.gif" />';
			}
			str += poiText +'</span>';
			str += '</div>';

			str += '<div class="clear"></div>';
			str += '<div class="garage_pop_buttons">';

			str += '<a href="' + poiDetailLink + '"';
			if (listingAdPay[j] == 'yes') str += ' target="_blank"';
			str += '><img src="http://media.nj.com/static/classifieds/merchandise/images/btn_moreinfo_gray.gif" /></a>';
			str += '<a href="#"><img id="dtp' + poiAdId + '" onclick="setDrivingTour(\''+poiAdId+'\',\'change\'); return false;" src="http://media.nj.com/static/classifieds/merchandise/images/btn_adddrivetour_green.gif" /></a>';
			str += '</div>';
			str += '</div>';
			str += '</div>';

			myMapIcon = new MQA.Icon('http://media.nj.com/static/classifieds/merchandise/images/icon_saletag.gif',44,58);
			myShadowIcon = new MQA.Icon('http://www.nj.com/images/spacer.gif',0,0);

			poi = new MQA.Poi(poiLatLng);

			poi.setRolloverContent(str);
			poi.setValue('key',poiAdId);
			poi.setValue('icon',myMapIcon);
			poi.setValue('shadow',myShadowIcon);

			myPoiCollection.add(poi);
			MQA.EventManager.addListener(poi, 'rolloveropen', doMouseOver);
		}
	}
}
function doMouseOver(e){
	var adId=this.getValue('key');
	setDrivingTour(adId);
}

function showRollover (adId) {
	var poiCollectionSize = myPoiCollection.getSize();
	for (var i=0; i < poiCollectionSize; i++) {
		var poi = myPoiCollection.getAt(i);
		if (poi.getValue('key') == adId) {
			poi.toggleInfoWindowRollover();
		}
	}
	setDrivingTour(adId);
}

$(document).ready(function() {
	var mapWindow = document.getElementById('mapWindow');
	if (mapWindow) startMap();
});

</script> 
