   <div class="main_container"> 
    <script type="text/javascript">
	var listingStreet = new Array();
	var listingCity= new Array();
	var listingState = new Array();
	var listingZip = new Array();

	var listingAdId = new Array();
	var listingAdIndex = new Array();
	var listingAdNumber = new Array();
	var listingImages = new Array();

	var listingDateTime = new Array();
	var listingText = new Array();

	var listingLat = new Array();
	var listingLng = new Array();
	var listingDetailLink = new Array();
	var listingAdPay = new Array();

function addListing(adId,adIndex,adNumber,street,city,state,zip,adText,dateTime,lat,lng,detailLink,isAdPay,imageArray) {
	listingAdId.push(adId);
	listingAdIndex.push(adIndex);
	listingAdNumber.push(adNumber);
	listingStreet.push(street);
	listingCity.push(city);
	listingState.push(state);
	listingZip.push(zip);
	listingText.push(adText);
	listingDateTime.push(dateTime);
	listingLat.push(lat);
	listingLng.push(lng);
	listingDetailLink.push(detailLink);
	listingAdPay.push(isAdPay);
	listingImages.push(imageArray);
}

function buildTour() {
	var tourCookieValue=readCookie('tour_id');
	if (tourCookieValue) {
		window.open('?property=nj.com&temp_type=clipboard&category=directions&tp=ME_nj&new=true','driving_tour','width=880,height=600,menubar=0,resizable=1,scrollbars=1,location=1');
	} else {
		alert('You must select at least one garage sale in order to build your driving tour.');
	}
}

function setDrivingTour (adId,mode) {
	var tourIds = readCookie('tour_id');
	var resultsElement = document.getElementById('dt'+adId);
	var popupElement = document.getElementById('dtp'+adId);
	var detailImageElement = document.getElementById('dtImage');

	var wrd = '';
	var detailImageSrc = '';
	var detailImageAlt = '';
	var popupImageSrc = '';

	var displayMode = '';

	if (tourIds.indexOf(adId) > -1) {
		if (mode == 'change') {
			RemoveCookieItem ('tour_id',adId);
			displayMode = 'add';
		} else {
			displayMode = 'remove';
		}
	} else {
		if (mode == 'change') {
			AppendCookie ('tour_id',adId);
			displayMode = 'remove';
		} else {
			displayMode = 'add';
		}
	}
	if (displayMode == 'remove') {
		if (resultsElement) {
			resultsElement.className = "drive_tour_remove";
			resultsElement.innerHTML = 'Remove from Driving Tour';
		}
		if (popupElement) {
			popupElement.src = 'http://media.nj.com/static/classifieds/merchandise/images/btn_remove_green_grey.gif';
		}
		if (detailImageElement) {
			detailImageElement.src = 'http://media.nj.com/static/classifieds/merchandise/images/btn_remove_green_small3.gif';
			detailImageElement.alt = 'Remove From Driving Tour';
		}
	} else {
		if (resultsElement) {
			resultsElement.className = "drive_tour_add";
			resultsElement.innerHTML = 'Add to Driving Tour';
		}
		if (popupElement) {
			popupElement.src = 'http://media.nj.com/static/classifieds/merchandise/images/btn_adddrivetour_green.gif';
		}
		if (detailImageElement) {
			detailImageElement.src = 'http://media.nj.com/static/classifieds/merchandise/images/btn_adddrivetour_green_small.gif';
			detailImageElement.alt = 'Add To Driving Tour';
		}
	}
}
</script> 
