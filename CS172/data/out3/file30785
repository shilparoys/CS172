  <script language="javascript">
	//globals
	var maj_count_is_one=false ;

	$(document).ready(function() {
		see_saved_ads();
		if ($('#categ_col').length > 0){
			var listring = '';
			var print_once = 'false';
			var cur_cat = '';
			var minor_select = '';



			jQuery.getJSON("/?property=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard and Estate Sales&AD_IMAGE_EXISTS=&cur_cat=&keywords=&date_created=&tl=3", function(jp) {
				if(jp.properties.length > 1){
					if('nj.com' =='nj.com'){		
						$('#categ_col').append('<p class="major_cat_gold">All Sources</p>');
					}else{
						$('#categ_col').append('<p class="major_cat"><a href="?cur_cat=&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard and Estate Sales&orderby=feature_value(featured_listing):nl,date_created:d">All Sources</a></p>');
					}	
					listring = '';
					$.each(jp.properties,function(i,item) {
						if(item.property =='nj.com'){		
							listring = "<li class='active'>"+item.name+"</li>" + listring;
						}else{
							listring = listring + "<li><a href='?cur_cat=&orig_cat=&orig_prop=nj.com&property="+item.property+"&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard and Estate Sales&orderby=feature_value(featured_listing):nl,date_created:d'>"+item.name+"</a></li>";
						}
					});
					$('#categ_col').append('<ul class="minor_cat">'+listring+'</ul>');
					$('#categ_col').append('<hr/>');
				}

				var img_exists = '';
					img_exists = readCookie('ad_image_exists' );
				if(!img_exists){
					$('#categ_col').append('<p class="major_cat_gold">Ads with Photos</p>');
					listring = '';
						listring = "<li><a title='Show only ads with photos' href='?property=nj.com&orig_prop=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard%20and%20Estate%20Sales&ad_image_exists=PHOTO1'>Show only ads with photos</a></li>";
					$('#categ_col').append('<ul class="minor_cat">'+listring+'</ul>');
				}
			});
			if($("#categ_col").children().length == 0){
				$("#categ_col").remove();
			}
		}
	});


function changeSort(sorterId){
	var sortElement = document.getElementById(sorterId);
	var url = "?property=nj.com&orig_prop=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard%20and%20Estate%20Sales";
	url += '&orderby=' + sortElement.options[sortElement.selectedIndex].value;
	location.href=url;
}

function changeCount(countId){
	var sortElement = document.getElementById(countId);
	var url = "?property=nj.com&orig_prop=nj.com&temp_type=browse&category=results&tp=ME_nj&classification=Garage-Yard%20and%20Estate%20Sales";
	if (sortElement.options[sortElement.selectedIndex].value > 10) {
		url += '&ads_per_page=' + sortElement.options[sortElement.selectedIndex].value;
	}
	location.href=url;
}

function orig_search_button(){	
		var img_exists = readCookie('ad_image_exists' );
		var dt_created = readCookie('date_created');
		//var adspp = readCookie('ads_per_page');
		//var orderby = '';

	if (((false) || maj_count_is_one) ||(false) || (false) || img_exists != '' || dt_created !='') {
		var str = '<a href="?cur_cat=&orig_cat=&orig_prop=nj.com&property=nj.com&temp_type=browse&category=results&tp=ME_nj';
			str += "&classification=Garage-Yard and Estate Sales";
		if (img_exists) str += '&ad_image_exists=' + img_exists;
		if (dt_created) str += '&date_created=' + dt_created;
		str +=  '"><img align="absmiddle" alt="Return to original search results" src="http://media.nj.com/static/classifieds/merchandise/images/btn_backtooriginal.gif" /></a>';

		document.getElementById('back_to_original').innerHTML = str;
	}
}

$(document).ready(function() {
		$('td.adIdTd > a').each( function() {
			var listingId = this.id;
			setDrivingTour(listingId);
			check_saved_ads(listingId);
		});
});

