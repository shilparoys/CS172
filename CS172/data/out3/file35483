  <script type="text/javascript">
$(document).ready( function () {
	//hide the exceptions
	$("tr.exception").css("display", "none");
	//display-hide exceptions
	$('.toggleException').toggle( 
		function() {
			var excTr = $(this).parents("tr").next();
			if ($.browser.msie) {
				excTr.css('display','block');
			}
			else {
				excTr.css('display','table-row');
			}
			$(this).html("&raquo; Hide Exceptions");
			return false;
		},
		function() {
			var excTr = $(this).parents("tr").next();
			excTr.css('display','none');
			$(this).html("&raquo; View Exceptions");
			return false;
		}
	);
	//highlight library based on url anchor
	if (location.href.lastIndexOf("#") > 0) {
		var section = 'a[name="' + location.href.substring(location.href.lastIndexOf("#")+1) + '"]';
		if ($.browser.msie) {
			$(section).parents("tr").next().css('display','block');
		}
		else {
			$(section).parents("tr").next().css('display','table-row');
		}
		$(section).parents("tr").css("background-color","#ffc");
		$(section).parents("tr").next().css("background-color","#ffc");
		$(section).parents("p").next().children("a.toggleException").html("&raquo; Hide Exceptions");
	}

});
</script> 
