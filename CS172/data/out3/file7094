       <script type="text/javascript">
//<![CDATA[
window.addEvent('domready', function() {
	var walkers = document.getElements('#handlers187 span');
	var spSlide187 = new slideshow_sp2($('sp-slide187'), {
		transition: 'cover-inplace-fade',
		size: {width: 700, height: 300},
		interval: 5000,
		fxOptions: {duration:  500, transition: Fx.Transitions.Circ.easeIn},
		onWalk: function (i, j) {
			walkers[i].addClass('active');
			walkers[j].removeClass('active');
		},
		onInitialized: function () {
			walkers[0].addClass('active');
		}		
	});

	var images = ([
		'http://www.ucad.sn/images/slide_wifi_etudiants2.png','http://www.ucad.sn/images/slide_wifi_personnels2.png'	
	]);
	var images_elements = document.getElements('#sp-slide187 img');

	var loader = Asset.images(images, {
		onProgress: function (i) {
			images_elements[i].src = images[i];
		},		
		onComplete: function () {
			document.getElement('.preload').style.visibility='hidden';
			spSlide187.play();
		}
	});
			spSlide187.addPlayerControls('previous', [$('prev187')]);
		spSlide187.addPlayerControls('next', [$('next187')]);
		
	spSlide187.addItemWalkers(walkers);		
	
});
//]]>
</script> 
