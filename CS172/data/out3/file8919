  <script language="javascript">

//Array function compatible with Netscape 2.0x and 3.0x
function MakeArray(n) {                 
  this.length = n;
  for (var i = 1; i <= n; i++) { 
      this[i] = 0 
  }
  return this
}

var DHTML = (document.getElementById || document.all || document.layers);

function getObj(name)
{
  if (document.getElementById)
  {
     this.obj = document.getElementById(name);
     this.style = document.getElementById(name).style;
  }
  else if (document.all)
  {
     this.obj = document.all[name];
     this.style = document.all[name].style;
  }
  else if (document.layers)
  {
     this.obj = getObjNN4(document,name);
     this.style = this.obj;
  }
}

function getObjNN4(obj,name)
{
     var x = obj.layers;
     var thereturn;
     for (var i=0;i<x.length;i++) {
         if (x[i].id == name) thereturn = x[i];
         else if (x[i].layers.length)
              var tmp = getObjNN4(x[i],name);
         if (tmp) thereturn = tmp;
     }
     return thereturn;
}


var recEQs = new MakeArray(20);
recEQs[0]="eq0";
recEQs[1]="eq1";
recEQs[2]="eq2";
recEQs[3]="eq3";
recEQs[4]="eq4";
recEQs[5]="eq5";
recEQs[6]="eq6";
recEQs[7]="eq7";
recEQs[8]="eq8";
recEQs[9]="eq9";
recEQs[10]="eq10";
recEQs[11]="eq11";
recEQs[12]="eq12";
recEQs[13]="eq13";
recEQs[14]="eq14";
recEQs[15]="eq15";
recEQs[16]="eq16";
recEQs[17]="eq17";
recEQs[18]="eq18";
recEQs[19]="eq19";
function setEQVis(eqIndex,v)
{
  if (!DHTML) return;
  var eq = new getObj(recEQs[eqIndex]);
  eq.style.visibility = (v) ? 'visible' : 'hidden'
}

function init() {
  setEQVis(0,1);
  setEQVis(1,1);
  setEQVis(2,1);
  setEQVis(3,1);
  setEQVis(4,1);
  setEQVis(5,1);
  setEQVis(6,1);
  setEQVis(7,1);
  setEQVis(8,1);
  setEQVis(9,1);
  setEQVis(10,1);
  setEQVis(11,1);
  setEQVis(12,1);
  setEQVis(13,1);
  setEQVis(14,1);
  setEQVis(15,1);
  setEQVis(16,1);
  setEQVis(17,1);
  setEQVis(18,1);
  setEQVis(19,1);
  setTimeout("setEQVis(0,0)",8000);
  setTimeout("setEQVis(1,0)",8000);
  setTimeout("setEQVis(2,0)",8000);
  setTimeout("setEQVis(3,0)",8000);
  setTimeout("setEQVis(4,0)",8000);
  setTimeout("setEQVis(5,0)",8000);
  setTimeout("setEQVis(6,0)",8000);
  setTimeout("setEQVis(7,0)",8000);
  setTimeout("setEQVis(8,0)",8000);
  setTimeout("setEQVis(9,0)",8000);
  setTimeout("setEQVis(10,0)",8000);
  setTimeout("setEQVis(11,0)",8000);
  setTimeout("setEQVis(12,0)",8000);
  setTimeout("setEQVis(13,0)",8000);
  setTimeout("setEQVis(14,0)",8000);
  setTimeout("setEQVis(15,0)",8000);
  setTimeout("setEQVis(16,0)",8000);
  setTimeout("setEQVis(17,0)",8000);
  setTimeout("setEQVis(18,0)",8000);
  setTimeout("setEQVis(19,0)",8000);
}
</script>
