      <div class="livewhaleCal section"> 
       <script type="text/javascript" src="/etc/designs/nyu/clientlibs/nyucore.js"></script> 
       <script type="text/javascript" src="/etc/designs/nyu/clientlibs/nyuutils.js"></script> 
       <script type="text/javascript" src="/etc/designs/nyu/clientlibs/components/calendar.js"></script> 
       <link rel="stylesheet" href="/etc/designs/nyu/clientlibs/components/calendar/cal.css" type="text/css"> 
       <hr> 
       <div class="miniCalfeed" id="miniCalfeed9217"></div> 
       <div class="calendarList" id="feed9217"> 
        <h3 class="entrytitle"></h3> 
        <div class="entries"> 
         <p class="date">&nbsp;</p> 
         <div class="entry"> 
          <em class="date">&nbsp;</em> 
          <h3 class="entrytitle">&nbsp;</h3> 
          <p class="entrydescription">&nbsp;</p> 
         </div> 
        </div> 
        <div class="moreButton"> 
         <p><a href="http://events.nyu.edu">go to calendar</a></p> 
