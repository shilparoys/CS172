        <section data-id="simpleFindfieldControl" class="find-field-outer branding-Above" id="findFieldOuter" role="search" aria-label="Search"> 
         <a href="http://library.camdencc.edu" class="logo" title="" target="_blank"> <img src="http://library.camdencc.edu/images2/edslogo.jpg" class="logo-img" alt="EBSCOhost"> </a> 
         <div class="find-field-inner "> 
          <fieldset class="find-field-controls" data-auto-complete="{&quot;displayGroups&quot;:null,&quot;url&quot;:&quot;http://global.ac.ebsco-content.com/autocomplete/rest/autoComplete&quot;,&quot;queryParams&quot;:{&quot;idx&quot;:&quot;rawqueries&quot;,&quot;filters&quot;:[{&quot;name&quot;:&quot;custid&quot;,&quot;values&quot;:[&quot;sjrlc044&quot;]}],&quot;max&quot;:10,&quot;token&quot;:&quot;ARCuOepbFypkoRlp7qFcUApZYGfXSOzp6jjtELuuKyHvm+FpagiXrENu+hfiV8r8E8o=&quot;,&quot;format&quot;:&quot;json&quot;},&quot;massageData&quot;:true,&quot;searchOnSelect&quot;:true,&quot;completions&quot;:{&quot;wrap&quot;:false,&quot;truncate&quot;:true},&quot;jQueryOverrides&quot;:{&quot;delay&quot;:150}}"> 
