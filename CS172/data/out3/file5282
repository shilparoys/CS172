function updateExample (i) { 
 
        /* Setting default tip to blank */ 
        tip = '&nbsp;'; 
 
        /* inserting case statements */ 
 
        switch (i) { 
 
        case "": 
                example = ''; 
        break; 
        case "FIND_WRD": 
                example = 'old sea Hemingway'; 
        break; 
        case "FIND_WTI": 
                example = 'sea old'; 
        break; 
        case "SCAN_TIT": 
                example = 'old man and the sea<br>(omit initial articles - a, an, the, etc.)'; 
        break; 
        case "FIND_WAU": 
                example = 'edgar allan poe'; 
        break; 
        case "SCAN_AUT": 
                example = 'poe, edgar allan'; 
 
        break; 
        case "FIND_WSU": 
                example = 'tudor england'; 
 
        break; 
        case "SCAN_SUB": 
                example = 'great britain history tudor'; 
 
        break; 
        case "FIND_WSE": 
                example = 'lecture mathematics'; 
 
        break; 
 
        case "FIND_WSRS1": 
                example = 'lecture mathematics'; 
 
        break; 
 
        case "SCAN_SRS": 
                example = 'lecture notes in<br>(omit initial articles - a, an, the, etc.) '; 
 
        break; 
        case "SCAN_PUB": 
                example = 'bantam'; 
 
        break; 
        case "SCAN_PLA": 
                example = 'new york'; 
 
        break; 
        case "FIND_020": 
                example = '115102116'; 
 
        break; 
        case "FIND_022": 
                example = '1130-2887<br> 11302887'; 
 
        break; 
        case "SCAN_SYS": 
                example = '&nbsp;'; 
 
        break; 
        case "FIND_BAR": 
                example = '&nbsp;'; 
                break; 
 
 
        case "SCAN_SUL": 
                example = 'history medicine'; 
                break; 
 
        case "SCAN_COL": 
                example = 'pamphlet'; 
                break; 
 
        case "SCAN_DOC": 
                example = 'periodical'; 
 
                break; 
        case "SCAN_CAL": 
                example = 're65'; 
                break; 
 
        case "SCAN_CLD": 
                example = '891.773'; 
                break; 
 
        case "FIND_WTT": 
                example = 'clinical medicine'; 
                break; 
 
        case "FIND_WPU": 
                example = 'Elsevier'; 
                break; 
 
        case "FIND_WYR": 
                example = '1961'; 
                break; 
 
        case "FIND_WDT": 
                example = 'guide'; 
                break; 
 
        case "SCAN_LNG": 
                example = 'spa'; 
                break; 
 
        case "SCAN_TTL": 
                example = 'Journal of Applied Psychology'; 
                break; 
 
      case "SCAN_SUD": 
                example = 'A 1.10:981'; 
                break; 
 
   case "SCAN_CUC": 
                example = '100'; 
                break; 
      } 
 
        document.getElementById('hint').innerHTML = example 
     return; 
} 
