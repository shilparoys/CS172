  <script>
	function rellenar_oculto(){
		/*alert(document.form1.searchtype.value);
		alert(document.form1.searchtype.options[document.form1.searchtype.selectedIndex].text);
		alert(document.form1.searchtype.selectedIndex);*/
		
		
		if(document.form1.searchtype.value=="X"){
			if(document.form1.searchtype.selectedIndex==2){
				document.form1.oculto.value="t:";
			}
			else{
				if(document.form1.searchtype.selectedIndex==4){
					document.form1.oculto.value="a:";
				}
				else{
					document.form1.oculto.value="";
				}
			}
		}
		else{
			document.form1.oculto.value="";
		}
	}

	function enviar_oculto(){
		document.form1.searcharg.value=document.form1.oculto.value+document.form1.searcharg.value;
	}

</script> 
