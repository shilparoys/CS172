        var jq = jQuery.noConflict();
        jq().loginCheck(); // do the login check off the bat, don't wait for document.ready
        jq(document).ready( function() {
            jq('.announce').announce();
            jq("a:contains('Log In')").loginSwitch({'message' : 'My PoW Work', 'href' : '/pows/portal.htm'});
        })
  </script> 
  <script type="text/javascript" src="/js/jquery.alerts.js"></script> 
  <script type="text/javascript">
	function searchBox() {
		jq('#search_text').css("color", "black");
		if('Search' == jq('#search_text').val()){
			jq('#search_text').val('');
		}
	}
	function searchBoxBlur() {
		if('' == jq('#search_text').val()) {
			jq('#search_text').css("color", "gray");
			jq('#search_text').val('Search');
		}
		
	}
  </script> 
