                     <script type="text/javascript">
//var glid;
function moveToMarker(lid, mid) {
  if(typeof(mid)==='undefined') mid = getFirstKey(getlocations_map);
  var map = getlocations_map[mid];
  var marker = getlocations_markers[mid].lids[lid];
  map.panTo(marker.position);
  google.maps.event.trigger(marker, 'click');
}

function moveToMarkerOpen(lid, mid) {
  if(typeof(mid)==='undefined') mid = getFirstKey(getlocations_map);

  setTimeout( function() {
    marker = getlocations_markers[mid].lids[lid];
    vis = marker.getVisible();
    if (! vis) {
      //marker.setVisible(true);
      //getlocations_markers[mid].lids[lid] = marker;
    }
    marker.setVisible(false);
    google.maps.event.trigger(marker, 'click');
    //google.maps.event.trigger(getlocations_markers[mid].lids[lid], 'click');
  }, 0);

}

function getFirstKey(data) {
  for (var prop in data)
    if (data.propertyIsEnumerable(prop))
      return prop;
}

</script> 
