 <body> 
  <div id="mediaspace"> 
   <!-- Start flash detection & installation ( will install flash player if not present or too old ) --> 
   <object id="player" classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0" name="player" width="0" height="0"> <param name="movie" value="http://media.greenriver.edu/common/jwp/player.swf"> <embed type="application/x-shockwave-flash" pluginspage="http://www.macromedia.com/go/getflashplayer"> </object> 
   <!-- End flash detection & installation --> 
  </div> 
  <script type="text/javascript">
	
// Required variables
	var so = new SWFObject('http://media.greenriver.edu/common/jwp/player.swf','mpl','420','390','9'); // dimensions = W , H , Must account for size of control bar & playlist if used.
		so.addVariable('file','http://media.greenriver.edu/content/TransferCenter/jwp/t123.mp4'); // name of media file to play.  MP4, FLV, F4V, MP3 formats supported.
		so.addVariable('image','http://media.greenriver.edu/content/TransferCenter/jwp/t123ff.png'); // name of image to display before video starts. JPG format supported. Can be larger than player, but should match aspect ratio.
	
// Use the following to incorporate a playlist ( must comment out the 'file' variable above when using a playlist )
		//so.addVariable('playlistfile','http://media.greenriver.edu/SampleCode/FLV-MP4-MP3/playlist.xml'); // playlist location in RSS format
		//so.addVariable('playlist','bottom'); // top, right, bottom, left
		//so.addVariable('playlistsize','250'); // refers to height when on bottom/top, width when on right/left ( 250 displays 3 items with a scrollbar to view additional if present )
		//so.addVariable('item','0'); // Default playlist item to play ( 0 is the first item, 1 is the second, etc... )
		
// Optional parameters ( change only if you understand what they do )
		so.addVariable('bufferlength','5');
		so.addVariable('volume','100'); // 0 - 100
		so.addVariable('stretching','exactfit'); // none, exactfit, uniform, fill
		so.addVariable('controlbar','bottom'); // bottom, over, none	
		so.addParam('allowfullscreen','true'); // true, false
		//so.addVariable('skin','http://media.greenriver.edu/common/skins/beelden.zip'); // uncomment to select this player skin
		so.addVariable('skin','http://media.greenriver.edu/common/skins/modieus.zip'); // uncomment to select this player skin
		so.addParam('allowscriptaccess','always'); 
		so.addVariable('displayTitle','true');
		
// Performance variables ( optimized for fairly old computer to keep compatibility high )
		so.addParam('wmode','window'); // window, opaque, transparent ( window mode uses less CPU )
		so.addVariable('smoothing','true'); // false = better performance for slower computers, but seriously reduces quality
		so.addVariable('quality','autolow'); // low, autolow, autohigh, medium, high, best ( setting too high will result in skipped frames on slower computers )
		so.write('mediaspace'); // Should match the div id
	
// Learn more about embedding this player at the following sites:
// jwplayer specific flashvars at:	http://developer.longtailvideo.com/trac/wiki/Player5FlashVars
//      flashvars descriptions at:	http://kb2.adobe.com/cps/127/tn_12701.html

</script>   
