       <script>
              oz_api = USN.rubicon.apiName;
              oz_site = USN.rubicon.site;
              oz_zone = USN.rubicon.getZone('leaderboardB');
              oz_ad_slot_size = USN.rubicon.getAdSize('leaderboardB');
