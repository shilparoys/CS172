   <script src="http://www.berkeley.edu/js/vendor/imagesloaded.pkgd.min.js"></script> 
   <script>
      // function creates an <iframe> (and YouTube player)
      // after the API code downloads.
	 var players = {};
      function onYouTubeIframeAPIReady() {
		var videos = document.getElementsByClassName('youtube');
		for (var i = 0; i < videos.length; ++i) {
			var thisid = videos[i].id;
			if (thisid.indexOf("_") == -1) {
				var videoid = thisid;
				players[i] = new YT.Player(videoid, {
					height: '1280',
					width: '720',
					videoId: videoid,
					playerVars: { 'autoplay': 0, 'controls': 2, 'modestbranding': 1, 'showinfo': 0, 'ohide': 1, 'enablejsapi': 1 }
				});
			} else {
//timeline videos are identified by category_url
				var n = thisid.indexOf("_");
				var videoid = thisid.substr(n+1);
				players[i] = new YT.Player(thisid, {
					height: '1280',
					width: '720',
					videoId: videoid,
					playerVars: { 'autoplay': 0, 'controls': 2, 'modestbranding': 1, 'showinfo': 0, 'ohide': 1, 'enablejsapi': 1 }
				});
			}
    	}
	  }
</script> 
   <script>
      $(document).ready(function () {
			var queryStr = getUrlVar(); 
			if (queryStr) {
				var myURL = document.location;
				var eid = getHash();
				var withoutHash = queryStr.substr(0,queryStr.indexOf('#'));
				var urlParam = withoutHash.split('=');
	            var catName = decodeURI(urlParam[1]);
				showCatName(catName);
				showTimelineEvent(eid);
			}
				
				$('.media').bind("click keypress", function() {
				 
				  $('.media').removeClass( 'selected' );
				  $('.media').attr('aria-expanded', 'false');
				  $(this).addClass('selected');
				  $(this).attr('aria-expanded', 'true');
				  $('html, body').animate({
			        scrollTop: $(this).offset().top - 50
			      }, 500);
			    
				});
			$('#theTabs li a').click(function() {
				var tabID = $(this).attr('href');
				$('.media').removeClass( 'selected' );
				$("div"+tabID+" ol li.media:first-child").addClass('selected');
			});

			$('#catSelect').on('change', function (e) {
				var thisVal = $(this).val();
				$('.media').removeClass( 'selected' );
				$("div"+thisVal+" ol li.media:first-child").addClass('selected');
				var $tab = $('[data-toggle="tab"][href="'+thisVal+'"]');
				$('#theTabs li a').each(function() {
					if ($(this).attr('href') == thisVal) {$tab.tab('show');}
				});
			});
	  
	function getHash() {
		var hash = window.location.hash.substring(1); 
		return hash;
	}

	function getUrlVar() {
		var query;
		var queryStr = window.location.href.slice(window.location.href.indexOf('?') + 1);
		if (queryStr != window.location.href) {
			query = queryStr;
		} else {
			query = '';
		}
		return query;
	}

	function showCatName(catName) {
				$('.media').removeClass( 'selected' );
				var tabID = $('a').filter(function(index) { return $(this).text() === catName; }).attr('href');
				var $tab = $('[data-toggle="tab"][href="'+tabID+'"]');
				$('#theTabs li a').each(function() {
					if ($(this).attr('href') == tabID) {$tab.tab('show');}
				});
	}
	function showTimelineEvent(eid) {
			  $('.media').removeClass( 'selected' );
			  $('.media').attr('aria-expanded', 'false');
			  $('#'+eid).addClass('selected');
			  $('#'+eid).attr('aria-expanded', 'true');
			  $('html, body').animate({
		        scrollTop: $('#'+eid).offset().top - 50
		    }, 500);
	}
				
	function handleClickAndPress(myfunc)
		{
		    return function (e) {
		        if (e.type != "keypress" || e.keyCode == 13) {myfunc(e);}
		    };
		}
  });
		
</script> 
