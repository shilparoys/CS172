   <script type="text/javascript">
	$(document).ready(function(){
		$("div.desc").hide();
		$("div.PasswordSection").hide();

		if($('#change_password').is(':checked')) { 
			var test = $(this).val();
			$("div.desc").hide();
			$("#change").show()
			$("div.PasswordSection").show(); }
		$("input[name$='ChangeCreate']").click(function() {
			var test = $(this).val();
			$("div.desc").hide();
			$("#"+test).show()
			$("div.PasswordSection").show();
		}); 
	});
	</script> 
