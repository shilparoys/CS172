                                $('div.social-hover-space').attr('id', 'sidr-social');
                                $('#mobile-share-button').sidr({ name: 'sidr-social', side: 'right' });
                                $('#mobile-menu-closeright').click(function () { $.sidr('close', 'sidr-social') });              
                            });
                    }
                }



            }).trigger('resize'); // invoke the handler immediately

            


                            
             
            //            if ($(window).width() < 801) {  //was 481
