  <script type="text/javascript">
    var loggedIn = 0;
    var uName = "";
    var uNameShort = "";
    var sLen = 10;

    if (document.cookie.length > 0 && document.cookie.indexOf("LH=") > 0) {
        var spos = document.cookie.indexOf("LH=")+3;      
        var epos = document.cookie.indexOf("LT=")-2;
        var cookieval = document.cookie.substring(spos, epos);

           if (cookieval != "")
            loggedIn = 1;
    }
   
    if (loggedIn) {

        document.getElementById('uniheader-main-membership-buttons').innerHTML='<a href="https://www.nurse.com/logout" id="registerButton"><span>LOG OUT</span></a><a href="https://www.nurse.com/profiles/edit" id="theAccountButton"><span>PROfile</span><em></em></a>'; 

        document.getElementById('uniheader-mini-membership-buttons').innerHTML='<a id="uniheader-logout" class="social-icon-top ic-logout" href="https://www.nurse.com/logout"></a><a id="uniheader-account" class="social-icon-top ic-account" href="https://www.nurse.com/profiles/edit"></a>';
    }
    else {

        document.getElementById('uniheader-main-membership-buttons').innerHTML='<a href="https://www.nurse.com/signup" id="registerButton"><span>REGISTER</span></a><a href="https://www.nurse.com/signup" id="loginButton"><span>SIGN IN</span><em></em></a>'; 

        document.getElementById('uniheader-mini-membership-buttons').innerHTML='<a id="uniheader-signup" class="social-icon-top ic-signup" href="https://www.nurse.com/signup"></a><a id="uniheader-signin" class="social-icon-top ic-signin" id="headerLoginButton" ></a>';
    }
   
 
</script> 
  <script src="/Areas/Jobs/Scripts/GhgScripts/JSmungeURL.js" type="text/javascript"></script> 
  <script src="/Scripts/jquery-ui-1.9.2.min.js" type="text/javascript"></script> 
  <script src="/js/Watermark.js" type="text/javascript"></script> 
  <script language="javascript" type="text/javascript">
        function callBack() {
        }
        $(function () {
            $('#inputCityStateZip').autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: '/jobs/Search/Suggestions/valid', type: "POST", dataType: "json", //Url to our action supplying a list
                        data: { query: request.term, maxRecords: 10 },
                        success: function (data) {
                            response($.map(data, function (item) {
                                return { label: item, value: item };
                            }));
                        }
                    });
                },
                minLength: 3,
                messages: {
                    noResults: '',
                    results: function () { }
                }
            });
        });
        
        //watermark on textboxes
        $(function () {
            Watermark("#inputTextSearch", "Keywords (Specialty, Company Name, etc.)", "job-field");
            Watermark("#inputCityStateZip", "City, State, or ZIP", "job-field");
        });

        //keword search-alpha numeric textbox
        $("#inputTextSearch").keypress(function (event) {
            var ch = String.fromCharCode(event.keyCode);
            if (event.keyCode <= 32) {
                return;
            }
            if ((event.keyCode <= 57 && event.keyCode >= 48) ||
              (event.keyCode >= 97 && event.keyCode <= 122) || (event.keyCode >= 65 && event.keyCode <= 90)) {
                return;
            }

            event.preventDefault();
        });

        //State or Zip alpha numeric
        $("#inputCityStateZip").keypress(function (event) {
            var ch = String.fromCharCode(event.keyCode);
            if (event.keyCode <= 32) {
                return;
            }
            if ((event.keyCode <= 57 && event.keyCode >= 48) ||
              (event.keyCode >= 97 && event.keyCode <= 122) || (event.keyCode >= 65 && event.keyCode <= 90)) {
                return;
            }
            if (event.keyCode == 44 || event.keyCode == 188)// allow comma
            {
                return;
            }

            event.preventDefault();
        });

    </script> 
