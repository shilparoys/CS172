   <div id="header-search-space"> 
    <form action="http://ce.nurse.com/RCourseSearch.aspx" method="post" name="stdForm" id="stdForm" onsubmit="return fixMiniSearchVars();"> 
     <span> <select name="site" id="minidemo-htmlselect"> <option value="education" selected>Education</option> <option value="jobs">Jobs</option> <option value="news">News</option> </select> <input type="hidden" id="inputTextSearchMini" name="inputTextSearch"> <input name="keywords" id="search-keywords" class="search square" type="text"> <input type="submit" value="GO"> </span> 
     <script type="text/javascript">
                        //fix search term for jobs and CE search, CE search uses get instead of post
                        function fixMiniSearchVars() {
                            document.getElementById("inputTextSearchMini").value = document.getElementById("search-keywords").value;
                            if (document.getElementById("stdForm").action == 'http://ce.nurse.com/RCourseSearch.aspx') {
                                window.location = 'http://ce.nurse.com/RCourseSearch.aspx?SearchT=' + document.getElementById("search-keywords").value;
                                return false;
                            }
                            else if (document.getElementById("stdForm").action == 'https://www.google.com/') {
                                window.location = 'https://www.google.com/?gws_rd=ssl#q=site:news.nurse.com+' + document.getElementById("search-keywords").value.replace(" ", "+");
                                return false;
                            }
                            else
                                return true;
                        };

                        $('#minidemo-htmlselect').ddslick({
                            onSelected: function (data) {
                                var str = data.selectedData.text;
                                switch (str) {
                                    case 'Education':
                                        document.getElementById("stdForm").action = 'http://ce.nurse.com/RCourseSearch.aspx';
                                        break;
                                    case 'Jobs':
                                        document.getElementById("stdForm").action = 'http://www.nurse.com/jobs/search-result/all-jobs';
                                        break;
                                    case 'News':
                                        document.getElementById("stdForm").action = 'https://www.google.com/';
                                        break;
                                    default:

                                        break;
                                }
                            }
                        });
                    </script> 
    </form> 
