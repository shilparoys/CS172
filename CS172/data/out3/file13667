        <li><a href="http://www.nurse.com/jobs/recruiter">Post a Job</a> |</li> 
        <li><a href="http://mediakit.nurse.com/">Media Kit</a> |</li> 
        <li><a href="http://www.nurse.com/contact">Help</a></li> 
       </ul> 
       <div class="clear"></div> 
      </div> 
      <div class="clear"></div> 
      <div class="lighter"> 
       <img src="//www.nurse.com/images/mag-left.gif" class="mag-left" alt="mag-left"> 
       <form action="http://ce.nurse.com/RCourseSearch.aspx" method="post" name="stdForm2" id="stdForm2" onsubmit="return fixSearchVars();"> 
        <select name="site" id="demo-htmlselect"> <option value="education" selected>Education</option> <option value="jobs">Jobs</option> <option value="news">News</option> </select> 
        <input name="keywords" id="search-keywords2" class="search square" type="text"> 
        <input type="submit" value="GO"> 
        <input type="hidden" id="inputTextSearchHead" name="inputTextSearch"> 
        <script type="text/javascript">
                //fix search term for jobs and CE search, CE search uses get instead of post
                function fixSearchVars() {
                    document.getElementById("inputTextSearchHead").value = document.getElementById("search-keywords2").value;
                    if (document.getElementById("stdForm2").action == 'http://ce.nurse.com/RCourseSearch.aspx') {
                        window.location = 'http://ce.nurse.com/RCourseSearch.aspx?SearchT=' + document.getElementById("search-keywords2").value;
                        return false;
                    }
                    else if (document.getElementById("stdForm2").action == 'https://www.google.com/') {
                        window.location = 'https://www.google.com/?gws_rd=ssl#q=site:news.nurse.com+' + document.getElementById("search-keywords2").value.replace(" ", "+");
                         return false;
                    }
                    else
                        return true;
                };

                $('#demo-htmlselect').ddslick({
                    showSelectedHTML: false,
                    onSelected: function (data) {
                        var str = data.selectedData.text;
                        switch (str) {
                            case 'Education':
                                document.getElementById("stdForm2").action = 'http://ce.nurse.com/RCourseSearch.aspx';
                                break;
                            case 'Jobs':
                                document.getElementById("stdForm2").action = 'http://www.nurse.com/jobs/search-result/all-jobs';
                                break;
                            case 'News':
                                document.getElementById("stdForm2").action = 'https://www.google.com/';
                                break;
                            default:

                                break;
                        }
                    }
                });
            </script> 
       </form> 
      </div> 
     </div> 
