  <script type="text/javascript"> <!--To unrandomize the slideshow, set random:1 to random:0 below on line 362 or thereabouts. -->
	$('#section-featured-slideshow').cycle({ 
		fx:'scrollHorz',
		timeout:0,
		random:1,
		slideResize:0,
		next:'#section-featured-slideshow-nav-next', 
		prev:'#section-featured-slideshow-nav-prev'
	});

	$("#section-featured-slideshow").touchwipe({
        wipeLeft: function(e) {
            e.preventDefault();
            $("#section-featured-slideshow").cycle("next");
        },
        wipeRight: function(e) {
            e.preventDefault();
            $("#section-featured-slideshow").cycle("prev");
        },
        preventDefaultEvents: false
    });

	$('#section-featured-slideshow-finals').cycle({ 
		fx:'fade',
		speed:1000,
		timeout:2000
	});
	$('#section-featured-slideshow-finals').cycle('pause');
	$('#section-featured-slideshow-finals-play').click(function (event) {
		event.preventDefault();
		$('#section-featured-slideshow-finals').cycle('resume');
	});

	$( "#section-mobilennav-toggle" ).click(function() {
		$( "#section-mobilennav-links" ).toggle( "slide" );
	});

	$( "#section-mainnav-schools" ).click(function() {
		if ($( "#section-mainnav-schools" ).hasClass( "link-selected" )) {
			$( "#section-mainnav-schools" ).removeClass( "link-selected" );
		} else {
			$( "#section-mainnav-schools" ).addClass( "link-selected" );
		}
		$( "#section-mainnav-schools-list" ).toggle();
	});

	$( "#section-mobilennav-links-schools" ).click(function() {
		if ($( "#section-mobilennav-links-schools" ).hasClass( "link-selected" )) {
			$( "#section-mobilennav-links-schools" ).removeClass( "link-selected" );
		} else {
			$( "#section-mobilennav-links-schools" ).addClass( "link-selected" );
		}
		$( "#section-mobilennav-links-schools-list" ).toggle();
	});

	$( "#section-featured-news-topstory-headline" ).click(function() {
		if ($( "#section-featured-slideshow" ).hasClass( "opacity-reduced" )) {
			$( "#section-featured-slideshow" ).removeClass( "opacity-reduced" );
			$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_open" );
			$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_off" );
		} else {
			$( "#section-featured-slideshow" ).addClass( "opacity-reduced" );
			$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_off" );
			$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_open" );
		}
		$( "#section-featured-news-topstory-preview" ).toggle();
	});
	$( "#section-featured-news-topstory-headline" )
	.mouseenter(function() {
		$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_off" );
		$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_on" );
	})
	.mouseleave(function() {
			$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_on" );
			$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_off" );
	});
	
	/*For Urgent news when posted */
	$( "#section-featured-news-topstory-headline-urgent" ).click(function() {
		if ($( "#section-featured-slideshow" ).hasClass( "opacity-reduced" )) {
			$( "#section-featured-slideshow" ).removeClass( "opacity-reduced" );
			$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_open" );
			$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_off" );
		} else {
			$( "#section-featured-slideshow" ).addClass( "opacity-reduced" );
			$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_off" );
			$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_open" );
		}
		$( "#section-featured-news-topstory-preview" ).toggle();
	});

	$( "#section-featured-news-topstory-preview-close" ).click(function() {
		$( "#section-featured-slideshow" ).removeClass( "opacity-reduced" );
		$( "#section-featured-news-topstory-arrow" ).removeClass( "news_link_open" );
		$( "#section-featured-news-topstory-arrow" ).addClass( "news_link_off" );
		$( "#section-featured-news-topstory-preview" ).toggle();
	});

	$( "#az-index-search" ).focus(function() {
		$( "#atoz-results" ).show();
	});

	$( "#section-content-links-featured-more" ).click(function() {
		if ($( "#section-content-links-featured-more" ).hasClass( "link-more" )) {
			$( "#section-content-links-featured-content" ).addClass( "content-height-auto" );
			$( "#section-content-links-featured-more" ).removeClass( "link-more" );
			$( "#section-content-links-featured-more" ).addClass( "link-less" );
		} else {
			$( "#section-content-links-featured-content" ).removeClass( "content-height-auto" );
			$( "#section-content-links-featured-more" ).removeClass( "link-less" );
			$( "#section-content-links-featured-more" ).addClass( "link-more" );
		}
	});

	$( "#section-content-links-giving-more" ).click(function() {
		if ($( "#section-content-links-giving-more" ).hasClass( "link-more" )) {
			$( "#section-content-links-giving" ).addClass( "content-height-auto" );
			$( "#section-content-links-giving-more" ).removeClass( "link-more" );
			$( "#section-content-links-giving-more" ).addClass( "link-less" );
		} else {
			$( "#section-content-links-giving" ).removeClass( "content-height-auto" );
			$( "#section-content-links-giving-more" ).removeClass( "link-less" );
			$( "#section-content-links-giving-more" ).addClass( "link-more" );
		}
	});

	$( "#section-content-links-featured-header" ).click(function() {
		if ($( "#section-content-links-featured-more" ).hasClass( "link-more" )) {
			$( "#section-content-links-featured" ).addClass( "content-height-auto" );
			$( "#section-content-links-featured-more" ).removeClass( "link-more" );
			$( "#section-content-links-featured-more" ).addClass( "link-less" );
		} else {
			$( "#section-content-links-featured" ).removeClass( "content-height-auto" );
			$( "#section-content-links-featured-more" ).removeClass( "link-less" );
			$( "#section-content-links-featured-more" ).addClass( "link-more" );
		}
	});

	$( "#section-content-links-giving-header" ).click(function() {
		if ($( "#section-content-links-giving-more" ).hasClass( "link-more" )) {
			$( "#section-content-links-giving" ).addClass( "content-height-auto" );
			$( "#section-content-links-giving-more" ).removeClass( "link-more" );
			$( "#section-content-links-giving-more" ).addClass( "link-less" );
		} else {
			$( "#section-content-links-giving" ).removeClass( "content-height-auto" );
			$( "#section-content-links-giving-more" ).removeClass( "link-less" );
			$( "#section-content-links-giving-more" ).addClass( "link-more" );
		}
	});


