        <script language="javascript">

    function confirmLoadCart()
    {
        document.getElementById('ctl00_ContentPlaceHolder1_hdnBack').value = parseInt(document.getElementById('ctl00_ContentPlaceHolder1_hdnBack').value) + 1;
        if (document.getElementById('ctl00_ContentPlaceHolder1_hdnBack').value > 1)
        {
            __doPostBack('ctl00$ContentPlaceHolder1$btnSignIn', '');
        }

        else
        {
            if (true == confirm('There are items saved previously in your cart. Do you want to retain them?'))
            {
                document.getElementById('ctl00_ContentPlaceHolder1_hdnLoadPrevCart').value = '1';
                __doPostBack('ctl00$ContentPlaceHolder1$btnSignIn', '');
            }
            else
            {
                document.getElementById('ctl00_ContentPlaceHolder1_hdnLoadPrevCart').value = '2';
                __doPostBack('ctl00$ContentPlaceHolder1$btnSignIn', '');
            } 
        }
    }
</script> 
