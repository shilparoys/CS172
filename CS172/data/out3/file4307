   <script type="text/javascript">

    function checkcart(name) {
        var el = FindElement(name);
        var cartcell = FindElement("cartmenuitem");
        if (el != null){
            cartcell.innerHTML = 'Cart: ' + el.value + ' items';
        }
    }
</script> 
