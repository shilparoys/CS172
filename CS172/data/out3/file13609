    var $R = jQuery.noConflict(); //Setting JQuery no conflict
    $R(document).ready(function () {
        $R('img').removeAttr('width');
        $R('img').removeAttr('height');

        $R('.scfRequired').each(function () {

            var ast = '<span class="scfRequired">' + $R(this).html() + '</span>';

            $R(this).siblings('Label').prepend(ast);
            $R(this).remove();

        });


        $R('.scfValidatorRequired').each(function () {

            var ast = '<span class="scfValidatorRequired">' + $R(this).html() + '</span>';

            $R(this).siblings('Label').prepend(ast);
            $R(this).remove();

        });

       

    });

    
</script> 
