       <script>
    $R(document).ready(function () {



        $R('.smaller').click(function () {

            var fontSize = $R('.content').css("font-size");
            var fontSizeInt = parseInt(fontSize.replace("px", "")) - 2;
            //alert(fontSizeInt);
            if (fontSizeInt > 7) {
                $R('.content').animate({ "font-size": fontSizeInt + "px" }, 400);
                $R('p').animate({ "font-size": fontSizeInt + "px" }, 400);
            }
        });

        $R('.bigger').click(function () {

            var fontSize = $R('.content').css("font-size");
            var fontSizeInt = parseInt(fontSize.replace("px", "")) + 2;
            //alert(fontSizeInt);
            if (fontSizeInt < 19) {
                $R('.content').animate({ "font-size": fontSizeInt + "px" }, 400);
                $R('p').animate({ "font-size": fontSizeInt + "px" }, 400);
            }
        });

    });
</script> 
