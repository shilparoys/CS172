     <script type="text/javascript">
        
        function ClearTextBox(txtbox)
        {
            if(txtbox.value=="Search")
                txtbox.value="";
        }
        
        function ResetTextBox(txtbox)
        {
            if(txtbox.value.length==0)
                txtbox.value="Search";
        }
        
        function ValidateSearch()
        {
            var isvalidfound=true;
            var ctl_prefix="ctlsearch1_";
            var searchtext=document.getElementById(ctl_prefix+"tbxsearch").value;
            if(searchtext.length==0 || searchtext=="Search")
                isvalidfound = false;
                            
            return isvalidfound;
        }
        
    </script> 
