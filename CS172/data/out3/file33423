  <script type="text/javascript">
$(document).ready(function()
{
	$('.event-modal').fancybox({
		'hideOnContentClick' : false,  
		'centerOnScroll' : true, 
		'transitionIn' : 'elastic', 
		'transitionOut' : 'elastic', 
		'speedIn' : 300, 
		'speedOut' : 300,
	});
	
	$("a.fancyGallery").fancybox(
	{
		'titlePosition'	: 'over',
		'centerOnScroll' : true
	});
	
	$("#thumbnails").click(function()
	{
		$('.big_thumb, .description').hide();
		$('.thumb').show();
	});
	
	$("#listing").click(function()
	{
		$('.thumb').hide();
		$('.big_thumb, .description').show();
	});
});
</script> 
  <script type="text/javascript">
$(document).ready(function() {
	
	$('#filter').click(function(e)
	{
		e.preventDefault();
		$('#filterBox').toggle();
	});
	
	// Close if click again and already open
	$('#quickLinksClose').click(function(e)
	{
		e.preventDefault();
		
		$('#quickLinks').slideToggle();
		
		if($('#prospective-links').is(":visible"))
		{
			$('#prospective-links').fadeToggle();
		}
		
		if($('#community-links').is(":visible"))
		{
			$('#community-links').fadeToggle();
		}
		
		if($('#recruiter-links').is(":visible"))
		{
			$('#recruiter-links').fadeToggle();
		}
		
		if($('#alumni-links').is(":visible"))
		{
			$('#alumni-links').fadeToggle();
		}
	});
	
	// Prospective Students
	$('#menu-item-3600').click(function(e)
	{
		e.preventDefault();
		
		// Close if click again and already open
		if($('#prospective-links').is(":visible"))
		{
			$('#quickLinks').slideToggle();
			$('#prospective-links').fadeToggle();
		}
		else
		{
			if($('#quickLinks').is(":hidden"))
			{
				$('#quickLinksWrapper').css('height', '125px');
				$('#quickLinks').css('height', '175px');
				$('#quickLinks').slideToggle();
			}
			else
			{
				$("#quickLinksWrapper").animate({height:"125px"}, 300);
				$("#quickLinks").animate({height:"175px"}, 300);
			}
			
			// Hide other links
			if($('#community-links').is(":visible"))
			{
				$('#community-links').hide();
			}
			if($('#recruiter-links').is(":visible"))
			{
				$('#recruiter-links').hide();
			}
			if($('#alumni-links').is(":visible"))
			{
				$('#alumni-links').hide();
			}
			
			// Show prospective links
			if($('#prospective-links').is(":hidden"))
			{
				//alert('show prospective');
				$('#prospective-links').delay(300).fadeIn();
			}	
		}
	});
	
	// JSG Community
	$('#menu-item-3601').click(function(e)
	{
		e.preventDefault();
		
		if($('#community-links').is(":visible"))
		{
			$('#quickLinks').slideToggle();
			$('#community-links').fadeToggle();
		}
		else
		{
			if($('#quickLinks').is(":hidden"))
			{
				$('#quickLinksWrapper').css('height', '240px');
				$('#quickLinks').css('height', '290px');
				$('#quickLinks').slideToggle();
			}
			else
			{
				$("#quickLinksWrapper").animate({height:"240px"}, 300);
				$("#quickLinks").animate({height:"290px"}, 300);
			}
			
			// Hide other links
			if($('#prospective-links').is(":visible"))
			{
				$('#prospective-links').hide();
			}
			if($('#recruiter-links').is(":visible"))
			{
				$('#recruiter-links').hide();
			}
			if($('#alumni-links').is(":visible"))
			{
				$('#alumni-links').hide();
			}
			
			// Show community links
			if($('#community-links').is(":hidden"))
			{
				$('#community-links').delay(300).fadeIn();
			}
		}
	});
	
	// Alumni & Friends
	$('#menu-item-2601').click(function(e)
	{
		e.preventDefault();
		
		if($('#alumni-links').is(":visible"))
		{
			$('#quickLinks').slideToggle();
			$('#alumni-links').fadeToggle();
		}
		else
		{
			if($('#quickLinks').is(":hidden"))
			{
				$('#quickLinksWrapper').css('height', '125px');
				$('#quickLinks').css('height', '175px');
				$('#quickLinks').slideToggle();
			}
			else
			{
				$("#quickLinksWrapper").animate({height:"125px"}, 300);
				$("#quickLinks").animate({height:"175px"}, 300);
			}
			
			// Hide other links
			if($('#prospective-links').is(":visible"))
			{
				$('#prospective-links').hide();
			}
			if($('#community-links').is(":visible"))
			{
				$('#community-links').hide();
			}
			if($('#recruiter-links').is(":visible"))
			{
				$('#recruiter-links').hide();
			}
			
			// Show recruiter links
			if($('#alumni-links').is(":hidden"))
			{
				$('#alumni-links').delay(300).fadeIn();
			}
		}
	});
	
	// Recruiters & Companies
	$('#menu-item-3602').click(function(e)
	{
		e.preventDefault();
		
		if($('#recruiter-links').is(":visible"))
		{
			$('#quickLinks').slideToggle();
			$('#recruiter-links').fadeToggle();
		}
		else
		{
			if($('#quickLinks').is(":hidden"))
			{
				$('#quickLinksWrapper').css('height', '125px');
				$('#quickLinks').css('height', '175px');
				$('#quickLinks').slideToggle();
			}
			else
			{
				$("#quickLinksWrapper").animate({height:"125px"}, 300);
				$("#quickLinks").animate({height:"175px"}, 300);
			}
			
			// Hide other links
			if($('#prospective-links').is(":visible"))
			{
				$('#prospective-links').hide();
			}
			if($('#community-links').is(":visible"))
			{
				$('#community-links').hide();
			}
			if($('#alumni-links').is(":visible"))
			{
				$('#alumni-links').hide();
			}
			
			// Show recruiter links
			if($('#recruiter-links').is(":hidden"))
			{
				$('#recruiter-links').delay(300).fadeIn();
			}
		}
	});
	
});
</script> 
