       <script type="text/javascript">
var url1 = 'http://jobs.nj.com/jobs/search/results?format=json&output=facets';
$.getJSON(url1, function(data) {
	$(document).ready(function() {
		var numCompanies = data.JobFacets[4].Facets.length;
		var companyList = '';
		var maxCompanies = 13;
		if (numCompanies < 13) maxCompanies = numCompanies;
		var companyCount = 0;
		for (var i=1; i < maxCompanies; i++) {
			var companyName = data.JobFacets[4].Facets[i].Label;
			var companyNameLower = companyName.toLowerCase();
			if (companyCount < 10 && companyNameLower.indexOf('confidential') < 0 && companyNameLower.indexOf('employment ads') < 0 ) {
				companyList += ' <li><span><a href="' + data.JobFacets[4].Facets[i].Url + '">' + companyName + '</a></span></li>';
				companyCount = companyCount + 1;
			}
		}
		$('#company_list').html(companyList);
	});
});
</script> 
