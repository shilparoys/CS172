   <script>
(function($) {
	var imageSrc = "";

	/* for random bg image */
	var rnd_no = Math.round((3-1)*Math.random());
	var screenWidth = document.documentElement.clientWidth;
	var mainPath = "/static/classifieds/jobs/njo/";
	var fullPath = mainPath + "" + "img/";

	if (screenWidth < 1025) { 
		imageSrc = fullPath + "bg_search_jobs" + rnd_no + "_1030.jpg";
	} else if (screenWidth < 961) {
		//android 7
		imageSrc = fullPath + "bg_search_jobs" + rnd_no + "_966.jpg";
	} else {
		//desktop
		imageSrc = fullPath + "bg_search_jobs" + rnd_no + "_2000.jpg";
	}
	console.log("imageSrc : " + imageSrc);
	$(".search_n_jobs_now").attr('style', 'background: url(' + imageSrc + ') repeat-x center top !important');
	
})(jQuery);

</script> 
   <div class="wrapper clearfix"> 
    <div id="content"> 
     <div id="news-river"> 
      <div class="wrapper"> 
       <div class="clearfix"></div> 
       <script type="text/javascript">
$(document).ready(function() {
	var savedKeywords = new Array();
	var savedLocations = new Array();
	var jobLimit = 0;
	var startIndex = 0;
	var adicioUrl = 'http://jobs.nj.com/jobs/search/results?format=json&kwsPrimary=';

	var aiSearchCookie = readAICookie('aiSearch');
	if (aiSearchCookie) {
		parseAICookie();
		var recJobCount = 0;
		var recJobCountAttempts = jobLimit - 1;
		var getSearchesCount = jobLimit - 1;

		if (savedKeywords.length > 0) {
			getRecommendedJobs();
			getSearches();
			$('.my_recent_searches_wrapper').css('display','block');
		}
	}

	function readAICookie (cookieName) {
		var cookieValue='';
		var cookies = document.cookie.split(';');
		for (var i=0; i < cookies.length; i++) {
			if (cookies[i].indexOf(cookieName + '=') > -1) {
				cookieValue= unescape(cookies[i].substring(cookies[i].indexOf('=')+1));
			}
		}
		return cookieValue;
	}

	function parseAICookie() {
		var cookieValue = aiSearchCookie.split('|');
		jobLimit = cookieValue.length;
		if (jobLimit > 5) startIndex = jobLimit - 5;
		for (var i=startIndex; i < jobLimit; i++) {
			var fields = cookieValue[i].split(':');
			savedLocations[i] = fields[0];
			if (fields[1]) savedLocations[i] += ', ' + fields[1];
			savedKeywords[i] = fields[2];
		}
	}

	function getSearches() {
		var keyword = savedKeywords[getSearchesCount];
		
		var location = savedLocations[getSearchesCount];

		var criteria = keyword;
		if (location) criteria += '&location=' + location;

		var queryUrl = adicioUrl + criteria + '&rows=1&modifiedDate=[NOW-7DAYS+TO+NOW]';
		var resultsUrl = 'http://jobs.nj.com/jobs/results/keyword/' + keyword + '?sort=PostDate desc';
		if (location) {
			var convertedLocation = encodeURIComponent(location);
			resultsUrl += '&location=' + convertedLocation.replace('%20','+');
		}
		$.ajax({
			dataType: "json",
			url: queryUrl,
			success: function(data) {
				var jobCount = data.Total;
				var str = '<li><strong>' + jobCount + ' <span';
				if (jobCount == 0) str += ' class="grey"';
				
				if (keyword.length > 55) {
					keyword = keyword.replace(/\W*\s(\S)*$/, '...');
				}
				str += '>New</span></strong><a href="' + resultsUrl + '"><strong>' + keyword + '</strong><br />';
				if (location) {
								
					if (location.length > 55) {
						location = location.replace(/\W*\s(\S)*$/, '...');
					}	
					str += location;
					
				} else {
					str += '<br />';
				}
				str += '</a></li>';
				$('#recent_searches_list').append(str);

				getSearchesCount = getSearchesCount - 1;
				if (getSearchesCount >= startIndex) getSearches();
			}
		});
	}

	function getRecommendedJobs() {
		var criteria = savedKeywords[recJobCountAttempts];
		var location = savedLocations[recJobCountAttempts];

		if (location) criteria += '&location=' + location;

		var queryUrl = adicioUrl + criteria + '&rows=5';
		$.ajax({
			dataType: "json",
			url: queryUrl,
			success: function(data){
				var numJobs = data.Jobs.length;
				for (var j=0; j < numJobs; j++) {
					if (recJobCount  < 5 ) {
						var job = data.Jobs[j].JobTitle;
						
						if (job.length > 80) {
							job = job.replace(/\W*\s(\S)*$/, '...');
						}
						
						var jobLocation = '';
						if (data.Jobs[j].City != '-') {
							jobLocation = data.Jobs[j].City + ', ';
						}
						jobLocation += data.Jobs[j].State;
						
						if (jobLocation.length > 55) {
							jobLocation = jobLocation.replace(/\W*\s(\S)*$/, '...');
						}
						
						$('#recommended_jobs_list').append('<li><a href="' + data.Jobs[j].Url + '"><strong>' + job + '</strong><br />' + jobLocation +'</a></li>');
					}
					recJobCount = recJobCount + 1;
				}
				recJobCountAttempts = recJobCountAttempts - 1;
				if (recJobCountAttempts < startIndex) {
					if (recJobCount == 0) $('#recommended_jobs_list').append('<em class="no_jobs">Currently there are no new recommended jobs available.</em>');
				} else {
					if (recJobCount  < 5) getRecommendedJobs();
				}
			}
		});
	}
	$('.my_recent_searches a.btn_clear_recent').on('click',function(e){
		e.preventDefault();
		document.cookie = 'aiSearch=; path=/; domain=nj.com; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
		$('.my_recent_searches ul').remove();
		$(this).remove();
	});
});
</script> 
