     <script type="text/javascript">

      $(document).ready(function () {
          if (getParameterByName("tab") == "employers") {
              toggle('tab2');
          }
          else {
              toggle('tab1');
          }

      });

      function getParameterByName(name) {
          name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
          var regexS = "[\\?&]" + name + "=([^&#]*)";
          var regex = new RegExp(regexS);
          var results = regex.exec(window.location.href);
          if (results == null)
              return "";
          else
              return decodeURIComponent(results[1].replace(/\+/g, " "));
      }

      function toggle(id) {
          if (id == 'tab1') {
              document.getElementById('tab2').style.display = 'none';
              document.getElementById('tab1').style.display = 'block';
              document.getElementById('LiTab1').className = "active";
              document.getElementById('LiTab2').className = "inActive";
              document.getElementById('forJobSeekerModulesPane').style.display = 'block';
              document.getElementById('forEmployersModulePane').style.display = 'none';

          } else {
              document.getElementById('tab1').style.display = 'none';
              document.getElementById('tab2').style.display = 'block';
              document.getElementById('LiTab2').className = "active";
              document.getElementById('LiTab1').className = "inActive";
              document.getElementById('forJobSeekerModulesPane').style.display = 'none';
              document.getElementById('forEmployersModulePane').style.display = 'block';
          }
      }

      function ShowAnswersDropdown(thisObj) {

          if (thisObj.className == "collapsed") {
              thisObj.className = "expand";
              thisObj.title = 'Close selection';
              thisObj.parentNode.style.backgroundColor = '#F5F9FC';
              
              var showAnswersDiv = thisObj.parentNode.getElementsByTagName("div");
              for (var i = 0; i < showAnswersDiv.length; i++)

                  if (showAnswersDiv[i].className.indexOf("answersListDropDown") >= 0) {
                      showAnswersDiv[i].style.display = "block";
                  }
          }
          else {
              thisObj.className = "collapsed";
              thisObj.parentNode.style.backgroundColor = 'transparent';
              thisObj.title = 'Expand selection';
              var showAnswersDiv = thisObj.parentNode.getElementsByTagName("div");
              for (var i = 0; i < showAnswersDiv.length; i++)

                  if (showAnswersDiv[i].className.indexOf("answersListDropDown") >= 0) {
                      showAnswersDiv[i].style.display = "none";
                  }
          }
      }
        
</script> 
