function getStylesheet() {
      var currentTime = new Date().getHours();
      if (4 <= currentTime&&currentTime < 16) {
       document.write("<link rel='stylesheet' href='css/slaf_morning.css' type='text/css'>");
	   document.write("<link rel='stylesheet' href='css/style_morning.css' type='text/css'>");
	   document.write("<link rel='stylesheet' href='css/news_morning.css' type='text/css'>");
	   
