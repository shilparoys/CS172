                           if(!navigator.userAgent.match(/OS 5/i)) {
                              $("#sb-container").css("top", $(window).scrollTop()); 
                              $(window).bind('scroll', function() { 
                                   $("#sb-container").css("top", $(window).scrollTop()); 
                              });                     
                           }
                      } 
               }, 
               onClose: function() { 
                      if((navigator.userAgent.match(/iPhone/i))||(navigator.userAgent.match(/iPad/i))) { 
                              $(window).unbind('scroll'); 
                      } 
               } 
           });
