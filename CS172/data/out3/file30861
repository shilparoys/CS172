  <script type="text/javascript">
				function imageExists( url ) {
					if ( url != null ) return url;
					else return "http://media.nj.com/design/alpha/img/default_vid_thumbnail.png";
				}
				function urlExists( url, id ) {
					var dev_env;
					
					if ( url.match(/videos\.(.*)?\.com/g) && dev_env != "1" ) {
						var http = new XMLHttpRequest();
						http.open('HEAD', url, false);
						http.send();
						// if video id passed to function...
						if ( id ) {
							if ( http.status != 404 ) return "href='" + url + "'";
							else return "href='#video' onclick=\"playVideo(" + id + ");\"";
						} else {
							if ( http.status != 404 ) return url;
							else return "#video";
						}
					} else {
						// Not production
						if ( id ) {
							return "href='" + url + "'";
						} else {
							return url;
						}
					}
				}
			</script> 
