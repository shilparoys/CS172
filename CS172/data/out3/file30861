  <script type="text/javascript">
    // video injection code
    var adv_player;
    var adv_modVP;
    var adv_nextVideo = 0;
    var adv_videos = new Array();
    var adv_token = "LaSy7_vKMpoPlgEH_Prv3O2_mbz-ciL5FbGUOIG2g_ySWBjj5zjfew.."; // MT supplied variable (read id)
    var adv_vid_refid = 'mt-17144197'; // MT supplied variable (reference id)
    var adv_vid_external_id = ""; // External Brightcove Video
    var adv_vid_id = ""; // populated after BC call
    var adv_cat1 = "Sports Videos"; // MT supplied variable (category 1)
    var adv_author = "shindash"; // MT supplied variable (author)
    var adv_blogID = "3340"; // MT supplied blog id
    var adv_cat_bc = "http://videos.nj.com/nj//index.html?category=Sports Videos"; // default category breadcrumb link

    function ellipsis_count( text ) {
        var count = text.length;
        if ( count > 75 ) {
            return text.substr(0, 75) + "...";
        } else {
            return text;
        }
    }

    function adv_pretty_tags(tag_arr){
        var clean_tag_arr = new Array();
        for(var i=0; i < tag_arr.length; i++){
            var tag_name = (tag_arr[i]['name']) ? tag_arr[i]["name"] : tag_arr[i];
            var pretty_tag = tag_name.replace('cat1_', '');
            pretty_tag = pretty_tag.replace('cat2_', '');
            pretty_tag = pretty_tag.replace(/-/g, ' ');
            var tag_html = '<a href="http://videos.nj.com/nj/index.html?tag=' + tag_arr[i]["name"] + '"><div>' + pretty_tag.toUpperCase() + '</div></a>';
            if(tag_name.indexOf('@') != 0) clean_tag_arr.push(tag_html);
        }
        var clean_tag_str = clean_tag_arr.join(" ");
        return clean_tag_str;
    }

    function adv_pub_date(date_str){
        var mnth;
        var dy;
        var yr;
        var mn = [ "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December" ];
        var dt_prts = date_str.toString().split(" ");
        for(i=0; mn.length > i; i++){
                if(mn[i].indexOf(dt_prts[1]) > -1) mnth = mn[i];
        }
        dy = dt_prts[2];
        yr = dt_prts[3];
        var date_html = "PUBLISHED: " + mnth + " " + dy + ", " + yr + "<br />";
        return date_html;
    }

    function get_adv_bc (permalink, cat) {
        var up_arr = permalink.split('/');
        if (cat == "undefined") cat = adv_cat1;
        var bc = up_arr[0] + "//" + up_arr[2] + "/" + up_arr[3] + "/index.html?category=" + cat;
        return bc;
    }

    function get_adv_author (permalink, author) {
        var up_arr = permalink.split('/');
        if (author == "undefined") author = adv_author;
        var bc = up_arr[0] + "//" + up_arr[2] + "/" + up_arr[3] + "/index.html?author=" + author;
        return bc;
    }

    function myTemplateLoaded(experienceID) {
        adv_player = brightcove.api.getExperience(experienceID);
        adv_modVP = adv_player.getModule(brightcove.api.modules.APIModules.VIDEO_PLAYER);
    }

    function onTemplateReady(evt) {
        adv_modVP.addEventListener(brightcove.api.events.MediaEvent.CHANGE, onMediaBegin);
        adv_modVP.addEventListener(brightcove.api.events.MediaEvent.BEGIN, onMediaBegin);
        adv_modVP.addEventListener(brightcove.api.events.MediaEvent.COMPLETE, onMediaComplete);
        adv_modVP.cueVideoByID(adv_vid_id);
    }

    function onMediaBegin(evt) {
        if ( ! adv_vid_external_id ) {
            document.getElementById("mediaCategory").innerHTML = "<a href='" + adv_cat_bc + "'>" + evt.media.customFields.category1 + "</a> &rsaquo; <a href='" + adv_author_bc + "'>" + evt.media.customFields.display_name + "</a><div id='share'><a href='javascript:void(0);' onclick=\"javascript:showShareUI('simpleShare')\">Share &#10138;</a></div>";
            document.getElementById("mediaHeadline").innerHTML = "<a href='"+ evt.media.customFields.permalink +"'/>"+evt.media.displayName+"</a>";
            var parsedHTML = $("#mediaDescription").html( evt.media.longDescription ).text();
            document.getElementById("mediaDescription").innerHTML = parsedHTML;       
        } else {
            document.getElementById("mediaCategory").innerHTML = "";
            document.getElementById("mediaHeadline").innerHTML = "<a href='#'>" + evt.media.displayName + "</a><div id='share'><a href='javascript:void(0);' onclick=\"javascript:showShareUI('simpleShare')\">Share &#10138;</a></div>";
            var description = "";
            if ( evt.media.longDescription ) { description = evt.media.longDescription; } else { description = evt.media.shortDescription; }
            var parsedHTML = $("#mediaDescription").html( description ).text();
            document.getElementById("mediaDescription").innerHTML = parsedHTML;
        }
        document.getElementById("mediaTags").innerHTML = adv_pub_date(evt.media.publishedDate) + adv_pretty_tags(evt.media.tags);
        var thisthingthatbreaks = adv_pub_date(evt.media.publishedDate) + adv_pretty_tags(evt.media.tags);
    }

    function onMediaComplete(evt) {
        adv_nextVideo++;
        if (adv_nextVideo == adv_videos.length) adv_nextVideo = 0;
        adv_modVP.cueVideoByID(adv_videos[adv_nextVideo]);
    }

    function playVideo(videoID){
        adv_modVP.loadVideoByID(videoID);
    }

    function cueBCVideo(videoID){
        adv_modVP.cueVideo(videoID);
    }

    function loadVideos(){
        var vidCall = "";
        var apiCall = "";
        if ( adv_vid_external_id ) { 
            vidCall = "http://api.brightcove.com/services/library?command=find_video_by_id&token=" + adv_token + "&video_id=" + adv_vid_external_id + "&video_fields=id,referenceId,name,thumbnailURL,videoStillURL,shortDescription,longDescription,playsTrailingWeek,tags,publishedDate&custom_fields=username,permalink,category1";
            apiCall = "http://api.brightcove.com/services/library?command=search_videos&token=" + adv_token + "&all=blogid:" + adv_blogID + "&video_fields=id,referenceId,name,thumbnailURL,videoStillURL,longDescription,playsTrailingWeek&sort_by=PUBLISH_DATE:DESC&custom_fields=username,permalink,category1&page_size=5";
        } else {
            vidCall = "http://api.brightcove.com/services/library?command=find_video_by_reference_id&token=" + adv_token + "&reference_id=" + adv_vid_refid + "&video_fields=id,referenceId,name,thumbnailURL,videoStillURL,longDescription,playsTrailingWeek,tags,publishedDate&custom_fields=username,permalink,category1";
            apiCall = "http://api.brightcove.com/services/library?command=search_videos&token=" + adv_token + "&any=reference_id:" + adv_vid_refid + "&any=category1:" + adv_cat1 + "&all=blogid:" + adv_blogID + "&any=all&video_fields=id,referenceId,name,thumbnailURL,videoStillURL,longDescription,playsTrailingWeek&sort_by=PUBLISH_DATE:DESC&custom_fields=username,permalink,category1&page_size=5";
        }

        $.ajax({
            url: vidCall,
            dataType:'jsonp',
            success: function(data) {
                adv_vid_id = data.id;
                if ( ! adv_vid_external_id ) {
                    adv_cat_bc = get_adv_bc(data.customFields.permalink, data.customFields.category1);
                    adv_author_bc = get_adv_author(data.customFields.permalink, data.customFields.username);
                }
            }
        });

        $.ajax({
            url: apiCall,
            dataType:'jsonp',
            success: function (data) {
                try {
                    var vid_div_iter = 1;
                    for (var i=0; i < data.items.length; i++){
                        var permalinkHTML;
                        if ( data.items[i].customFields && data.items[i].customFields.permalink ) {
                            permalinkHTML = "<a style='text-decoration:none' href='" + urlExists(data.items[i].customFields.permalink) + "'>";
                        }
                        $("#video-" + vid_div_iter).html("<div class='thumbcontainer'>" + permalinkHTML + "<img title='" + data.items[i].name + "' src='" + imageExists(data.items[i].videoStillURL) + "'/><div class='thumbhead'>"+ data.items[i].name +"</div></a></div>");
                        vid_div_iter++;
                        adv_videos.push(data.items[i].id);
                    }                    
                } catch (err) {
                    BCLcontainingBlock.innerHTML = "<div class='vid_err'>loadVideos catch<br>vid_div_iter: "+vid_div_iter+"<br>urlExists(data.items[i].customFields.permalink: "+urlExists(data.items[1].customFields.permalink)+"</div>";
                }
            },
            error: function (error) {
                BCLcontainingBlock.innerHTML = "<div class='vid_err'>loadVideos api call error</div>";
            }
        });
    }


    // initialize video load
    $(document).ready(function() { loadVideos(); });

</script> 
