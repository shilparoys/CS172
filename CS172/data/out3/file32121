  <script type="text/javascript">
    $(document).ready(function() {
        function setCookie(){
	    var d = new Date();
	    var n = d.getTime().toString() + Math.random().toString().slice(2);
            return n;
        };
	function populateU(){
	    $("input[name='u']").each(function(){
                var u = $(this).attr("value");
	        if(u === ""){
	            $(this).attr("value",$.cookie('sessionId'));
	        };
            });
        };
	if($.cookie('sessionId') === null){
	    $.cookie('sessionId',setCookie(),{path:'/'});
	};
	populateU();
    });
</script> 
