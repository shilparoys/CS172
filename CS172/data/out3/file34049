<style type="text/css">

#show-hide-area .show { padding-left:55px; padding-top:10px; cursor: pointer; color:#fff;}
#show-hide-area .hide { padding-left:55px; padding-top:10px; cursor: pointer; color:#fff;}
#show-all {cursor: pointer; color:#000; padding-left:65px; color:#fff;}
#image-map div.step  {filter: alpha(opacity=0);}
#button.submit-proposal, #button.report-innovation {position: relative}
#button a {color:white; position: absolute; top: -14px; font-size:10px;}

</style>
	
<![endif]--> 
  <script src="http://cdn.jquerytools.org/1.2.7/full/jquery.tools.min.js"></script> 
  <script language="javascript" type="text/javascript">

// any page can use this functionality by wrapping the content in the html pattern below 
// where the span tag contain the show or hide button and the next div tag has the content 
// the example below the content is starts off hidden. 
// 
//<div id="show-hide-area">
//<h3>FAQ question?<span class="show">show</span></h3>
//<div class="content hide-content">Answer to questions.</div>
//</div>



jQuery(document).ready(function() {
	
	// 
	jQuery('#show-hide-area h3 span').each(function(){
		jQuery(this).click(function() {
			if (jQuery(this).attr('class') == 'show')
			{
				// hide the content 
				jQuery(this).removeClass('show');
				jQuery(this).addClass('hide');
				jQuery(this).parent().next('div').removeClass('hide-content');
				jQuery(this).parent().next('div').addClass('show-content');
			}
			else 
			{
				// show the content 
				jQuery(this).removeClass('hide');
				jQuery(this).addClass('show');			
				jQuery(this).parent().next('div').removeClass('show-content');
				jQuery(this).parent().next('div').addClass('hide-content');
			}
			return false;
		});
	});
	
	
// to add a show all / hide all button, add this line with the matching styles and buttons 
//<div id="show-all" class="showall">Show All</div>
	
	jQuery('#show-all').click(function() {
			if (jQuery(this).attr('class') == 'showall')
			{
				// hide the content 
				jQuery(this).removeClass('showall');
				jQuery(this).addClass('hideall');
				jQuery('#show-hide-area h3 span').removeClass('show');
				jQuery('#show-hide-area h3 span').addClass('hide');
				jQuery('#show-hide-area h3 span').parent().next('div').removeClass('hide-content');
				jQuery('#show-hide-area h3 span').parent().next('div').addClass('show-content');

			}
			else 
			{
				// show the content 
				jQuery(this).removeClass('hideall');
				jQuery(this).addClass('showall');	
				jQuery('#show-hide-area h3 span').removeClass('hide');
				jQuery('#show-hide-area h3 span').addClass('show');	
				jQuery('#show-hide-area h3 span').parent().next('div').removeClass('show-content');
				jQuery('#show-hide-area h3 span').parent().next('div').addClass('hide-content');
			}
			return false;							   
	
	});
	
	// to add a show all / hide links for bios to the contact / staff page - 
	
	jQuery('#bio-area div').click(function() {
			if (jQuery(this).attr('class') == 'show')
			{
				
				//You just click show, so hide any currently opened content 
				jQuery('#bio-area div').next('p').hide();
				jQuery('#bio-area div').removeClass('hide');
				jQuery('#bio-area div').addClass('show');
				jQuery('#bio-area div').html ('View Bio');
				
				// show the content, change the buttons to hide 
				jQuery(this).removeClass('show');
				jQuery(this).addClass('hide');
				jQuery(this).html ('Hide Bio');
				jQuery(this).next('p').show('400');
				
				
			}
			else 
			{
				// hide the content 
				jQuery(this).removeClass('hide');
				jQuery(this).addClass('show');
				jQuery(this).html ('View Bio');
				jQuery(this).next('p').hide();
			}
			return false;							   
	
	});
	
		
		//jQuery(this).next('p').prepend('<span class="close" onclick="closeall()">x</span>');

});


function closeall () {
				
		//You just click show, so hide any currently opened content 
		jQuery('#bio-area div').next('p').hide();
		jQuery('#bio-area div').removeClass('hide');
		jQuery('#bio-area div').addClass('show');
		jQuery('#bio-area div').html ('View Bio');		

		return false;
}

</script> 
  <script>
$(function() {$(".slidetabs").tabs(".images > div", {effect: 'fade',fadeOutSpeed: "slow",rotate: true}).slideshow({autoplay: true, interval: 6000, clickable: false});});
</script> 
  <script>
function textClear(ctrl) {
  if (ctrl.value == ctrl.defaultValue) {
    ctrl.value = "";
  }
}
function textRestore(ctrl) {
  if (ctrl.value == "") {
    ctrl.value = ctrl.defaultValue;
  }
}
</script> 
