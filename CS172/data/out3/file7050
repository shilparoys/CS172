	});
	$('#defaultH').click(function (){
		contrastSet("defaultH", contrast);
		createCookie("hcc", "defaultH", 1);
   		$('link[href="/sites/all/themes/acquia_marina/css/hcontra.css?H"]').attr('href','/sites/all/themes/acquia_marina/css/hdefault.css?H');
	});
	function contrastSet(elem, arr){
		$.each(arr, function( key, value ) {
			$("#"+value).removeClass("activeH");
		});
		$("#"+elem).addClass("activeH");
	}
	if(readCookie("hcs")!=null){
		//console.log(readCookie("hcs"));
		contrastSet(readCookie("hcs"), size);
		$( "#"+readCookie("hcs") ).trigger( "click" );
	}else{
		contrastSet("smallH", size);
	}
	if(readCookie("hcc")!=null){
		contrastSet(readCookie("hcc"), contrast);
		$( "#"+readCookie("hcc") ).trigger( "click" );
	}else{
		contrastSet("colorH", contrast);
	}

function createCookie(name, value, days) {
    var expires;

    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
        expires = "; expires=" + date.toGMTString();
    } else {
        expires = "";
    }
    document.cookie = escape(name) + "=" + escape(value) + expires + "; path=/";
}

function readCookie(name) {
    var nameEQ = escape(name) + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) === ' ') c = c.substring(1, c.length);
        if (c.indexOf(nameEQ) === 0) return unescape(c.substring(nameEQ.length, c.length));
    }
    return null;
}

function eraseCookie(name) {
    createCookie(name, "", -1);
}
});
