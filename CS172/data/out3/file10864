      <script>
                    
            window.fbAsyncInit = function() {
                FB.init({
                    appId   : '289302581098674',
                    oauth   : true,
                    status  : true, // check login status
                    cookie  : true, // enable cookies to allow the server to access the session
                    xfbml   : true // parse XFBML
                });
            };

    function fb_login(){

        FB.login(function(response) {

            if (response.authResponse) {

                var accessToken  =   response.authResponse.accessToken;	
                checkFBLogin('login', null, accessToken);

            } else {

                console.log("User cancelled login or did not fully authorize.");
                //alert("User cancelled login or did not fully authorize.");
            }

        }, {
            scope: 'publish_stream,email'
        });
    }

    function save(popupSignUp) {
        
        if(popupSignUp == "bouncePopup") {
                     
            _gaq.push(['_trackEvent', 'BounceCampaign', 'Bounce Pop up', 'ButtonClick']);
        }
        
        var scholarIDs = "";
        
        if($("#hiddenIds").length != 0) {
            scholarIDs = $("#hiddenIds").val();
        }

        FB.login(function(response) {

            if (response.authResponse) {

                var accessToken  =   response.authResponse.accessToken;
                checkFBLogin('save', scholarIDs, accessToken);

            } else {
                console.log("User cancelled login or did not fully authorize.");
            }

        }, {
            scope: 'publish_stream,email'
        });
    }

    (function() {
        var e = document.createElement('script');
        e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
        e.async = true;
        document.getElementById('fb-root').appendChild(e);
    }());


    function checkFBLogin(operation,scholarIDs, accessToken) {
        
        var saveActive = "";
        if($("#saveActive").length != 0) {
            saveActive = $("#saveActive").val();
        }
            
        var url = "http://"+window.location.hostname+"/wp-content/themes/sandbox/fb_authentication.php";
        var data = "access_token=" + accessToken + "&operation="  + operation + "&scholarIDs=" + scholarIDs+"&searchPageLocation="+window.top.location+"&saveActive="+saveActive;

        $.ajax({
            url : url,
            type : 'post',
            dataType : 'json',
            data : data,
            success : function (data) {

                 if(data.message == "emailExist") {

                     _gaq.push(['_trackEvent', 'SignUp', 'By Facebook', 'Unsuccessfull signup']);

                     if($("#signup_popup").is(":visible")) {

                         document.getElementById('fbsignup_error').innerHTML = "The account with this email is already exist.";
                     } else {

                         document.getElementById('fbsavesignup_error').innerHTML = "The account with this email is already exist.";
                     }

                     return false;

                 } else if(data.message == "loginSuccess") {

                    if(saveActive == "active") {
                        saveRankingSearchCriteria();
                    }
                    eval(data.steps);

                 } else if(data.message == "fail") {

                     _gaq.push(['_trackEvent', 'SignUp', 'By Facebook', 'UnSuccessfull signup']);

                     if($("#signup_popup").is(":visible")) {

                         document.getElementById('fbsignup_error').innerHTML = "Your signup with facebook is fail.";
                     } else {

                         document.getElementById('fbsavesignup_error').innerHTML = "Your signup with facebook is fail.";
                     }
                     return false;
                 }
            }
        }) ;
    }

    getCustomBootstrapMsg = function(type, message) {

        var alertClass = "info";
        if(type == "error"){
            alertClass = "alert-error";
        } else if(type == "success"){
            alertClass = "alert-success";
        }

        var html = '<div class="alert '+alertClass+'"><button class="close" data-dismiss="alert">×</button><i class="cus-cross-octagon">'+
                    '</i>'+message+'</div>';
        return html;
    }

    function showLoginPopUp(objs) {
        
        if(objs == "active") {
            
            if($("#saveActive").length != 0) {
             
               $("#saveActive").val('active');
            }
            
        } else if(objs == "notActive") {
            
            if($("#saveActive").length != 0) {
               
               $("#saveActive").val('notActive');
            }
        }
        
        var uname = "";
        var password = "";
        
        if($.cookie('unameRemember') != "" && $.cookie('unameRemember') !=undefined) {            
           uname  = $.cookie('unameRemember');
        }
        
        if($.cookie('passwordRemember') != "" && $.cookie('passwordRemember') !=undefined) {
           password = $.cookie('passwordRemember');
        }

        var html = '' +
                '<div id="login-dialog" class="modal hide fade in" style="display: none; width: 600px; left: 50%; margin-left: -300px">' +
                '<div class="modal-header"><a class="close_modal" data-dismiss="modal" onclick="return closeModal(this);" >&times;</a>' +
                '<h3>Login</h3>' +
                '</div>' +
                '<div class="modal-body">' +
                '<div id="login_tiny_help_message" class="" style="display: none;"></div>' +

                '<div id="loginInner">' +
                '<div class="span3">' +
                '<form method="post" action="/userlogin" id="signin-form">' +
                '<input type="hidden" name="action" value="login" />' +
                '<center ><p id="Login_error" style="color:red;"></p></center>'+
                '<label for="login_email_id">Email</label>' +
                    '<input type="text" class="text"  name="email" id="login_email_id" value="'+uname+'">' +
                '<label for="login_password" class="">Password</label>' +
                '<input type="password" class="text" name="password" value="'+password+'" id="login-password-id" /> <br/>' +
                '<button type="button" class="button" onclick="javascript:userSignIn();"><span>Login</span></button>'+
                '<img id="login-loader" style="display: none;" src="/wp-content/themes/sandbox/images/ajaxloader.gif" />' +
                '&nbsp;&nbsp;<input type="checkbox" id="check"  name="remember_me" checked > Remember Me'+
                '</form>' +
                '<a href="javascript:void(0);" style="margin-top:15px;display:block;" class="clear" onclick="return showForgotPassword();">Forgot your password?</a>' +
                '</div>' +

                '<div class="span2 pull-right">' +
                '<div class="row">' +
                '<ul style="margin: 20px 0;">' +
                '<li><button class="btn btn-large" type="button" onclick="save();" style="width: 160px;">' +
                '<img alt="F" src="/wp-content/themes/sandbox/images/f.png" style="margin:0 5px -4px 0">Facebook</button></li>' +
                '<li style="margin: 25px 0 0 0"><button class="btn btn-large" onclick="googleLogin();" style="width: 160px;text-align: left;">' +
                '<img alt="G" src="/wp-content/themes/sandbox/images/g.png" style="margin:0 5px -4px 0">Google</button></li>' +
                '</ul>' +
                '</div>' +
                '</div>' +
                '</div>' +

                '<div id="forgotPassword" class="form-horizontal hide clear" style="margin: 10px 20px 0;">' +

                '<div class="control-group">' +
                '<label class="help-block">Please enter your registered email address.' +
                ' We will send you an email with the instruction to recover your password.</label>' +
                '</div>' +

                '<div class="hide" id="fpwMsg"></div>' +

                '<div class="control-group">' +
                '<label class="control-label">Email Address</label>' +
                '<div class="controls">' +
                '<input type="text" name="email" id="fpwEmailId"/>' +
                '</div>' +
                '</div>' +
                '<div class="control-group">' +
                '<label class="control-label"></label>' +
                '<div class="controls">'+
                '<button type="button" class="button" onclick="javascript:sendMail();"><span>Send</span></button>'+
                '<a href="javascript:void(0);" style="margin-left: 20px" onclick="showLogin();">Login Here</a>' +
                '</div>' +
                '</div>' +
                '</div>' +

                '</div>' +
                '<div class="modal-footer">' +
                '<span>Don\'t have an account?</span>&nbsp;&nbsp;' +
                '<button type="button" class="button" onclick="javascript:showSignUp();"><span>Create Account!</span></button>'+
                '</div>' +
                '</div>' ;
        if($("#login-dialog").length == 0){
            $('body').append(html);
        }
        createBackDrop();
        $("#login-dialog").show();
    }

    function showLogin() {

        $('#loginInner').show();
        $('#signup_tiny_help_message').hide();
        $('#forgotPassword').hide();
        $('#fpwMsg').hide();

        $('#signup-dialog').hide();
        showLoginPopUp();
    }
    function showForgotPassword(){

        //hide errors if any
        $('#login_tiny_help_message').hide();
        //check if $.cookie is set or not for remember me functionality
        if( $.cookie("unameRemember")==null && $.cookie("passwordRemember")==null) {
            $("#login_email_id").val('');
            $("#login-password-id").val('');
        }
        //clear input box
        $("#fpwEmailId").val('');
        $('#loginInner').hide();
        $('#forgotPassword').show();
    }

    function showSignUp() {
        
        $('#login_tiny_help_message').hide();
        $('#login-dialog').hide();
        $("#signup-email").val('');
        $("#signup-password").val('');
        $("#signup-repeat-password").val('');
        showSignUpPopUp();
    }

    function createBackDrop() {

        if($('.modal-backdrop').length == 0 ){
           $('body').append('<div class="modal-backdrop fade in"></div>');        
        }

        if(!$('.modal-backdrop').is(':visible')){
          $('.modal-backdrop').show();
        }
    }
    
    function closeModal(elem) {
        
       $(elem).closest('.modal').hide();
       $("#signup-dialog").find(":text").val("");
       $("#signup-dialog").find(":password").val("");
       $("#forgotPassword").find(":text").val("");
       $("#fpwMsg").empty();
       removeBackDrop();
    }
    
    function removeBackDrop() {
        
        $('.modal-backdrop').remove();
        $('#login_tiny_help_message').empty();
        $('#signup_tiny_help_message').remove();
    }

    function showSignUpPopUp(objs) {
        
         if(objs == "active") {

             if($("#saveActive").length != 0) {

                $("#saveActive").val('active');
             }

         } else if(objs == "notActive") {

             if($("#saveActive").length != 0) {

                $("#saveActive").val('notActive');
             }
         }

         function format(value) {
             
           value+="";
           var rgx = /(\d+)(\d{3})/;
           while (rgx.test(value)) {
                value = value.replace(rgx, '$1' + ',' + '$2');
           }
           return value;
         }
         var html = '' +
                    '<div style="width: 600px; left: 50%; margin-left: -300px; display: block;" class="modal hide fade in" id="signup-dialog" aria-hidden="false">'+

                    '<div class="modal-header"><a data-dismiss="modal" onclick="return closeModal(this);" class="close_modal">×</a>'+
                    '<h3>Sign Up</h3>'+
                    '</div>'+
                     '<div class="modal-body">'+
                    '<div style="display: none;" class="" id="signup_tiny_help_message"></div>'+
                     '<div class="span3">'+
                    '<form action="/" method="post" id="signup-form" name="userloginform">'+
                    '<center ><p id="register_error" style="color:red;"></p></center>'+
                    '<input type="hidden" value="signup" name="action">'+
                    '<ul>'+
                    '<li><label for="signup-email">Email Address</label></li>'+
                    '<li><input type="text" id="signup-email" value="" maxlength="50" name="email"></li>'+
                    '<li><label for="signup-password">Password</label></li>'+
                    '<li><input type="password" id="signup-password" value="" maxlength="20" name="password"></li>'+
                    '<li><label for="signup-repeat-password">Confirm Password</label></li>'+
                    '<li><input type="password" maxlength="20" id="signup-repeat-password" name="repeat-password"></li>'+
                    '<li><span class="help-block">*Your password must be at least 9 characters.</span></li>'+
                    '<li>'+
                    '<button type="button" class="button" onclick="javascript:userSignUp();"><span>Sign Up Now!</span></button>'+
                    '&nbsp;&nbsp;<input type="checkbox" id="signup_check" name="signup_check" checked> Remember Me'+
                    '<img src="/wp-content/themes/sandbox/images/ajaxloader.gif" style="display: none;" id="signup-loader">'+
                    '</li>'+
                    '</ul>'+
                    '</form>'+
                    '</div>'+

                    '<div class="span2 pull-right">'+
                    '<div class="row">'+
                    '<ul style="margin: 20px 0;">'+
                    '<li><button style="width: 160px;" onclick="save();" type="button" class="btn btn-large">'+
                    '<img style="margin:0 5px -4px 0" src="/wp-content/themes/sandbox/images/f.png" alt="F">Facebook</button></li>'+
                    '<li style="margin: 25px 0 0 0"><button style="width: 160px;text-align: left;" onclick="googleLogin();" class="btn btn-large">'+
                    '<img style="margin:0 5px -4px 0" src="/wp-content/themes/sandbox/images/g.png" alt="F">Google</button></li>'+
                    '</ul>'+
