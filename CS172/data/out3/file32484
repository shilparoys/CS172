           <script>
$(function() {
  var carouselApi = $('#video-carousel').getPRESTO_Carousel()
  var sliderApi = $('#thumb-slider1 .slider').scrollable({ circular: true, next: '#thumb-slider1 > .next', prev: '#thumb-slider1 > .prev', keyboard: false }).navigator({ navi: '#thumb-slider1 > .nav' }).data('scrollable');
  //var controls = $('#thumb-slider1 .control');
  var controls = $('#thumb-slider1 .item').not('.cloned').children('.control');
  var columnLimit = 5;
  var slideCount = 3;

  $('#video-carousel').bind('afterSlide', function(e, activeIndex, lastIndex) {

    if (lastIndex >= 0) {
      controls.eq(lastIndex).removeClass('active');
    }
    controls.eq(activeIndex).addClass('active');

    var activeControlSlideIndex = Math.floor( (activeIndex + columnLimit) / columnLimit ) - 1;
    var activeControlPositionInSlide = controls.eq(activeIndex).index();
    var currentSlideIndex = sliderApi.getIndex();

    if (lastIndex >= 0 && activeControlPositionInSlide == 0 && activeControlSlideIndex != currentSlideIndex) {
      sliderApi.seekTo(activeControlSlideIndex);
    }
  });

  controls.click(function(e) {
    e.preventDefault();
    carouselApi.run(controls.index(this));
  });
});
</script> 
