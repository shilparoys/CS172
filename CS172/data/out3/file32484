       <script>
$(function() {

var api;

api = $('#rcol-tabs > .tabs').tabs($('#rcol-tabs > .panes > .pane'), {
    current: 'active',
    initialIndex: 0,
    onBeforeClick: function(e, i) {
        $('#rcol-tabs > .tabs').removeClass('state' + (this.getIndex() + 1)).addClass('state' + (i + 1));
        $('#rcol-tabs > .panes > .tab_title').removeClass('active').eq(i).addClass('active');
    },
	onClick: function() {
		$(document).trigger("ui_updated");
	}
}).data('tabConfig', { 'initialIndex': 0 }).data('tabs');


});
</script> 
