                  <!-- Pestanyes --> 
                  <script type="text/javascript">
function mostrar(c){ 
 for (i=0; i<=4; i++){
    if (i == c){
	 document.getElementById("m"+i).className = "item_pestanyes_actiu";
         document.getElementById("c"+i).className= "mostrar_div";
    }else{

	 document.getElementById("m"+i).className = "item_pestanyes_inactiu";
         document.getElementById("c"+i).className="amagar_div";
   }
 }
}
</script> 
                  <!-- Consulta catàleg --> 
                  <script type="text/javascript">
function canviaText(op)
	{
		document.getElementById('paraulaClau').style.display="none";
  document.getElementById('titol').style.display="none";
  document.getElementById('autor').style.display="none";
  document.getElementById('materia').style.display="none";
  document.getElementById('isbn').style.display="none";
		switch(op.value)
			{
				case 'X': //paraula clau
				  document.getElementById('paraulaClau').style.display="block";
			  break;
				case 't': //titol
				  document.getElementById('titol').style.display="block";						  
				 break;
				case 'a': //autor
				  document.getElementById('autor').style.display="block";						  
				 break;
				case 'd': //materia
				  document.getElementById('materia').style.display="block";						  
				 break;
				case 'i': //isbn/issn
				  document.getElementById('isbn').style.display="block";						  
				 break;
			}
	}
function submitccuc(lang)
	{
		obj = getObj("cerca");
		obj.action = 'http://puc.cbuc.cat/search*' + lang + '/X?SEARCH=' + obj.searcharg.value + '&TYPE=' + obj.searchtype.value + '&searchscope=23';
		window.location.href = obj.action;
		return false;
	}
</script> 
                  <script type="text/javascript">
function enviar_ccuc()
{
	document.cerca.searchscope.value='23';
	document.cerca.action="http://ccuc-classic.cbuc.cat/search*cat/";
	document.cerca.submit();
}
function enviar_uvic()
{
	document.cerca.searchscope.value='2';
	document.cerca.action="http://pleiades.cbuc.cat/search*cat~S2/";
	document.cerca.submit();
}
</script> 
                  <!-- REvistes electròniques --> 
                  <script type="text/javascript">
function enviar_sfx()
{
document.consulta3.action ="http://sfx.cbuc.cat/uvic/az/default?param_lang_save="+document.consulta3.param_lang_save.value+"&param_perform_value=searchTitle&param_jumpToPage_value=&param_type_value=textSearch&param_chinese_checkbox_active=1&param_pattern_value="+encodeURIComponent(document.consulta3.param_pattern_value.value)+"&param_chinese_checkbox_value=0&param_textSearchType_value=contains";
document.consulta3.submit();
}
function enviar_sfxR()
{
document.consulta5.action = "http://sfx.cbuc.cat/uvic/azbook?param_lang_save="+document.consulta5.param_lang_save.value+"&param_perform_value=ebook&param_type_value=textSearch&param_pattern_value="+encodeURIComponent(document.consulta5.param_pattern_value.value)+"&param_textSearchType_value=contains";
document.consulta5.submit();
}
</script> 
