  <script language="javascript">
	//Detect the browser
		function detectBrowser(){
			var xx;
			var browser=navigator.appName;
				if(browser=="Microsoft Internet Explorer"){
					xx = new ActiveXObject("Microsoft.XMLHTTP");
				}else{
					xx = new XMLHttpRequest();
				}
					return xx;
		}
			var request = detectBrowser();
  </script> 
