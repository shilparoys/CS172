  <script type="text/javascript"><!--

    var formManagers = {}
    function FormManager(formName) {
        formManagers[formName] = this;
        this.formHandlers = [];
        this.ctlHandlers = {};
        this.ctlPostChgHandlers = {};
        this.fieldErrorArea= undefined;
        this.fieldErrorShowType= 1;
        this.formErrorArea= 'errors';
        this.formErrorShowType= 2;
        
    }
    FormManager.prototype = {

        addFormHandler: function(props) {
            this.formHandlers.push(new DefaultFormHandler(props));
        },
        
        addCtlHandler: function(ctlName, props) {
            if (!this.ctlHandlers[ctlName]) this.ctlHandlers[ctlName] = [];
            var ctlHandler = new DefaultCtlHandler(props);
            this.ctlHandlers[ctlName].push(ctlHandler);
        },
        
        setCtlPostChgHandler: function(ctlName, fun) {
            this.ctlPostChgHandlers[ctlName] = fun;
        },
        
        setFieldErrorHandling: function(fieldErrorArea, fieldErrorShowType) {
            this.fieldErrorArea = fieldErrorArea;
            this.fieldErrorShowType = fieldErrorShowType;
        },

        setFormErrorHandling: function(formErrorArea, formErrorShowType) {
            this.formErrorArea = formErrorArea;
            this.formErrorShowType = formErrorShowType;
        },

        arg_ctlValue: function(ctl) {return ctl.value},

        afterErrorCleanCtl: function(ctl) {
           if (ctl.focus) eventFocus(ctl);
           if (ctl.value!=null) ctl.value = "";
        }
    };
    
    function onEventHandler(ctl, ev, type) {
        var formHandler = formManagers[ctl.form.name];
        ev = ev || window.event;
        var ctlHandlerArr = formHandler.ctlHandlers[ctl.name];
        if (!ctlHandlerArr) return true;
        var success = true;
        var failedHandlers = [];
        for (i in ctlHandlerArr) {
            var actCtlHandler = ctlHandlerArr[i];
            if (actCtlHandler[type]) {
                var ret =  actCtlHandler[type](ctl, ev);
                success = ret && success;
                if(!ret) {
                	failedHandlers.push(actCtlHandler);
            	}
            }
        }
        var errorDiv = document.getElementById(formHandler.fieldErrorArea);
        handleErrorMessage(errorDiv, formHandler.fieldErrorShowType, failedHandlers);
        return success;
    }
    function onChangeHandler(ctl, ev) {
         this.inChg = true;
         var success = onEventHandler(ctl, ev, 'onchange');
         var formHandler = formManagers[ctl.form.name];
         if (formHandler.ctlPostChgHandlers[ctl.name]) {
             success = formHandler.ctlPostChgHandlers[ctl.name](ctl, success);
         }
         this.inChg = false;
         return success;
    }

    function onSubmitHandler(form, ev) {
        if (this.inChg) return false;
        var formHandler = formManagers[form.name];
        var errorDiv = document.getElementById(formHandler.formErrorArea);
        if(errorDiv) errorDiv.innerHTML = "";

        var success = true;
        var failedHandlers = [];
        for (ctlName in formHandler.ctlHandlers) {
            var ctl = form[ctlName];
            var ctlHandlerArr = formHandler.ctlHandlers[ctlName];
            if (!ctlHandlerArr) return true;
            var ctlOk=true;
            for (i in ctlHandlerArr) {
                var actCtlHandler = ctlHandlerArr[i];
                if (actCtlHandler.onsubmit) {
						var ret = actCtlHandler.onsubmit(ctl, ev);
                  ctlOk = ret && ctlOk;
                  if(!ret) {
                    	failedHandlers.push(actCtlHandler);
                	}
                }
            }
           	var highlight=document.getElementById(ctlName+"_highlight");
            if (ctlOk) {
              	if (highlight!=null) {
              		highlight.style.display="none";
              	}
            }
            else {
              	if (highlight!=null) {
              		highlight.style.display="";
              	}
            }
            success=ctlOk && success;
        }
        for (i in formHandler.formHandlers) {
        	var actFormHandler = formHandler.formHandlers[i];
            if (actFormHandler.onsubmit) {
				var ret = actFormHandler.onsubmit(form, ev);
                success = ret && success;
                if(!ret) {
               		failedHandlers.push(actFormHandler);
               	}
            }
        }
        handleErrorMessage(errorDiv, formHandler.formErrorShowType, failedHandlers);
        return success;
    }
    
    function msgSubst(ctx, msg, args) {
        if (args) {
            for(i in args) {
                var val = (typeof args[i] == "function") ? args[i](ctx) : args[i];
                msg = msg.replace(new RegExp("\\{"+i+"\\}"), val);
            }
        }
        return msg;
    }
    
    function handleErrorMessage(div, type, failedHandlers) {
        var msg2show = "";
        var firstCtl;
        var firstHandler;
        for (var i in failedHandlers) {
            errMsg = failedHandlers[i].errorMessage;
            msg2show += errMsg.message + "\n";
            if(!firstCtl) {
            	firstCtl = errMsg.ctl;
            	firstHandler = failedHandlers[i];
            }
            if(type==1) break;
        }

		if(msg2show.length>0) {        
	        if(div) {
		        div.innerHTML = msg2show.replace(/\n/g, "<br>");
	        }
	        else {
	            alert(msg2show);
	        }
	        
	        if(firstCtl) {
	            if (firstHandler.afterError) {
	                firstHandler.afterError(firstCtl);
	            } else {
	                if (firstCtl.focus) eventFocus(firstCtl);
    	            if (firstCtl.select) firstCtl.select();
    	        }
	        }
	    }
    }
    
    function checkMandatory(str) {
        return isNotBlank(str);
    }

    function DefaultCtlHandler(props) {
        this.cond = props.cond;
        if (this.cond==isNotEmpty) this.cond = checkMandatory;
        this.when = props.when;
        this.msg = props.msg;
        this.msgArgs = props.msgArgs;
        this.afterError = props.afterError;
        this.errorMessage = [];
    }
    DefaultCtlHandler.prototype = {
        handleChangeError: function(ctl, msg, args) {
            this.errorMessage["message"] = msgSubst(ctl, msg, args);
            this.errorMessage["ctl"] = ctl;
        },
        handleChange: function(ctl, isFinal) {
            if (isFinal && this.when=="field") return true;
            if (!isFinal && this.when=="form") return true;
            if (this.cond!=checkMandatory && isEmpty(ctl.value)) return true;
            if (!this.cond(ctl.value, ctl)) {
                this.handleChangeError(ctl, this.msg, this.msgArgs);
                return false;
            }
            return true;
        },
        
        onchange: function(ctl, ev) { return this.handleChange(ctl, false) },
        onsubmit: function(ctl, ev) { return this.handleChange(ctl, true) }
    };
    
    function DefaultFormHandler(props) {
        this.cond = props.cond;
        this.msg = props.msg;
        this.msgArgs = props.msgArgs;
        this.ctl = props.ctl;
        this.errorMessage = [];
    }
    DefaultFormHandler.prototype = {
        handleSubmitError: function(form, msg, args) {
            this.errorMessage["message"] = msgSubst(form, msg, args);
            this.errorMessage["ctl"] = eval(this.ctl);
        },
        
        onsubmit: function(form, ev) {
            if (!this.cond(form)) {
                this.handleSubmitError(form, this.msg, this.msgArgs);
                return false;
            }
            return true;
        }
    };
    
    function confirmEmail(ctl, msg, msgArgs) {
        if (isConfirmEmail(ctl.value)) {
            if (confirm(msgSubst(ctl, msg, msgArgs))) {
                ctl.select();
                eventFocus(ctl);
                return false;
            } else {
                return true;
            }
        }
        return true;
    }
    
    function disablePaste(event) {

	 var pressedKey = String.fromCharCode(event.keyCode).toLowerCase();

	  if (event.ctrlKey && (pressedKey == "v" ||pressedKey == "c")) {
	    // disable key press porcessing
    	 event.returnValue = false;
    	 return false;
	  }

	}
	
	function correctEmail(element){
	  var email = element.value;
	  email = email.replace(/[@]+[@]*/g,"@");
	  email = email.replace(/[.]+[.]*/g,".");
 	  email = email.replace(",com",".com");
 	  if(email.indexOf('@')>0 &&  email.indexOf('.',email.indexOf('@')+1)<0){
 	    email = email+".com";
 	  }
 	  

 	  element.value =email;
 	  return true;
	}
	

    
//--></script> 
