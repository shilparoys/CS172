    function isEmpty(str) { return str==null || str.length==0 }
    function isNotEmpty(str) { return !isEmpty(str) }
    
    function isBlank(str) { return str==null || /^\s*$/.test(str) }
    function isNotBlank(str) { return !isBlank(str) }

    function isAlpha(str) {
        if (str==null) return false;
        return /^[A-Za-z]+?[A-Za-z\s]*$/.test(str);
    }
    
    function isMoneyAmount(srcNumber){
      var txtNumber = '' + srcNumber;
	  var arrNumber = txtNumber.split('.');

	  if (isNaN(txtNumber) || txtNumber == "" || (arrNumber.length == 2 && arrNumber[1].length >2 )) {
		return false;
	  }
	  
	  return true;
	}  
          

    function isDigits(str) {
        if (str==null) return false;
        return /^[0-9]+$/.test(str);
    }

    function isNumber(str) {
        if (str==null) return false;
        return /^[0-9]+\.?[0-9]*$/.test(str);
    }

    function isRange(low, high, str) {
        if (!isDigits(str)) return false;
        var val = parseInt(str);
        return low<=val && val<=high;
    }
    function isRangeFun(low, high) {
        return function(str) {
            return isRange(low, high, str);
        }
    }
    
    function isGreater(low, str) {
        if (!isNumber(str)) return true;
        var val = parseFloat(str);
        return low<=val;
    }

    function isLower(high, str) {
        if (!isNumber(str)) return true;
        return parseFloat(str)<=parseFloat(high);
    }
    
    function isDoubleRange(low, high, str) {
        if (!isNumber(str)) return false;
        var val = parseFloat(str);
        return low<=val && val<=high;
    }
    function isDoubleRangeFun(low, high) {
        return function(str) {
            return isDoubleRange(low, high, str);
        }
    }

    function isLengthRange(low, high, str) {
        if (low!=null && str.length<low) return false;
        if (high!=null && str.length>high) return false;
        return true;
    }
    function isLengthRangeFun(low, high) {
        return function(str) {
            return isLengthRange(low, high, str);
        }
    }
    
    function isEmail(str) {
        return _isUsecEmail(str);
    }
    
    function _isUsecEmail(str) { // USEC-specific validation
        if (!_isPh2Email(str)) return false;
        if (!/@/.test(str)) return false; // must have an @
        if (/@.*@/.test(str)) return false; // address cannot have multiple @s
        if(/.*[\(\)\<\>\,\;\:\/\'\"\[\]].*/.test(str)) return false; // cannot have embedded ()<>,;:\/"''[]
        if (!/\./.test(str)) return false; // must have a "."
        if (/\.(000|ooo|OOO)$/.test(str)) return false; // E-mail address cannot have extenstions that contain:
                                                             // Numeric 0: @avon.000, @AVON.000, Alpha o: @avon.ooo, @avon.OOO
		var localPart = str.substr(0, str.indexOf("@"));
        if(localPart.length==0)return false;
        if(localPart.search(/[^A-Za-z0-9\.\!\#\$\%\&\'\*\+\-\/\=\?\^\_\`\{\|\}\~]/)!=-1) return false; // any other characters are forbidden in localpart
        return true;
    }
    
    function _isPh2Email(str) { // Phase 2 email validation
        // (this is not a restriction): // There can be any number of dots or dashes in the first segment.
        if(str.search(/^[-.]|[-.]$/)!=-1) return false; // Cannot be started or ended by dot or dash.
        if(str.search(/[-.][-.]/)!=-1) return false; // There cannot be two or more dots or dashes next to each other.
        if(str.search(/\.@|@\./)!=-1) return false; // There cannot be a dot before or after the @.
        if(str.search(/\.[A-Za-z]{2,6}$/)==-1) return false; // The last segment check. can be 2-6 char
        if(str.search(/''/)!=-1) return false; // ' accepted but cannot be duplicated.
        // (this is not a restriction): // Numbers are also accepted.
        if(str.search(/@.*@/)!=-1) return false; // There can be only one @ sign.
        if(str.search(/\s/)!=-1) return false; // Cannot contain any SPACE characters.
        return true;
    }
    
    function isConfirmEmail(str) {
        if (str == null) return false;
        return /@(avon.com|youravon.com|avon.net)$/.test(str);
    }

    function isAlphanumeric(str) { 
        if (str==null) return false;
        return /^[A-Za-z0-9]+$/.test(str) && !/^[ *]+$/.test(str); // any other characters are forbidden
    }
    
    function isAlphanumericWS(str) { 
        if (str==null) return false;
        return /^[A-Za-z0-9\s]+$/.test(str) && !/^[\s]+$/.test(str); // any other characters are forbidden
    }

    function isAllowedChar(str) { 
        if (str==null) return false;
        return /^[A-Za-z0-9-#.\'\s\u00D3\u00E1\u00E9\u00ED\u00F3\u00F6\u00FA\u00FC\u00C1\u00DA\u00CD\u00DC\u00C9\u00F1\u00D1]+$/.test(str); // any other characters are forbidden
    }

    
    function _focusFun(ctl) {
		if (ctl.focus) ctl.focus();
    }
    function eventFocus(ctl) {
        setTimeout('_focusFun(document.' + ctl.form.name + '["' + ctl.name + '"])', 1);
    }

    // check Esc, Tab, Backspace, Ins, Del, Home, ..., F1, ..., arrow, win, etc. keys
    function isSpecialKey(k)
    {
        return k==8 || k==9 || k==27 || k==91 || k==93 || (111<=k && k<=123) || (33<=k && k<=40) || k==45 || k==46;
    }
    
	function isCreditCard(cards, cardType, cardNumber) {
		for(var i=0; i<cards.length; i++) {
			var card = cards[i];
			if(card.vendor == cardType) {
				for(var j=0; j<card.ranges.length; j++) {
					var range = card.ranges[j]
					if(range.low <= cardNumber && range.high >= cardNumber) {
						var mod10 = true;
						if(range.mod10) {
							mod10 = checkMod10(cardNumber);
						}
						return mod10 && cardNumber.length == range.length;
					}
				}
			}
		}
		return false;
	}

	function z(digit) {
		if (digit == 0) return 0;
		return (digit*2-1) % 9 + 1;
	}

	function checkMod10(number) {
		checksum = 0;
		for (place = 0; place < 16; place++) {
			digit = number % 10;
			number = Math.floor(number/10);
	
			if ((place & 1) == 0) // even position, just add digit
				checksum += digit;
			else // odd position, must double and add
				checksum += z(digit);

			if (number == 0)
				break;
		}

		// good checksum should be 0 mod 10
		return ((checksum % 10) == 0);
	}
    
