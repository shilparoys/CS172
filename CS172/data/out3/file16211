if (s.pageName=="login.page" && location.search!=null && location.search.indexOf("timeout=yes")!=-1) {
    if (location.search.indexOf("realtimeout=yes")!=-1) {
        s.pageName = "login_real_timeout.page"
        if (location.search.indexOf("starcount")!=-1 && location.search.indexOf("endcount")!=-1 &&location.search.indexOf("pagename")!=-1){
        	s.prop2=getParamValue(location.search,"starcount");
        	s.prop3=getParamValue(location.search,"endcount"); 
       		s.prop4=getParamValue(location.search,"pagename");        
        }  
    } if (location.search.indexOf("collision=yes")!=-1) {
     	s.pageName = "login_session_timeout_collision.page";
    } else if (location.search.indexOf("not_ya=yes")!=-1){ 
        s.pageName = "login_session_timeout_previous_not_ya.page";
    	s.prop1=getParamValue(location.search,"previous_page");
    } else { //(location.search.indexOf("unknown=yes")!=-1)
        s.pageName = "login_session_timeout_unknown.page";
    	s.prop1=getParamValue(location.search,"previous_page");
    }
}


