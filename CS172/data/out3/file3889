'use strict';

YUI.add('validation-constraint', function(Y){
  var validationConstraint;

  validationConstraint = {

    USER_NAME_MIN_LENGTH : 4,
    MOBILE_NUMBER_MIN_LENGTH : 2,

    MIN_AGE : 0,
    MAX_AGE : 150,
    CONTAINED_WORD_LENGTH_THRESHOLD   : 4,

    WATCH_LIST_ENABLED                : false,

    LOG_IN_URL                        : 'http://login.yahoo.com',
    USER_NAME_SUGGESTION_URL          : '/reg_json?PartnerName=yahoo_default&',
    USER_NAME_AVAILABLE_URL           : '/reg_json?PartnerName=yahoo_default&',
    WATCH_LIST_URL                    : 'http://watchlist.yahoo.com?',

    CODE_MIN_LENGTH : 4,

    PASSWORD_SCORE_THRESHOLD : {
      WEAKEST   : 5,
      WEAKER    : 10,
      WEAK      : 12,
      STRONG    : 16,
      STRONGER  : 17
    }
  };

  Y.namespace('Validator');
  Y.Validator.ValidationConstraint = validationConstraint;

});
'use strict';

YUI().use('enter-information-validator', 'show-hide-password', 'password-meter', 'user-name-suggestions-mobile', 'show-date-fields', 'tooltip-use', function(Y){
      var passwordNode = Y.one('#password');
      var passwordMeter = new Y.Widgets.PasswordMeter({
        passwordNode: passwordNode,
        validationFunctions : new Y.Validator.ValidationFunctions(),
        passwordScoreThreshold : Y.Validator.ValidationConstraint.PASSWORD_SCORE_THRESHOLD,
        fields : Y.Validator.Fields
      });

      passwordMeter.init();
      if(!Y.one('html').hasClass('old-android')) {
        Y.use('relative-country-code-drop-down');
        Y.use('pseudo-domain-drop-down', 'pseudo-general-drop-down');
	Y.use('mobile-number-formatter');
      }
    });
