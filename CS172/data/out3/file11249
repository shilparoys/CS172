	jQuery( function( $ ) {
		var tmpLoc = $( location ).attr( 'href' );
		if ( tmpLoc.indexOf( '?' ) > 0 ) {
			$( '<input type="button" class="adel-custom-search-reset" value="Reset Search Form"/>' ).insertAfter( '.adel-custom-search-submit' ).click( function() {
				tmpLoc = tmpLoc.split( '?' ).shift();
				$( location ).attr( 'href', tmpLoc );
			} );
		}
		$( '.reset-adel-form' ).click( function() {
			$( '#adel-text-query' ).attr( 'value', '' ).val( '' );
			$( '#adel-group-select' ).val( '' );
			/*$( '#adel-group-select option:selected' ).removeAttr( 'selected' );*/
			$( '#adel-group-select option[value=""]' ).attr( 'selected', 'selected' );
			$( '#adel-text-query' ).focus();
		} );
	} );
</script> 
