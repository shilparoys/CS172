connect(window, "onDOMload", function(e){
  connectEach($$("label"), "onclick", function(e){
    var forattr = getNodeAttribute(e.src(), "for");
    if(forattr && $(forattr)){
      $(forattr).click()
    } else {
      var firstChildInput = getFirstElementByTagAndClassName("input", null, e.src());
      if(firstChildInput){
        firstChildInput.click()
      }
    }
  });
});
