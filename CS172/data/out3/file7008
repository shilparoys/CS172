                  <script>

(function(){
	var libs = [
		'http://unaab.edu.ng/modules/mod_btslideshow/assets/js/jquery-1.6.2.min.js', 
		'http://unaab.edu.ng/modules/mod_btslideshow/assets/skitter/js/jquery.skitter.min.js', 
