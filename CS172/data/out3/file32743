        jQuery(document).ready(function() {

            jQuery(".quicklinks").click(function(e) {
				e.preventDefault();
                jQuery("#block-block-53").toggle();
				jQuery(".quicklinks").toggleClass("menu-open");
            });

			jQuery("div#block-block-53").mouseup(function() {
				return false
			});
			jQuery(document).mouseup(function(e) {
				if(jQuery(e.target).parent("a.quicklinks").length==0) {
					jQuery(".quicklinks").removeClass("menu-open");
					jQuery("div#block-block-53").hide();
				}
			});

        });

